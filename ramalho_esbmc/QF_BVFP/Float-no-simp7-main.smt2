(set-info :smt-lib-version 2.6)
(set-logic QF_BVFP)
(set-info :source |ESBMC floating-point test cases contributed by Mikhail Ramalho.|)
(set-info :category "crafted")
(set-info :status unsat)

(declare-fun |c::main::main::1::g@1!0&0#1| () (_ FloatingPoint 8 24))
(declare-fun |c::main::main::1::target@1!0&0#1| () (_ FloatingPoint 8 24))
(declare-fun |c::main::main::1::result@1!0&0#1| () (_ FloatingPoint 8 24))
(declare-fun |c::main::$tmp::tmp$1@1!0&0#1| () Bool)
(declare-fun |c::argc'&0#0| () (_ BitVec 32))
(declare-fun |execution_statet::__guard_exec@0!0| () Bool)
(assert (= (fp.neg (fp #b0 #x4d #b00111100100100000001010))
   |c::main::main::1::g@1!0&0#1|))
(assert (= (fp.neg (fp #b0 #x00 #b00000000000000000000001))
   |c::main::main::1::target@1!0&0#1|))
(assert (= (fp.mul roundNearestTiesToEven
           (fp #b0 #x1a #b10011110000011000010001)
           |c::main::main::1::g@1!0&0#1|)
   |c::main::main::1::result@1!0&0#1|))
(assert (= (fp.eq |c::main::main::1::result@1!0&0#1| |c::main::main::1::target@1!0&0#1|)
   |c::main::$tmp::tmp$1@1!0&0#1|))
(assert (let ((a!1 (=> |execution_statet::__guard_exec@0!0|
               (=> (and (bvsge |c::argc'&0#0| #x00000001)
                        (bvsle |c::argc'&0#0| #x7ffffffe)
                        (not |c::main::$tmp::tmp$1@1!0&0#1|))
                   false))))
(let ((a!2 (=> (and true
                    (bvsge |c::argc'&0#0| #x00000001)
                    (=> (bvsge |c::argc'&0#0| #x00000001)
                        (bvsle |c::argc'&0#0| #x7ffffffe)))
               a!1)))
  (or (not a!2)))))

(check-sat)
(exit)
