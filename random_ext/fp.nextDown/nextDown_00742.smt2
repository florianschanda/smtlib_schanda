(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF0DCF595)))
;; x should be Float32(0xF0DCF595 [Rational(-547068740952859574755133489152), -547068740952859574755133489152.000000])

(declare-const result Float32)
(assert (= result (fp.nextDown x)))
(assert (= result (fp #b1 #b11100001 #b10111001111010110010110)))
(check-sat)
(exit)
