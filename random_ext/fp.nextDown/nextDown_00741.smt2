(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01000001 #b01010011101100010111010)))
;; x should be Float32(0xA0A9D8BA [Rational(-5565533, 19342813113834066795298816), -0.000000])

(declare-const result Float32)
(assert (= result (fp.nextDown x)))
(assert (not (= result (fp #b1 #b01000001 #b01010011101100010111011))))
(check-sat)
(exit)
