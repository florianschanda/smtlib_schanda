  y), (x * 256), y)) and ((not ((0 <  x) and (0 <  y))) -> ((((x <  0) and
  (y <  0)) -> pos_div_relation(fxp_div(x, y), ((-x) * 256), (-y))) and
  ((not ((x <  0) and (y <  0))) -> ((((x <  0) and (0 <  y)) ->
  pos_div_relation((-fxp_div(x, y)), ((-x) * 256), y)) and
  ((not ((x <  0) and (0 <  y))) -> (((0 <  x) and (y <  0)) ->
  pos_div_relation((-fxp_div(x, y)), (x * 256), (-y)))))))))))))

