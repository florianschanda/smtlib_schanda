  (forall number_list:(int,bool) farray.
  (forall low:int. forall high:int [has_true(number_list, low, high)].
  ((has_true(number_list, low, high) = true) <->
  (exists v:int. (((low <= v) and (v <= high)) and
  ((number_list[v]) = true))))))

