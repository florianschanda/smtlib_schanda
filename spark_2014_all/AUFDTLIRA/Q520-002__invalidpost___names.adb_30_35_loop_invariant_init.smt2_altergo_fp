  forall result:int. forall state1:int. forall result1:int.
  forall label_length1:int. forall result2:int. forall i1:int.
  (dynamic_property(1, 2147483647, to_rep1(first(name1)),
  to_rep1(last(name1))) -> (((0 <= 2) -> in_range5(state)) -> (((0 <= 255) ->
  in_range6(label_length)) -> (((0 <= 255) -> in_range3(ch)) -> ((({
  int__content = result } = { int__content = state }) and (state1 = 0)) ->
  ((({ int__content = result1 } = { int__content = label_length }) and
  (label_length1 = 0)) -> ((not ((not (length({ elts = name; rt = name1
  }) <  1)) -> (255 <  length({ elts = name; rt = name1 })))) -> ((({
  int__content = result2 } = { int__content = i }) and
  (i1 = to_rep1(first(name1)))) -> (((to_rep1(first(name1)) <= i1) and
  (i1 <= to_rep1(last(name1)))) ->
  (label_length1 <  ((i1 - to_rep1(first(name1))) + 1))))))))))))
