  time_idle) = true) -> (((((10 <= time_idle) and ((result = status) and
  (status1 = of_int(1)))) or ((not (10 <= time_idle)) and
  (((result1 = status) and (status2 = of_int(0))) and
  (status1 = status2)))) and ((status1 = status3) and
  (status4 = status1))) -> ((((always_set_until_now(9,
