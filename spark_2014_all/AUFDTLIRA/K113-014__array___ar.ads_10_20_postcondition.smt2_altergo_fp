 (* File "system.ads", line 1, characters 0-0 *)
goal WP_parameter_def :
  (forall x:(int,integer) farray. forall o:integer. forall o1:int.
  forall o2:(int,integer) farray. forall result:(int,integer) farray.
  forall x1:(int,integer) farray. forall x2:(int,integer) farray.
  forall x3:(int,integer) farray. ((in_range(y) and (((1 <= y) and
  (y <= 10)) and ((((to_rep(o) = 5) and ((((1 <= y) and (y <= 10)) and
  (o1 = y)) and (o2 = (x[o1 <- o])))) and (({ map__content = result } = {
  map__content = x }) and (x1 = o2))) and ((x1 = x2) and (x3 = x1))))) ->
  (to_rep((x2[y])) = 5)))
