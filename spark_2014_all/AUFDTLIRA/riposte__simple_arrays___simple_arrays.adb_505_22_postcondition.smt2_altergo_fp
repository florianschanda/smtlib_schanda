goal WP_parameter_def :
  (forall a:(int,enum_t) farray. forall b:(int,enum_t) farray.
  forall tmp:int. forall o:enum_t. forall o1:(int,enum_t) farray.
  forall o2:enum_t. forall o3:(int,enum_t) farray. forall result:int.
  forall tmp1:int. forall result1:(int,enum_t) farray.
  forall a1:(int,enum_t) farray. forall result2:(int,enum_t) farray.
  forall b1:(int,enum_t) farray. forall a2:(int,enum_t) farray.
  forall b2:(int,enum_t) farray. forall tmp2:int.
  forall a3:(int,enum_t) farray. forall b3:(int,enum_t) farray.
  forall tmp3:int. (((((0 <= 2) -> in_range1(tmp)) and (((result = tmp) and
  (tmp1 = to_rep((a[0])))) and ((((to_rep(o) = to_rep((b[1]))) and
  (o1 = (a[0 <- o]))) and ((result1 = a) and (a1 = o1))) and
  (((to_rep(o2) = tmp1) and (o3 = (b[1 <- o2]))) and ((result2 = b) and
  (b1 = o3)))))) and (((tmp2 = tmp1) and ((b1 = b2) and (a1 = a2))) and
  ((tmp3 = tmp1) and ((b3 = b1) and (a3 = a1))))) -> (not (bool_eq3(a2, 0, 2,
  a, 0, 2) = true))))
