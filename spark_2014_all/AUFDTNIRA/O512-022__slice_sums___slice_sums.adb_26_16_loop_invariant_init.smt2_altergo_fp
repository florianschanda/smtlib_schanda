logic slide : (t,component_type) farray, t, t -> (t,component_type) farray

axiom slide_eq :
  (forall a:(t,component_type) farray.
  (forall first:t [slide(a, first, first)]. (slide(a, first, first) = a)))

axiom slide_def :
  (forall a:(t,component_type) farray.
  (forall old_first:t.
  (forall new_first:t.
  (forall i:t [(slide(a, old_first, new_first)[i])]. ((slide(a, old_first,
  new_first)[i]) = (a[sub(i, sub(new_first, old_first))]))))))

logic concat : (t,component_type) farray, t, t, (t,component_type) farray, t,
  t -> (t,component_type) farray

axiom concat_def :
  (forall a:(t,component_type) farray. forall b:(t,component_type) farray.
  (forall a_first:t. forall a_last:t. forall b_first:t. forall b_last:t.
  (forall i:t [(concat(a, a_first, a_last, b, b_first, b_last)[i])].
  (((le(a_first, i) and le(i, a_last)) -> ((concat(a, a_first, a_last, b,
  b_first, b_last)[i]) = (a[i]))) and (gt(i, a_last) -> ((concat(a, a_first,
  a_last, b, b_first, b_last)[i]) = (b[add(sub(i, a_last), sub(b_first,
  one))])))))))

