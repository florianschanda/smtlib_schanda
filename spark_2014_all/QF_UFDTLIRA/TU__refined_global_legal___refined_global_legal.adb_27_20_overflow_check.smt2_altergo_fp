goal WP_parameter_def :
  (forall var:int. forall var2:int. forall var3:int. forall var4:int.
  forall pr_state:us_private. forall o:int. forall o1:int. forall o2:int.
  forall result:bool. forall result1:int. forall var1:int.
  forall result2:int. forall var21:int. forall result3:int. forall var31:int.
  ((((- 2147483648) <= 2147483647) -> in_range(var)) ->
  ((((- 2147483648) <= 2147483647) -> in_range(var2)) ->
  ((((- 2147483648) <= 2147483647) -> in_range(var3)) -> (in_range(var4) ->
  ((result = priv_f(pr_state)) -> ((not (result = true)) ->
  ((((o = (1 - var4)) and in_range((1 - var4))) and ((result1 = var) and
  (var1 = o))) -> ((((o1 = (2 - var4)) and in_range((2 - var4))) and
  ((result2 = var2) and (var21 = o1))) -> ((((o2 = (3 - var4)) and
  in_range((3 - var4))) and ((result3 = var3) and (var31 = o2))) ->
  in_range((4 - var4))))))))))))
