goal WP_parameter_def :
  (forall c:int. forall o:int. forall o1:int. forall c1:int.
  forall result:int. forall c2:int. forall result1:int. forall c3:int.
  forall result2:int. forall c4:int. (in_range(a) -> (in_range(b) ->
  (in_range1(c) -> ((((c <= (a + b)) and ((({ int__content = result } = {
  int__content = c }) and (c2 = a)) and ((((o = (c2 + b)) and
  in_range1((c2 + b))) and ((c2 = result1) and (c3 = o))) and
  (((o1 = (c3 + 1)) and in_range1((c3 + 1))) and ((result2 = c3) and
  (c4 = o1)))))) or ((not (c <= (a + b))) and ((c4 = c) and (c4 = c1)))) ->
  ((a + b) <  c4))))))
