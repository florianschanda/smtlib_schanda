goal WP_parameter_def :
  (forall x:int. forall i:int. forall result:int. forall x1:int.
  forall result1:int. forall i1:int. forall x2:int. forall i2:int.
  forall j:int. forall result2:int. forall x3:int. forall result3:int.
  forall j1:int. ((({ int__content = result } = { int__content = x }) and
  (x1 = 0)) -> (in_range1(x1) -> ((({ int__content = result1 } = {
  int__content = i }) and (i1 = 1)) -> (((1 <= i1) and (i1 <= 10)) ->
  ((x2 = ((i2 - 1) * 10)) -> ((((((-2147483648) <= 2147483647) ->
  in_range1(x2)) and ((1 <= 10) -> in_range2(i2))) and ((1 <= i2) and
  (i2 <= 10))) -> (((x2 = result2) and (x3 = 0)) -> (((j = result3) and
  (j1 = 1)) -> (((1 <= j1) and (j1 <= 10)) ->
  (x3 = (i2 * (j1 - 1)))))))))))))
