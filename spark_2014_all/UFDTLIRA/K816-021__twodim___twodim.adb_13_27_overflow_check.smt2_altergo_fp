  (forall o:int. forall o1:int. forall o2:int.
  (dynamic_property2((-2147483648), 2147483647, to_rep(first(m1)),
  to_rep(last(m1)), (-2147483648), 2147483647, to_rep(first1(m2)),
  to_rep(last1(m2))) -> ((((o = length_2({ elts = m; rt = m1; rt_2 = m2
  })) and in_range(length_2({ elts = m; rt = m1; rt_2 = m2 }))) and
  (((o1 = length({ elts = m; rt = m1; rt_2 = m2 })) and in_range(length({
  elts = m; rt = m1; rt_2 = m2 }))) and (o2 = (o1 + o)))) -> in_range(o2))))
