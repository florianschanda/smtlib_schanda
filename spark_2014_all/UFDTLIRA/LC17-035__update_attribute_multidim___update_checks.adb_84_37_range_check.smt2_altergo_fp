axiom Select_neq :
  (forall m:map.
  (forall i:int. forall i2:int.
  (forall j:int. forall j2:int.
  (forall k:int. forall k2:int.
  (forall a:et1 [get(set(m, i, j, k, a), i2, j2, k2)| set(m, i, j, k, a),
