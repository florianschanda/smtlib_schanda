  (((o1 = (z2 + 1)) and in_range2((z2 + 1))) -> (((o2 = (y2 + 1)) and
  in_range1((y2 + 1))) -> (((o3 = (x2 + 1)) and in_range((x2 + 1))) ->
  (((o4 = (z1 - 1)) and in_range2((z1 - 1))) ->
