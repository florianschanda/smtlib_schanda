goal WP_parameter_def :
  (forall o:int. forall o1:int. forall o2:int. (in_range1(i1) ->
  (in_range1(i2) -> (((((o = addthree(i1, i2, i1)) and (in_range(o) and
  (o = ((i1 + i2) + i1)))) and (o1 = ((i1 + i2) + o))) and ((o2 = o1) and
  in_range(o1))) -> in_range2(o2)))))
