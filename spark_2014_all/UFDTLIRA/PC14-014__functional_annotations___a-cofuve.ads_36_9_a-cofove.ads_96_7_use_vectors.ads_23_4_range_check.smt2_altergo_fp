  (forall x:int. forall f:int. forall l:int [id(x, f, l)]. (((in_range(x) and
  in_range(f)) and in_range(l)) -> ((((f <= x) and (x <= l)) -> ((f <= id(x,
  f, l)) and (id(x, f, l) <= l))) and in_range(id(x, f, l)))))

