logic user_eq : example_pt, example_pt -> bool

logic value__size : int

logic object__size : example_pt -> int

logic value__alignment : int

logic object__alignment : example_pt -> int

axiom value__size_axiom : (0 <= value__size)

axiom object__size_axiom : (forall a:example_pt. (0 <= object__size(a)))

axiom value__alignment_axiom : (0 <= value__alignment)

axiom object__alignment_axiom :
  (forall a:example_pt. (0 <= object__alignment(a)))

