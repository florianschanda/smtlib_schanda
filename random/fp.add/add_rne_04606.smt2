(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00001110010010110101010)))
;; x should be Float32(0x4A0725AA [Rational(4428501, 2), 2214250.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11011000100111101011111)))
;; y should be Float32(0x4AEC4F5F [Rational(15486815, 2), 7743407.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result ((_ to_fp 8 24) #x4B17F11A)))
(check-sat)
(exit)
