(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11011001101110110000001)))
;; x should be Float32(0xCAECDD81 [Rational(-15523201, 2), -7761600.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10100000101010011111100)))
;; y should be Float32(0x805054FC [Rational(-1316159, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #xCAECDD82)))
(check-sat)
(exit)
