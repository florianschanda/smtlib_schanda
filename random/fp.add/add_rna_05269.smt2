(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A84E329)))
;; x should be Float32(0x4A84E329 [Rational(8708905, 2), 4354452.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01000100110001101010100)))
;; y should be Float32(0x49A26354 [Rational(2660565, 2), 1330282.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x4AAD7BFE))))
(check-sat)
(exit)
