(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAF6FEEF)))
;; x should be Float32(0xCAF6FEEF [Rational(-16187119, 2), -8093559.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111001 #b00101111000110110110000)))
;; y should be Float32(0x7C978DB0 [Rational(6295284716395810943690383775030575104), 6295284716395810943690383775030575104.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x7C978DB0))))
(check-sat)
(exit)
