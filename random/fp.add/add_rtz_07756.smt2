(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10111010 #b00001010001001001111001)))
;; x should be Float32(0xDD051279 [Rational(-599303724845760512), -599303724845760512.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x803CBA4F)))
;; y should be Float32(0x803CBA4F [Rational(-3979855, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xDD051279)))
(check-sat)
(exit)
