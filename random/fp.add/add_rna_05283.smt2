(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A84F70F)))
;; x should be Float32(0x4A84F70F [Rational(8713999, 2), 4356999.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111100000110110001001)))
;; y should be Float32(0x007E0D89 [Rational(8261001, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b0 #b10010101 #b00001001111011100001111)))
(check-sat)
(exit)
