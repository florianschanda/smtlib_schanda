(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01111000001101110111000)))
;; x should be Float32(0x003C1BB8 [Rational(492407, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x83C25F27)))
;; y should be Float32(0x83C25F27 [Rational(-12738343, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b00000111 #b10000010110111010111000)))
(check-sat)
(exit)
