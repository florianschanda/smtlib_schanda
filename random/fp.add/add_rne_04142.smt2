(assert (= y (fp #b1 #b10011010 #b01111110011100011100001)))
(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b10011010 #b01111110011100011100001)))
(check-sat)
(exit)
