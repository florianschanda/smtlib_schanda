(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802EBB01)))
;; x should be Float32(0x802EBB01 [Rational(-3062529, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00101100011010100101110)))
;; y should be Float32(0xCA16352E [Rational(-4922007, 2), -2461003.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result ((_ to_fp 8 24) #xCA16352E)))
(check-sat)
(exit)
