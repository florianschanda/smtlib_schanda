(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x003B4992)))
;; x should be Float32(0x003B4992 [Rational(1942729, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010101 #b11011111001000011010110)))
;; y should be Float32(0xEAEF90D6 [Rational(-144808619632330687754797056), -144808619632330687754797056.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #xEAEF90D5)))
(check-sat)
(exit)
