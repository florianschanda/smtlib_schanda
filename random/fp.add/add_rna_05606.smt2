(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00010010001000000100110)))
;; x should be Float32(0xCA091026 [Rational(-4491283, 2), -2245641.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9A8AFBCD)))
;; y should be Float32(0x9A8AFBCD [Rational(-9108429, 158456325028528675187087900672), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b10010100 #b00010010001000000100110))))
(check-sat)
(exit)
