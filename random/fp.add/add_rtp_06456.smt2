(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xED1F34C6)))
;; x should be Float32(0xED1F34C6 [Rational(-3079494733298589677129826304), -3079494733298589677129826304.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11001000 #b00100001000110100111010)))
;; y should be Float32(0xE4108D3A [Rational(-10666030371688689958912), -10666030371688689958912.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b1 #b11011010 #b00111110011010011101010))))
(check-sat)
(exit)
