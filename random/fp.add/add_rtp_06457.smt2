(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF20158D0)))
;; x should be Float32(0xF20158D0 [Rational(-2561979775443714439620822302720), -2561979775443714439620822302720.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8032E37E)))
;; y should be Float32(0x8032E37E [Rational(-1667519, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xF20158D0))))
(check-sat)
(exit)
