(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A9DC9AD)))
;; x should be Float32(0x4A9DC9AD [Rational(10340781, 2), 5170390.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00000100010011110011110)))
;; y should be Float32(0x4A02279E [Rational(4264911, 2), 2132455.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x4ADEDD7C)))
(check-sat)
(exit)
