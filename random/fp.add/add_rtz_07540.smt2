(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11110011111011010111011)))
;; x should be Float32(0xCAF9F6BB [Rational(-16381627, 2), -8190813.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00010111010001000010101)))
;; y should be Float32(0xCA8BA215 [Rational(-9150997, 2), -4575498.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xCB42CC68)))
(check-sat)
(exit)
