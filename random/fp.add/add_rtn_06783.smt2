(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x07EFFBD4)))
;; x should be Float32(0x07EFFBD4 [Rational(3931893, 10889035741470030830827987437816582766592), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00100001 #b11110100101100011110101)))
;; y should be Float32(0x10FA58F5 [Rational(16406773, 166153499473114484112975882535043072), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b0 #b00100001 #b11110100101100100110000))))
(check-sat)
(exit)
