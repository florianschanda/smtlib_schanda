(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01110111101101111000000)))
;; x should be Float32(0x803BDBC0 [Rational(-61295, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A9A5637)))
;; y should be Float32(0x4A9A5637 [Rational(10114615, 2), 5057307.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b0 #b10010101 #b00110100101011000110110)))
(check-sat)
(exit)
