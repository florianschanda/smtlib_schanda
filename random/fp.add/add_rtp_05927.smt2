(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A21E672)))
;; x should be Float32(0x4A21E672 [Rational(5305145, 2), 2652572.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010101 #b11011100110111101101001)))
;; y should be Float32(0x6AEE6F69 [Rational(144125232328004006850854912), 144125232328004006850854912.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #x6AEE6F6A)))
(check-sat)
(exit)
