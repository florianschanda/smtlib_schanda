(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00100001110001110010010)))
;; x should be Float32(0xCA10E392 [Rational(-4747721, 2), -2373860.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF0FFC944)))
;; y should be Float32(0xF0FFC944 [Rational(-633295941720850944998477135872), -633295941720850944998477135872.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b1 #b11100001 #b11111111100100101000100)))
(check-sat)
(exit)
