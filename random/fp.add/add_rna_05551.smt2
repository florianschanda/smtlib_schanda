(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10111101 #b10000110101001001110000)))
;; y should be Float32(0x5EC35270 [Rational(7037217465394266112), 7037217465394266112.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b0 #b10111101 #b10000110101001001110000))))
(check-sat)
(exit)
