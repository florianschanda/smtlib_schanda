(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9353FC8)))
;; x should be Float32(0xC9353FC8 [Rational(-1484793, 2), -742396.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01100011010011000001111)))
;; y should be Float32(0x4AB1A60F [Rational(11642383, 2), 5821191.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b0 #b10010101 #b00110101111111000010110))))
(check-sat)
(exit)
