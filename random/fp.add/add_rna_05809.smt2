(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01110100000000110101111)))
;; x should be Float32(0x803A01AF [Rational(-3801519, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01101110010000000100101)))
;; y should be Float32(0x4AB72025 [Rational(12001317, 2), 6000658.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b0 #b10010101 #b01101110010000000100101))))
(check-sat)
(exit)
