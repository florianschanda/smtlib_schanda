(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8030B1AD)))
;; x should be Float32(0x8030B1AD [Rational(-3191213, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80425CDA)))
;; y should be Float32(0x80425CDA [Rational(-2174573, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b1 #b00000000 #b11100110000111010000111)))
(check-sat)
(exit)
