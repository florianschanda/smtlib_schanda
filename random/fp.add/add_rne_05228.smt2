(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFFD09EDF)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b00101011 #b10110110110100100000101)))
;; y should be Float32(0x95DB6905 [Rational(-14379269, 162259276829213363391578010288128), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
