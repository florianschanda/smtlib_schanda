(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11101111110111111110101)))
;; x should be Float32(0x4AF7EFF5 [Rational(16248821, 2), 8124410.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111000 #b01101111110000111000010)))
;; y should be Float32(0x7C37E1C2 [Rational(3819076950144891396591848429251985408), 3819076950144891396591848429251985408.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b0 #b11111000 #b01101111110000111000011)))
(check-sat)
(exit)
