(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00010011101010111110001)))
;; x should be Float32(0xCA89D5F1 [Rational(-9033201, 2), -4516600.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xBA000F97)))
;; y should be Float32(0xBA000F97 [Rational(-8392599, 17179869184), -0.000489])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA89D5F1))))
(check-sat)
(exit)
