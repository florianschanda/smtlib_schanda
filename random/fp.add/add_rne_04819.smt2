(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x05E34189)))
;; x should be Float32(0x05E34189 [Rational(14893449, 696898287454081973172991196020261297061888), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x44CB1591)))
;; y should be Float32(0x44CB1591 [Rational(13309329, 8192), 1624.673950])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b0 #b10001001 #b10010110001010110010001)))
(check-sat)
(exit)
