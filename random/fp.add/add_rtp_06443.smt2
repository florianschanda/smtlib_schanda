(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010011 #b11000000100001110000010)))
;; x should be Float32(0xE9E04382 [Rational(-33889772528094847950651392), -33889772528094847950651392.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00100101111010011010110)))
;; y should be Float32(0xCA12F4D6 [Rational(-4815467, 2), -2407733.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xE9E04382))))
(check-sat)
(exit)
