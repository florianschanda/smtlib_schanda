(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10100110 #b10100011010110000000100)))
;; x should be Float32(0x5351AC04 [Rational(900534108160), 900534108160.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAEDFD39)))
;; y should be Float32(0xCAEDFD39 [Rational(-15596857, 2), -7798428.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b0 #b10100110 #b10100011010101110001101)))
(check-sat)
(exit)
