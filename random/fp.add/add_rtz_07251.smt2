(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABFA6F5)))
;; x should be Float32(0x4ABFA6F5 [Rational(12560117, 2), 6280058.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10100101 #b00010111111010100010010)))
;; y should be Float32(0xD28BF512 [Rational(-300556025856), -300556025856.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b1 #b10100101 #b00010111111010001010010))))
(check-sat)
(exit)
