(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b00011001101101010111000)))
;; x should be Float32(0xC90CDAB8 [Rational(-1153879, 2), -576939.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01110010010100010001101)))
;; y should be Float32(0xCAB9288D [Rational(-12134541, 2), -6067270.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b10010101 #b10010101100001111100100)))
(check-sat)
(exit)
