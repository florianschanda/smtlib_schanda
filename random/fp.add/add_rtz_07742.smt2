(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00011110 #b00110100011101110101101)))
;; x should be Float32(0x8F1A3BAD [Rational(-10107821, 1329227995784915872903807060280344576), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA679E42)))
;; y should be Float32(0xCA679E42 [Rational(-7589665, 2), -3794832.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b1 #b10010100 #b11001111001111001000010)))
(check-sat)
(exit)
