(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10000010 #b11010010010101011010111)))
;; x should be Float32(0xC1692AD7 [Rational(-15280855, 1048576), -14.572959])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x576A748A)))
;; y should be Float32(0x576A748A [Rational(257786252361728), 257786252361728.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b0 #b10101110 #b11010100111010010001010))))
(check-sat)
(exit)
