(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE591D3)))
;; x should be Float32(0xCAE591D3 [Rational(-15045075, 2), -7522537.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA86782F)))
;; y should be Float32(0xCA86782F [Rational(-8812591, 2), -4406295.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xCB360501))))
(check-sat)
(exit)
