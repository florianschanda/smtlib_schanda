(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEA2080DC)))
;; x should be Float32(0xEA2080DC [Rational(-48509163082961049660424192), -48509163082961049660424192.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0069AA5D)))
;; y should be Float32(0x0069AA5D [Rational(6924893, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b1 #b11010100 #b01000001000000011011100)))
(check-sat)
(exit)
