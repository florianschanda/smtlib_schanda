(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10001111111111011000011)))
;; x should be Float32(0xCAC7FEC3 [Rational(-13106883, 2), -6553441.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00111000001101011001110)))
;; y should be Float32(0xCA1C1ACE [Rational(-5115239, 2), -2557619.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #xCB0B0615)))
(check-sat)
(exit)
