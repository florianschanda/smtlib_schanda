(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000110 #b10001110011011010001110)))
;; x should be Float32(0x0347368E [Rational(6527815, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11100011101010010011110)))
;; y should be Float32(0xCA71D49E [Rational(-7924303, 2), -3962151.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA71D49E))))
(check-sat)
(exit)
