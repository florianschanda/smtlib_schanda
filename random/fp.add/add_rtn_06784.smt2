(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x728B38DB)))
;; x should be Float32(0x728B38DB [Rational(5515155252393616926570228744192), 5515155252393616926570228744192.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2C136A9C)))
;; y should be Float32(0x2C136A9C [Rational(2415271, 1152921504606846976), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #x728B38DB)))
(check-sat)
(exit)
