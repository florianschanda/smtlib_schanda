(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB63D5B)))
;; x should be Float32(0xCAB63D5B [Rational(-11943259, 2), -5971629.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AE88417)))
;; y should be Float32(0x4AE88417 [Rational(15238167, 2), 7619083.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #x49C91AF0)))
(check-sat)
(exit)
