(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001111 #b01011010000000101110001)))
;; x should be Float32(0x07AD0171 [Rational(11338097, 43556142965880123323311949751266331066368), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8A7EF8BD)))
;; y should be Float32(0x8A7EF8BD [Rational(-16709821, 1361129467683753853853498429727072845824), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b1 #b00010100 #b11110011001000010110001))))
(check-sat)
(exit)
