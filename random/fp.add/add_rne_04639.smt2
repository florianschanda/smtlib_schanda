(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00100001110101000010011)))
;; x should be Float32(0x4A90EA13 [Rational(9497107, 2), 4748553.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00100101111000000011010)))
;; y should be Float32(0x8012F01A [Rational(-620557, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A90EA13))))
(check-sat)
(exit)
