(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100100 #b11001010100001000111100)))
;; x should be Float32(0x3265423C [Rational(3756175, 281474976710656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00101000000001000110011)))
;; y should be Float32(0x00140233 [Rational(1311283, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b0 #b01100100 #b11001010100001000111100))))
(check-sat)
(exit)
