(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01011010010001111111100)))
;; x should be Float32(0xC9AD23FC [Rational(-2836735, 2), -1418367.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00178E71)))
;; y should be Float32(0x00178E71 [Rational(1543793, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b1 #b10010011 #b01011010010001111111011))))
(check-sat)
(exit)
