(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5187F8A6)))
;; x should be Float32(0x5187F8A6 [Rational(72999026688), 72999026688.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11100101001011000100000)))
;; y should be Float32(0x80729620 [Rational(-234673, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #x5187F8A6)))
(check-sat)
(exit)
