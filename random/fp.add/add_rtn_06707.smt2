(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4994C99C)))
;; y should be Float32(0x4994C99C [Rational(2437735, 2), 1218867.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b0 #b10010011 #b00101001100100110011100))))
(check-sat)
(exit)
