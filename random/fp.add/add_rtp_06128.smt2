(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01010100 #b01101011011100101111000)))
;; x should be Float32(0x2A35B978 [Rational(1488687, 9223372036854775808), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0E8C3478)))
;; y should be Float32(0x0E8C3478 [Rational(1148559, 332306998946228968225951765070086144), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b0 #b01010100 #b01101011011100101111001)))
(check-sat)
(exit)
