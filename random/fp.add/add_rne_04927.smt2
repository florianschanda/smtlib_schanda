(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01100101100101010100111)))
;; x should be Float32(0xCAB2CAA7 [Rational(-11717287, 2), -5858643.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10011110 #b11100101111110001100110)))
;; y should be Float32(0x4F72FC66 [Rational(4076627456), 4076627456.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b0 #b10011110 #b11100101010001100000001))))
(check-sat)
(exit)
