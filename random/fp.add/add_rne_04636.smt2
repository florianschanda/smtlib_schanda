(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB4FB49)))
;; x should be Float32(0x4AB4FB49 [Rational(11860809, 2), 5930404.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEACAD51D)))
;; y should be Float32(0xEACAD51D [Rational(-122604707289292232649080832), -122604707289292232649080832.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xEACAD51D))))
(check-sat)
(exit)
