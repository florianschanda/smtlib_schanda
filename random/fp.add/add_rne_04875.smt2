(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00244DBE)))
;; x should be Float32(0x00244DBE [Rational(1189599, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010001 #b00011000100001010000010)))
;; y should be Float32(0xA88C4282 [Rational(-4596033, 295147905179352825856), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b01010001 #b00011000100001010000010)))
(check-sat)
(exit)
