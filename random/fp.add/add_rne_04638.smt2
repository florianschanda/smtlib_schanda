(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A136D5A)))
;; x should be Float32(0x4A136D5A [Rational(4830893, 2), 2415446.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10100110101010010001010)))
;; y should be Float32(0x8053548A [Rational(-2730565, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b0 #b10010100 #b00100110110110101011010)))
(check-sat)
(exit)
