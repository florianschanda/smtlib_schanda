(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11011100 #b01100010100111110011110)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10011010110100100011100)))
(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b11011100 #b01100010100111110011110)))
(check-sat)
(exit)
