(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01000001101001100110110)))
;; x should be Float32(0x4A20D336 [Rational(5269915, 2), 2634957.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11011110 #b10100000101000111001110)))
;; y should be Float32(0xEF5051CE [Rational(-64471777841724030404819681280), -64471777841724030404819681280.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xEF5051CD))))
(check-sat)
(exit)
