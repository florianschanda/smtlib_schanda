(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000110 #b11010010110000111011011)))
;; x should be Float32(0x436961DB [Rational(15294939, 65536), 233.382248])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00010000011111100101101)))
;; y should be Float32(0xCA883F2D [Rational(-8929069, 2), -4464534.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA883D5A))))
(check-sat)
(exit)
