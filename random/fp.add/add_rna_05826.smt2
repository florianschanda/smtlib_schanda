(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11000001000100110010000)))
;; x should be Float32(0x80608990 [Rational(-395417, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10100100111011011011101)))
;; y should be Float32(0xCAD276DD [Rational(-13792989, 2), -6896494.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b10010101 #b10100100111011011011101))))
(check-sat)
(exit)
