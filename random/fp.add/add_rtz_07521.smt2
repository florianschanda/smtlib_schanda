(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC7E44E40)))
;; x should be Float32(0xC7E44E40 [Rational(-233785, 2), -116892.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49F703BC)))
;; y should be Float32(0x49F703BC [Rational(4047087, 2), 2023543.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b0 #b10010011 #b11010001011111011011000)))
(check-sat)
(exit)
