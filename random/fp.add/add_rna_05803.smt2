(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101101 #b01100000000101100100010)))
;; x should be Float32(0xB6B00B22 [Rational(-5768593, 1099511627776), -0.000005])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8069513F)))
;; y should be Float32(0x8069513F [Rational(-6902079, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b01101101 #b01100000000101100100010))))
(check-sat)
(exit)
