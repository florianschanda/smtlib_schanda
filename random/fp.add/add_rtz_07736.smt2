(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xBA466259)))
;; x should be Float32(0xBA466259 [Rational(-13001305, 17179869184), -0.000757])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111010 #b00001101011100000111010)))
;; y should be Float32(0x7D06B83A [Rational(11192069402306634190602771646419828736), 11192069402306634190602771646419828736.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x7D06B839))))
(check-sat)
(exit)
