(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AA1DF23)))
;; x should be Float32(0x4AA1DF23 [Rational(10608419, 2), 5304209.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8058F06A)))
;; y should be Float32(0x8058F06A [Rational(-2914357, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b0 #b10010101 #b01000011101111100100011)))
(check-sat)
(exit)
