(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA84889FD)))
;; x should be Float32(0xA84889FD [Rational(-13142525, 1180591620717411303424), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10111011110101011000101)))
;; y should be Float32(0x4ADDEAC5 [Rational(14543557, 2), 7271778.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x4ADDEAC4)))
(check-sat)
(exit)
