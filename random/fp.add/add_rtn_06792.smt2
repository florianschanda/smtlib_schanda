(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1C0A58D4)))
;; x should be Float32(0x1C0A58D4 [Rational(2266677, 4951760157141521099596496896), 0.000000])

(declare-const y Float32)
(assert (= y (_ -oo 8 24)))
;; y should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
