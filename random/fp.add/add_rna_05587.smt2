(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01000110011100001001011)))
;; x should be Float32(0xCAA3384B [Rational(-10696779, 2), -5348389.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011111 #b10101000011111011010111)))
;; y should be Float32(0x0FD43ED7 [Rational(13909719, 664613997892457936451903530140172288), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b10010101 #b01000110011100001001011))))
(check-sat)
(exit)
