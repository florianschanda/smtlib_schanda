(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10110110100100100101001)))
;; x should be Float32(0xCADB4929 [Rational(-14371113, 2), -7185556.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00110110100011000101011)))
;; y should be Float32(0x4A9B462B [Rational(10176043, 2), 5088021.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #xCA0005FC)))
(check-sat)
(exit)
