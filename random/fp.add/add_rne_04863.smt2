(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11010100011111000101011)))
;; x should be Float32(0x006A3E2B [Rational(6962731, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00000000001111001111011)))
;; y should be Float32(0xCA801E7B [Rational(-8396411, 2), -4198205.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b1 #b10010101 #b00000000001111001111011))))
(check-sat)
(exit)
