(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10000011 #b01001101010001011111010)))
;; x should be Float32(0xC1A6A2FA [Rational(-5460349, 262144), -20.829578])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b10001101010001110010110)))
;; y should be Float32(0xCA46A396 [Rational(-6509003, 2), -3254501.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b1 #b10010100 #b10001101010001111101010))))
(check-sat)
(exit)
