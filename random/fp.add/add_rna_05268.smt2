(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b01100011000111111101100)))
;; x should be Float32(0x49B18FEC [Rational(2909179, 2), 1454589.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01001000101110011111100)))
;; y should be Float32(0x49A45CFC [Rational(2692927, 2), 1346463.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b0 #b10010100 #b01010101111011001110100))))
(check-sat)
(exit)
