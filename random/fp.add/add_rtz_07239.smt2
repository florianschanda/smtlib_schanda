(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11111100000110110000001)))
;; x should be Float32(0x4AFE0D81 [Rational(16649601, 2), 8324800.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9A645B4)))
;; y should be Float32(0xC9A645B4 [Rational(-2724205, 2), -1362102.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x4AD47C14)))
(check-sat)
(exit)
