(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00001000110000010101011)))
;; x should be Float32(0x800460AB [Rational(-286891, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF0EE8995)))
;; y should be Float32(0xF0EE8995 [Rational(-590590070458986225044555825152), -590590070458986225044555825152.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xF0EE8996))))
(check-sat)
(exit)
