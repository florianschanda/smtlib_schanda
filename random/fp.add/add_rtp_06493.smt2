(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x800EBE8E)))
;; x should be Float32(0x800EBE8E [Rational(-483143, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000010 #b01101000000111101010101)))
;; y should be Float32(0xC1340F55 [Rational(-11800405, 1048576), -11.253743])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b1 #b10000010 #b01101000000111101010101))))
(check-sat)
(exit)
