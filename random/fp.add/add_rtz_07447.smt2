(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x58EB3D4B)))
;; x should be Float32(0x58EB3D4B [Rational(2069187870588928), 2069187870588928.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10100001 #b01100001000001010001111)))
;; y should be Float32(0xD0B0828F [Rational(-23690770432), -23690770432.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b0 #b10110001 #b11010110011110010011010)))
(check-sat)
(exit)
