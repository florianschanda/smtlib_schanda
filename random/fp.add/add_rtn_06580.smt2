(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A95466F)))
;; x should be Float32(0x4A95466F [Rational(9782895, 2), 4891447.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x23CAE7F4)))
;; y should be Float32(0x23CAE7F4 [Rational(3324413, 151115727451828646838272), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b0 #b10010101 #b00101010100011001101111))))
(check-sat)
(exit)
