(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00000000001110101001101)))
;; x should be Float32(0x4A801D4D [Rational(8396109, 2), 4198054.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000011 #b00101000000000110110000)))
;; y should be Float32(0x019401B0 [Rational(606235, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b0 #b10010101 #b00000000001110101001101))))
(check-sat)
(exit)
