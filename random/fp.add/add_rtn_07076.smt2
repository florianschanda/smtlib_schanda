(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE63D6DA2)))
;; x should be Float32(0xE63D6DA2 [Rational(-223637408424157857710080), -223637408424157857710080.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49950154)))
;; y should be Float32(0x49950154 [Rational(2441301, 2), 1220650.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xE63D6DA2))))
(check-sat)
(exit)
