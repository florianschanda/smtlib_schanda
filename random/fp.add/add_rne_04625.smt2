(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b01011001101011100001100)))
;; x should be Float32(0x49ACD70C [Rational(2831811, 2), 1415905.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9F07E1C)))
;; y should be Float32(0xC9F07E1C [Rational(-3940231, 2), -1970115.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b1 #b10010010 #b00001110100111000100000))))
(check-sat)
(exit)
