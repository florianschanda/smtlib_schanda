(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB9C3A9)))
;; x should be Float32(0x4AB9C3A9 [Rational(12174249, 2), 6087124.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9C22F54)))
;; y should be Float32(0xC9C22F54 [Rational(-3181525, 2), -1590762.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b0 #b10010101 #b00010010011011111010100))))
(check-sat)
(exit)
