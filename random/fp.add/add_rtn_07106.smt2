(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA615AFB6)))
;; x should be Float32(0xA615AFB6 [Rational(-4904923, 9444732965739290427392), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC339B4E2)))
;; y should be Float32(0xC339B4E2 [Rational(-6085233, 32768), -185.706573])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b1 #b10000110 #b01110011011010011100011)))
(check-sat)
(exit)
