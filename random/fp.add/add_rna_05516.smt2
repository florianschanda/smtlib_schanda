(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01111010101100011100010)))
;; x should be Float32(0x003D58E2 [Rational(2010225, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011111 #b10000100011010111001000)))
;; y should be Float32(0x0FC235C8 [Rational(1590969, 83076749736557242056487941267521536), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b0 #b00011111 #b10000100011010111001000)))
(check-sat)
(exit)
