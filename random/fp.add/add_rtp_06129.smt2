(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100001 #b10010001011000000110111)))
;; x should be Float32(0x30C8B037 [Rational(13152311, 9007199254740992), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00011010110111011111111)))
;; y should be Float32(0x000D6EFF [Rational(880383, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b0 #b01100001 #b10010001011000000111000))))
(check-sat)
(exit)
