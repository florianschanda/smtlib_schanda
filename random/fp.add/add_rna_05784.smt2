(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9DE8E61B)))
;; x should be Float32(0x9DE8E61B [Rational(-15263259, 2475880078570760549798248448), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x004ACF75)))
;; y should be Float32(0x004ACF75 [Rational(4902773, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x9DE8E61B))))
(check-sat)
(exit)
