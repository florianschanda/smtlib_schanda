(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10101100 #b01010010011001001011010)))
;; x should be Float32(0xD629325A [Rational(-46508430852096), -46508430852096.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01001110 #b10111001101101011000101)))
;; y should be Float32(0x275CDAC5 [Rational(14473925, 4722366482869645213696), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b10101100 #b01010010011001001011010)))
(check-sat)
(exit)
