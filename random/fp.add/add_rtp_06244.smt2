(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00000110000000100010001)))
;; x should be Float32(0xCA830111 [Rational(-8585489, 2), -4292744.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA734FDA)))
;; y should be Float32(0xCA734FDA [Rational(-7972845, 2), -3986422.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b1 #b10010101 #b11111001010100011111110)))
(check-sat)
(exit)
