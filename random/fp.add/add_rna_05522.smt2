(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0050A787)))
;; x should be Float32(0x0050A787 [Rational(5285767, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01111001011001010010010)))
;; y should be Float32(0xCA3CB292 [Rational(-6183241, 2), -3091620.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA3CB292))))
(check-sat)
(exit)
