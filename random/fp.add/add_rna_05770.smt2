(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11001100 #b11101000111101101111101)))
;; x should be Float32(0xE6747B7D [Rational(-288633844635128611995648), -288633844635128611995648.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A33BCCA)))
;; y should be Float32(0x4A33BCCA [Rational(5889637, 2), 2944818.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b11001100 #b11101000111101101111101)))
(check-sat)
(exit)
