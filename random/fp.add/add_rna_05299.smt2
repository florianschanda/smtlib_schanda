(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00110101000011101011001)))
;; x should be Float32(0x4A9A8759 [Rational(10127193, 2), 5063596.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCF9FFBCC)))
;; y should be Float32(0xCF9FFBCC [Rational(-5368158208), -5368158208.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b10011111 #b00111111101010100101010)))
(check-sat)
(exit)
