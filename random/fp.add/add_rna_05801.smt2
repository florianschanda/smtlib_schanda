(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01000011 #b11111010110100111101001)))
;; x should be Float32(0xA1FD69E9 [Rational(-16607721, 9671406556917033397649408), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11100010 #b11101100011010111100001)))
;; y should be Float32(0xF17635E1 [Rational(-1219175168271185726763658379264), -1219175168271185726763658379264.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b11100010 #b11101100011010111100001)))
(check-sat)
(exit)
