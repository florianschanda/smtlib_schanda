(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00100011100010101101000)))
;; x should be Float32(0x0011C568 [Rational(145581, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10010000011000100010101)))
;; y should be Float32(0xCAC83115 [Rational(-13119765, 2), -6559882.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b1 #b10010101 #b10010000011000100010100)))
(check-sat)
(exit)
