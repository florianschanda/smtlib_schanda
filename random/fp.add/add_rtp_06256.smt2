(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAA6E41B)))
;; x should be Float32(0xCAA6E41B [Rational(-10937371, 2), -5468685.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11011011 #b01010001101011100011110)))
;; y should be Float32(0xEDA8D71E [Rational(-6531692796524700363460182016), -6531692796524700363460182016.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #xEDA8D71E)))
(check-sat)
(exit)
