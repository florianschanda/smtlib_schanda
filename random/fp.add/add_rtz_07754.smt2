(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xBE327851)))
;; x should be Float32(0xBE327851 [Rational(-11696209, 67108864), -0.174287])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010110 #b10001001111001000100100)))
;; y should be Float32(0xAB44F224 [Rational(-3226761, 4611686018427387904), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xBE327851)))
(check-sat)
(exit)
