(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11011010010001000011100)))
;; x should be Float32(0xC9ED221C [Rational(-3885191, 2), -1942595.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10101110 #b00111011100001001000001)))
;; y should be Float32(0x571DC241 [Rational(173457639735296), 173457639735296.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b0 #b10101110 #b00111011100001001000000))))
(check-sat)
(exit)
