(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5CC25C34)))
;; x should be Float32(0x5CC25C34 [Rational(437660191119376384), 437660191119376384.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x499C6594)))
;; y should be Float32(0x499C6594 [Rational(2562405, 2), 1281202.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b0 #b10111001 #b10000100101110000110100)))
(check-sat)
(exit)
