(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01000000 #b01110010011100111001010)))
;; x should be Float32(0xA03939CA [Rational(-6069477, 38685626227668133590597632), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x98C4F318)))
;; y should be Float32(0x98C4F318 [Rational(-1613411, 316912650057057350374175801344), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b1 #b01000000 #b01110010011101101010011))))
(check-sat)
(exit)
