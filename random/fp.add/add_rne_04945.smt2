(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00011101010101110000010)))
;; x should be Float32(0xCA0EAB82 [Rational(-4675009, 2), -2337504.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00111001 #b10101011010011110001100)))
;; y should be Float32(0x9CD5A78C [Rational(-3500515, 2475880078570760549798248448), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result ((_ to_fp 8 24) #xCA0EAB82)))
(check-sat)
(exit)
