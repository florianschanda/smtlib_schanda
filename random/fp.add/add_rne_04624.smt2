(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF1EF45)))
;; x should be Float32(0x4AF1EF45 [Rational(15855429, 2), 7927714.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAD2775F)))
;; y should be Float32(0xCAD2775F [Rational(-13793119, 2), -6896559.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x497BBF30))))
(check-sat)
(exit)
