(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00799B00)))
;; x should be Float32(0x00799B00 [Rational(31131, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x001AE694)))
;; y should be Float32(0x001AE694 [Rational(440741, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x00948194))))
(check-sat)
(exit)
