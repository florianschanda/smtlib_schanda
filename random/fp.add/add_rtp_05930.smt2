(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A8DCFE1)))
;; x should be Float32(0x4A8DCFE1 [Rational(9293793, 2), 4646896.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00000111010000110001010)))
;; y should be Float32(0x0003A18A [Rational(118981, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b0 #b10010101 #b00011011100111111100010))))
(check-sat)
(exit)
