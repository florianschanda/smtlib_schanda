(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9579DFF7)))
;; x should be Float32(0x9579DFF7 [Rational(-16375799, 324518553658426726783156020576256), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01101010 #b11001011000111010000100)))
;; y should be Float32(0x35658E84 [Rational(3761057, 4398046511104), 0.000001])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #x35658E84)))
(check-sat)
(exit)
