(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11101101101011001110010)))
;; x should be Float32(0xCA76D672 [Rational(-8088377, 2), -4044188.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80660D26)))
;; y should be Float32(0x80660D26 [Rational(-3344019, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b10010100 #b11101101101011001110010)))
(check-sat)
(exit)
