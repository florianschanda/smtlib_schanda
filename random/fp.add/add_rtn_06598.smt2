(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00100011101010101000010)))
;; x should be Float32(0x4A11D542 [Rational(4778657, 2), 2389328.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8328594D)))
;; y should be Float32(0x8328594D [Rational(-11032909, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A11D541))))
(check-sat)
(exit)
