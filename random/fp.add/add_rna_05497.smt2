(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x40AE6BF8)))
;; x should be Float32(0x40AE6BF8 [Rational(1428863, 262144), 5.450680])

(declare-const y Float32)
(assert (= y (fp #b1 #b11001010 #b11101100011000010110110)))
;; y should be Float32(0xE57630B6 [Rational(-72662544561474105245696), -72662544561474105245696.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b11001010 #b11101100011000010110110)))
(check-sat)
(exit)
