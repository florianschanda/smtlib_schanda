(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10111000 #b10000001000011110000000)))
;; x should be Float32(0xDC408780 [Rational(-216768717416038400), -216768717416038400.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00427AA7)))
;; y should be Float32(0x00427AA7 [Rational(4356775, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #xDC40877F)))
(check-sat)
(exit)
