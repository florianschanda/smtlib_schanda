(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00010111111010110101100)))
;; x should be Float32(0xC98BF5AC [Rational(-2293099, 2), -1146549.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x807F04E9)))
;; y should be Float32(0x807F04E9 [Rational(-8324329, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b1 #b10010011 #b00010111111010110101100))))
(check-sat)
(exit)
