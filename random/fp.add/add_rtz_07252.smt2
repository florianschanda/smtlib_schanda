(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AAFA465)))
;; x should be Float32(0x4AAFA465 [Rational(11510885, 2), 5755442.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x87718E01)))
;; y should be Float32(0x87718E01 [Rational(-15830529, 87112285931760246646623899502532662132736), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b0 #b10010101 #b01011111010010001100100)))
(check-sat)
(exit)
