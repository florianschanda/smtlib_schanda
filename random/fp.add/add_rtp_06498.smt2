(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01101111001010110010111)))
;; x should be Float32(0x80379597 [Rational(-3642775, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b10001010111010101101010)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
