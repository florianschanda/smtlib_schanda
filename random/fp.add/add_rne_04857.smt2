(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10110010101100111110000)))
;; x should be Float32(0x005959F0 [Rational(365983, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10101011 #b10010011111101111011111)))
;; y should be Float32(0x55C9FBDF [Rational(27760451911680), 27760451911680.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x55C9FBDF))))
(check-sat)
(exit)
