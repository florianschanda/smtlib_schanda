(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF0A3FCCB)))
;; x should be Float32(0xF0A3FCCB [Rational(-406013316382545242337149190144), -406013316382545242337149190144.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC987F484)))
;; y should be Float32(0xC987F484 [Rational(-2227489, 2), -1113744.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b1 #b11100001 #b01000111111110011001011))))
(check-sat)
(exit)
