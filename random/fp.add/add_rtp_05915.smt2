(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A96ABD9)))
;; x should be Float32(0x4A96ABD9 [Rational(9874393, 2), 4937196.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A096722)))
;; y should be Float32(0x4A096722 [Rational(4502417, 2), 2251208.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b0 #b10010101 #b10110110101111101101010)))
(check-sat)
(exit)
