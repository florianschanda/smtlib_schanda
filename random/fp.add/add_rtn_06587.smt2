(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF81E25)))
;; x should be Float32(0x4AF81E25 [Rational(16260645, 2), 8130322.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA5A36E6)))
;; y should be Float32(0xCA5A36E6 [Rational(-7150451, 2), -3575225.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A8B02B2))))
(check-sat)
(exit)
