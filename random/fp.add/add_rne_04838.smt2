(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2B50C415)))
;; x should be Float32(0x2B50C415 [Rational(13681685, 18446744073709551616), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10001011100111110001111)))
;; y should be Float32(0x8045CF8F [Rational(-4575119, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x2B50C415))))
(check-sat)
(exit)
