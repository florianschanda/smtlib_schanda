(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00110110100111000100010)))
;; x should be Float32(0x001B4E22 [Rational(894737, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFFD80F98)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
