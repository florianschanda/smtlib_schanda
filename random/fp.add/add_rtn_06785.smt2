(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x14B81D36)))
;; x should be Float32(0x14B81D36 [Rational(6033051, 324518553658426726783156020576256), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10110010000100011011110)))
;; y should be Float32(0x005908DE [Rational(2917487, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x14B81D36))))
(check-sat)
(exit)
