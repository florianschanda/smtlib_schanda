(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01110001 #b00110111011110000010010)))
;; x should be Float32(0x389BBC12 [Rational(5103113, 68719476736), 0.000074])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11000101010010110100001)))
;; y should be Float32(0x8062A5A1 [Rational(-6464929, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b0 #b01110001 #b00110111011110000010001))))
(check-sat)
(exit)
