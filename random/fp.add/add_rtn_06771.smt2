(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2E7E37DA)))
;; x should be Float32(0x2E7E37DA [Rational(8330221, 144115188075855872), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A98C383)))
;; y should be Float32(0x4A98C383 [Rational(10011523, 2), 5005761.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b0 #b10010101 #b00110001100001110000011)))
(check-sat)
(exit)
