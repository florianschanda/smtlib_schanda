(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01000000111101010111001)))
;; x should be Float32(0x00207AB9 [Rational(2128569, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111101101110000101000)))
;; y should be Float32(0x807EDC28 [Rational(-1039237, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b00000000 #b10111100110000101101111)))
(check-sat)
(exit)
