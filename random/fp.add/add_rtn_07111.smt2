(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001111 #b11000101001011011110010)))
;; x should be Float32(0x87E296F2 [Rational(-7424889, 21778071482940061661655974875633165533184), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b11100110011101000101001)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
