(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111010101010101110000)))
;; x should be Float32(0x807D5570 [Rational(-513367, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x158E6538)))
;; y should be Float32(0x158E6538 [Rational(1166503, 20282409603651670423947251286016), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b0 #b00101011 #b00011100110010100110111))))
(check-sat)
(exit)
