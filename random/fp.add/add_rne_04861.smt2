(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01010111110111100011010)))
;; x should be Float32(0x002BEF1A [Rational(1439629, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000000)))
;; y should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b0 #b00000000 #b01010111110111100011010))))
(check-sat)
(exit)
