(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF5E2ECC4)))
;; x should be Float32(0xF5E2ECC4 [Rational(-575322884480071110389703152500736), -575322884480071110389703152500736.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10000101011110000101010)))
;; y should be Float32(0x8042BC2A [Rational(-2186773, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b1 #b11101011 #b11000101110110011000100)))
(check-sat)
(exit)
