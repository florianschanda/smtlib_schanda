(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001C5E4B)))
;; x should be Float32(0x001C5E4B [Rational(1859147, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00000000)))
;; y should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b0 #b00000000 #b00111000101111001001011)))
(check-sat)
(exit)
