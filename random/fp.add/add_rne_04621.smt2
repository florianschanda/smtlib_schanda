(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A1E9296)))
;; x should be Float32(0x4A1E9296 [Rational(5196107, 2), 2598053.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x006CDD94)))
;; y should be Float32(0x006CDD94 [Rational(1783653, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A1E9296))))
(check-sat)
(exit)
