(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x804F8220)))
;; x should be Float32(0x804F8220 [Rational(-162833, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00110110101000101010010)))
;; y should be Float32(0x001B5152 [Rational(895145, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #x803430CE)))
(check-sat)
(exit)
