(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11000110 #b10110100010100110100101)))
;; x should be Float32(0x635A29A5 [Rational(4024391012795394555904), 4024391012795394555904.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA2EB16A)))
;; y should be Float32(0xCA2EB16A [Rational(-5724341, 2), -2862170.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x635A29A4)))
(check-sat)
(exit)
