(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11100011 #b11101111100101010010010)))
;; x should be Float32(0x71F7CA92 [Rational(2454006077022108353168408576000), 2454006077022108353168408576000.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10001011001111111101000)))
;; y should be Float32(0x00459FE8 [Rational(570365, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x71F7CA92))))
(check-sat)
(exit)
