(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11011001000110001001100)))
;; x should be Float32(0xC9EC8C4C [Rational(-3875603, 2), -1937801.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00101110010010100110001)))
;; y should be Float32(0x4A972531 [Rational(9905457, 2), 4952728.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A38043C))))
(check-sat)
(exit)
