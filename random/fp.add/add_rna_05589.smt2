(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9D861C4)))
;; x should be Float32(0xC9D861C4 [Rational(-3545201, 2), -1772600.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x007459F3)))
;; y should be Float32(0x007459F3 [Rational(7625203, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b10010011 #b10110000110000111000100))))
(check-sat)
(exit)
