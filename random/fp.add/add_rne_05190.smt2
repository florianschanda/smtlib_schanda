(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80121463)))
;; x should be Float32(0x80121463 [Rational(-1184867, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEF4F6AD6)))
;; y should be Float32(0xEF4F6AD6 [Rational(-64192553756324914022624264192), -64192553756324914022624264192.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b1 #b11011110 #b10011110110101011010110))))
(check-sat)
(exit)
