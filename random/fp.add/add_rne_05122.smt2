(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101010 #b01111100010101001001100)))
;; x should be Float32(0x953E2A4C [Rational(-3115667, 81129638414606681695789005144064), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11111010110100101011001)))
;; y should be Float32(0x4AFD6959 [Rational(16607577, 2), 8303788.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b0 #b10010101 #b11111010110100101011001))))
(check-sat)
(exit)
