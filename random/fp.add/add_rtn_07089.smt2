(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x94304F1F)))
;; x should be Float32(0x94304F1F [Rational(-11554591, 1298074214633706907132624082305024), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1662643C)))
;; y should be Float32(0x1662643C [Rational(3709199, 20282409603651670423947251286016), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x16575F4A))))
(check-sat)
(exit)
