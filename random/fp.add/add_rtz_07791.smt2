(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01000011101110101010110)))
;; x should be Float32(0x8021DD56 [Rational(-1109675, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00001100 #b01010010000111000110010)))
;; y should be Float32(0x86290E32 [Rational(-5539609, 174224571863520493293247799005065324265472), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b1 #b00001100 #b01010010001001001101101)))
(check-sat)
(exit)
