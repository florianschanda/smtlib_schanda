(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00011101110110000100111)))
;; x should be Float32(0xCA8EEC27 [Rational(-9366567, 2), -4683283.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00100000 #b01000101100100110010001)))
;; y should be Float32(0x9022C991 [Rational(-10668433, 332306998946228968225951765070086144), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b1 #b10010101 #b00011101110110000100111)))
(check-sat)
(exit)
