(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x935A3814)))
;; x should be Float32(0x935A3814 [Rational(-3575301, 1298074214633706907132624082305024), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAF80851)))
;; y should be Float32(0xCAF80851 [Rational(-16255057, 2), -8127528.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAF80851))))
(check-sat)
(exit)
