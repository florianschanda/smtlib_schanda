(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x58E35E60)))
;; x should be Float32(0x58E35E60 [Rational(1999955816349696), 1999955816349696.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10100101100011101000100)))
;; y should be Float32(0x8052C744 [Rational(-1356241, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b0 #b10110001 #b11000110101111001011111)))
(check-sat)
(exit)
