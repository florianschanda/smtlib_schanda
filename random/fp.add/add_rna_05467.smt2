(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x43488BAE)))
;; x should be Float32(0x43488BAE [Rational(6571479, 32768), 200.545624])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00100000010010001111110)))
;; y should be Float32(0x4A10247E [Rational(4723263, 2), 2361631.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A1027A0))))
(check-sat)
(exit)
