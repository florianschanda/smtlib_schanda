(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x540E5943)))
;; x should be Float32(0x540E5943 [Rational(2445531676672), 2445531676672.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10110100110001101001100)))
;; y should be Float32(0x005A634C [Rational(1480915, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b0 #b10101000 #b00011100101100101000011))))
(check-sat)
(exit)
