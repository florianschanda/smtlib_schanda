(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AAA9949)))
;; x should be Float32(0x4AAA9949 [Rational(11180361, 2), 5590180.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x5F2E9E15)))
;; y should be Float32(0x5F2E9E15 [Rational(12582517498663927808), 12582517498663927808.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #x5F2E9E15)))
(check-sat)
(exit)
