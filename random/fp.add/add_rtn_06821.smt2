(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x006E9C4A)))
;; x should be Float32(0x006E9C4A [Rational(3624485, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010011 #b00100010101111010011001)))
;; y should be Float32(0x69915E99 [Rational(21967621080669281817985024), 21967621080669281817985024.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x69915E99))))
(check-sat)
(exit)
