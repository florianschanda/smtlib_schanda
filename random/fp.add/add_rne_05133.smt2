(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCE6FAA7E)))
;; x should be Float32(0xCE6FAA7E [Rational(-1005232000), -1005232000.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4364D5C0)))
;; y should be Float32(0x4364D5C0 [Rational(234327, 1024), 228.834961])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b1 #b10011100 #b11011111010101001111010))))
(check-sat)
(exit)
