(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x72AB86AB)))
;; x should be Float32(0x72AB86AB [Rational(6794846753785208034647005462528), 6794846753785208034647005462528.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x806DAA2C)))
;; y should be Float32(0x806DAA2C [Rational(-1796747, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b0 #b11100101 #b01010111000011010101010)))
(check-sat)
(exit)
