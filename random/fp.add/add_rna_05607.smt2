(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11101110111000001100011)))
;; x should be Float32(0xCAF77063 [Rational(-16216163, 2), -8108081.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800F2CC2)))
;; y should be Float32(0x800F2CC2 [Rational(-497249, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b10010101 #b11101110111000001100011))))
(check-sat)
(exit)
