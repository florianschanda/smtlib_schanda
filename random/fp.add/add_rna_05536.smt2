(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x002BD769)))
;; x should be Float32(0x002BD769 [Rational(2873193, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11100110001101101101001)))
;; y should be Float32(0x80731B69 [Rational(-7543657, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b00000000 #b10001110100010000000000)))
(check-sat)
(exit)
