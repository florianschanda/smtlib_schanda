(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00000101011101010011010)))
;; x should be Float32(0xCA02BA9A [Rational(-4283725, 2), -2141862.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCACCD223)))
;; y should be Float32(0xCACCD223 [Rational(-13423139, 2), -6711569.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b1 #b10010110 #b00001110001011110111000))))
(check-sat)
(exit)
