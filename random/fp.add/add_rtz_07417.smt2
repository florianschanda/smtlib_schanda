(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010010 #b10010010111001001011101)))
;; x should be Float32(0x0949725D [Rational(13202013, 5444517870735015415413993718908291383296), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11110101011111111011101)))
;; y should be Float32(0x4AFABFDD [Rational(16433117, 2), 8216558.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b0 #b10010101 #b11110101011111111011101))))
(check-sat)
(exit)
