(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11000110010111110011111)))
;; x should be Float32(0xCAE32F9F [Rational(-14888863, 2), -7444431.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00110000111000100011111)))
;; y should be Float32(0x4A98711F [Rational(9990431, 2), 4995215.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA157D00))))
(check-sat)
(exit)
