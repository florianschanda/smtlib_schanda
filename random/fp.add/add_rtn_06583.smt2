(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ACF252B)))
;; x should be Float32(0x4ACF252B [Rational(13575467, 2), 6787733.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0066BD98)))
;; y should be Float32(0x0066BD98 [Rational(841651, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b0 #b10010101 #b10011110010010100101011))))
(check-sat)
(exit)
