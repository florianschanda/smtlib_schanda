(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x64292DAC)))
;; x should be Float32(0x64292DAC [Rational(12483163015538460852224), 12483163015538460852224.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11100000 #b10010011010011100110111)))
;; y should be Float32(0x7049A737 [Rational(249634549264490238865374707712), 249634549264490238865374707712.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result ((_ to_fp 8 24) #x7049A738)))
(check-sat)
(exit)
