(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01011101010011110111101)))
;; x should be Float32(0x4AAEA7BD [Rational(11446205, 2), 5723102.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00010010010100010011011)))
;; y should be Float32(0x4A89289B [Rational(8988827, 2), 4494413.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #x4B1BE82C)))
(check-sat)
(exit)
