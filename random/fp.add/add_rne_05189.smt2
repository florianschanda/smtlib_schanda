(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8055BB8B)))
;; x should be Float32(0x8055BB8B [Rational(-5618571, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010000 #b10001110010001010001011)))
;; y should be Float32(0xE847228B [Rational(-3761552541580547177054208), -3761552541580547177054208.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xE847228B))))
(check-sat)
(exit)
