(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110100 #b10010111111011010010010)))
;; x should be Float32(0xFA4BF692 [Rational(-264759325589198850527317609016197120), -264759325589198850527317609016197120.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC8C9FEF0)))
;; y should be Float32(0xC8C9FEF0 [Rational(-827375, 2), -413687.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xFA4BF692))))
(check-sat)
(exit)
