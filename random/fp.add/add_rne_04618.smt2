(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01101111001100000000010)))
;; x should be Float32(0x4A379802 [Rational(6016001, 2), 3008000.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10111001 #b01101100110111001001111)))
;; y should be Float32(0x5CB66E4F [Rational(410797850742489088), 410797850742489088.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b0 #b10111001 #b01101100110111001001111))))
(check-sat)
(exit)
