(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA6F502E)))
;; x should be Float32(0xCA6F502E [Rational(-7841815, 2), -3920907.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10110011011101100110110)))
;; y should be Float32(0x8059BB36 [Rational(-2940315, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result ((_ to_fp 8 24) #xCA6F502E)))
(check-sat)
(exit)
