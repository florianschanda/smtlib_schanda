(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x48FC53C1)))
;; x should be Float32(0x48FC53C1 [Rational(16536513, 32), 516766.031250])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA19541A)))
;; y should be Float32(0xCA19541A [Rational(-5024269, 2), -2512134.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b1 #b10010011 #b11100111001001101000011)))
(check-sat)
(exit)
