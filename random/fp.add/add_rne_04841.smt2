(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x04B9DEDB)))
;; x should be Float32(0x04B9DEDB [Rational(12181211, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7F8C3352)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
