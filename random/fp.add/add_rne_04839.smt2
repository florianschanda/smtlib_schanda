(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100110 #b11110001111000101000000)))
;; x should be Float32(0x3378F140 [Rational(254917, 4398046511104), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80008F64)))
;; y should be Float32(0x80008F64 [Rational(-9177, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b0 #b01100110 #b11110001111000101000000)))
(check-sat)
(exit)
