(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000110 #b01111011010000001110111)))
;; x should be Float32(0x433DA077 [Rational(12427383, 65536), 189.626816])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAAF38E5)))
;; y should be Float32(0xCAAF38E5 [Rational(-11483365, 2), -5741682.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAAF376A))))
(check-sat)
(exit)
