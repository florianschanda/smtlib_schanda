(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3CFF56E7)))
;; x should be Float32(0x3CFF56E7 [Rational(16733927, 536870912), 0.031169])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11011101101000110011011)))
;; y should be Float32(0xCAEED19B [Rational(-15651227, 2), -7825613.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b10010101 #b11011101101000110011011)))
(check-sat)
(exit)
