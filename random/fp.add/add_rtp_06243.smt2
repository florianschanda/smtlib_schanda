(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00110110110101110001001)))
;; x should be Float32(0xCA9B6B89 [Rational(-10185609, 2), -5092804.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11001110010111010100110)))
;; y should be Float32(0xCA672EA6 [Rational(-7575379, 2), -3787689.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b1 #b10010110 #b00001111000000101101110)))
(check-sat)
(exit)
