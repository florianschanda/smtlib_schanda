(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101010 #b01100000101110000101011)))
;; x should be Float32(0xF5305C2B [Rational(-223562899315589289469454839709696), -223562899315589289469454839709696.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAF7E73F)))
;; y should be Float32(0xCAF7E73F [Rational(-16246591, 2), -8123295.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #xF5305C2B)))
(check-sat)
(exit)
