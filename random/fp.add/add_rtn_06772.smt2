(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01110000 #b01010011000001001001101)))
;; x should be Float32(0x3829824D [Rational(11108941, 274877906944), 0.000040])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4ABBEC3F)))
;; y should be Float32(0x4ABBEC3F [Rational(12315711, 2), 6157855.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #x4ABBEC3F)))
(check-sat)
(exit)
