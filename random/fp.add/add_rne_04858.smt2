(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01001111000101000000000)))
;; x should be Float32(0x00278A00 [Rational(5061, 1393796574908163946345982392040522594123776), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111100110001110000110)))
;; y should be Float32(0x001E6386 [Rational(995779, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x0045ED86))))
(check-sat)
(exit)
