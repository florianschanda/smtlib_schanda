(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01000001 #b00000110001010101000010)))
;; x should be Float32(0xA0831542 [Rational(-4295329, 19342813113834066795298816), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b00100111011011001111000)))
;; y should be Float32(0x4913B678 [Rational(1210063, 2), 605031.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x4913B677))))
(check-sat)
(exit)
