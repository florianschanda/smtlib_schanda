(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A5D0312)))
;; x should be Float32(0x4A5D0312 [Rational(7242121, 2), 3621060.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA9E536F)))
;; y should be Float32(0xCA9E536F [Rational(-10376047, 2), -5188023.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xC9BF4798))))
(check-sat)
(exit)
