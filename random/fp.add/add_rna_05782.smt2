(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDA26E4E6)))
;; x should be Float32(0xDA26E4E6 [Rational(-11744130656894976), -11744130656894976.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000110 #b10101000111100010110100)))
;; y should be Float32(0x035478B4 [Rational(3481133, 5575186299632655785383929568162090376495104), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b10110100 #b01001101110010011100110)))
(check-sat)
(exit)
