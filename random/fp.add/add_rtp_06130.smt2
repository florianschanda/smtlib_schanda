(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000001 #b01100010011110011001001)))
;; x should be Float32(0x20B13CC9 [Rational(11615433, 38685626227668133590597632), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00001011001000100001100)))
;; y should be Float32(0x0005910C [Rational(91203, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #x20B13CCA)))
(check-sat)
(exit)
