(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x804E58FE)))
;; x should be Float32(0x804E58FE [Rational(-2567295, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b01110011110000011101000)))
;; y should be Float32(0x4939E0E8 [Rational(1522717, 2), 761358.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b0 #b10010010 #b01110011110000011101000)))
(check-sat)
(exit)
