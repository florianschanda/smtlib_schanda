(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101001 #b10100001011111000101001)))
;; x should be Float32(0x94D0BE29 [Rational(-13680169, 649037107316853453566312041152512), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00110111101011100001111)))
;; y should be Float32(0xCA1BD70F [Rational(-10213135, 4), -2553283.750000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b1 #b10010100 #b00110111101011100001111))))
(check-sat)
(exit)
