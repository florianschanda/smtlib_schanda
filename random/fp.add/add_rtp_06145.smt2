(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x14E858F7)))
;; x should be Float32(0x14E858F7 [Rational(15227127, 649037107316853453566312041152512), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB3A73C17)))
;; y should be Float32(0xB3A73C17 [Rational(-10959895, 140737488355328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b1 #b01100111 #b01001110011110000010110)))
(check-sat)
(exit)
