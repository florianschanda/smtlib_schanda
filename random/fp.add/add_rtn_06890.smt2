(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00011010111100100110101)))
;; x should be Float32(0xCA8D7935 [Rational(-9271605, 2), -4635802.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1FC5C565)))
;; y should be Float32(0x1FC5C565 [Rational(12961125, 154742504910672534362390528), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b1 #b10010101 #b00011010111100100110101)))
(check-sat)
(exit)
