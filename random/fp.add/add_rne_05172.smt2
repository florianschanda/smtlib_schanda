(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11101101110011000001000)))
;; x should be Float32(0x8076E608 [Rational(-974017, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11001100 #b00001111011000010000110)))
;; y should be Float32(0x6607B086 [Rational(160193939465494016819200), 160193939465494016819200.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b0 #b11001100 #b00001111011000010000110)))
(check-sat)
(exit)
