(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11110000 #b11111011111010011101100)))
;; x should be Float32(0x787DF4EC [Rational(20603417359358683812270793390292992), 20603417359358683812270793390292992.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80800000)))
;; y should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x787DF4EB))))
(check-sat)
(exit)
