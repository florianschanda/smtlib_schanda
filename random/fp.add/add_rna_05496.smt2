(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x11B987FA)))
;; x should be Float32(0x11B987FA [Rational(6079485, 20769187434139310514121985316880384), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF9ECB520)))
;; y should be Float32(0xF9ECB520 [Rational(-153631963756710091142341822448140288), -153631963756710091142341822448140288.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b11110011 #b11011001011010100100000)))
(check-sat)
(exit)
