(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10111101 #b00001101101000011000111)))
;; x should be Float32(0x5E86D0C7 [Rational(4857241599526043648), 4857241599526043648.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xDDAE2042)))
;; y should be Float32(0xDDAE2042 [Rational(-1568387641202704384), -1568387641202704384.000000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result ((_ to_fp 8 24) #x5E36916D)))
(check-sat)
(exit)
