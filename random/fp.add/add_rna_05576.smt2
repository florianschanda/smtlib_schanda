(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC8C86D10)))
;; x should be Float32(0xC8C86D10 [Rational(-820945, 2), -410472.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB169B3)))
;; y should be Float32(0x4AB169B3 [Rational(11626931, 2), 5813465.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b0 #b10010101 #b01001001110001011100010)))
(check-sat)
(exit)
