(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100100101110010101101)))
;; x should be Float32(0x80325CAD [Rational(-3300525, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11001100111001111001010)))
;; y should be Float32(0xCA6673CA [Rational(-7551461, 2), -3775730.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA6673CA))))
(check-sat)
(exit)
