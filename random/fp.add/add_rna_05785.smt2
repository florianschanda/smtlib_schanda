(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x85C89B50)))
;; x should be Float32(0x85C89B50 [Rational(-821685, 43556142965880123323311949751266331066368), -0.000000])

