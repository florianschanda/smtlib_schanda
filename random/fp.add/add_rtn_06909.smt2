(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB1F7F3)))
;; x should be Float32(0xCAB1F7F3 [Rational(-11663347, 2), -5831673.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCDE64510)))
;; y should be Float32(0xCDE64510 [Rational(-482910720), -482910720.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (not (= result (fp #b1 #b10011011 #b11010010000110011110000))))
(check-sat)
(exit)
