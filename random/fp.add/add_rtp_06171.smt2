(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x000A2B9D)))
;; x should be Float32(0x000A2B9D [Rational(666525, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00001000110101101000010)))
;; y should be Float32(0xCA046B42 [Rational(-4339105, 2), -2169552.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b1 #b10010100 #b00001000110101101000001))))
(check-sat)
(exit)
