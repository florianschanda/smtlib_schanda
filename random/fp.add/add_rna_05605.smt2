(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00010000011110010010111)))
;; x should be Float32(0xCA883C97 [Rational(-8928407, 2), -4464203.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11101101 #b10111010101111010011111)))
;; y should be Float32(0xF6DD5E9F [Rational(-2244954588899960802646031452864512), -2244954588899960802646031452864512.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b11101101 #b10111010101111010011111))))
(check-sat)
(exit)
