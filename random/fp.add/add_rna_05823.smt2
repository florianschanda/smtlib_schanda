(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80417959)))
;; x should be Float32(0x80417959 [Rational(-4290905, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00011011110100001110010)))
;; y should be Float32(0x000DE872 [Rational(455737, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b00000000 #b01100111001000011100111))))
(check-sat)
(exit)
