(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001010 #b01001010000011000011000)))
;; x should be Float32(0x85250618 [Rational(-1351875, 174224571863520493293247799005065324265472), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA8A082D)))
;; y should be Float32(0xCA8A082D [Rational(-9046061, 2), -4523030.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result ((_ to_fp 8 24) #xCA8A082D)))
(check-sat)
(exit)
