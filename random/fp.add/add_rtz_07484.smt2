(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001B8F41)))
;; x should be Float32(0x001B8F41 [Rational(1806145, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xBEF24EFF)))
;; y should be Float32(0xBEF24EFF [Rational(-15879935, 33554432), -0.473259])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b1 #b01111101 #b11100100100111011111110))))
(check-sat)
(exit)
