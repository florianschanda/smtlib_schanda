(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011110 #b00111010011001000110010)))
;; x should be Float32(0x2F1D3232 [Rational(5151001, 36028797018963968), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA432C9A)))
;; y should be Float32(0xCA432C9A [Rational(-6395469, 2), -3197734.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b1 #b10010100 #b10000110010110010011001)))
(check-sat)
(exit)
