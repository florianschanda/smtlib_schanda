(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x003DF9CB)))
;; x should be Float32(0x003DF9CB [Rational(4061643, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011001 #b01001101001110001110001)))
;; y should be Float32(0x0CA69C71 [Rational(10919025, 42535295865117307932921825928971026432), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b0 #b00011001 #b01001101001110001110001))))
(check-sat)
(exit)
