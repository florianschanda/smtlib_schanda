(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10001110001110101100111)))
;; x should be Float32(0x4AC71D67 [Rational(13049191, 2), 6524595.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A6A96BA)))
;; y should be Float32(0x4A6A96BA [Rational(7687005, 2), 3843502.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b0 #b10010110 #b00111100011010001100010))))
(check-sat)
(exit)
