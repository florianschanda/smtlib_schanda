(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11010101111010011110111)))
;; x should be Float32(0x006AF4F7 [Rational(7009527, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AED2DE5)))
;; y should be Float32(0x4AED2DE5 [Rational(15543781, 2), 7771890.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b0 #b10010101 #b11011010010110111100101)))
(check-sat)
(exit)
