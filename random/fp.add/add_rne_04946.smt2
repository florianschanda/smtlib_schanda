(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAEE4A25)))
;; x should be Float32(0xCAEE4A25 [Rational(-15616549, 2), -7808274.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB229D54C)))
;; y should be Float32(0xB229D54C [Rational(-2782547, 281474976710656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b10010101 #b11011100100101000100101)))
(check-sat)
(exit)
