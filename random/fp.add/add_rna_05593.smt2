(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00010010001110010011110)))
;; x should be Float32(0xCA091C9E [Rational(-4492879, 2), -2246439.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11100111100001011101010)))
;; y should be Float32(0xCA73C2EA [Rational(-7987573, 2), -3993786.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b10010101 #b01111100110111111000100)))
(check-sat)
(exit)
