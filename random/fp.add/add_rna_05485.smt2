(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x06CC5005)))
;; x should be Float32(0x06CC5005 [Rational(13389829, 174224571863520493293247799005065324265472), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC8CDAB30)))
;; y should be Float32(0xC8CDAB30 [Rational(-842419, 2), -421209.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result (fp #b1 #b10010001 #b10011011010101100110000))))
(check-sat)
(exit)
