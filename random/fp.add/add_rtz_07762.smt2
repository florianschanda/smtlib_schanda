(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10011010011110001010001)))
;; x should be Float32(0x804D3C51 [Rational(-5061713, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00001001100001010010110)))
;; y should be Float32(0x4A04C296 [Rational(4350283, 2), 2175141.500000])

(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (not (= result (fp #b0 #b10010100 #b00001001100001010010101))))
(check-sat)
(exit)
