(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01001011 #b10111100011011101100011)))
;; x should be Float32(0xA5DE3763 [Rational(-14563171, 37778931862957161709568), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11110110010110111111111)))
;; y should be Float32(0x807B2DFF [Rational(-8072703, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #xA5DE3764)))
(check-sat)
(exit)
