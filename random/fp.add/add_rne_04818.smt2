(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111011 #b11111101100001100000001)))
;; x should be Float32(0x1DFEC301 [Rational(16696065, 2475880078570760549798248448), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01000000 #b00010101000100010011011)))
;; y should be Float32(0x200A889B [Rational(9078939, 77371252455336267181195264), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b0 #b01000000 #b00100100111111010110011))))
(check-sat)
(exit)
