(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10101011 #b00000110111110110111001)))
;; x should be Float32(0xD5837DB9 [Rational(-18071999741952), -18071999741952.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAC95CD5)))
;; y should be Float32(0xCAC95CD5 [Rational(-13196501, 2), -6598250.500000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xD5837DBC))))
(check-sat)
(exit)
