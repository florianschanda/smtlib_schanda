(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b11101010111011111111100)))
;; x should be Float32(0xC97577FC [Rational(-4021759, 4), -1005439.750000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAB9DAAB)))
;; y should be Float32(0xCAB9DAAB [Rational(-12180139, 2), -6090069.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #xCAD889AB)))
(check-sat)
(exit)
