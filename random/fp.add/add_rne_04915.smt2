(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00111100000111011010011)))
;; x should be Float32(0xCA9E0ED3 [Rational(-10358483, 2), -5179241.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00111011011100110111001)))
;; y should be Float32(0x4A9DB9B9 [Rational(10336697, 2), 5168348.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result ((_ to_fp 8 24) #xC62A3400)))
(check-sat)
(exit)
