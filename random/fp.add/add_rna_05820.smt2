(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10101001010001011010000)))
;; x should be Float32(0x8054A2D0 [Rational(-346669, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11110001 #b00010001101001000110001)))
;; y should be Float32(0x7888D231 [Rational(22200486279920831475678703349923840), 22200486279920831475678703349923840.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x7888D231))))
(check-sat)
(exit)
