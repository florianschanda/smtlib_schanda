(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001000 #b00000100010000010111101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10110101010110101011010)))
(declare-const result Float32)
(assert (= result (fp.add RTZ x y)))
(assert (= result (fp #b1 #b00001000 #b00000010110101101100010)))
(check-sat)
(exit)
