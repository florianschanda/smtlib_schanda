(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01110111 #b11001001000100110000101)))
;; x should be Float32(0xBBE48985 [Rational(-14977413, 2147483648), -0.006974])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB7DC4E58)))
;; y should be Float32(0xB7DC4E58 [Rational(-1804747, 68719476736), -0.000026])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xBBE565D3))))
(check-sat)
(exit)
