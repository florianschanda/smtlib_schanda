(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4917DDB8)))
;; x should be Float32(0x4917DDB8 [Rational(1244087, 2), 622043.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA259186)))
;; y should be Float32(0xCA259186 [Rational(-5425347, 2), -2712673.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result (fp #b1 #b10010011 #b11111110011010000110000))))
(check-sat)
(exit)
