(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8F8D5713)))
;; x should be Float32(0x8F8D5713 [Rational(-9262867, 664613997892457936451903530140172288), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF8A8AEF)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
