(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010000 #b10010101010110100000101)))
;; x should be Float32(0x084AAD05 [Rational(13282565, 21778071482940061661655974875633165533184), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01010101010011001110011)))
;; y should be Float32(0x002AA673 [Rational(2795123, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #x084AAD5A)))
(check-sat)
(exit)
