(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1F9B6905)))
;; x should be Float32(0x1F9B6905 [Rational(10184965, 154742504910672534362390528), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10011110101001100001011)))
;; y should be Float32(0xCACF530B [Rational(-13587211, 2), -6793605.500000])

(declare-const result Float32)
(assert (= result (fp.add RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xCACF530A))))
(check-sat)
(exit)
