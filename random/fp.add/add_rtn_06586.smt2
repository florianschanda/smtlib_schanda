(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ACEB3E7)))
;; x should be Float32(0x4ACEB3E7 [Rational(13546471, 2), 6773235.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAFF17B9)))
;; y should be Float32(0xCAFF17B9 [Rational(-16717753, 2), -8358876.500000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result ((_ to_fp 8 24) #xC9C18F48)))
(check-sat)
(exit)
