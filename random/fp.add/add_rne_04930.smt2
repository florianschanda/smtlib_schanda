(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01110011000111110010100)))
;; x should be Float32(0xC9B98F94 [Rational(-3040229, 2), -1520114.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x000BF79C)))
;; y should be Float32(0x000BF79C [Rational(196071, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result (fp #b1 #b10010011 #b01110011000111110010100)))
(check-sat)
(exit)
