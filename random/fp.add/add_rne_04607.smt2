(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00100101101010101100001)))
;; x should be Float32(0x4A92D561 [Rational(9622881, 2), 4811440.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A1651B6)))
;; y should be Float32(0x4A1651B6 [Rational(4925659, 2), 2462829.500000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (= result ((_ to_fp 8 24) #x4ADDFE3C)))
(check-sat)
(exit)
