(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB122DF)))
;; x should be Float32(0xCAB122DF [Rational(-11608799, 2), -5804399.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2CD9FAA3)))
;; y should be Float32(0x2CD9FAA3 [Rational(14285475, 2305843009213693952), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b1 #b10010101 #b01100010010001011011111)))
(check-sat)
(exit)
