(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA60A64A)))
;; x should be Float32(0xCA60A64A [Rational(-7361317, 2), -3680658.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x84A4322A)))
;; y should be Float32(0x84A4322A [Rational(-5380373, 1393796574908163946345982392040522594123776), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RTN x y)))
(assert (= result (fp #b1 #b10010100 #b11000001010011001001011)))
(check-sat)
(exit)
