(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A6E5F3A)))
;; x should be Float32(0x4A6E5F3A [Rational(7810973, 2), 3905486.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b00011100001101011000100)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
