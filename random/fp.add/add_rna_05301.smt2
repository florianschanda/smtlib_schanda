(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AA19663)))
;; x should be Float32(0x4AA19663 [Rational(10589795, 2), 5294897.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010100000001011110110)))
;; y should be Float32(0x800A02F6 [Rational(-328059, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNA x y)))
(assert (= result (fp #b0 #b10010101 #b01000011001011001100011)))
(check-sat)
(exit)
