(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11101011101010100011110)))
;; x should be Float32(0xCA75D51E [Rational(-8055439, 2), -4027719.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00398323)))
;; y should be Float32(0x00398323 [Rational(3769123, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.add RNE x y)))
(assert (not (= result (fp #b1 #b10010100 #b11101011101010100011110))))
(check-sat)
(exit)
