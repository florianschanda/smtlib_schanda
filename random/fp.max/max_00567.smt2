(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010110 #b01111000100100000101011)))
;; x should be Float32(0xEB3C482B [Rational(-227618857684312068810866688), -227618857684312068810866688.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10100001 #b01010110101011100111001)))
;; y should be Float32(0xD0AB5739 [Rational(-22996961280), -22996961280.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result ((_ to_fp 8 24) #xD0AB5739)))
(check-sat)
(exit)
