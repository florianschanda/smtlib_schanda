(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01101110100001011010010)))
;; x should be Float32(0x4A3742D2 [Rational(6005097, 2), 3002548.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10011111111111100000000)))
;; y should be Float32(0x804FFF00 [Rational(-20479, 2787593149816327892691964784081045188247552), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b10010100 #b01101110100001011010010))))
(check-sat)
(exit)
