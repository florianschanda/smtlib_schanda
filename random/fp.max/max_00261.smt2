(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00101101 #b01111110000011111001000)))
;; x should be Float32(0x16BF07C8 [Rational(1564921, 5070602400912917605986812821504), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xABD437B2)))
;; y should be Float32(0xABD437B2 [Rational(-6953945, 4611686018427387904), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b00101101 #b01111110000011111001000)))
(check-sat)
(exit)
