(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB52A3911)))
;; x should be Float32(0xB52A3911 [Rational(-11155729, 17592186044416), -0.000001])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11011101011110011010100)))
;; y should be Float32(0xC9EEBCD4 [Rational(-3911477, 2), -1955738.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result ((_ to_fp 8 24) #xB52A3911))))
(check-sat)
(exit)
