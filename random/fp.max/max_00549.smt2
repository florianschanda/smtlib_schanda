(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11100111 #b00100000101110011100010)))
;; x should be Float32(0xF3905CE2 [Rational(-22875202480385722531972984602624), -22875202480385722531972984602624.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x03714332)))
;; y should be Float32(0x03714332 [Rational(7905689, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result ((_ to_fp 8 24) #x03714332)))
(check-sat)
(exit)
