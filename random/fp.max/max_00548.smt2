(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111100 #b00100000110000101000001)))
;; x should be Float32(0xFE106141 [Rational(-47978450636232500334173307848909389824), -47978450636232500334173307848909389824.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1BAAC0C2)))
;; y should be Float32(0x1BAAC0C2 [Rational(5595233, 19807040628566084398385987584), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b00110111 #b01010101100000011000010))))
(check-sat)
(exit)
