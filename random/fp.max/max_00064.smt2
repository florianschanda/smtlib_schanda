(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b10101001010010000100100)))
;; x should be Float32(0x49D4A424 [Rational(3483913, 2), 1741956.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9CDCBE8A)))
;; y should be Float32(0x9CDCBE8A [Rational(-7233349, 4951760157141521099596496896), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b10010011 #b10101001010010000100100))))
(check-sat)
(exit)
