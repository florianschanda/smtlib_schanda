(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
;; x should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01101100101100110011101)))
;; y should be Float32(0xCAB6599D [Rational(-11950493, 2), -5975246.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000000001)))
(check-sat)
(exit)
