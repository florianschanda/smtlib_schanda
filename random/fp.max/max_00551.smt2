(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b01011100000100101010101)))
;; x should be Float32(0xC8AE0955 [Rational(-11405653, 32), -356426.656250])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x007378B4)))
;; y should be Float32(0x007378B4 [Rational(1891885, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b00000000 #b11100110111100010110100))))
(check-sat)
(exit)
