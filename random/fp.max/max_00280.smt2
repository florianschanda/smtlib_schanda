(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001A5C3B)))
;; x should be Float32(0x001A5C3B [Rational(1727547, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7D764B97)))
;; y should be Float32(0x7D764B97 [Rational(20461410741958098013044396255537528832), 20461410741958098013044396255537528832.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result ((_ to_fp 8 24) #x7D764B97))))
(check-sat)
(exit)
