(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b11110011101010100001100)))
;; x should be Float32(0x49F9D50C [Rational(4093251, 2), 2046625.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAD9594B)))
;; y should be Float32(0xCAD9594B [Rational(-14244171, 2), -7122085.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b10010011 #b11110011101010100001100))))
(check-sat)
(exit)
