(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011001 #b11110101011101010110011)))
;; x should be Float32(0x4CFABAB3 [Rational(131454360), 131454360.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b00101011110001001000011)))
;; y should be Float32(0x4895E243 [Rational(9822787, 32), 306962.093750])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b10011001 #b11110101011101010110011)))
(check-sat)
(exit)
