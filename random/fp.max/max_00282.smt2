(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00129CAD)))
;; x should be Float32(0x00129CAD [Rational(1219757, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00010000000000100101100)))
;; y should be Float32(0x0008012C [Rational(131147, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b00000000 #b00100101001110010101101)))
(check-sat)
(exit)
