(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00101110110101111111010)))
;; x should be Float32(0xCA176BFA [Rational(-4961789, 2), -2480894.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10010010111100101101001)))
;; y should be Float32(0xCAC97969 [Rational(-13203817, 2), -6601908.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b1 #b10010100 #b00101110110101111111010)))
(check-sat)
(exit)
