(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001101 #b00100110000010110000000)))
;; x should be Float32(0x06930580 [Rational(75275, 1361129467683753853853498429727072845824), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x195AEA27)))
;; y should be Float32(0x195AEA27 [Rational(14346791, 1267650600228229401496703205376), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result ((_ to_fp 8 24) #x195AEA27))))
(check-sat)
(exit)
