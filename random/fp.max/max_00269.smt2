(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00368184)))
;; x should be Float32(0x00368184 [Rational(893025, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01100000000111110110111)))
;; y should be Float32(0x4AB00FB7 [Rational(11538359, 2), 5769179.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result ((_ to_fp 8 24) #x4AB00FB7))))
(check-sat)
(exit)
