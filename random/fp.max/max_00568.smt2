(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDC52F791)))
;; x should be Float32(0xDC52F791 [Rational(-237527789006225408), -237527789006225408.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11011111010111001101001)))
;; y should be Float32(0x806FAE69 [Rational(-7319145, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result ((_ to_fp 8 24) #x806FAE69))))
(check-sat)
(exit)
