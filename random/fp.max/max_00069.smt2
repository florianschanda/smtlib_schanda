(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49261268)))
;; x should be Float32(0x49261268 [Rational(1360461, 2), 680230.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b01000000000000010000101)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b10010010 #b01001100001001001101000)))
(check-sat)
(exit)
