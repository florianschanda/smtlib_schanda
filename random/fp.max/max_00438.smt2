(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111111111111111111111)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10110110111001100011011)))
;; y should be Float32(0x4ADB731B [Rational(14381851, 2), 7190925.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b10010101 #b10110110111001100011011))))
(check-sat)
(exit)
