(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xAA19ADC9)))
;; x should be Float32(0xAA19ADC9 [Rational(-10071497, 73786976294838206464), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF3E8F3D)))
;; y should be Float32(0xFF3E8F3D [Rational(-253297054876890318954499063210672390144), -253297054876890318954499063210672390144.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b1 #b01010100 #b00110011010110111001001)))
(check-sat)
(exit)
