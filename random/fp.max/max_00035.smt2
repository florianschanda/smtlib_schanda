(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AFC09A3)))
;; x should be Float32(0x4AFC09A3 [Rational(16517539, 2), 8258769.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11111010011110101011111)))
;; y should be Float32(0x4AFD3D5F [Rational(16596319, 2), 8298159.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result ((_ to_fp 8 24) #x4AFD3D5F)))
(check-sat)
(exit)
