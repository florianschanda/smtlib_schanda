(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001010 #b01000100111101110101001)))
;; x should be Float32(0x85227BA9 [Rational(-10648489, 1393796574908163946345982392040522594123776), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7F883A3C)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b1 #b00001010 #b01000100111101110101001))))
(check-sat)
(exit)
