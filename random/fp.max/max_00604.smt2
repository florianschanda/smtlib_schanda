(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8032CCA3)))
;; x should be Float32(0x8032CCA3 [Rational(-3329187, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00100000 #b01011110101111110111010)))
;; y should be Float32(0x902F5FBA [Rational(-5746653, 166153499473114484112975882535043072), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b1 #b00000000 #b01100101100110010100011)))
(check-sat)
(exit)
