(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x06151B0B)))
;; x should be Float32(0x06151B0B [Rational(9771787, 348449143727040986586495598010130648530944), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11010111000100000101010)))
;; y should be Float32(0x806B882A [Rational(-3523605, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result ((_ to_fp 8 24) #x06151B0B))))
(check-sat)
(exit)
