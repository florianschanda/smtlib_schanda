(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01001100100010011110001)))
;; x should be Float32(0xCAA644F1 [Rational(-10896625, 2), -5448312.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01000011 #b00000000101000001101111)))
;; y should be Float32(0x2180506F [Rational(8409199, 9671406556917033397649408), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result ((_ to_fp 8 24) #x2180506F))))
(check-sat)
(exit)
