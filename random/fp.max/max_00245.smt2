(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01101101 #b01101001110001001010111)))
;; x should be Float32(0x36B4E257 [Rational(11854423, 2199023255552), 0.000005])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00100001011011101110000)))
;; y should be Float32(0x0010B770 [Rational(68471, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b01101101 #b01101001110001001010111)))
(check-sat)
(exit)
