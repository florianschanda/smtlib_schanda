(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10101001011000111011101)))
;; x should be Float32(0x4AD4B1DD [Rational(13939165, 2), 6969582.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0045D5D6)))
;; y should be Float32(0x0045D5D6 [Rational(2288363, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b10010101 #b10101001011000111011101))))
(check-sat)
(exit)
