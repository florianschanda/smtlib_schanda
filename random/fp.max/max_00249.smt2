(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x168A6349)))
;; x should be Float32(0x168A6349 [Rational(9069385, 40564819207303340847894502572032), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA20367A)))
;; y should be Float32(0xCA20367A [Rational(-5249853, 2), -2624926.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b00101101 #b00010100110001101001001))))
(check-sat)
(exit)
