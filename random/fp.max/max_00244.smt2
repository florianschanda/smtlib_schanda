(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x10E2B85F)))
;; x should be Float32(0x10E2B85F [Rational(14858335, 166153499473114484112975882535043072), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10110101010110100110101)))
;; y should be Float32(0x005AAD35 [Rational(5942581, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b00100001 #b11000101011100001011111))))
(check-sat)
(exit)
