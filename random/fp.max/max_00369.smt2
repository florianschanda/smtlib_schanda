(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b10111101101101110010100)))
;; x should be Float32(0xC9DEDB94 [Rational(-3651301, 2), -1825650.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA4AEE86C)))
;; y should be Float32(0xA4AEE86C [Rational(-2865691, 37778931862957161709568), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b1 #b01001001 #b01011101110100001101100))))
(check-sat)
(exit)
