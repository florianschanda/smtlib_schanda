(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01010001101111001011111)))
;; x should be Float32(0xCAA8DE5F [Rational(-11066975, 2), -5533487.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00111100 #b11100010110100011101111)))
;; y should be Float32(0x9E7168EF [Rational(-15821039, 1237940039285380274899124224), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b1 #b00111100 #b11100010110100011101111))))
(check-sat)
(exit)
