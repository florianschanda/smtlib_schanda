(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000110 #b11000100011100111111010)))
;; x should be Float32(0x036239FA [Rational(7412989, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC5A82EE9)))
;; y should be Float32(0xC5A82EE9 [Rational(-11022057, 2048), -5381.863770])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b00000110 #b11000100011100111111010)))
(check-sat)
(exit)
