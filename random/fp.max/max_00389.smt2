(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b00000000000000000000000)))
;; x should be Float32(-oo)

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111011010111001011110)))
;; y should be Float32(0x001DAE5E [Rational(972591, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b00000000 #b00111011010111001011110))))
(check-sat)
(exit)
