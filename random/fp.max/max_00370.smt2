(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01100011010101111001111)))
;; x should be Float32(0xCAB1ABCF [Rational(-11643855, 2), -5821927.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10000111100101100001011)))
;; y should be Float32(0x8043CB0B [Rational(-4442891, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b1 #b00000000 #b10000111100101100001011)))
(check-sat)
(exit)
