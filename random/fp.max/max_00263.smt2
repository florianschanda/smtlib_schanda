(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01101001 #b00000110100101110101000)))
;; x should be Float32(0x34834BA8 [Rational(1075573, 4398046511104), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10101110010100100010100)))
;; y should be Float32(0x80572914 [Rational(-1428037, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result ((_ to_fp 8 24) #x34834BA8)))
(check-sat)
(exit)
