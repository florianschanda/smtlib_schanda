(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10000100101111111000010)))
;; x should be Float32(0x4A425FC2 [Rational(6369249, 2), 3184624.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10110110110010111101000)))
;; y should be Float32(0x805B65E8 [Rational(-748733, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b10010100 #b10000100101111111000010)))
(check-sat)
(exit)
