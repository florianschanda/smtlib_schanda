(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00011100110111100100000)))
;; x should be Float32(0x000E6F20 [Rational(29561, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x5B33A3C1)))
;; y should be Float32(0x5B33A3C1 [Rational(50564070155223040), 50564070155223040.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b10110110 #b01100111010001111000001))))
(check-sat)
(exit)
