(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB5CED9)))
;; x should be Float32(0xCAB5CED9 [Rational(-11914969, 2), -5957484.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8057C7CC)))
;; y should be Float32(0x8057C7CC [Rational(-1438195, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result ((_ to_fp 8 24) #x8057C7CC)))
(check-sat)
(exit)
