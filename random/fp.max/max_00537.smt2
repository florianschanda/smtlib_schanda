(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00011111 #b11011110100011000010011)))
;; x should be Float32(0x8FEF4613 [Rational(-15681043, 664613997892457936451903530140172288), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4995A484)))
;; y should be Float32(0x4995A484 [Rational(2451745, 2), 1225872.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b10010011 #b00101011010010010000100)))
(check-sat)
(exit)
