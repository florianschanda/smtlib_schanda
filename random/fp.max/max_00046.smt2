(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01000001001101001101101)))
;; x should be Float32(0x4AA09A6D [Rational(10525293, 2), 5262646.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x3A47DAB5)))
;; y should be Float32(0x3A47DAB5 [Rational(13097653, 17179869184), 0.000762])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b10010101 #b01000001001101001101101)))
(check-sat)
(exit)
