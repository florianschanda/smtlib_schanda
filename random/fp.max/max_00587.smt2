(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01111110010111001110110)))
;; x should be Float32(0x803F2E76 [Rational(-2070331, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111000 #b01000011000010011000000)))
;; y should be Float32(0x7C2184C0 [Rational(3354609136395968029768755626450616320), 3354609136395968029768755626450616320.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b11111000 #b01000011000010011000000))))
(check-sat)
(exit)
