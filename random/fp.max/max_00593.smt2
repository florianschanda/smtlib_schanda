(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100000001110000011100)))
;; x should be Float32(0x80301C1C [Rational(-788231, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01111011000011110001111)))
;; y should be Float32(0xCABD878F [Rational(-12421007, 2), -6210503.500000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result ((_ to_fp 8 24) #x80301C1C))))
(check-sat)
(exit)
