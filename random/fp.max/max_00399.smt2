(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b00000000000000000000000)))
;; x should be Float32(-oo)

(declare-const y Float32)
(assert (= y (fp #b1 #b01010000 #b00000100001011101000011)))
;; y should be Float32(0xA8021743 [Rational(-8525635, 1180591620717411303424), -0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b1 #b01010000 #b00000100001011101000011)))
(check-sat)
(exit)
