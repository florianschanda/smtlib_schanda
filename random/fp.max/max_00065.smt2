(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A203456)))
;; x should be Float32(0x4A203456 [Rational(5249579, 2), 2624789.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11011101 #b00011010110101110011010)))
;; y should be Float32(0xEE8D6B9A [Rational(-21883734345973390968625299456), -21883734345973390968625299456.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b10010100 #b01000000011010001010110)))
(check-sat)
(exit)
