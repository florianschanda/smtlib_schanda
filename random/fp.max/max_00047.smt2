(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AEDBA2B)))
;; x should be Float32(0x4AEDBA2B [Rational(15579691, 2), 7789845.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4D03419B)))
;; y should be Float32(0x4D03419B [Rational(137632176), 137632176.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (= result (fp #b0 #b10011010 #b00000110100000110011011)))
(check-sat)
(exit)
