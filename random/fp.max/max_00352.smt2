(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10011110111000110001101)))
;; x should be Float32(0xCACF718D [Rational(-13595021, 2), -6797510.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00110100000110111101000)))
;; y should be Float32(0x001A0DE8 [Rational(213437, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.max x y)))
(assert (not (= result (fp #b0 #b00000000 #b00110100000110111101000))))
(check-sat)
(exit)
