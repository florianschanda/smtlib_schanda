(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101001 #b11100000011100000010001)))
(declare-const result Float32)
(assert (= result (fp.neg x)))
(assert (= result ((_ to_fp 8 24) #x34F03811)))
(check-sat)
(exit)
