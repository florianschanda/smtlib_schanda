(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFFE6C62A)))
(declare-const result Float32)
(assert (= result (fp.neg x)))
(assert (= result (fp #b1 #b11111111 #b11001101100011000101010)))
(check-sat)
(exit)
