(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11010100 #b01111000001010111011111)))
;; x should be Float32(0x6A3C15DF [Rational(56845334351904746156064768), 56845334351904746156064768.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD39C6795)))
;; y should be Float32(0xD39C6795 [Rational(-1343505432576), -1343505432576.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b10100111 #b00111000110011110010101)))
(check-sat)
(exit)
