(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00110011 #b11111111001100110111011)))
;; x should be Float32(0x99FF99BB [Rational(-16751035, 633825300114114700748351602688), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0008986B)))
;; y should be Float32(0x0008986B [Rational(563307, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result ((_ to_fp 8 24) #x99FF99BB))))
(check-sat)
(exit)
