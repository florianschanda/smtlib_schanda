(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01101110011001111001010)))
;; x should be Float32(0x803733CA [Rational(-1808869, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11100000110100010111010)))
;; y should be Float32(0x4A7068BA [Rational(7877725, 2), 3938862.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b00000000 #b01101110011001111001010)))
(check-sat)
(exit)
