(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b01111000010010011100110)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b00010111 #b00101011111011011000110)))
;; y should be Float32(0x0B95F6C6 [Rational(4914019, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b0 #b00010111 #b00101011111011011000110)))
(check-sat)
(exit)
