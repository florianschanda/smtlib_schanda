(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x103B0540)))
;; x should be Float32(0x103B0540 [Rational(191509, 5192296858534827628530496329220096), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCABC7227)))
;; y should be Float32(0xCABC7227 [Rational(-12349991, 2), -6174995.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10010101 #b01111000111001000100111))))
(check-sat)
(exit)
