(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001110 #b10100001101101001110000)))
;; x should be Float32(0x6750DA70 [Rational(986281689095487134629888), 986281689095487134629888.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10110101111110001010100)))
;; y should be Float32(0xC9DAFC54 [Rational(-3587861, 2), -1793930.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b10010011 #b10110101111110001010100)))
(check-sat)
(exit)
