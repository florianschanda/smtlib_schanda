(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110100 #b00100110110101111001010)))
;; x should be Float32(0xFA136BCA [Rational(-191363465030259567881384803040231424), -191363465030259567881384803040231424.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b11011111001010111010111)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result ((_ to_fp 8 24) #xFA136BCA))))
(check-sat)
(exit)
