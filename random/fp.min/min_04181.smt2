(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x529C5617)))
;; x should be Float32(0x529C5617 [Rational(335729623040), 335729623040.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD5728895)))
;; y should be Float32(0xD5728895 [Rational(-16666776829952), -16666776829952.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10101010 #b11100101000100010010101))))
(check-sat)
(exit)
