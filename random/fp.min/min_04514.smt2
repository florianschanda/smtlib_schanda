(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111100111000001000011)))
;; x should be Float32(0x807E7043 [Rational(-8286275, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11111101010000110001011)))
;; y should be Float32(0xCAFEA18B [Rational(-16687499, 2), -8343749.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAFEA18B))))
(check-sat)
(exit)
