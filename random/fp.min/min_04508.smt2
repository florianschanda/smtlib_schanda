(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10101011000111000000110)))
;; x should be Float32(0x80558E06 [Rational(-2803459, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011010 #b01011110111100110101101)))
;; y should be Float32(0x0D2F79AD [Rational(11499949, 21267647932558653966460912964485513216), 0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #x80558E06)))
(check-sat)
(exit)
