(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1BF1D7B8)))
;; x should be Float32(0x1BF1D7B8 [Rational(1981175, 4951760157141521099596496896), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4ADDBA75)))
;; y should be Float32(0x4ADDBA75 [Rational(14531189, 2), 7265594.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b0 #b00110111 #b11100011101011110111000)))
(check-sat)
(exit)
