(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00100011001111101011101)))
;; x should be Float32(0x00119F5D [Rational(1154909, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00011100100011101111110)))
;; y should be Float32(0xCA0E477E [Rational(-4662207, 2), -2331103.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b10010100 #b00011100100011101111110)))
(check-sat)
(exit)
