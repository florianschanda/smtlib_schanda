(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0D261712)))
;; x should be Float32(0x0D261712 [Rational(5442441, 10633823966279326983230456482242756608), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00109620)))
;; y should be Float32(0x00109620 [Rational(33969, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result ((_ to_fp 8 24) #x00109620))))
(check-sat)
(exit)
