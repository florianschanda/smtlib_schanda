(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10011100000011000010011)))
;; x should be Float32(0x004E0613 [Rational(5113363, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11011100 #b11110101011101100000100)))
;; y should be Float32(0x6E7ABB04 [Rational(19399335118267533578861543424), 19399335118267533578861543424.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b0 #b00000000 #b10011100000011000010011)))
(check-sat)
(exit)
