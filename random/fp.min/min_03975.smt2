(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00011101011101011110011)))
;; x should be Float32(0x4A8EBAF3 [Rational(9353971, 2), 4676985.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE917583B)))
;; y should be Float32(0xE917583B [Rational(-11435278460637617688936448), -11435278460637617688936448.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b11010010 #b00101110101100000111011)))
(check-sat)
(exit)
