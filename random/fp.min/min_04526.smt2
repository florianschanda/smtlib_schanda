(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10101101001010000001011)))
;; x should be Float32(0x8056940B [Rational(-5673995, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11110100 #b00101101010011011110010)))
;; y should be Float32(0xFA16A6F2 [Rational(-195557645497439693384911832683118592), -195557645497439693384911832683118592.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #xFA16A6F2)))
(check-sat)
(exit)
