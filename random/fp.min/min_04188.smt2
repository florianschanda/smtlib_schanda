(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01000011100011110100010)))
;; x should be Float32(0x0021C7A2 [Rational(1106897, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01011101010101100111100)))
;; y should be Float32(0x49AEAB3C [Rational(2861775, 2), 1430887.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #x0021C7A2)))
(check-sat)
(exit)
