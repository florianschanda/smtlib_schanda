(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11010001111011111110110)))
;; x should be Float32(0x0068F7F6 [Rational(3439611, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010010 #b10111111101000110011001)))
;; y should be Float32(0xE95FD199 [Rational(-16911265920051583710658560), -16911265920051583710658560.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b11010010 #b10111111101000110011001)))
(check-sat)
(exit)
