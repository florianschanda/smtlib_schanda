(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x004835CC)))
;; x should be Float32(0x004835CC [Rational(1183091, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10111111 #b01011100011000010011111)))
;; y should be Float32(0xDFAE309F [Rational(-25103413967660777472), -25103413967660777472.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #xDFAE309F)))
(check-sat)
(exit)
