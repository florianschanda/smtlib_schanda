(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10110001100100010011110)))
;; x should be Float32(0x8058C89E [Rational(-2909263, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10101101 #b10100101110110100100000)))
;; y should be Float32(0xD6D2ED20 [Rational(-115957942976512), -115957942976512.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10101101 #b10100101110110100100000))))
(check-sat)
(exit)
