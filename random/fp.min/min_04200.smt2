(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x005F15D8)))
;; x should be Float32(0x005F15D8 [Rational(778939, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x11D92E2A)))
;; y should be Float32(0x11D92E2A [Rational(7116565, 20769187434139310514121985316880384), 0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b0 #b00000000 #b10111110001010111011000))))
(check-sat)
(exit)
