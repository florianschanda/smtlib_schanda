(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00010010101000001101010)))
;; x should be Float32(0x4A09506A [Rational(4499509, 2), 2249754.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000011 #b01111010010000000001110)))
;; y should be Float32(0xC1BD200E [Rational(-6197255, 262144), -23.640652])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10000011 #b01111010010000000001110))))
(check-sat)
(exit)
