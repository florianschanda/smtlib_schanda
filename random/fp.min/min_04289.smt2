(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10001100 #b10101101111111000000000)))
;; x should be Float32(0xC656FE00 [Rational(-27519, 2), -13759.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01111110 #b11110011011101110110011)))
;; y should be Float32(0xBF79BBB3 [Rational(-16366515, 16777216), -0.975520])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result ((_ to_fp 8 24) #xC656FE00))))
(check-sat)
(exit)
