(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x499C72E4)))
;; x should be Float32(0x499C72E4 [Rational(2563257, 2), 1281628.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A37A53A)))
;; y should be Float32(0x4A37A53A [Rational(6017693, 2), 3008846.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b0 #b10010011 #b00111000111001011100100))))
(check-sat)
(exit)
