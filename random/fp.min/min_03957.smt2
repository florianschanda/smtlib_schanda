(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A2C1CF6)))
;; x should be Float32(0x4A2C1CF6 [Rational(5639803, 2), 2819901.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01101000 #b01001000001001101001111)))
;; y should be Float32(0x3424134F [Rational(10752847, 70368744177664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #x3424134F)))
(check-sat)
(exit)
