(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10010001101001000110101)))
;; x should be Float32(0x8048D235 [Rational(-4772405, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01101100111010111101011)))
;; y should be Float32(0xCAB675EB [Rational(-11957739, 2), -5978869.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAB675EB))))
(check-sat)
(exit)
