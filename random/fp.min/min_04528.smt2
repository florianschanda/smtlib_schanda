(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01010011100001101001011)))
;; x should be Float32(0x8029C34B [Rational(-2736971, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111011000010110100101)))
;; y should be Float32(0x807D85A5 [Rational(-8226213, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #x807D85A5)))
(check-sat)
(exit)
