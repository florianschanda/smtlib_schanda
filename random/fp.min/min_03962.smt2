(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b11001001111000000001000)))
;; x should be Float32(0x4964F008 [Rational(1875457, 2), 937728.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAFCB9A5)))
;; y should be Float32(0xCAFCB9A5 [Rational(-16562597, 2), -8281298.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b10010101 #b11111001011100110100101)))
(check-sat)
(exit)
