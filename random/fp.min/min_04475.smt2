(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b01101010110001110111010)))
;; x should be Float32(0xC93563BA [Rational(-5943773, 8), -742971.625000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01100110100000100000010)))
;; y should be Float32(0xCA334102 [Rational(-5873793, 2), -2936896.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #xCA334102)))
(check-sat)
(exit)
