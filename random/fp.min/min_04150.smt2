(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6D3BC9BD)))
;; x should be Float32(0x6D3BC9BD [Rational(3632348965849958167412211712), 3632348965849958167412211712.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10000100010001011011010)))
;; y should be Float32(0x4A4222DA [Rational(6361453, 2), 3180726.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b0 #b10010100 #b10000100010001011011010)))
(check-sat)
(exit)
