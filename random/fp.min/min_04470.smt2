(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD65AD85B)))
;; x should be Float32(0xD65AD85B [Rational(-60155693629440), -60155693629440.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x41043C47)))
;; y should be Float32(0x41043C47 [Rational(8666183, 1048576), 8.264716])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #xD65AD85B)))
(check-sat)
(exit)
