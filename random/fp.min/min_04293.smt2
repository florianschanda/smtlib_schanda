(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11010011011101000101001)))
;; x should be Float32(0xCAE9BA29 [Rational(-15317545, 2), -7658772.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b01111000111011101001000)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b10010101 #b11010011011101000101001)))
(check-sat)
(exit)
