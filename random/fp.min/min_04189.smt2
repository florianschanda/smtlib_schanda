(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x006A9B7F)))
;; x should be Float32(0x006A9B7F [Rational(6986623, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10011110110101010110010)))
;; y should be Float32(0x4A4F6AB2 [Rational(6796633, 2), 3398316.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b0 #b00000000 #b11010101001101101111111)))
(check-sat)
(exit)
