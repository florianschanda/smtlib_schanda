(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111111 #b11110110001000100110101)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b10110100011001010011000)))
;; y should be Float32(0xC95A3298 [Rational(-1787475, 2), -893737.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b10010010 #b10110100011001010011000)))
(check-sat)
(exit)
