(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAFD114B)))
;; x should be Float32(0xCAFD114B [Rational(-16585035, 2), -8292517.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01001100010011100101000)))
;; y should be Float32(0x80262728 [Rational(-312549, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b10010101 #b11111010001000101001011)))
(check-sat)
(exit)
