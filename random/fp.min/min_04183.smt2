(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01110101 #b01111111110000000100000)))
;; x should be Float32(0x3ABFE020 [Rational(392961, 268435456), 0.001464])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8034DCB6)))
;; y should be Float32(0x8034DCB6 [Rational(-1732187, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b00000000 #b01101001101110010110110))))
(check-sat)
(exit)
