(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB5E097)))
;; x should be Float32(0xCAB5E097 [Rational(-11919511, 2), -5959755.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00111010010100010011100)))
;; y should be Float32(0x499D289C [Rational(2574887, 2), 1287443.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10010101 #b01101011110000010010111))))
(check-sat)
(exit)
