(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA9817BD)))
;; x should be Float32(0xCA9817BD [Rational(-9967549, 2), -4983774.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x91DAF66F)))
;; y should be Float32(0x91DAF66F [Rational(-14349935, 41538374868278621028243970633760768), -0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10010101 #b00110000001011110111101))))
(check-sat)
(exit)
