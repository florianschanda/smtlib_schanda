(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A64ADBA)))
;; x should be Float32(0x4A64ADBA [Rational(7493341, 2), 3746670.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAF3A799)))
;; y should be Float32(0xCAF3A799 [Rational(-15968153, 2), -7984076.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10010101 #b11100111010011110011001))))
(check-sat)
(exit)
