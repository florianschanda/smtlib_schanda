(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00001001010001000111001)))
;; x should be Float32(0x0004A239 [Rational(303673, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101000100000011111101)))
;; y should be Float32(0x005440FD [Rational(5521661, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result ((_ to_fp 8 24) #x0004A239))))
(check-sat)
(exit)
