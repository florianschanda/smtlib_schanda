(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10101100 #b00101000001010100111011)))
;; x should be Float32(0x5614153B [Rational(40704726269952), 40704726269952.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11110111111110100011000)))
;; y should be Float32(0x807BFD18 [Rational(-1015715, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b00000000 #b11110111111110100011000))))
(check-sat)
(exit)
