(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10000100011000111001011)))
;; x should be Float32(0x4AC231CB [Rational(12726731, 2), 6363365.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010010001000111010011)))
;; y should be Float32(0x800911D3 [Rational(-594387, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b00000000 #b00010010001000111010011))))
(check-sat)
(exit)
