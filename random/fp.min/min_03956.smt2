(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A8A1E09)))
;; x should be Float32(0x4A8A1E09 [Rational(9051657, 2), 4525828.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x506FD357)))
;; y should be Float32(0x506FD357 [Rational(16094419968), 16094419968.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b0 #b10010101 #b00010100001111000001001))))
(check-sat)
(exit)
