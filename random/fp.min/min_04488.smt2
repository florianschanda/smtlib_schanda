(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA00072CE)))
;; x should be Float32(0xA00072CE [Rational(-4208999, 38685626227668133590597632), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000000 #b00100100001001011000100)))
;; y should be Float32(0xE01212C4 [Rational(-42102763133589258240), -42102763133589258240.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b11000000 #b00100100001001011000100)))
(check-sat)
(exit)
