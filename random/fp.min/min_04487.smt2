(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11001101 #b10000101101000011010011)))
;; x should be Float32(0xE6C2D0D3 [Rational(-459995612298192380493824), -459995612298192380493824.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10111000 #b11110011110100001011011)))
;; y should be Float32(0xDC79E85B [Rational(-281370986962485248), -281370986962485248.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b11001101 #b10000101101000011010011))))
(check-sat)
(exit)
