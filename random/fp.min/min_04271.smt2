(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCADD6A31)))
;; x should be Float32(0xCADD6A31 [Rational(-14510641, 2), -7255320.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x484FB923)))
;; y should be Float32(0x484FB923 [Rational(13613347, 64), 212708.546875])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10010101 #b10111010110101000110001))))
(check-sat)
(exit)
