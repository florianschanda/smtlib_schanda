(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA9FFB1B)))
;; x should be Float32(0xCA9FFB1B [Rational(-10484507, 2), -5242253.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b11010011111111101011000)))
;; y should be Float32(0x4969FF58 [Rational(1916907, 2), 958453.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b10010101 #b00111111111101100011011))))
(check-sat)
(exit)
