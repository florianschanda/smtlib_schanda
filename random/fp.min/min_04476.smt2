(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101100 #b10100011000010111101110)))
;; x should be Float32(0xB65185EE [Rational(-6865655, 2199023255552), -0.000003])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCACD9AA3)))
;; y should be Float32(0xCACD9AA3 [Rational(-13474467, 2), -6737233.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result ((_ to_fp 8 24) #xCACD9AA3)))
(check-sat)
(exit)
