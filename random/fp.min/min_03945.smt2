(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AE4B247)))
;; x should be Float32(0x4AE4B247 [Rational(14987847, 2), 7493923.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b10101101110100101111100)))
;; y should be Float32(0x49D6E97C [Rational(3521119, 2), 1760559.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result ((_ to_fp 8 24) #x49D6E97C))))
(check-sat)
(exit)
