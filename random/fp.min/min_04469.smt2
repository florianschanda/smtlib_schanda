(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01111100 #b00010111111011011001001)))
;; x should be Float32(0xBE0BF6C9 [Rational(-9172681, 67108864), -0.136684])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7E27556E)))
;; y should be Float32(0x7E27556E [Rational(55606162898528203201776876363110678528), 55606162898528203201776876363110678528.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (not (= result (fp #b1 #b01111100 #b00010111111011011001001))))
(check-sat)
(exit)
