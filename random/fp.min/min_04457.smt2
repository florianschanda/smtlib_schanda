(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB79E93BA)))
;; x should be Float32(0xB79E93BA [Rational(-5196253, 274877906944), -0.000019])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b11110110010011110100100)))
;; y should be Float32(0x49FB27A4 [Rational(4114921, 2), 2057460.500000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b01101111 #b00111101001001110111010)))
(check-sat)
(exit)
