(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000111 #b10111001001101100101010)))
;; x should be Float32(0x23DC9B2A [Rational(7228821, 302231454903657293676544), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00001111 #b00111000100100001010000)))
;; y should be Float32(0x079C4850 [Rational(640133, 2722258935367507707706996859454145691648), 0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b0 #b00001111 #b00111000100100001010000)))
(check-sat)
(exit)
