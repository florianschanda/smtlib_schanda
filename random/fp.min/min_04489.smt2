(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00100101 #b11101110101101111101111)))
;; x should be Float32(0x92F75BEF [Rational(-16210927, 10384593717069655257060992658440192), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10010100111101010100100)))
;; y should be Float32(0x804A7AA4 [Rational(-1220265, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.min x y)))
(assert (= result (fp #b1 #b00100101 #b11101110101101111101111)))
(check-sat)
(exit)
