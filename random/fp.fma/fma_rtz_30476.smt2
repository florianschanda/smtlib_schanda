(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00450D2A)))
;; x should be Float32(0x00450D2A [Rational(2262677, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x254AB912)))
;; y should be Float32(0x254AB912 [Rational(6642825, 37778931862957161709568), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b11100110110000011010100)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
