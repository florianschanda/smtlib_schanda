(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b10110101001110001011100)))
;; x should be Float32(0xC9DA9C5C [Rational(-3581719, 2), -1790859.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00000000)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x6A4CAC03)))
;; z should be Float32(0x6A4CAC03 [Rational(61858292394167537936367616), 61858292394167537936367616.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x6A4CAC03))))
(check-sat)
(exit)
