(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b01111100111100011000100)))
;; x should be Float32(0x49BE78C4 [Rational(3120689, 2), 1560344.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01100111110100110100000)))
;; y should be Float32(0x8033E9A0 [Rational(-106317, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xB09DDD3A)))
;; z should be Float32(0xB09DDD3A [Rational(-5172893, 4503599627370496), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b01100001 #b00111011101110100111011)))
(check-sat)
(exit)
