(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAAE6A43)))
;; x should be Float32(0xCAAE6A43 [Rational(-11430467, 2), -5715233.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00001100110111001011111)))
;; y should be Float32(0x4A866E5F [Rational(8810079, 2), 4405039.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC3AA127F)))
;; z should be Float32(0xC3AA127F [Rational(-11145855, 32768), -340.144501])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD5B72DA2))))
(check-sat)
(exit)
