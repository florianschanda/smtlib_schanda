(assert (= z (fp #b0 #b10000010 #b10111001001001110001010)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10000010 #b10111001001001110001011)))
(check-sat)
(exit)
