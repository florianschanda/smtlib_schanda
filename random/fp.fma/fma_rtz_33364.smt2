(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9B7F694E)))
;; x should be Float32(0x9B7F694E [Rational(-8369319, 39614081257132168796771975168), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x39768E4A)))
;; y should be Float32(0x39768E4A [Rational(8079141, 34359738368), 0.000235])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b01100110011000101010111)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
