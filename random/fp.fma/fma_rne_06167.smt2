(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x800F443D)))
;; x should be Float32(0x800F443D [Rational(-1000509, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB551E1)))
;; y should be Float32(0x4AB551E1 [Rational(11882977, 2), 5941488.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01101100110000001110101)))
;; z should be Float32(0xCAB66075 [Rational(-11952245, 2), -5976122.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCAB66075))))
(check-sat)
(exit)
