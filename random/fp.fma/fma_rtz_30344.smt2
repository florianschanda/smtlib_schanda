(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x004AFBA0)))
;; x should be Float32(0x004AFBA0 [Rational(153565, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AA43CF7)))
;; y should be Float32(0x4AA43CF7 [Rational(10763511, 2), 5381755.500000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0B406C37))))
(check-sat)
(exit)
