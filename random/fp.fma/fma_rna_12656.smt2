(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111011 #b00111011111100000011101)))
;; x should be Float32(0xFD9DF81D [Rational(-26247134143628366899526945752993497088), -26247134143628366899526945752993497088.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB52A93)))
;; y should be Float32(0x4AB52A93 [Rational(11872915, 2), 5936457.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x48AF6BD0)))
;; z should be Float32(0x48AF6BD0 [Rational(718525, 2), 359262.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
