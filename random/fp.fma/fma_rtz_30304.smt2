(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x099BDEB3)))
;; x should be Float32(0x099BDEB3 [Rational(10215091, 2722258935367507707706996859454145691648), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10101000101111111010101)))
;; y should be Float32(0x80545FD5 [Rational(-5529557, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01010000011101000010101)))
;; z should be Float32(0x80283A15 [Rational(-2636309, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x80283A15)))
(check-sat)
(exit)
