(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10100101 #b10110101101000010101101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11010100 #b01001111101101111000101)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8045BA86)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b11111011 #b00011110111100111111010))))
(check-sat)
(exit)
