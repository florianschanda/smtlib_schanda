(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011110 #b11010011101101001101100)))
;; x should be Float32(0x2F69DA6C [Rational(3831451, 18014398509481984), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805CD7A4)))
;; y should be Float32(0x805CD7A4 [Rational(-1521129, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111000111010110011110)))
;; z should be Float32(0x007C759E [Rational(4078287, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b00000000 #b11111000111010110011110))))
(check-sat)
(exit)
