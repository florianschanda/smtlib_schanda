(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111111111111111111111)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00110000 #b11000111110010100100101)))
;; y should be Float32(0x1863E525 [Rational(14935333, 5070602400912917605986812821504), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01001010110010100000111)))
;; z should be Float32(0x00256507 [Rational(2450695, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00000000 #b01001010110010100000111)))
(check-sat)
(exit)
