(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10001001100111001101100)))
;; y should be Float32(0xC9C4CE6C [Rational(-3224475, 2), -1612237.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11010010100010111001001)))
;; z should be Float32(0x4AE945C9 [Rational(15287753, 2), 7643876.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x4AE945C9)))
(check-sat)
(exit)
