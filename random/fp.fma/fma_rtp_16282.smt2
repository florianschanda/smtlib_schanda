(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3183A7C7)))
;; x should be Float32(0x3183A7C7 [Rational(8628167, 2251799813685248), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10111011111000000111011)))
;; y should be Float32(0x4ADDF03B [Rational(14544955, 2), 7272477.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8006376D)))
;; z should be Float32(0x8006376D [Rational(-407405, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x3CE446C5))))
(check-sat)
(exit)
