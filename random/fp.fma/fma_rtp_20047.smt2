(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802DC93D)))
;; x should be Float32(0x802DC93D [Rational(-3000637, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCADE135F)))
;; y should be Float32(0xCADE135F [Rational(-14553951, 2), -7276975.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A0EDA12)))
;; z should be Float32(0x4A0EDA12 [Rational(4680969, 2), 2340484.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x4A0EDA13)))
(check-sat)
(exit)
