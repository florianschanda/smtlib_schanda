(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01001111 #b01111011010000110011011)))
;; x should be Float32(0x27BDA19B [Rational(12427675, 2361183241434822606848), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0001340A)))
;; y should be Float32(0x0001340A [Rational(39429, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8017DE23)))
;; z should be Float32(0x8017DE23 [Rational(-1564195, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b00000000 #b00101111101111000100010))))
(check-sat)
(exit)
