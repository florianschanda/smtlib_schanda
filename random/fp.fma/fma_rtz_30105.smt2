(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111101 #b01111100000000011010101)))
;; x should be Float32(0x1EBE00D5 [Rational(12452053, 618970019642690137449562112), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x6BD83D5A)))
;; y should be Float32(0x6BD83D5A [Rational(522835403198363167908429824), 522835403198363167908429824.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01100000010011000100000)))
;; z should be Float32(0x00302620 [Rational(98609, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4B207E3C))))
(check-sat)
(exit)
