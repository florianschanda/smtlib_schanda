(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC966E2B8)))
;; x should be Float32(0xC966E2B8 [Rational(-1891415, 2), -945707.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10110111 #b00110110100001010100100)))
;; y should be Float32(0x5B9B42A4 [Rational(87403787064442880), 87403787064442880.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4ADD36DD)))
;; z should be Float32(0x4ADD36DD [Rational(14497501, 2), 7248750.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b11001011 #b00011000000011101011111)))
(check-sat)
(exit)
