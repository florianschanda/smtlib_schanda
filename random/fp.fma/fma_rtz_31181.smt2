(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11001111010100100001011)))
(declare-const y Float32)
(assert (= y (fp #b0 #b01000010 #b10000101111011100101000)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A85E4D5)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10010101 #b00001011110010011010100)))
(check-sat)
(exit)
