(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110001 #b10100011100110010001111)))
;; x should be Float32(0x18D1CC8F [Rational(13749391, 2535301200456458802993406410752), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7D46ABC6)))
;; y should be Float32(0x7D46ABC6 [Rational(16504940115332770086886028397722468352), 16504940115332770086886028397722468352.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x47170680)))
;; z should be Float32(0x47170680 [Rational(77325, 2), 38662.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10101101 #b01000101101000011111101))))
(check-sat)
(exit)
