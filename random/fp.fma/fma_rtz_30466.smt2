(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x004DF81E)))
;; x should be Float32(0x004DF81E [Rational(2554895, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x345434E3)))
;; y should be Float32(0x345434E3 [Rational(13907171, 70368744177664), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x006A178A)))
;; z should be Float32(0x006A178A [Rational(3476421, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b00000000 #b11010100001011110001011)))
(check-sat)
(exit)
