(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01100000010001011001100)))
;; x should be Float32(0xC9B022CC [Rational(-2885811, 2), -1442905.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10011011101100111111000)))
;; y should be Float32(0x004DD9F8 [Rational(637759, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8A5641AE))))
(check-sat)
(exit)
