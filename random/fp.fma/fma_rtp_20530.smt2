(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011001 #b10100001100000101101011)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10100100001110001011010)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10111010 #b00010001111101010011110)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xDD08FA9E)))
(check-sat)
(exit)
