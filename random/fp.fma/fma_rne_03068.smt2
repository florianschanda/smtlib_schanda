(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10011111010010010100010)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80000000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00101000000011001011010)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b00000000 #b00101000000011001011010)))
(check-sat)
(exit)
