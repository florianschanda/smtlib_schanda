(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10011101011011011101111)))
;; y should be Float32(0xCACEB6EF [Rational(-13547247, 2), -6773623.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01110110111101010011000)))
;; z should be Float32(0x003B7A98 [Rational(487251, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b00000000 #b01110110111101010011000))))
(check-sat)
(exit)
