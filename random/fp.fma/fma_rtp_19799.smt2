(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11001001 #b10100000111010001001011)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11100011 #b01010010010101110101001)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11100011 #b01010010010101110101000))))
(check-sat)
(exit)
