(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111110 #b00011100101110110010000)))
;; x should be Float32(0x3F0E5D90 [Rational(583129, 1048576), 0.556115])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011100 #b10101110001000111110101)))
;; y should be Float32(0x0E5711F5 [Rational(14094837, 5316911983139663491615228241121378304), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111110 #b11111111111111111111111)))
;; z should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b11111110 #b11111111111111111111111))))
(check-sat)
(exit)
