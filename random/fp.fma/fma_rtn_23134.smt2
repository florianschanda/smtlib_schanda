(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6E0C8525)))
;; x should be Float32(0x6E0C8525 [Rational(10872215809139230369644544000), 10872215809139230369644544000.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01100110111010110110101)))
;; y should be Float32(0x4AB375B5 [Rational(11761077, 2), 5880538.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA36B3B6)))
;; z should be Float32(0xCA36B3B6 [Rational(-5986779, 2), -2993389.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x7945036A)))
(check-sat)
(exit)
