(assert (= z (fp #b0 #b00000000 #b01001110010100011100000)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x13507E45)))
(check-sat)
(exit)
