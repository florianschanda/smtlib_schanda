(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAED75D7)))
;; x should be Float32(0xCAED75D7 [Rational(-15562199, 2), -7781099.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01101000001010010000111)))
;; y should be Float32(0xCAB41487 [Rational(-11801735, 2), -5900867.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00011100 #b00111100011100110011010)))
;; z should be Float32(0x8E1E399A [Rational(-5184717, 2658455991569831745807614120560689152), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10101100 #b01001110000100111100110))))
(check-sat)
(exit)
