(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7D608191)))
;; y should be Float32(0x7D608191 [Rational(18651238643747792361671542992543940608), 18651238643747792361671542992543940608.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4DD7565B)))
;; z should be Float32(0x4DD7565B [Rational(451595104), 451595104.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10011011 #b10101110101011001011011)))
(check-sat)
(exit)
