(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA15A23A)))
;; x should be Float32(0xCA15A23A [Rational(-4903197, 2), -2451598.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x806B9DAC)))
;; y should be Float32(0x806B9DAC [Rational(-1763179, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x804DA1B5)))
;; z should be Float32(0x804DA1B5 [Rational(-5087669, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0AFB9BD8))))
(check-sat)
(exit)
