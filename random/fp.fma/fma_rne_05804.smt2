(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000001 #b11000110011011101001010)))
;; x should be Float32(0x80E3374A [Rational(-7445413, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001111 #b00010011111110101000000)))
;; y should be Float32(0x4789FD40 [Rational(141301, 2), 70650.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x001B9D19)))
;; z should be Float32(0x001B9D19 [Rational(1809689, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00010001 #b11101001111001010011110)))
(check-sat)
(exit)
