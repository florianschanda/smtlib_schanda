(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10110001010101000100001)))
;; x should be Float32(0x0058AA21 [Rational(5810721, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10011010 #b11011101100110101110011)))
;; y should be Float32(0x4D6ECD73 [Rational(250402608), 250402608.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x803DE57F)))
;; z should be Float32(0x803DE57F [Rational(-4056447, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b00011100 #b01001010110101010100101)))
(check-sat)
(exit)
