(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x731D7267)))
;; y should be Float32(0x731D7267 [Rational(12474227325218554646826019979264), 12474227325218554646826019979264.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01110011 #b10110010101001101110011)))
;; z should be Float32(0xB9D95373 [Rational(-14242675, 34359738368), -0.000415])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b01110011 #b10110010101001101110011)))
(check-sat)
(exit)
