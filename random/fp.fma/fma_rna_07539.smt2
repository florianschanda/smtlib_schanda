(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AACD09D)))
;; x should be Float32(0x4AACD09D [Rational(11325597, 2), 5662798.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000001 #b10011111110111110010000)))
;; y should be Float32(0xE0CFEF90 [Rational(-119866822519674634240), -119866822519674634240.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80800000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b11011000 #b00011000101111001100111))))
(check-sat)
(exit)
