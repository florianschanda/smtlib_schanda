(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0001019B)))
;; x should be Float32(0x0001019B [Rational(65947, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11101100 #b11101000010001011101111)))
;; y should be Float32(0xF67422EF [Rational(-1237918916933459968098183757692928), -1237918916933459968098183757692928.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x005A81AE)))
;; z should be Float32(0x005A81AE [Rational(2965719, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b01100111 #b11101011010101011100011)))
(check-sat)
(exit)
