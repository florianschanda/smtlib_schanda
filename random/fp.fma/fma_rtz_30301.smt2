(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x408EBC1C)))
;; x should be Float32(0x408EBC1C [Rational(2338567, 524288), 4.460463])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01101110101111100100010)))
;; y should be Float32(0x80375F22 [Rational(-1814417, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80800000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x813B7DD3)))
(check-sat)
(exit)
