(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10001010111101110110001)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00111100011001110100000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01010000110110101011011)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x00286D5B)))
(check-sat)
(exit)
