(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x470E1A80)))
;; x should be Float32(0x470E1A80 [Rational(72757, 2), 36378.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x002064B2)))
;; y should be Float32(0x002064B2 [Rational(1061465, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x738C0EE1)))
;; z should be Float32(0x738C0EE1 [Rational(22193095100887838771245217742848), 22193095100887838771245217742848.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b11100111 #b00011000000111011100001)))
(check-sat)
(exit)
