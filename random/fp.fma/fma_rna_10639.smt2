(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00110010011000101010101)))
;; x should be Float32(0xCA993155 [Rational(-10039637, 2), -5019818.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0051CB62)))
;; y should be Float32(0x0051CB62 [Rational(2680241, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01111001011001011111101)))
;; z should be Float32(0x803CB2FD [Rational(-3977981, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00010110 #b10000111100100101000101))))
(check-sat)
(exit)
