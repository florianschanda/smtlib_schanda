(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAAD096D)))
;; x should be Float32(0xCAAD096D [Rational(-11340141, 2), -5670070.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8BCA89C3)))
;; y should be Float32(0x8BCA89C3 [Rational(-13273539, 170141183460469231731687303715884105728), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xA45C13D7)))
;; z should be Float32(0xA45C13D7 [Rational(-14422999, 302231454903657293676544), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xA45C13D6))))
(check-sat)
(exit)
