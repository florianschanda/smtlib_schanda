(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x803E044F)))
;; x should be Float32(0x803E044F [Rational(-4064335, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCABA5F53)))
;; y should be Float32(0xCABA5F53 [Rational(-12214099, 2), -6107049.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01001000110010111100101)))
;; z should be Float32(0xCAA465E5 [Rational(-10773989, 2), -5386994.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xCAA465E4)))
(check-sat)
(exit)
