(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x989F771B)))
;; x should be Float32(0x989F771B [Rational(-10450715, 2535301200456458802993406410752), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111111001100111011010)))
;; y should be Float32(0x007F99DA [Rational(4181229, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCED8B9C4)))
;; z should be Float32(0xCED8B9C4 [Rational(-1818026496), -1818026496.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCED8B9C4))))
(check-sat)
(exit)
