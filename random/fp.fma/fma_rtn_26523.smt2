(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD08D20EE)))
;; x should be Float32(0xD08D20EE [Rational(-18941964288), -18941964288.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101111100110101110101)))
;; y should be Float32(0x0057CD75 [Rational(5754229, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11100001001111100100100)))
;; z should be Float32(0x80709F24 [Rational(-1845193, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x91419DD4)))
(check-sat)
(exit)
