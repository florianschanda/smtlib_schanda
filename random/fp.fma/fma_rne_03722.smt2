(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01001110011111000011100)))
;; x should be Float32(0xC9A73E1C [Rational(-2740103, 2), -1370051.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000001 #b00000000000000000000000)))
;; y should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10111110 #b11110111001001011011001)))
;; z should be Float32(0xDF7B92D9 [Rational(-18127790044142895104), -18127790044142895104.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b10111110 #b11110111001001011011001)))
(check-sat)
(exit)
