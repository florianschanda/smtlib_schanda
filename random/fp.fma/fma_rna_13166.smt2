(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8049E387)))
;; z should be Float32(0x8049E387 [Rational(-4842375, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
