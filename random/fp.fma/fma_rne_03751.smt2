(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC94DFC18)))
;; x should be Float32(0xC94DFC18 [Rational(-1687427, 2), -843713.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x445D1614)))
;; y should be Float32(0x445D1614 [Rational(3622277, 4096), 884.344971])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x617D7B58)))
;; z should be Float32(0x617D7B58 [Rational(292244631532070764544), 292244631532070764544.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11000010 #b11111010111101101011000)))
(check-sat)
(exit)
