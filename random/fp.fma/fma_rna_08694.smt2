(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x48C5DA70)))
;; y should be Float32(0x48C5DA70 [Rational(810407, 2), 405203.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA96A563)))
;; z should be Float32(0xCA96A563 [Rational(-9872739, 2), -4936369.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xCA96A563)))
(check-sat)
(exit)
