(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AC733A7)))
;; x should be Float32(0x4AC733A7 [Rational(13054887, 2), 6527443.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805C9FAB)))
;; y should be Float32(0x805C9FAB [Rational(-6070187, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xBF2A5CFB)))
;; z should be Float32(0xBF2A5CFB [Rational(-11164923, 16777216), -0.665481])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b01111110 #b01010100101110011111011))))
(check-sat)
(exit)
