(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAA72D39)))
;; x should be Float32(0xCAA72D39 [Rational(-10956089, 2), -5478044.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9828926B)))
;; y should be Float32(0x9828926B [Rational(-11047531, 5070602400912917605986812821504), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b01000110 #b10111000010101010010111)))
(check-sat)
(exit)
