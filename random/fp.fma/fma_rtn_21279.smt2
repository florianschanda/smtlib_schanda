(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b11010111101110101011000)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8013AA66)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11001001000100110001100)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00010010 #b00100001111001110010111))))
(check-sat)
(exit)
