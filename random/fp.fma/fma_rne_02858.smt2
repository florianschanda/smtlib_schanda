(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11110010 #b00101110100100010011100)))
;; x should be Float32(0x7917489C [Rational(49094344920223382338455858791317504), 49094344920223382338455858791317504.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10110101001010110110111)))
;; y should be Float32(0x805A95B7 [Rational(-5936567, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x12C65FA0)))
;; z should be Float32(0x12C65FA0 [Rational(406269, 324518553658426726783156020576256), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b01110011 #b10101100010000000000001)))
(check-sat)
(exit)
