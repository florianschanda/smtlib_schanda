(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11000010 #b01010100001101001000111)))
;; x should be Float32(0xE12A1A47 [Rational(-196114998418684772352), -196114998418684772352.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x15C56192)))
;; y should be Float32(0x15C56192 [Rational(6467785, 81129638414606681695789005144064), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00361E93)))
;; z should be Float32(0x00361E93 [Rational(3546771, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xB783270E)))
(check-sat)
(exit)
