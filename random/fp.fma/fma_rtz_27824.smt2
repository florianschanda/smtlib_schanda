(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF949CF)))
;; x should be Float32(0x4AF949CF [Rational(16337359, 2), 8168679.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49D6393C)))
;; y should be Float32(0x49D6393C [Rational(3509839, 2), 1754919.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x1C08BC9A)))
;; z should be Float32(0x1C08BC9A [Rational(4480589, 9903520314283042199192993792), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10101010 #b10100001001101101101110)))
(check-sat)
(exit)
