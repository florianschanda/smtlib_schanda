(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x882DF94A)))
;; x should be Float32(0x882DF94A [Rational(-5700773, 10889035741470030830827987437816582766592), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01101101011001111000101)))
;; y should be Float32(0x0036B3C5 [Rational(3584965, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xFFA8EC48)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
