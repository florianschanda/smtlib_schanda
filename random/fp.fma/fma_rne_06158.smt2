(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01011101111011000000110)))
;; x should be Float32(0x802EF606 [Rational(-1538819, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49DE9E94)))
;; y should be Float32(0x49DE9E94 [Rational(3647397, 2), 1823698.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AB19613)))
;; z should be Float32(0x4AB19613 [Rational(11638291, 2), 5819145.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AB19613))))
(check-sat)
(exit)
