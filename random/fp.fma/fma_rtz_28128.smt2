(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AD8A393)))
;; x should be Float32(0x4AD8A393 [Rational(14197651, 2), 7098825.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x803235F6)))
;; y should be Float32(0x803235F6 [Rational(-1645307, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000101 #b11001101110100001010111)))
;; z should be Float32(0x02E6E857 [Rational(15132759, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x8B29F5FF)))
(check-sat)
(exit)
