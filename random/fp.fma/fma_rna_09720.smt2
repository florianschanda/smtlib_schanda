(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x36AE04F6)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10101111000111010000000)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA623E8A)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xCA623E8A)))
(check-sat)
(exit)
