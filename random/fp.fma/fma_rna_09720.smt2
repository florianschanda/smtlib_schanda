(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3B935D75)))
;; x should be Float32(0x3B935D75 [Rational(9657717, 2147483648), 0.004497])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010101101001000111000)))
;; y should be Float32(0x800AD238 [Rational(-88647, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b10010001010100100010100)))
;; z should be Float32(0xC9C8A914 [Rational(-3287621, 2), -1643810.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10010011 #b10010001010100100010100)))
(check-sat)
(exit)
