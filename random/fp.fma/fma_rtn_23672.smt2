(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0012CE23)))
;; x should be Float32(0x0012CE23 [Rational(1232419, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01011011011110011011101)))
;; y should be Float32(0xCAADBCDD [Rational(-11386077, 2), -5693038.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC978B211)))
;; z should be Float32(0xC978B211 [Rational(-16298513, 16), -1018657.062500])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10010010 #b11110001011001000010010))))
(check-sat)
(exit)
