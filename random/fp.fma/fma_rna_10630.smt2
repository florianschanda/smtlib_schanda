(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01001111000110001011011)))
;; x should be Float32(0xCAA78C5B [Rational(-10980443, 2), -5490221.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10111100011001001011100)))
;; y should be Float32(0x005E325C [Rational(1543319, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x001ED9DE)))
;; z should be Float32(0x001ED9DE [Rational(1010927, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00010110 #b11101101001100111111011))))
(check-sat)
(exit)
