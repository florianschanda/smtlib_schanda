(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01101001110010111101011)))
;; z should be Float32(0x4AB4E5EB [Rational(11855339, 2), 5927669.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10010101 #b01101001110010111110001))))
(check-sat)
(exit)
