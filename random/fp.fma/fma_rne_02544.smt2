(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x355CE4D0)))
;; x should be Float32(0x355CE4D0 [Rational(904781, 1099511627776), 0.000001])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00000000)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xAA36C9DA)))
;; z should be Float32(0xAA36C9DA [Rational(-5989613, 36893488147419103232), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b01010100 #b01101101100100111011010))))
(check-sat)
(exit)
