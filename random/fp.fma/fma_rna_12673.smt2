(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x94167D30)))
;; x should be Float32(0x94167D30 [Rational(-616403, 81129638414606681695789005144064), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00010100100100110100111)))
;; y should be Float32(0x4A8A49A7 [Rational(9062823, 2), 4531411.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b01011110010010000111000)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
