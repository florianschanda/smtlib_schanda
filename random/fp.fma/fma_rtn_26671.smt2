(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF74AB5D3)))
;; x should be Float32(0xF74AB5D3 [Rational(-4111452338689791574551277143588864), -4111452338689791574551277143588864.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x909AA9CF)))
;; y should be Float32(0x909AA9CF [Rational(-10136015, 166153499473114484112975882535043072), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10010000 #b11101001110111110101101))))
(check-sat)
(exit)
