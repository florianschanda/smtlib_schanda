(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x806A4C7A)))
;; x should be Float32(0x806A4C7A [Rational(-3483197, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80258DD7)))
;; y should be Float32(0x80258DD7 [Rational(-2461143, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11110100 #b11110000010100000001000)))
;; z should be Float32(0x7A782808 [Rational(322125387781520858201805432012406784), 322125387781520858201805432012406784.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x7A782808)))
(check-sat)
(exit)
