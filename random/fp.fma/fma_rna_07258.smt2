(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10010010001111110000011)))
;; x should be Float32(0x4AC91F83 [Rational(13180803, 2), 6590401.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b11110001101000000010000)))
;; y should be Float32(0x48F8D010 [Rational(1019137, 2), 509568.500000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
