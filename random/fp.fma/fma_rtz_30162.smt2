(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010110 #b10100101000111010100001)))
;; x should be Float32(0x0B528EA1 [Rational(13799073, 340282366920938463463374607431768211456), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00011100010100111000010)))
;; y should be Float32(0xCA0E29C2 [Rational(-4658401, 2), -2329200.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11000000110010100010001)))
;; z should be Float32(0x00606511 [Rational(6317329, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b00101011 #b11010011101101011101011))))
(check-sat)
(exit)
