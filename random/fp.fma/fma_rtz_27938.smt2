(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABC86D1)))
;; x should be Float32(0x4ABC86D1 [Rational(12355281, 2), 6177640.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10110001 #b10000010000011101111000)))
;; y should be Float32(0x58C10778 [Rational(1697902577582080), 1697902577582080.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x314D6DEC)))
;; z should be Float32(0x314D6DEC [Rational(3365755, 1125899906842624), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11001000 #b00011100010011100100011))))
(check-sat)
(exit)
