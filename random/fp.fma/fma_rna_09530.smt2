(declare-const y Float32)
(assert (= y (fp #b0 #b11110101 #b10001110000000111000110)))
;; y should be Float32(0x7AC701C6 [Rational(516651522217106087043418119234125824), 516651522217106087043418119234125824.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b00100010010010111101100)))
;; z should be Float32(0xC99125EC [Rational(-2378107, 2), -1189053.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b11110010 #b10000111010100001101010)))
(check-sat)
(exit)
