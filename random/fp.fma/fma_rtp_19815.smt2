(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xABF668FD)))
;; x should be Float32(0xABF668FD [Rational(-16148733, 9223372036854775808), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF964B289)))
;; y should be Float32(0xF964B289 [Rational(-74216550432103446928416630582018048), -74216550432103446928416630582018048.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00111001 #b10001110010011110001011)))
;; z should be Float32(0x9CC7278B [Rational(-13051787, 9903520314283042199192993792), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x65DC215B)))
(check-sat)
(exit)
