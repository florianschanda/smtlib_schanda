(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01010110 #b10100111000111100100101)))
;; x should be Float32(0x2B538F25 [Rational(13864741, 18446744073709551616), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11001110000101010111000)))
;; y should be Float32(0x00670AB8 [Rational(844119, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01010001110100100110011)))
;; z should be Float32(0xCAA8E933 [Rational(-11069747, 2), -5534873.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCAA8E933))))
(check-sat)
(exit)
