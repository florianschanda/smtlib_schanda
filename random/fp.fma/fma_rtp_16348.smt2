(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000110 #b11000110001011001100000)))
;; x should be Float32(0x23631660 [Rational(465075, 37778931862957161709568), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000001 #b00000000000000000000000)))
;; y should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00011010 #b00100100101000101111011)))
;; z should be Float32(0x0D12517B [Rational(9589115, 21267647932558653966460912964485513216), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00011010 #b00100100101000101111100)))
(check-sat)
(exit)
