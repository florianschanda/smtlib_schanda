(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1872BCD5)))
;; x should be Float32(0x1872BCD5 [Rational(15908053, 5070602400912917605986812821504), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00110111010001101010011)))
;; y should be Float32(0x4A9BA353 [Rational(10199891, 2), 5099945.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7FC1CC6F)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
