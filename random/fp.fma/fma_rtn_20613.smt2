(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01001001111101001101001)))
;; y should be Float32(0x4AA4FA69 [Rational(10812009, 2), 5406004.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00111111 #b10000111001101110110111)))
;; z should be Float32(0x9FC39BB7 [Rational(-12819383, 154742504910672534362390528), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x9FC39BB7))))
(check-sat)
(exit)
