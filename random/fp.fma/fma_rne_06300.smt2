(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01001001100100011101101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11001011111111101001100)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAF187F9)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010101 #b11100011000011111111001))))
(check-sat)
(exit)
