(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9E3087C)))
;; x should be Float32(0xC9E3087C [Rational(-3719711, 2), -1859855.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE3085576)))
;; y should be Float32(0xE3085576 [Rational(-2514915303564974751744), -2514915303564974751744.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x804B329E)))
;; z should be Float32(0x804B329E [Rational(-2464079, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x6D71D099))))
(check-sat)
(exit)
