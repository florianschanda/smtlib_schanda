(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -oo 8 24)))
;; x should be Float32(-oo)

(declare-const y Float32)
(assert (= y (fp #b1 #b00000011 #b10110110100000111110011)))
;; y should be Float32(0x81DB41F3 [Rational(-14369267, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01000000 #b00010010000001100110101)))
;; z should be Float32(0x20090335 [Rational(8979253, 77371252455336267181195264), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (_ +oo 8 24))))
(check-sat)
(exit)
