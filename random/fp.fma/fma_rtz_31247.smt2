(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01111000000111110110101)))
;; x should be Float32(0xCABC0FB5 [Rational(-12324789, 2), -6162394.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA989D41)))
;; y should be Float32(0xCA989D41 [Rational(-10001729, 2), -5000864.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA7E908E)))
;; z should be Float32(0xCA7E908E [Rational(-8341575, 2), -4170787.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10101011 #b11000000011100110101111))))
(check-sat)
(exit)
