(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8D6E53DF)))
;; y should be Float32(0x8D6E53DF [Rational(-15619039, 21267647932558653966460912964485513216), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4B6E93FE)))
;; z should be Float32(0x4B6E93FE [Rational(15635454), 15635454.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10010110 #b11011101001001111111110)))
(check-sat)
(exit)
