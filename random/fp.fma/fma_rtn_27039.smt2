(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01111011111110100110010)))
;; x should be Float32(0x803DFD32 [Rational(-2031257, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x801D18DB)))
;; y should be Float32(0x801D18DB [Rational(-1906907, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AC4F8F1)))
;; z should be Float32(0x4AC4F8F1 [Rational(12908785, 2), 6454392.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10010101 #b10001001111100011110001)))
(check-sat)
(exit)
