(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11100110101010010001110)))
;; x should be Float32(0x0073548E [Rational(3779143, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b10000100111000000101000)))
;; y should be Float32(0x49427028 [Rational(1592837, 2), 796418.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b00001100110101110110100)))
;; z should be Float32(0xC9866BB4 [Rational(-2202349, 2), -1101174.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xC9866BB4)))
(check-sat)
(exit)
