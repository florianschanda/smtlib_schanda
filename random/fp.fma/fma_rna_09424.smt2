(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111110 #b00101111111100100100101)))
;; x should be Float32(0x1F17F925 [Rational(9959717, 309485009821345068724781056), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A222BD2)))
;; y should be Float32(0x4A222BD2 [Rational(5314025, 2), 2657012.500000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
