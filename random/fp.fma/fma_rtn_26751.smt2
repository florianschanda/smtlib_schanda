(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x800CBA4B)))
;; x should be Float32(0x800CBA4B [Rational(-834123, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A9EB825)))
;; y should be Float32(0x4A9EB825 [Rational(10401829, 2), 5200914.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00011100010011100101010)))
;; z should be Float32(0x800E272A [Rational(-463765, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x89FC8445)))
(check-sat)
(exit)
