(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10010000011011100001010)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01111001101110011010001)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0048F9DF)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10101011 #b00100111011010100011110)))
(check-sat)
(exit)
