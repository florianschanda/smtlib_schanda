(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA2EC81A)))
;; x should be Float32(0xCA2EC81A [Rational(-5727245, 2), -2863622.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01110110111111011110100)))
;; y should be Float32(0x49BB7EF4 [Rational(3071933, 2), 1535966.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00010001 #b01110110100010010111010)))
;; z should be Float32(0x88BB44BA [Rational(-6136413, 5444517870735015415413993718908291383296), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD48002D8))))
(check-sat)
(exit)
