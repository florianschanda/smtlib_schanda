(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000011001110111110100)))
;; x should be Float32(0x80019DF4 [Rational(-26493, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7FED2C64)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b11001110 #b11110001000000001101000)))
;; z should be Float32(0x67788068 [Rational(1173515564982886780108800), 1173515564982886780108800.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
