(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00591014)))
;; x should be Float32(0x00591014 [Rational(1459205, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAA0BDC1)))
;; y should be Float32(0xCAA0BDC1 [Rational(-10534337, 2), -5267168.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x3F29C8D7)))
;; z should be Float32(0x3F29C8D7 [Rational(11126999, 16777216), 0.663221])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b01111110 #b01010011100100011010110))))
(check-sat)
(exit)
