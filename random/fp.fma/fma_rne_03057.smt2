(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11010010000001110011100)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0028DE71)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10011011 #b01111010101001110101100)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10011011 #b01111010101001110101100))))
(check-sat)
(exit)
