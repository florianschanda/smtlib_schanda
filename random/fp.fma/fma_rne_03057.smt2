(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10001001100100010110110)))
;; x should be Float32(0x0044C8B6 [Rational(2253915, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01010001101001001111010)))
;; y should be Float32(0x0028D27A [Rational(1337661, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC2590F53)))
;; z should be Float32(0xC2590F53 [Rational(-14225235, 262144), -54.264965])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10000100 #b10110010000111101010011))))
(check-sat)
(exit)
