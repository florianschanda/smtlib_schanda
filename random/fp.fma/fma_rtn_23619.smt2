(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00110011100010111000010)))
;; x should be Float32(0x0019C5C2 [Rational(844513, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x006C0F1C)))
;; y should be Float32(0x006C0F1C [Rational(1770439, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01011110100000000001011)))
;; z should be Float32(0x4AAF400B [Rational(11485195, 2), 5742597.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10010101 #b01011110100000000001011))))
(check-sat)
(exit)
