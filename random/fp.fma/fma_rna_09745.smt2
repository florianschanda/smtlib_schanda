(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x01180D0B)))
;; x should be Float32(0x01180D0B [Rational(9964811, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7FB74BA4)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b1 #b10100101 #b10000110111101110100111)))
;; z should be Float32(0xD2C37BA7 [Rational(-419796582400), -419796582400.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
