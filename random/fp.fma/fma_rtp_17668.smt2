(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA7313D2)))
;; x should be Float32(0xCA7313D2 [Rational(-7965161, 2), -3982580.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10011100010110000010011)))
;; y should be Float32(0x804E2C13 [Rational(-5123091, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xF758B509)))
;; z should be Float32(0xF758B509 [Rational(-4395343557168931048782656255819776), -4395343557168931048782656255819776.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11101110 #b10110001011010100001000)))
(check-sat)
(exit)
