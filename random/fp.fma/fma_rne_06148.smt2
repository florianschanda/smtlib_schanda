(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11010101100101101111110)))
;; x should be Float32(0x806ACB7E [Rational(-3499455, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (_ +zero 8 24)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAF92A55)))
;; z should be Float32(0xCAF92A55 [Rational(-16329301, 2), -8164650.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCAF92A55))))
(check-sat)
(exit)
