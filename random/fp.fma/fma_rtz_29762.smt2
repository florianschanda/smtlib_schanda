(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000000000000000000001)))
;; x should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00756B48)))
;; y should be Float32(0x00756B48 [Rational(961897, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0F3D5F62)))
;; z should be Float32(0x0F3D5F62 [Rational(6205361, 664613997892457936451903530140172288), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x0F3D5F62)))
(check-sat)
(exit)
