(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0020BC9E)))
;; x should be Float32(0x0020BC9E [Rational(1072719, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01111111100011001000010)))
;; y should be Float32(0xCA3FC642 [Rational(-6284065, 2), -3142032.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b11000110010100100111100)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
