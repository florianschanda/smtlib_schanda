(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AA9C699)))
;; x should be Float32(0x4AA9C699 [Rational(11126425, 2), 5563212.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000100 #b11101010111110000101011)))
;; y should be Float32(0x82757C2B [Rational(-16088107, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111001 #b11010010101110010010101)))
;; z should be Float32(0x7CE95C95 [Rational(9693463637746923332103281839001567232), 9693463637746923332103281839001567232.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x7CE95C95))))
(check-sat)
(exit)
