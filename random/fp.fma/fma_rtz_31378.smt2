(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA822041)))
;; x should be Float32(0xCA822041 [Rational(-8527937, 2), -4263968.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11010010111010111001110)))
;; y should be Float32(0x806975CE [Rational(-3455719, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x001E1A74)))
;; z should be Float32(0x001E1A74 [Rational(493213, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00010110 #b10101100110110001110001))))
(check-sat)
(exit)
