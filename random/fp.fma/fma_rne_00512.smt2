(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AC5AE2F)))
;; x should be Float32(0x4AC5AE2F [Rational(12955183, 2), 6477591.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01101011 #b01110101110111101110111)))
;; y should be Float32(0x35BAEF77 [Rational(12250999, 8796093022208), 0.000001])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11000011110000010110101)))
;; z should be Float32(0x8061E0B5 [Rational(-6414517, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10000010 #b00100000101100101111000))))
(check-sat)
(exit)
