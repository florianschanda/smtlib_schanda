(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111110 #b11111111111111111111111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00110110001101000111011)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10000010001011100110101)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b01111110 #b10110001101000111010111)))
(check-sat)
(exit)
