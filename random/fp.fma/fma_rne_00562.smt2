(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A742262)))
;; x should be Float32(0x4A742262 [Rational(7999793, 2), 3999896.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA7CB9F2)))
;; y should be Float32(0xCA7CB9F2 [Rational(-8281337, 2), -4140668.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC9C228EC)))
;; z should be Float32(0xC9C228EC [Rational(-3181115, 2), -1590557.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xD571032E)))
(check-sat)
(exit)
