(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11010010001100001111101)))
;; x should be Float32(0xCAE9187D [Rational(-15276157, 2), -7638078.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111000000110001010011)))
;; y should be Float32(0x001C0C53 [Rational(1838163, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8ACC4F33))))
(check-sat)
(exit)
