(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4408B9DA)))
;; x should be Float32(0x4408B9DA [Rational(4480237, 8192), 546.903931])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAB9ED93)))
;; y should be Float32(0xCAB9ED93 [Rational(-12184979, 2), -6092489.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111110 #b11111111111111111111111)))
;; z should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
