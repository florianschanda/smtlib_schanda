(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b11010010000010100110110)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11001010 #b11101000101001100110110)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b00000010001111000111110)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xF05E64B5))))
(check-sat)
(exit)
