(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01111000011110101010011)))
;; x should be Float32(0x4ABC3D53 [Rational(12336467, 2), 6168233.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10101110 #b10001001011100011100001)))
;; y should be Float32(0xD744B8E1 [Rational(-216298327900160), -216298327900160.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b10111010100100100001010)))
;; z should be Float32(0x4A5D490A [Rational(7251077, 2), 3625538.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11000101 #b00100001010011011100101)))
(check-sat)
(exit)
