(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10001110 #b00011110010000011011000)))
;; y should be Float32(0xC70F20D8 [Rational(-1172507, 32), -36640.843750])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0065CC90)))
;; z should be Float32(0x0065CC90 [Rational(416969, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b00000000 #b11001011100110010010000))))
(check-sat)
(exit)
