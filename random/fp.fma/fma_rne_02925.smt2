(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x005C22DD)))
;; x should be Float32(0x005C22DD [Rational(6038237, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A38C202)))
;; y should be Float32(0x4A38C202 [Rational(6054145, 2), 3027072.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01110111110110101011001)))
;; z should be Float32(0x803BED59 [Rational(-3927385, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x0B04FDC2)))
(check-sat)
(exit)
