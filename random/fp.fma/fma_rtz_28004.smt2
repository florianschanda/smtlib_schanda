(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b01000111111010100011000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10001011011011111101111)))
(declare-const z Float32)
(assert (= z (fp #b1 #b01001110 #b10011011110010111111100)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10101000 #b11111010100001011010001)))
(check-sat)
(exit)
