(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8048118E)))
;; x should be Float32(0x8048118E [Rational(-2361543, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10111101 #b10000001101100110000001)))
;; y should be Float32(0x5EC0D981 [Rational(6948139981114179584), 6948139981114179584.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAF6B04B)))
;; z should be Float32(0xCAF6B04B [Rational(-16166987, 2), -8083493.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010101 #b11101101011000001001011))))
(check-sat)
(exit)
