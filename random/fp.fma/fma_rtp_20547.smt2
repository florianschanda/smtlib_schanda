(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFFD0BDFE)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805A5FF4)))
;; y should be Float32(0x805A5FF4 [Rational(-1480701, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x35F734FA)))
;; z should be Float32(0x35F734FA [Rational(8100477, 4398046511104), 0.000002])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
