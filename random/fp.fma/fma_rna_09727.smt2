(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11110111 #b11100101111110011101111)))
;; x should be Float32(0x7BF2FCEF [Rational(2523331885032778832455799351998939136), 2523331885032778832455799351998939136.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11010011011010101110011)))
;; y should be Float32(0x8069B573 [Rational(-6927731, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x805164D9)))
;; z should be Float32(0x805164D9 [Rational(-5334233, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xBCC8ABF0)))
(check-sat)
(exit)
