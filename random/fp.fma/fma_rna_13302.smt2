(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01111101000011111000110)))
;; x should be Float32(0x803E87C6 [Rational(-2048995, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x898A6657)))
;; y should be Float32(0x898A6657 [Rational(-9070167, 2722258935367507707706996859454145691648), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b00001101000000101011010)))
;; z should be Float32(0x4A06815A [Rational(4407469, 2), 2203734.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4A06815A))))
(check-sat)
(exit)
