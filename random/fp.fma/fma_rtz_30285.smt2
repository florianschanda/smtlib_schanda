(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5412BB70)))
;; x should be Float32(0x5412BB70 [Rational(2520839618560), 2520839618560.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00100010 #b00100101001000000011101)))
;; y should be Float32(0x9112901D [Rational(-9605149, 83076749736557242056487941267521536), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807002F1)))
;; z should be Float32(0x807002F1 [Rational(-7340785, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xA5A802FF))))
(check-sat)
(exit)
