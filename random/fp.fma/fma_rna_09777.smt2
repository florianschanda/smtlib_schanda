(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10011011110001100011101)))
;; x should be Float32(0x004DE31D [Rational(5104413, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A65EFB2)))
;; y should be Float32(0x4A65EFB2 [Rational(7534553, 2), 3767276.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAE50CD3)))
;; z should be Float32(0xCAE50CD3 [Rational(-15011027, 2), -7505513.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCAE50CD3))))
(check-sat)
(exit)
