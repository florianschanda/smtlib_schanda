(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2296CB4F)))
;; x should be Float32(0x2296CB4F [Rational(9882447, 2417851639229258349412352), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8006C892)))
;; y should be Float32(0x8006C892 [Rational(-222281, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b10010000000100101100011)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
