(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x000DAB92)))
;; x should be Float32(0x000DAB92 [Rational(447945, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10101110110111011110010)))
;; y should be Float32(0x80576EF2 [Rational(-2865017, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10001111101000001011001)))
;; z should be Float32(0x0047D059 [Rational(4706393, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b00000000 #b10001111101000001011000)))
(check-sat)
(exit)
