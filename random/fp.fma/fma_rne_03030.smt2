(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x005C0917)))
;; x should be Float32(0x005C0917 [Rational(6031639, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01111101 #b00001110011011101011011)))
;; y should be Float32(0x3E87375B [Rational(8861531, 33554432), 0.264094])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0020D98F)))
;; z should be Float32(0x0020D98F [Rational(2152847, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b00000000 #b01110010010011111101000))))
(check-sat)
(exit)
