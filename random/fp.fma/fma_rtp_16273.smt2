(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10110001 #b01100010001010110001101)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A314A7A)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10000111111101000101110)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b11000110 #b11101010100011011010111))))
(check-sat)
(exit)
