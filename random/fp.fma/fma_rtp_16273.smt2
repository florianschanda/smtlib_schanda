(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2970913B)))
;; x should be Float32(0x2970913B [Rational(15765819, 295147905179352825856), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AA0BE4F)))
;; y should be Float32(0x4AA0BE4F [Rational(10534479, 2), 5267239.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x000A01E5)))
;; z should be Float32(0x000A01E5 [Rational(655845, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01101001 #b00101110000110110011011))))
(check-sat)
(exit)
