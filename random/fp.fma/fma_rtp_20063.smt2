(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8039CA2E)))
;; x should be Float32(0x8039CA2E [Rational(-1893655, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA4F7D9A)))
;; y should be Float32(0xCA4F7D9A [Rational(-6799053, 2), -3399526.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8055ED6E)))
;; z should be Float32(0x8055ED6E [Rational(-2815671, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00010101 #b01110110101101101010010)))
(check-sat)
(exit)
