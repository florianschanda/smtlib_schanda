(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10111110111000111110101)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA5A1736)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01001110010100110001010)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00010110 #b01000101001111101100011)))
(check-sat)
(exit)
