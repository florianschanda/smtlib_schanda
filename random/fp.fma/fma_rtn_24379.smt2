(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b10111110000100001101100)))
;; x should be Float32(0xC9DF086C [Rational(-3654171, 2), -1827085.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA5D7E3E)))
;; y should be Float32(0xCA5D7E3E [Rational(-7257887, 2), -3628943.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4ADC955F)))
;; z should be Float32(0x4ADC955F [Rational(14456159, 2), 7228079.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10101001 #b10000001111100001001111)))
(check-sat)
(exit)
