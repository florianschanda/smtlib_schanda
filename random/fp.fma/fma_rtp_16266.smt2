(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01101000 #b00001001000110001100001)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01111010101010110101010)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49DCC13C)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010011 #b10111001100000101000000))))
(check-sat)
(exit)
