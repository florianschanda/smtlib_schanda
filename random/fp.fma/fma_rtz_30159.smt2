(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x517FC29F)))
;; x should be Float32(0x517FC29F [Rational(68655116288), 68655116288.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAD132FB)))
;; y should be Float32(0xCAD132FB [Rational(-13710075, 2), -6855037.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10111001 #b10100010000000011010010))))
(check-sat)
(exit)
