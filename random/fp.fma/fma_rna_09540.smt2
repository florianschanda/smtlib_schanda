(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2022379E)))
;; x should be Float32(0x2022379E [Rational(5315535, 38685626227668133590597632), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111111001011111000011)))
;; y should be Float32(0x007F97C3 [Rational(8361923, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01010010111011100110111)))
;; z should be Float32(0x4AA97737 [Rational(11106103, 2), 5553051.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AA97737))))
(check-sat)
(exit)
