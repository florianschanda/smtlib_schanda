(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEC9B917D)))
;; x should be Float32(0xEC9B917D [Rational(-1504564408186342674635161600), -1504564408186342674635161600.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01010000111110010010110)))
;; y should be Float32(0x00287C96 [Rational(1326667, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b01011001 #b10001001101001110001001))))
(check-sat)
(exit)
