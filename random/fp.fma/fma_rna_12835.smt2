(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00100111 #b11010010111110101101000)))
;; x should be Float32(0x93E97D68 [Rational(-1912749, 324518553658426726783156020576256), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b01010001000001110010100)))
;; y should be Float32(0xC9A88394 [Rational(-2760933, 2), -1380466.500000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b00111100 #b00110011011001001001110)))
(check-sat)
(exit)
