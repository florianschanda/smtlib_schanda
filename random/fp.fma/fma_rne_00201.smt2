(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA4A71AA)))
;; y should be Float32(0xCA4A71AA [Rational(-6633685, 2), -3316842.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCACD5635)))
;; z should be Float32(0xCACD5635 [Rational(-13456949, 2), -6728474.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCACD5635))))
(check-sat)
(exit)
