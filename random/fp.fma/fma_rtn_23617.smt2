(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01010011001001001111000)))
;; x should be Float32(0x00299278 [Rational(340559, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11001000 #b10101110110010110101000)))
;; y should be Float32(0x645765A8 [Rational(15893500322565886836736), 15893500322565886836736.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b00011100111011000101101)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
