(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01111101 #b01000110011000000000001)))
;; x should be Float32(0xBEA33001 [Rational(-10694657, 33554432), -0.318726])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000001 #b00000000000000000000000)))
;; y should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01011010 #b00011001001101100011000)))
;; z should be Float32(0x2D0C9B18 [Rational(1151843, 144115188075855872), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b01011010 #b00011001001101100011000))))
(check-sat)
(exit)
