(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00101100000010011000101)))
;; x should be Float32(0xCA9604C5 [Rational(-9831621, 2), -4915810.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAA1EAED)))
;; y should be Float32(0xCAA1EAED [Rational(-10611437, 2), -5305718.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x136C150E)))
;; z should be Float32(0x136C150E [Rational(7735943, 2596148429267413814265248164610048), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10101011 #b01111011100010101010111))))
(check-sat)
(exit)
