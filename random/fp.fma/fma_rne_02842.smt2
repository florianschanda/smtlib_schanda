(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b01010011001000010010111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11000000 #b10111110101000101100111)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAC2093F)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11010100 #b00100111110101011011111)))
(check-sat)
(exit)
