(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11100001 #b10111010110101001110100)))
;; x should be Float32(0x70DD6A74 [Rational(548199048815267390076322054144), 548199048815267390076322054144.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x93327B1B)))
;; y should be Float32(0x93327B1B [Rational(-11696923, 5192296858534827628530496329220096), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b11001010000110110010011)))
;; z should be Float32(0xCAE50D93 [Rational(-15011219, 2), -7505609.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xCAE51739)))
(check-sat)
(exit)
