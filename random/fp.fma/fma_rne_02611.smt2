(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11101101 #b10000101001011100111010)))
;; x should be Float32(0x76C2973A [Rational(1973384432889323807618188961644544), 1973384432889323807618188961644544.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x007FFFFF)))
;; y should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00101100 #b00010111010110111111010)))
;; z should be Float32(0x160BADFA [Rational(4577021, 40564819207303340847894502572032), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x37C29738))))
(check-sat)
(exit)
