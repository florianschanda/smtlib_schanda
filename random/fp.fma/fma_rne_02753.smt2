(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111110 #b01101011110101000110101)))
;; x should be Float32(0x7F35EA35 [Rational(241806339669675916199197683256098029568), 241806339669675916199197683256098029568.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b00000000000000000000000)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x91A864D8)))
;; z should be Float32(0x91A864D8 [Rational(-1379483, 5192296858534827628530496329220096), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
