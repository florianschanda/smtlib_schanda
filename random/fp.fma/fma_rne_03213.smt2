(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00031D38)))
;; x should be Float32(0x00031D38 [Rational(25511, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80714B4C)))
;; y should be Float32(0x80714B4C [Rational(-1856211, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AD4D2E7)))
;; z should be Float32(0x4AD4D2E7 [Rational(13947623, 2), 6973811.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10010101 #b10101001101001011100111)))
(check-sat)
(exit)
