(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x807677C8)))
;; y should be Float32(0x807677C8 [Rational(-970489, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xBEB74D1B)))
;; z should be Float32(0xBEB74D1B [Rational(-12012827, 33554432), -0.358010])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xBEB74D1B)))
(check-sat)
(exit)
