(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b01111110111100000101000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00101011 #b10000111011111100011010)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b00101011 #b10000111011111100011011))))
(check-sat)
(exit)
