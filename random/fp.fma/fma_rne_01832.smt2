(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A73FD12)))
;; y should be Float32(0x4A73FD12 [Rational(7995017, 2), 3997508.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x6EB996DE)))
;; z should be Float32(0x6EB996DE [Rational(28718557027625114575763931136), 28718557027625114575763931136.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x6EB996DE))))
(check-sat)
(exit)
