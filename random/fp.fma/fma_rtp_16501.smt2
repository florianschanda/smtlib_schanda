(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11110100110010100101110)))
;; z should be Float32(0x007A652E [Rational(4010647, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
