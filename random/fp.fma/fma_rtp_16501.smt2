(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6B074361)))
;; x should be Float32(0x6B074361 [Rational(163523173536502354641158144), 163523173536502354641158144.000000])

(declare-const y Float32)
