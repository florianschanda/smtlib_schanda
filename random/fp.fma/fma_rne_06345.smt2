(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8012A3BB)))
;; x should be Float32(0x8012A3BB [Rational(-1221563, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA8EAD67)))
;; y should be Float32(0xCA8EAD67 [Rational(-9350503, 2), -4675251.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x803856B2)))
;; z should be Float32(0x803856B2 [Rational(-1846105, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x0A263717)))
(check-sat)
(exit)
