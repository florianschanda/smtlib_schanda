(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80739A63)))
;; x should be Float32(0x80739A63 [Rational(-7576163, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11100010100111011000001)))
;; y should be Float32(0xCAF14EC1 [Rational(-15814337, 2), -7907168.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010110 #b00010001000111110100001)))
;; z should be Float32(0xCB088FA1 [Rational(-8949665), -8949665.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010110 #b00010001000111110100000))))
(check-sat)
(exit)
