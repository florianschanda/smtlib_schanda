(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x082E8205)))
;; x should be Float32(0x082E8205 [Rational(11436549, 21778071482940061661655974875633165533184), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7FB2D9D3)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x007733E2)))
;; z should be Float32(0x007733E2 [Rational(3906033, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
