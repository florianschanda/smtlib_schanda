(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x004B518C)))
;; x should be Float32(0x004B518C [Rational(1234019, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x804D59DA)))
;; y should be Float32(0x804D59DA [Rational(-2534637, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b01000001001001101100010)))
;; z should be Float32(0xCA209362 [Rational(-5261745, 2), -2630872.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010100 #b01000001001001101100010))))
(check-sat)
(exit)
