(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6E2EC1FE)))
;; x should be Float32(0x6E2EC1FE [Rational(13521228468296578569678618624), 13521228468296578569678618624.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805784C2)))
;; y should be Float32(0x805784C2 [Rational(-2867809, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xAD40E12A)))
;; z should be Float32(0xAD40E12A [Rational(-6320277, 576460752303423488), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b01011110 #b00000111000101100101110)))
(check-sat)
(exit)
