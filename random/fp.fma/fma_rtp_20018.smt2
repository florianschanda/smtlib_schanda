(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8033F2AC)))
;; x should be Float32(0x8033F2AC [Rational(-851115, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x003E2857)))
;; y should be Float32(0x003E2857 [Rational(4073559, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC9A41AEC)))
;; z should be Float32(0xC9A41AEC [Rational(-2688699, 2), -1344349.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10010011 #b01001000001101011101100)))
(check-sat)
(exit)
