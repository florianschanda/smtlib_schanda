(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00001100100010101001111)))
;; x should be Float32(0x4A86454F [Rational(8799567, 2), 4399783.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10100111 #b10111011001000000010101)))
;; y should be Float32(0xD3DD9015 [Rational(-1903210135552), -1903210135552.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b01000010110011110111100)))
;; z should be Float32(0xC9A167BC [Rational(-2644463, 2), -1322231.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10111101 #b11010000110101011001111))))
(check-sat)
(exit)
