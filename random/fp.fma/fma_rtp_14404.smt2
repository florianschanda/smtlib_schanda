(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A1C0A26)))
;; x should be Float32(0x4A1C0A26 [Rational(5113107, 2), 2556553.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80030246)))
;; y should be Float32(0x80030246 [Rational(-98595, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b01010100111111001011010)))
;; z should be Float32(0x4A2A7E5A [Rational(5586733, 2), 2793366.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x4A2A7E5A)))
(check-sat)
(exit)
