(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80262BEE)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010010011010001110110)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10000001011000101100101)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010101 #b10000001011000101100101))))
(check-sat)
(exit)
