(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9C738E4)))
;; x should be Float32(0xC9C738E4 [Rational(-3264057, 2), -1632028.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD5DF7518)))
;; y should be Float32(0xD5DF7518 [Rational(-30711750852608), -30711750852608.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11000000 #b01011011110010110101110))))
(check-sat)
(exit)
