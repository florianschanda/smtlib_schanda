(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000001 #b11101110110000011110001)))
;; x should be Float32(0x20F760F1 [Rational(16212209, 38685626227668133590597632), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11001010 #b11111111101110111001100)))
;; y should be Float32(0x657FDDCC [Rational(75518430207577067356160), 75518430207577067356160.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80800000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10001101 #b11101110011111111100011)))
(check-sat)
(exit)
