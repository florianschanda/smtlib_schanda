(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7F92A4FB)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAED98BF)))
;; y should be Float32(0xCAED98BF [Rational(-15571135, 2), -7785567.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8047DD04)))
;; z should be Float32(0x8047DD04 [Rational(-1177409, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
