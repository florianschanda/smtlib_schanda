(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b10110001000011100010100)))
;; x should be Float32(0x49D88714 [Rational(3547589, 2), 1773794.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11001001 #b11100111000000110001110)))
;; y should be Float32(0xE4F3818E [Rational(-35935153671912053276672), -35935153671912053276672.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11101001011101001101011)))
;; z should be Float32(0x8074BA6B [Rational(-7649899, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xEF4DF5CC))))
(check-sat)
(exit)
