(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10010010101011111000101)))
;; x should be Float32(0x004957C5 [Rational(4806597, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11001100 #b01110110100100110110000)))
;; y should be Float32(0xE63B49B0 [Rational(-221110456687638781886464), -221110456687638781886464.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x004C8ED9)))
;; z should be Float32(0x004C8ED9 [Rational(5017305, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xA6D6A0E5))))
(check-sat)
(exit)
