(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01001000100110001000001)))
;; x should be Float32(0x4AA44C41 [Rational(10767425, 2), 5383712.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF968E96)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b00010111 #b00101100110100100101010)))
;; z should be Float32(0x0B96692A [Rational(4928661, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
