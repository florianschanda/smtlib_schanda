(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11011001110000001101001)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x83F6F778)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00100110 #b10100001000101001001000)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b00100110 #b10100001000101001001000))))
(check-sat)
(exit)
