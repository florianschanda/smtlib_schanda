(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000011110001011110000)))
;; x should be Float32(0x8001E2F0 [Rational(-7727, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11011000 #b01000010100001000011111)))
;; y should be Float32(0xEC21421F [Rational(-779797219979563914831593472), -779797219979563914831593472.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10100010 #b00000110000101000101000)))
;; z should be Float32(0x51030A28 [Rational(35175694336), 35175694336.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10100010 #b00000110000101000101000))))
(check-sat)
(exit)
