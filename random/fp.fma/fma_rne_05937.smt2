(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCF76E58F)))
;; x should be Float32(0xCF76E58F [Rational(-4142239488), -4142239488.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00434D21)))
;; y should be Float32(0x00434D21 [Rational(4410657, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00161E39)))
;; z should be Float32(0x00161E39 [Rational(1449529, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x9001D0EF)))
(check-sat)
(exit)
