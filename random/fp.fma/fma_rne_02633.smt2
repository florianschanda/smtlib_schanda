(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11011000 #b11010110111101000110010)))
;; x should be Float32(0x6C6B7A32 [Rational(1138698474630206552998412288), 1138698474630206552998412288.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000001 #b00000000000000000000000)))
;; y should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b10010111011011000000010)))
;; z should be Float32(0xCA4BB602 [Rational(-6675201, 2), -3337600.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA4BB602))))
(check-sat)
(exit)
