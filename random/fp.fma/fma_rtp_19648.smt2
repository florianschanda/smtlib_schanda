(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC4A5F11E)))
;; x should be Float32(0xC4A5F11E [Rational(-5437583, 4096), -1327.534912])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0078F6A8)))
;; y should be Float32(0x0078F6A8 [Rational(990933, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AE04265)))
;; z should be Float32(0x4AE04265 [Rational(14697061, 2), 7348530.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010101 #b11000000100001001100101))))
(check-sat)
(exit)
