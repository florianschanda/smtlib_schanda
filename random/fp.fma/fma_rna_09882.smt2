(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10000010010001111111100)))
;; x should be Float32(0x004123FC [Rational(1067263, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01000010 #b00010001000000011000101)))
;; y should be Float32(0x210880C5 [Rational(8945861, 19342813113834066795298816), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x47C9A440)))
;; z should be Float32(0x47C9A440 [Rational(206481, 2), 103240.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x47C9A440))))
(check-sat)
(exit)
