(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x06E6426B)))
;; x should be Float32(0x06E6426B [Rational(15090283, 174224571863520493293247799005065324265472), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00110000 #b11111101110010101011110)))
;; y should be Float32(0x187EE55E [Rational(8352431, 2535301200456458802993406410752), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b00000001 #b00000000000000000000000)))
(check-sat)
(exit)
