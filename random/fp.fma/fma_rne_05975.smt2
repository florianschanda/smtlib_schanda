(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA4A9D201)))
;; x should be Float32(0xA4A9D201 [Rational(-11129345, 151115727451828646838272), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA6F6B3E)))
;; y should be Float32(0xCA6F6B3E [Rational(-7845279, 2), -3922639.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x002E40F5)))
;; z should be Float32(0x002E40F5 [Rational(3031285, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b01011111 #b00111101101001000110011)))
(check-sat)
(exit)
