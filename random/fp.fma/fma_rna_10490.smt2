(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA851EE9)))
;; x should be Float32(0xCA851EE9 [Rational(-8724201, 2), -4362100.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A252D4E)))
;; y should be Float32(0x4A252D4E [Rational(5412519, 2), 2706259.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49E46AAC)))
;; z should be Float32(0x49E46AAC [Rational(3742379, 2), 1871189.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10101010 #b01010111100100011110101))))
(check-sat)
(exit)
