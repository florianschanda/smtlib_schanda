(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10111101 #b10010110100111100001011)))
;; x should be Float32(0xDECB4F0B [Rational(-7324970103743709184), -7324970103743709184.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01010001101001001111110)))
;; y should be Float32(0x4A28D27E [Rational(5531967, 2), 2765983.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8035EB90)))
;; z should be Float32(0x8035EB90 [Rational(-220857, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11010011 #b00001100001001100001010))))
(check-sat)
(exit)
