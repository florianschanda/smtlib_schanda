(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6141A057)))
;; x should be Float32(0x6141A057 [Rational(223235956849686609920), 223235956849686609920.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AE1B7BD)))
;; y should be Float32(0x4AE1B7BD [Rational(14792637, 2), 7396318.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01011111 #b10010000100100010110000)))
;; z should be Float32(0x2FC848B0 [Rational(820363, 2251799813685248), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x6CAAB8E5)))
(check-sat)
(exit)
