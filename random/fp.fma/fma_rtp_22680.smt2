(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11100110 #b01110111110111011000010)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11011010 #b11000111001010001110000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111110 #b11111111111111111111111))))
(check-sat)
(exit)
