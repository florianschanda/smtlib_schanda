(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6512FCAF)))
;; x should be Float32(0x6512FCAF [Rational(43382918505281227849728), 43382918505281227849728.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80659E41)))
;; y should be Float32(0x80659E41 [Rational(-6659649, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b01001011 #b11010010110001000111001))))
(check-sat)
(exit)
