(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001100 #b10011010100111100011100)))
;; x should be Float32(0x464D4F1C [Rational(3363783, 256), 13139.777344])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b00111011101000110101111)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x98E11BF5)))
;; z should be Float32(0x98E11BF5 [Rational(-14752757, 2535301200456458802993406410752), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
