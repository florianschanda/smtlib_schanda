(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA877B25)))
;; x should be Float32(0xCA877B25 [Rational(-8878885, 2), -4439442.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0075B519)))
;; y should be Float32(0x0075B519 [Rational(7714073, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A708196)))
;; z should be Float32(0x4A708196 [Rational(7880907, 2), 3940453.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4A708196))))
(check-sat)
(exit)
