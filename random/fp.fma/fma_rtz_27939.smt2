(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABBFFB3)))
;; x should be Float32(0x4ABBFFB3 [Rational(12320691, 2), 6160345.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00001010 #b00101101101001100011101)))
;; y should be Float32(0x0516D31D [Rational(9884445, 1393796574908163946345982392040522594123776), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01001110011000010001001)))
;; z should be Float32(0x00273089 [Rational(2568329, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x105D85B7))))
(check-sat)
(exit)
