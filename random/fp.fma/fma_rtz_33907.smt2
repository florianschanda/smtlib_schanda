(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11101001101011110100010)))
;; x should be Float32(0x8074D7A2 [Rational(-3828689, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00001000101111001100111)))
;; y should be Float32(0x80045E67 [Rational(-286311, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b10011111010110100001110)))
;; z should be Float32(0xCA4FAD0E [Rational(-6805127, 2), -3402563.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA4FAD0D))))
(check-sat)
(exit)
