(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00111001000110001110001)))
;; x should be Float32(0x001C8C71 [Rational(1870961, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01010010010110011101001)))
;; y should be Float32(0x80292CE9 [Rational(-2698473, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC85A9D5A)))
;; z should be Float32(0xC85A9D5A [Rational(-7163565, 32), -223861.406250])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC85A9D5A))))
(check-sat)
(exit)
