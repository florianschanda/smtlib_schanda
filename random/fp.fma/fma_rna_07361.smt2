(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A2B130E)))
;; x should be Float32(0x4A2B130E [Rational(5605767, 2), 2802883.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0FE47F0A)))
;; y should be Float32(0x0FE47F0A [Rational(7487365, 332306998946228968225951765070086144), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11011100 #b10001111110000000110110)))
;; z should be Float32(0x6E47E036 [Rational(15464642836457855143051788288), 15464642836457855143051788288.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b11011100 #b10001111110000000110110))))
(check-sat)
(exit)
