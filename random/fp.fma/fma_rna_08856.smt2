(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00100101100100010011001)))
;; y should be Float32(0xCA92C899 [Rational(-9619609, 2), -4809804.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AE10CE9)))
;; z should be Float32(0x4AE10CE9 [Rational(14748905, 2), 7374452.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AE10CE9))))
(check-sat)
(exit)
