(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCACC4331)))
;; x should be Float32(0xCACC4331 [Rational(-13386545, 2), -6693272.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010011 #b11101011101011001110110)))
;; y should be Float32(0x69F5D676 [Rational(37149948618589868700729344), 37149948618589868700729344.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
