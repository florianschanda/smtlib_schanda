(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b10011111101011011101001)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0690C93E)))
;; y should be Float32(0x0690C93E [Rational(4744351, 87112285931760246646623899502532662132736), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xD3A8502A)))
;; z should be Float32(0xD3A8502A [Rational(-1445798871040), -1445798871040.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
