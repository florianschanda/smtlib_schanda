(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE4E62D)))
;; x should be Float32(0xCAE4E62D [Rational(-15001133, 2), -7500566.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01010011010001101000010)))
;; y should be Float32(0xCA29A342 [Rational(-5558689, 2), -2779344.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7FDD0D7A)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
