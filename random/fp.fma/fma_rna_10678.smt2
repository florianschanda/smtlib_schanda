(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11101101000000000000011)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11001000011010110000100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b10011011000100011000100)))
