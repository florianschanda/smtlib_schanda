(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010110 #b00001101100010000011001)))
;; x should be Float32(0x0B06C419 [Rational(8832025, 340282366920938463463374607431768211456), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x718166E9)))
;; y should be Float32(0x718166E9 [Rational(1281535264382230869396850475008), 1281535264382230869396850475008.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807FFFFF)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b01111010 #b00010000011110111111011))))
(check-sat)
(exit)
