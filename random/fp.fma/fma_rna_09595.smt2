(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11100001 #b01011110001001000101100)))
;; x should be Float32(0x70AF122C [Rational(433454761340909572930966388736), 433454761340909572930966388736.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAB2AB35)))
;; y should be Float32(0xCAB2AB35 [Rational(-11709237, 2), -5854618.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7FD4D888)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
