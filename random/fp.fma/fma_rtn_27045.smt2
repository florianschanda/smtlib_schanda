(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11110001110001101101000)))
;; x should be Float32(0x8078E368 [Rational(-990317, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x802D22B8)))
;; y should be Float32(0x802D22B8 [Rational(-369751, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7F017C7A)))
;; z should be Float32(0x7F017C7A [Rational(172116730720684111734320613885644636160), 172116730720684111734320613885644636160.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x7F017C7A))))
(check-sat)
(exit)
