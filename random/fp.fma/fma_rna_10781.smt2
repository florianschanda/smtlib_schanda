(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9032FB8)))
;; x should be Float32(0xC9032FB8 [Rational(-1074679, 2), -537339.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00100010 #b00010010111000111011011)))
;; y should be Float32(0x910971DB [Rational(-9007579, 83076749736557242056487941267521536), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x6709D8FB)))
;; z should be Float32(0x6709D8FB [Rational(650966791329166177337344), 650966791329166177337344.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x6709D8FB))))
(check-sat)
(exit)
