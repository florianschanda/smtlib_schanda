(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC6BD49)))
;; x should be Float32(0xCAC6BD49 [Rational(-13024585, 2), -6512292.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01000101000001010011111)))
;; y should be Float32(0x4AA2829F [Rational(10650271, 2), 5325135.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01001010010111010011101)))
;; z should be Float32(0x80252E9D [Rational(-2436765, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b10101011 #b11111000101001001100000)))
(check-sat)
(exit)
