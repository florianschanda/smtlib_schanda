(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00110001101111000110011)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x490FE958)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00100100011100011100110)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10101000 #b01010111101111011101011))))
(check-sat)
(exit)
