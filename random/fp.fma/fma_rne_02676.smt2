(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6421BF60)))
;; x should be Float32(0x6421BF60 [Rational(11934863271704985075712), 11934863271704985075712.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111100 #b10110100100101011011010)))
;; y should be Float32(0x7E5A4ADA [Rational(72540088653484208400423404092564439040), 72540088653484208400423404092564439040.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
