(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010010 #b10100101100111110001101)))
;; x should be Float32(0xE952CF8D [Rational(-15928409560746283518394368), -15928409560746283518394368.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000001 #b00000000000000000000000)))
;; y should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b11011010101011010011100)))
;; z should be Float32(0xC9ED569C [Rational(-3888551, 2), -1944275.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010011 #b11011010101011010011011))))
(check-sat)
(exit)
