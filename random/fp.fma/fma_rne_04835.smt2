(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x19A6182E)))
;; y should be Float32(0x19A6182E [Rational(5442583, 316912650057057350374175801344), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10011010001100101000011)))
;; z should be Float32(0x004D1943 [Rational(5052739, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x004D1943))))
(check-sat)
(exit)
