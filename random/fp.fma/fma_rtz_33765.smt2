(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01001101011100110010110)))
;; x should be Float32(0x8026B996 [Rational(-1268939, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01101110110110011101001)))
;; y should be Float32(0xCAB76CE9 [Rational(-12020969, 2), -6010484.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A53CDCE)))
;; z should be Float32(0x4A53CDCE [Rational(6940391, 2), 3470195.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x4A53CDCE)))
(check-sat)
(exit)
