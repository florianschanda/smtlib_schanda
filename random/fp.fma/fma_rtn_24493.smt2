(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01000111110001010101001)))
;; x should be Float32(0xCAA3E2A9 [Rational(-10740393, 2), -5370196.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10011010 #b01111010101110000000011)))
;; y should be Float32(0xCD3D5C03 [Rational(-198557744), -198557744.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49FDFDE4)))
;; z should be Float32(0x49FDFDE4 [Rational(4161401, 2), 2080700.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x5872727C))))
(check-sat)
(exit)
