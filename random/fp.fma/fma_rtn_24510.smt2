(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAFA5BE3)))
;; x should be Float32(0xCAFA5BE3 [Rational(-16407523, 2), -8203761.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE13DFE0F)))
;; y should be Float32(0xE13DFE0F [Rational(-219046342558836850688), -219046342558836850688.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b11111011100101111010011)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
