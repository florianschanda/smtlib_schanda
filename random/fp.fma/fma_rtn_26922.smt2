(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807D79F5)))
;; x should be Float32(0x807D79F5 [Rational(-8223221, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCABCA073)))
;; y should be Float32(0xCABCA073 [Rational(-12361843, 2), -6180921.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x802D74A5)))
;; z should be Float32(0x802D74A5 [Rational(-2978981, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b00010111 #b01110001110100001101000))))
(check-sat)
(exit)
