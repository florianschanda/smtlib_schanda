(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01011110001011111001001)))
;; x should be Float32(0x002F17C9 [Rational(3086281, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF3805F75)))
;; y should be Float32(0xF3805F75 [Rational(-20341494644159515810231840931840), -20341494644159515810231840931840.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b00110101111110110010100)))
;; z should be Float32(0xC99AFD94 [Rational(-2539365, 2), -1269682.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC99AFD95))))
(check-sat)
(exit)
