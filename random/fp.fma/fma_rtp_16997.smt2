(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11101010111101001010011)))
;; y should be Float32(0x4AF57A53 [Rational(16087635, 2), 8043817.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b10001100111111011100100)))
;; z should be Float32(0xC9C67EE4 [Rational(-3252153, 2), -1626076.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10010011 #b10001100111111011100100)))
(check-sat)
(exit)
