(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0964DA50)))
;; x should be Float32(0x0964DA50 [Rational(937381, 340282366920938463463374607431768211456), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x67C7E7A4)))
;; y should be Float32(0x67C7E7A4 [Rational(1888047890835017048260608), 1888047890835017048260608.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01000110 #b01110101100000111010001)))
;; z should be Float32(0xA33AC1D1 [Rational(-12239313, 1208925819614629174706176), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x31B2B4C8)))
(check-sat)
(exit)
