(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111101 #b11111101000101100111000)))
;; x should be Float32(0x3EFE8B38 [Rational(2085223, 4194304), 0.497156])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8034B42E)))
;; y should be Float32(0x8034B42E [Rational(-1726999, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x0065CC48)))
(check-sat)
(exit)
