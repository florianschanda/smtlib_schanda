(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF6EA2EF1)))
;; x should be Float32(0xF6EA2EF1 [Rational(-2374901464308756991447261247438848), -2374901464308756991447261247438848.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x802BADDE)))
;; y should be Float32(0x802BADDE [Rational(-1431279, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10100011101011010111001)))
;; z should be Float32(0x4AD1D6B9 [Rational(13751993, 2), 6875996.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AD1D6BA))))
(check-sat)
(exit)
