(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10111001110010110010101)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11110100001011100111110)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A9A0429)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10010101 #b00110100000010000101001)))
(check-sat)
(exit)
