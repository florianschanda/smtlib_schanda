(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x72121FF1)))
;; x should be Float32(0x72121FF1 [Rational(2894299278377395527854747287552), 2894299278377395527854747287552.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00000100000000111100101)))
;; y should be Float32(0x000201E5 [Rational(131557, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAF732A1)))
;; z should be Float32(0xCAF732A1 [Rational(-16200353, 2), -8100176.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10010101 #b11101110011001010100001)))
(check-sat)
(exit)
