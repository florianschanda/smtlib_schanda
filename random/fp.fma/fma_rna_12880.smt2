(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01010000 #b11111000111010100111000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11111110 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10110001 #b10000101000111111010111)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b11010000 #b11111000111010100110111)))
(check-sat)
(exit)
