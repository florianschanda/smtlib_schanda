(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xAC37BDBB)))
;; x should be Float32(0xAC37BDBB [Rational(-12041659, 4611686018427387904), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF7FFFFF)))
;; y should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11010110 #b01101011111110000110100)))
;; z should be Float32(0xEB35FC34 [Rational(-220006568934622864112353280), -220006568934622864112353280.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b11011000 #b00010100011111010101101))))
(check-sat)
(exit)
