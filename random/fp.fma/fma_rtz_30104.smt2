(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01010100 #b10000100101011111001000)))
;; x should be Float32(0x2A4257C8 [Rational(1592057, 9223372036854775808), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01111100 #b00101111100110111010000)))
;; y should be Float32(0x3E17CDD0 [Rational(621789, 4194304), 0.148246])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000110 #b01000111001000111100100)))
;; z should be Float32(0x032391E4 [Rational(2679929, 5575186299632655785383929568162090376495104), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b01010001 #b11001100111110000001010)))
(check-sat)
(exit)
