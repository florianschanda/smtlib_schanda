(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A32D9EA)))
;; x should be Float32(0x4A32D9EA [Rational(5860597, 2), 2930298.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000101 #b10001001110110110100011)))
;; y should be Float32(0xC2C4EDA3 [Rational(-12905891, 131072), -98.464134])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xCD8994DD)))
(check-sat)
(exit)
