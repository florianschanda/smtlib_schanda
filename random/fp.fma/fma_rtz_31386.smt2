(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA8651B1)))
;; x should be Float32(0xCA8651B1 [Rational(-8802737, 2), -4401368.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800D1FDD)))
;; y should be Float32(0x800D1FDD [Rational(-860125, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00101000 #b00101101001110101110100)))
;; z should be Float32(0x94169D74 [Rational(-2467677, 324518553658426726783156020576256), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x94169D6D))))
(check-sat)
(exit)
