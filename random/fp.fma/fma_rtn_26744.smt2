(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x804D6248)))
;; x should be Float32(0x804D6248 [Rational(-633929, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00100000101111111001100)))
;; y should be Float32(0x49905FCC [Rational(2365427, 2), 1182713.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01011011101001100100101)))
;; z should be Float32(0xCAADD325 [Rational(-11391781, 2), -5695890.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10010101 #b01011011101001100100110))))
(check-sat)
(exit)
