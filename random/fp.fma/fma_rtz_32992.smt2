(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80000001)))
;; x should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x31DC4031)))
;; y should be Float32(0x31DC4031 [Rational(14434353, 2251799813685248), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x27E2499C)))
;; z should be Float32(0x27E2499C [Rational(3707495, 590295810358705651712), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b01001111 #b11000100100100110011011))))
(check-sat)
(exit)
