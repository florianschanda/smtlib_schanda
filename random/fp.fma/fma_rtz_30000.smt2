(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x10B733BA)))
;; x should be Float32(0x10B733BA [Rational(6003165, 83076749736557242056487941267521536), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AD5AD8D)))
;; y should be Float32(0x4AD5AD8D [Rational(14003597, 2), 7001798.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807F6A7E)))
;; z should be Float32(0x807F6A7E [Rational(-4175167, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x1C18EA3C)))
(check-sat)
(exit)
