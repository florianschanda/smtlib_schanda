(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00010110 #b10010000000111110010111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01001101000001110110100)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00000000 #b01001101000001110110100))))
(check-sat)
(exit)
