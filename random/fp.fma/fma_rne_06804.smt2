(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7F9DF629)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCF36E4EE)))
;; y should be Float32(0xCF36E4EE [Rational(-3068456448), -3068456448.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b00011000001000000010010)))
;; z should be Float32(0x4A0C1012 [Rational(4589577, 2), 2294788.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
