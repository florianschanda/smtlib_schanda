(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11001011010100011000001)))
;; x should be Float32(0x4AE5A8C1 [Rational(15050945, 2), 7525472.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00001100 #b11101110011101001001110)))
;; y should be Float32(0x86773A4E [Rational(-8101159, 174224571863520493293247799005065324265472), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x91DDCA20)))
(check-sat)
(exit)
