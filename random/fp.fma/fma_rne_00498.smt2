(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ACA61AB)))
;; x should be Float32(0x4ACA61AB [Rational(13263275, 2), 6631637.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x039FCE8C)))
;; y should be Float32(0x039FCE8C [Rational(2618275, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111110 #b11111111111111111111111)))
;; z should be Float32(0x7F7FFFFF [Rational(340282346638528859811704183484516925440), 340282346638528859811704183484516925440.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
