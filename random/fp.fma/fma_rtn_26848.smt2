(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x803F79C0)))
;; x should be Float32(0x803F79C0 [Rational(-64999, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00100101 #b11000001100011001001000)))
;; y should be Float32(0x12E0C648 [Rational(1841353, 1298074214633706907132624082305024), 0.000000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000000001)))
(check-sat)
(exit)
