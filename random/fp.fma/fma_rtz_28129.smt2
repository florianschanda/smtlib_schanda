(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01001000101100100111011)))
;; x should be Float32(0x4AA4593B [Rational(10770747, 2), 5385373.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x801EA85D)))
;; y should be Float32(0x801EA85D [Rational(-2009181, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x004BD336)))
;; z should be Float32(0x004BD336 [Rational(2484635, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00010101 #b00111010111010001010100)))
(check-sat)
(exit)
