(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49974F8C)))
;; x should be Float32(0x49974F8C [Rational(2479075, 2), 1239537.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7020666E)))
;; y should be Float32(0x7020666E [Rational(198565725861316075331034021888), 198565725861316075331034021888.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x001D2FAA)))
;; z should be Float32(0x001D2FAA [Rational(956373, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b11110100 #b01111011001110010000100)))
(check-sat)
(exit)
