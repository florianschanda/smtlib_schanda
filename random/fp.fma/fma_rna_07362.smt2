(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11001011110001001001011)))
(declare-const y Float32)
(assert (= y (fp #b0 #b01111010 #b10110100001100001101010)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00110010110001111001111)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x48C3D8A0))))
(check-sat)
(exit)
