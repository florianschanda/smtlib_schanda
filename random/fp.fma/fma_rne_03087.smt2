(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00110001001100001000010)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b01110110100110100011000)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x006754B1)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00010010 #b00011111111010011001111)))
(check-sat)
(exit)
