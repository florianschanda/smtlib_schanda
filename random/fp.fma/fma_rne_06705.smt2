(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFF9D0571)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01001100111000000011110)))
;; y should be Float32(0xCA26701E [Rational(-5453839, 2), -2726919.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01011110 #b10100110100110011000101)))
;; z should be Float32(0xAF534CC5 [Rational(-13847749, 72057594037927936), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
