(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111110 #b11111111111111111111111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01011010010100011111111)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10011011111010101010000)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b01111111 #b01101001010001111111011))))
(check-sat)
(exit)
