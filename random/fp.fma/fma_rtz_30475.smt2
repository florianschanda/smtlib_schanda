(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x007BB270)))
;; x should be Float32(0x007BB270 [Rational(506663, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011101 #b10010011010110000000110)))
;; y should be Float32(0x0EC9AC06 [Rational(6608387, 1329227995784915872903807060280344576), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11010001001000000010000)))
;; z should be Float32(0x80689010 [Rational(-428289, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00000000 #b11010001001000000001111)))
(check-sat)
(exit)
