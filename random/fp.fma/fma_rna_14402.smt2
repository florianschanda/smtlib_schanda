(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111110 #b11111111111111111111111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000001 #b00000000000000000000000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b01111101 #b11100111101000111111001)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10000000 #b11000011000010111000000))))
(check-sat)
(exit)
