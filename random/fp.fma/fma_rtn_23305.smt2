(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x67B9EB78)))
;; x should be Float32(0x67B9EB78 [Rational(1755962862198981321031680), 1755962862198981321031680.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC5983400)))
;; y should be Float32(0xC5983400 [Rational(-9741, 2), -4870.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b11100001101001010001001)))
;; z should be Float32(0xCAF0D289 [Rational(-15782537, 2), -7891268.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xEDDD1327)))
(check-sat)
(exit)
