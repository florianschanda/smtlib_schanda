(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b01110101001110110001111)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b10110010 #b11111110111101000000100)))
;; y should be Float32(0x597F7A04 [Rational(4494392291229696), 4494392291229696.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b10111100010000011000010)))
;; z should be Float32(0x4A5E20C2 [Rational(7278689, 2), 3639344.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
