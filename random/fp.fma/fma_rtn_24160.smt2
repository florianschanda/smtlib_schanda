(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10111000001100011010100)))
;; y should be Float32(0x805C18D4 [Rational(-1508917, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b10011101010000010111000)))
;; z should be Float32(0xC94EA0B8 [Rational(-1692695, 2), -846347.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10010010 #b10011101010000010111000))))
(check-sat)
(exit)
