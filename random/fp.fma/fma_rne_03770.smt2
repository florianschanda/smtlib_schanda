(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA3E8D32)))
;; x should be Float32(0xCA3E8D32 [Rational(-6243993, 2), -3121996.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x003D9E74)))
;; y should be Float32(0x003D9E74 [Rational(1009565, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x588F56F3)))
;; z should be Float32(0x588F56F3 [Rational(1260828854583296), 1260828854583296.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10110001 #b00011110101011011110011))))
(check-sat)
(exit)
