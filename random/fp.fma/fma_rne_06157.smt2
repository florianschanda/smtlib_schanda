(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8066716B)))
;; x should be Float32(0x8066716B [Rational(-6713707, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A3DC122)))
;; y should be Float32(0x4A3DC122 [Rational(6217873, 2), 3108936.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00010110 #b00101111101111000001010))))
(check-sat)
(exit)
