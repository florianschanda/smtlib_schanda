(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b11101110001001111110000)))
;; x should be Float32(0xC8F713F0 [Rational(-1012031, 2), -506015.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00000101001000001100001)))
;; y should be Float32(0x00029061 [Rational(168033, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10001000011001111001101)))
;; z should be Float32(0x804433CD [Rational(-4469709, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x871E6250))))
(check-sat)
(exit)
