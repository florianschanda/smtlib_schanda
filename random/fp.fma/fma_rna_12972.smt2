(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF2DF98E5)))
;; x should be Float32(0xF2DF98E5 [Rational(-8857599403093241741943737679872), -8857599403093241741943737679872.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80445BEF)))
;; y should be Float32(0x80445BEF [Rational(-4479983, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x336ED3A4)))
(check-sat)
(exit)
