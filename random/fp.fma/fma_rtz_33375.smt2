(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE832CE83)))
;; x should be Float32(0xE832CE83 [Rational(-3377562811099199936921600), -3377562811099199936921600.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10011000001000100010111)))
;; y should be Float32(0x004C1117 [Rational(4985111, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b00001101111000010010110)))
;; z should be Float32(0xCA06F096 [Rational(-4421707, 2), -2210853.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA06F096))))
(check-sat)
(exit)
