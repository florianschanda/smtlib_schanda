(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x07AE7ED1)))
;; x should be Float32(0x07AE7ED1 [Rational(11435729, 43556142965880123323311949751266331066368), 0.000000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b10001111 #b00010111100011001000000)))
;; z should be Float32(0x478BC640 [Rational(143129, 2), 71564.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10001111 #b00010111100011001000000)))
(check-sat)
(exit)
