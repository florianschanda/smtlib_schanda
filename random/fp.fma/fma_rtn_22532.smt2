(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x414FEC69)))
;; y should be Float32(0x414FEC69 [Rational(13626473, 1048576), 12.995217])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8D20F19F)))
;; z should be Float32(0x8D20F19F [Rational(-10547615, 21267647932558653966460912964485513216), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8D20F19C))))
(check-sat)
(exit)
