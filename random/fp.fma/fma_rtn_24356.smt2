(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01001101101101100111011)))
;; x should be Float32(0xCAA6DB3B [Rational(-10935099, 2), -5467549.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10111111001011100000010)))
;; y should be Float32(0x005F9702 [Rational(3132289, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00001000 #b11000111011101001110111)))
;; z should be Float32(0x8463BA77 [Rational(-14924407, 5575186299632655785383929568162090376495104), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00010110 #b11110010011101010101101)))
(check-sat)
(exit)
