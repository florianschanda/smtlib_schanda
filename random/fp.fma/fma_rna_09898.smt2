(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x006497F0)))
;; x should be Float32(0x006497F0 [Rational(412031, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11000011 #b01011110101001001110110)))
;; y should be Float32(0x61AF5276 [Rational(404265268707191685120), 404265268707191685120.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00110100100001110000001)))
;; z should be Float32(0x801A4381 [Rational(-1721217, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x2289C888)))
(check-sat)
(exit)
