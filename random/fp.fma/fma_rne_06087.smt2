(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9BC3E670)))
;; x should be Float32(0x9BC3E670 [Rational(-802407, 2475880078570760549798248448), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9729A422)))
;; y should be Float32(0x9729A422 [Rational(-5558801, 10141204801825835211973625643008), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x00000001))))
(check-sat)
(exit)
