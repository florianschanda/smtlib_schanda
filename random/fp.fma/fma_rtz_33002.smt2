(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80000001)))
;; x should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00110000 #b00101101101111110100010)))
;; y should be Float32(0x1816DFA2 [Rational(4943825, 2535301200456458802993406410752), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8060714E)))
;; z should be Float32(0x8060714E [Rational(-3160231, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8060714E))))
(check-sat)
(exit)
