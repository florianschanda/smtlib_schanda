(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11101110 #b01010110011111110010101)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b11000111101010011000100)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
