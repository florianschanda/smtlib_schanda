(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b01111010000011011100011)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b00101111 #b11110101000010010100100)))
;; y should be Float32(0x17FA84A4 [Rational(4104489, 2535301200456458802993406410752), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b01000010110101010101000)))
;; z should be Float32(0xC9216AA8 [Rational(-1322325, 2), -661162.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
