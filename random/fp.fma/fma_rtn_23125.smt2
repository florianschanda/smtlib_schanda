(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x283E5AAB)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00100010011001010101100)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AC9CBBD)))
