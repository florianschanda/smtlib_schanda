(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000000000000000000001)))
;; x should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010000 #b00011000111001000011001)))
;; y should be Float32(0xE80C7219 [Rational(-2652944151464016668000256), -2652944151464016668000256.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00101100 #b11000011000010011001110)))
;; z should be Float32(0x166184CE [Rational(7389799, 40564819207303340847894502572032), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00111011 #b00011000111000001010110)))
(check-sat)
(exit)
