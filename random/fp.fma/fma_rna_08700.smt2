(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A020406)))
;; y should be Float32(0x4A020406 [Rational(4260355, 2), 2130177.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC3E5C523)))
;; z should be Float32(0xC3E5C523 [Rational(-15058211, 32768), -459.540131])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC3E5C523))))
(check-sat)
(exit)
