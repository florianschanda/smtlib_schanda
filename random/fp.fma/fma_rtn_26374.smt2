(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8DAAF755)))
;; x should be Float32(0x8DAAF755 [Rational(-11204437, 10633823966279326983230456482242756608), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A1BE5EE)))
;; y should be Float32(0x4A1BE5EE [Rational(5108471, 2), 2554235.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b10100011101011001001100)))
;; z should be Float32(0x49D1D64C [Rational(3437971, 2), 1718985.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x49D1D64B))))
(check-sat)
(exit)
