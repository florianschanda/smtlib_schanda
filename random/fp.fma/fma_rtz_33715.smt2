(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8003798A)))
;; x should be Float32(0x8003798A [Rational(-113861, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01001101 #b10010101001011000010100)))
;; y should be Float32(0x26CA9614 [Rational(3319173, 2361183241434822606848), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x005FDB6F)))
;; z should be Float32(0x005FDB6F [Rational(6282095, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00000000 #b10111111101101101101110))))
(check-sat)
(exit)
