(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4D74619F)))
;; y should be Float32(0x4D74619F [Rational(256252400), 256252400.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8025C056)))
;; z should be Float32(0x8025C056 [Rational(-1237035, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00000000 #b01001011100000001010110))))
(check-sat)
(exit)
