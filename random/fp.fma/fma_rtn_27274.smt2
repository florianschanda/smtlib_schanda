(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFF8807F9)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10011110001010011101100)))
;; y should be Float32(0xC9CF14EC [Rational(-3392827, 2), -1696413.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01101000000000010111110)))
;; z should be Float32(0x003400BE [Rational(1704031, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
