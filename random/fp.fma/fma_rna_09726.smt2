(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01001001 #b11000110010010010110011)))
;; x should be Float32(0x24E324B3 [Rational(14886067, 151115727451828646838272), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8028F858)))
;; y should be Float32(0x8028F858 [Rational(-335627, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b10011001111011111111110)))
;; z should be Float32(0xC94CF7FE [Rational(-6716415, 8), -839551.875000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10010010 #b10011001111011111111110))))
(check-sat)
(exit)
