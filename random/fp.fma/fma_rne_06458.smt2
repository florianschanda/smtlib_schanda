(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80402061)))
;; x should be Float32(0x80402061 [Rational(-4202593, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000011 #b11011100010010000110011)))
;; y should be Float32(0xC1EE2433 [Rational(-15606835, 524288), -29.767675])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8E38E27A)))
;; z should be Float32(0x8E38E27A [Rational(-6058301, 2658455991569831745807614120560689152), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00011100 #b01110001110001001111001))))
(check-sat)
(exit)
