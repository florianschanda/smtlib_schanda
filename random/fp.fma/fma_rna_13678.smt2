(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7FC4A6D2)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF300197C)))
;; y should be Float32(0xF300197C [Rational(-10149091833873001052709408735232), -10149091833873001052709408735232.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x9869291F)))
;; z should be Float32(0x9869291F [Rational(-15280415, 5070602400912917605986812821504), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
