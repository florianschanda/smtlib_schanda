(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A162612)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01010010010110101000111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01111011100110010101001)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10010101 #b01111011100110010101010)))
(check-sat)
(exit)
