(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A3E27CE)))
;; x should be Float32(0x4A3E27CE [Rational(6231015, 2), 3115507.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10011111000011110000001)))
;; y should be Float32(0x004F8781 [Rational(5212033, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A047B42)))
;; z should be Float32(0x4A047B42 [Rational(4341153, 2), 2170576.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4A047B43))))
(check-sat)
(exit)
