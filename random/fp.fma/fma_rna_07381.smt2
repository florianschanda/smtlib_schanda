(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49E57AAC)))
;; x should be Float32(0x49E57AAC [Rational(3759787, 2), 1879893.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01111111001011111000000)))
;; y should be Float32(0x003F97C0 [Rational(65119, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10111100110110100010000)))
;; z should be Float32(0x005E6D10 [Rational(386769, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0A6404EB))))
(check-sat)
(exit)
