(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA8F9B71)))
;; x should be Float32(0xCA8F9B71 [Rational(-9411441, 2), -4705720.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xDF326136)))
;; y should be Float32(0xDF326136 [Rational(-12853614185120006144), -12853614185120006144.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x801B181F)))
;; z should be Float32(0x801B181F [Rational(-1775647, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x6A482139)))
(check-sat)
(exit)
