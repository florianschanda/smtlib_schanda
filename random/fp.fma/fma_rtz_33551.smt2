(declare-const x Float32)
(assert (= x (fp #b1 #b11100001 #b11011010001010000011100)))
;; x should be Float32(0xF0ED141C [Rational(-586978064562500753770665738240), -586978064562500753770665738240.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8069DFC3)))
