(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111111111111111111111)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x871103A4)))
;; y should be Float32(0x871103A4 [Rational(-2375913, 21778071482940061661655974875633165533184), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10101111 #b11011101011111001110110)))
;; z should be Float32(0x57EEBE76 [Rational(525003581816832), 525003581816832.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x57EEBE76))))
(check-sat)
(exit)
