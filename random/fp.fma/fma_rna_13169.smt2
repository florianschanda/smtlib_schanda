(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11101110011011100111000)))
;; x should be Float32(0x80773738 [Rational(-976615, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AACA309)))
;; z should be Float32(0x4AACA309 [Rational(11313929, 2), 5656964.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010101 #b01011001010001100001001))))
(check-sat)
(exit)
