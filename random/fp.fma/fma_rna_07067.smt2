(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA264C4A)))
;; y should be Float32(0xCA264C4A [Rational(-5449253, 2), -2724626.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00101110011111111111010)))
;; z should be Float32(0x80173FFA [Rational(-761853, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x80173FFA))))
(check-sat)
(exit)
