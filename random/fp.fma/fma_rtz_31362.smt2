(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA0EE3EA)))
;; x should be Float32(0xCA0EE3EA [Rational(-4682229, 2), -2341114.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA7DB04DB)))
;; y should be Float32(0xA7DB04DB [Rational(-14353627, 2361183241434822606848), -0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
