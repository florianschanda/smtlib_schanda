(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x6A645B26)))
;; y should be Float32(0x6A645B26 [Rational(69016380799587847627603968), 69016380799587847627603968.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01100010111011110110010)))
;; z should be Float32(0x803177B2 [Rational(-1620953, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b00000000 #b01100010111011110110010))))
(check-sat)
(exit)
