(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A14DFDA)))
;; x should be Float32(0x4A14DFDA [Rational(4878317, 2), 2439158.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE1C855BE)))
;; y should be Float32(0xE1C855BE [Rational(-461940898810088652800), -461940898810088652800.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAAB6A2F)))
;; z should be Float32(0xCAAB6A2F [Rational(-11233839, 2), -5616919.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b11011000 #b11010010000000101111110))))
(check-sat)
(exit)
