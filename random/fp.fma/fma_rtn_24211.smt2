(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9468648)))
;; x should be Float32(0xC9468648 [Rational(-1626313, 2), -813156.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01100001101111001011100)))
;; y should be Float32(0x49B0DE5C [Rational(2897815, 2), 1448907.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10100111 #b00010010010100011000010)))
(check-sat)
(exit)
