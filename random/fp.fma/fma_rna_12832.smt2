(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00111111 #b00100111011110001110111)))
;; x should be Float32(0x9F93BC77 [Rational(-9682039, 154742504910672534362390528), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b11000101010111010101000)))
;; y should be Float32(0xC962AEA8 [Rational(-1856981, 2), -928490.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b01010011 #b00000101101000100101100))))
(check-sat)
(exit)
