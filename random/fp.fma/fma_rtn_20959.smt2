(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01100010011101100100101)))
;; x should be Float32(0x4AB13B25 [Rational(11615013, 2), 5807506.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00011110100100010100111)))
;; y should be Float32(0x4A8F48A7 [Rational(9390247, 2), 4695123.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11000011111110111100111)))
;; z should be Float32(0x4AE1FDE7 [Rational(14810599, 2), 7405299.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10101011 #b10001100110010010101111))))
(check-sat)
(exit)
