(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE84D01)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00011100111111110110001)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00011000011111011011101)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10101100 #b00000010100111010010100)))
(check-sat)
(exit)
