(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE69703)))
;; x should be Float32(0xCAE69703 [Rational(-15111939, 2), -7555969.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01101110110101110001110)))
;; y should be Float32(0xCA376B8E [Rational(-6010311, 2), -3005155.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01010000111101110011001)))
;; z should be Float32(0xCAA87B99 [Rational(-11041689, 2), -5520844.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x55A536D2))))
(check-sat)
(exit)
