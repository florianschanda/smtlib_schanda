(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3E6EA35B)))
;; x should be Float32(0x3E6EA35B [Rational(15639387, 67108864), 0.233045])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD5FC6B0C)))
;; y should be Float32(0xD5FC6B0C [Rational(-34692086628352), -34692086628352.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b01110110000100111101010)))
;; z should be Float32(0x4A3B09EA [Rational(6128885, 2), 3064442.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xD4EB4C91)))
(check-sat)
(exit)
