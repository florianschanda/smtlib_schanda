(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011001 #b11011111111110011011111)))
;; x should be Float32(0x2CEFFCDF [Rational(15727839, 2305843009213693952), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x694B1FA7)))
;; y should be Float32(0x694B1FA7 [Rational(15347588459312436935131136), 15347588459312436935131136.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A3A9CDA)))
;; z should be Float32(0x4A3A9CDA [Rational(6114925, 2), 3057462.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x56BE6B31))))
(check-sat)
(exit)
