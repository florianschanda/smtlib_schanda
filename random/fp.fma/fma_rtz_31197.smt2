(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10001111 #b10110111001111011000000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11000001 #b01001111101110101001011)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01111111010000110011110)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xE9100267)))
(check-sat)
(exit)
