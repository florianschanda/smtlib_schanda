(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00110101011100001100001)))
;; x should be Float32(0xCA9AB861 [Rational(-10139745, 2), -5069872.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1C64C460)))
;; y should be Float32(0x1C64C460 [Rational(468515, 618970019642690137449562112), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80003749)))
;; z should be Float32(0x80003749 [Rational(-14153, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b01001111 #b00010100100001011100101))))
(check-sat)
(exit)
