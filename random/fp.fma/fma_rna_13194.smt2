(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80103387)))
;; x should be Float32(0x80103387 [Rational(-1061767, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9B35584)))
;; y should be Float32(0xC9B35584 [Rational(-2938209, 2), -1469104.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x30D97DA6)))
;; z should be Float32(0x30D97DA6 [Rational(7126739, 4503599627370496), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b01100001 #b10110010111110110100110)))
(check-sat)
(exit)
