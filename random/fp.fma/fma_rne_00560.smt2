(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49F9D53C)))
;; x should be Float32(0x49F9D53C [Rational(4093263, 2), 2046631.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA5EE356)))
;; y should be Float32(0xCA5EE356 [Rational(-7303595, 2), -3651797.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10000011011101111111111)))
;; z should be Float32(0x0041BBFF [Rational(4307967, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10101001 #b10110011000010011000110))))
(check-sat)
(exit)
