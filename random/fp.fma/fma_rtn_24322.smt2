(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b00101001111111000010000)))
;; x should be Float32(0xC894FE10 [Rational(-610273, 2), -305136.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00111000 #b11011011011000110000110)))
;; y should be Float32(0x1C6DB186 [Rational(7788739, 9903520314283042199192993792), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01101010001000100101101)))
;; z should be Float32(0x4AB5112D [Rational(11866413, 2), 5933206.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10010101 #b01101010001000100101100)))
(check-sat)
(exit)
