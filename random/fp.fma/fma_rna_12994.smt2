(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101111 #b11010011001011111001111)))
;; x should be Float32(0xB7E997CF [Rational(-15308751, 549755813888), -0.000028])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF8DA6DA)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b1 #b00010000 #b00111110100011111010110)))
;; z should be Float32(0x881F47D6 [Rational(-5219307, 10889035741470030830827987437816582766592), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
