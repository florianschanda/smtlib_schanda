(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00000000)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x520FCC0E)))
;; y should be Float32(0x520FCC0E [Rational(154400948224), 154400948224.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x589F45D3)))
;; z should be Float32(0x589F45D3 [Rational(1400977932419072), 1400977932419072.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10110001 #b00111110100010111010011))))
(check-sat)
(exit)
