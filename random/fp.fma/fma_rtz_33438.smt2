(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01111010 #b10011000110100001010111)))
;; x should be Float32(0xBD4C6857 [Rational(-13396055, 268435456), -0.049904])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b00000000000000000000000)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z (fp #b1 #b11000001 #b00101100010010100111000)))
;; z should be Float32(0xE0962538 [Rational(-86552922019829121024), -86552922019829121024.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
