(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111110 #b11111111111111111111111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11100100101011110100000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01010001010110001000111)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010101 #b01010001010110001001110))))
(check-sat)
(exit)
