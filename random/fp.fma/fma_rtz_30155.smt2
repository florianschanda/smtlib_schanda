(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x570D3561)))
;; x should be Float32(0x570D3561 [Rational(155260400173056), 155260400173056.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10000100110101110110111)))
;; y should be Float32(0xCAC26BB7 [Rational(-12741559, 2), -6370779.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11010000010110110001001)))
;; z should be Float32(0x4AE82D89 [Rational(15216009, 2), 7608004.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11000100 #b10101100111101110111011))))
(check-sat)
(exit)
