(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x086D7FBB)))
;; x should be Float32(0x086D7FBB [Rational(15564731, 21778071482940061661655974875633165533184), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11001001101101001010110)))
;; y should be Float32(0x4A64DA56 [Rational(7499051, 2), 3749525.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC968A258)))
;; z should be Float32(0xC968A258 [Rational(-1905739, 2), -952869.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010010 #b11010001010001001010111))))
(check-sat)
(exit)
