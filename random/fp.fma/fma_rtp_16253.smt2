(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011100 #b00101111010110111110111)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00000000000000000000000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b01011001 #b01110010011100000100100)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01011001 #b01110010011100000100100))))
(check-sat)
(exit)
