(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00110110111100100011010)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00000000000000000000000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00010111 #b01011111101010101010011)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00010111 #b01011111101010101010011))))
(check-sat)
(exit)
