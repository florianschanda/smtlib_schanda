(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11001010111110011000101)))
;; x should be Float32(0x00657CC5 [Rational(6651077, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA8C355D)))
;; y should be Float32(0xCA8C355D [Rational(-9188701, 2), -4594350.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A7ADF96)))
;; z should be Float32(0x4A7ADF96 [Rational(8220619, 2), 4110309.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4A7ADF95))))
(check-sat)
(exit)
