(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA91F145)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b10000000110010111001000)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A220286)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10101000 #b10110110101110111101010)))
(check-sat)
(exit)
