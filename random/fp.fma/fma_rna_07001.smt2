(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10000011 #b11101101000010010011111)))
;; y should be Float32(0x41F6849F [Rational(16155807, 524288), 30.814756])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0030C014)))
;; z should be Float32(0x0030C014 [Rational(798725, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0030C014))))
(check-sat)
(exit)
