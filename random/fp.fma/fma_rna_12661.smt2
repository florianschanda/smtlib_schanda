(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC1AD057F)))
;; x should be Float32(0xC1AD057F [Rational(-11339135, 524288), -21.627684])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AADA871)))
;; y should be Float32(0x4AADA871 [Rational(11380849, 2), 5690424.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCCEABD1E))))
(check-sat)
(exit)
