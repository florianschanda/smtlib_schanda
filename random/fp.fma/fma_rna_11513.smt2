(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFF7FFFFF)))
;; x should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00110010 #b11111110010100110111101)))
;; y should be Float32(0x997F29BD [Rational(-16722365, 1267650600228229401496703205376), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000111001101111011110)))
;; z should be Float32(0x80039BDE [Rational(-118255, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10110010 #b11111110010100110111100))))
(check-sat)
(exit)
