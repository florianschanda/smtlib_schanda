(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00100010 #b00110010101111111010011)))
;; x should be Float32(0x11195FD3 [Rational(10051539, 83076749736557242056487941267521536), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b11100011101001011011000)))
;; y should be Float32(0xC971D2D8 [Rational(-1981019, 2), -990509.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x9B10E188))))
(check-sat)
(exit)
