(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00110001 #b11110000001111100111101)))
(declare-const y Float32)
(assert (= y (fp #b1 #b01101011 #b11100110101101101111111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00101110001101111011101)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x0F6BDE55)))
(check-sat)
(exit)
