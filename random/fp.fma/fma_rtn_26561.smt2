(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xAF805160)))
;; x should be Float32(0xAF805160 [Rational(-262795, 1125899906842624), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01000001001010011001011)))
;; y should be Float32(0xCAA094CB [Rational(-10523851, 2), -5261925.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x802A74BF)))
;; z should be Float32(0x802A74BF [Rational(-2782399, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b01110101 #b01000001111101011100001)))
(check-sat)
(exit)
