(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11010001001001100100100)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11011110 #b10100011010110001001101)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11011110 #b10100011010110001001101))))
(check-sat)
(exit)
