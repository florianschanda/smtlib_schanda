(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b00000010000110001111100)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11011011010110110000001)))
;; y should be Float32(0xCAEDAD81 [Rational(-15576449, 2), -7788224.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC90FDED8)))
;; z should be Float32(0xC90FDED8 [Rational(-1178587, 2), -589293.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
