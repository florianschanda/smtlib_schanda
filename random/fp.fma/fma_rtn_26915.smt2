(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x805715E3)))
;; x should be Float32(0x805715E3 [Rational(-5707235, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10100011001010011011011)))
;; y should be Float32(0xCAD194DB [Rational(-13735131, 2), -6867565.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA94E1B7)))
;; z should be Float32(0xCA94E1B7 [Rational(-9757111, 2), -4878555.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xCA94E1B7)))
(check-sat)
(exit)
