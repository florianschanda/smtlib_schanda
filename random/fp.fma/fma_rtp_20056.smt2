(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11010011000010011101000)))
;; x should be Float32(0x806984E8 [Rational(-864413, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10011001100011111110100)))
;; y should be Float32(0xC9CCC7F4 [Rational(-3355133, 2), -1677566.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA7D043A)))
;; z should be Float32(0xCA7D043A [Rational(-8290845, 2), -4145422.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA7D0439))))
(check-sat)
(exit)
