(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11011100 #b10100011110010110101111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b01111000 #b11101101000011110110100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b11010110 #b10010100010000111111001)))
(check-sat)
(exit)
