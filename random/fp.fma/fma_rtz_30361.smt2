(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00779606)))
;; x should be Float32(0x00779606 [Rational(3918595, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00001111010001000100110)))
;; y should be Float32(0x4A07A226 [Rational(4444435, 2), 2222217.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8048FF71)))
;; z should be Float32(0x8048FF71 [Rational(-4783985, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0AFD6F6A))))
(check-sat)
(exit)
