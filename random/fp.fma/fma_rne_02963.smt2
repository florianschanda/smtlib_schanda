(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11001110110100110000011)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11111110 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01010110110010000011111)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10000000 #b10011101101001100000101))))
(check-sat)
(exit)
