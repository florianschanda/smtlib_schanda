(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01001000101111001000111)))
;; x should be Float32(0x80245E47 [Rational(-2383431, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01101001 #b01111111001110001111101)))
;; y should be Float32(0x34BF9C7D [Rational(12557437, 35184372088832), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA76FB46)))
;; z should be Float32(0xCA76FB46 [Rational(-8093091, 2), -4046545.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10010100 #b11101101111101101000110))))
(check-sat)
(exit)
