(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF5E319)))
;; x should be Float32(0x4AF5E319 [Rational(16114457, 2), 8057228.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x5C68E6CB)))
;; y should be Float32(0x5C68E6CB [Rational(262223816598487040), 262223816598487040.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AA4E069)))
;; z should be Float32(0x4AA4E069 [Rational(10805353, 2), 5402676.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x67DFB37B)))
(check-sat)
(exit)
