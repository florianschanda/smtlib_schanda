(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00100000 #b01110010111101001110001)))
;; x should be Float32(0x90397A71 [Rational(-12155505, 332306998946228968225951765070086144), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b10110111010111100100010)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10011000010001101000101)))
;; z should be Float32(0x4ACC2345 [Rational(13378373, 2), 6689186.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
