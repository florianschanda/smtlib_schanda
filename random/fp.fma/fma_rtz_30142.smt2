(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00011111 #b11110001000100100110110)))
;; x should be Float32(0x0FF88936 [Rational(8144027, 332306998946228968225951765070086144), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80000000)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b00101111 #b00100010110001010101000)))
;; z should be Float32(0x179162A8 [Rational(1190997, 1267650600228229401496703205376), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x179162A8))))
(check-sat)
(exit)
