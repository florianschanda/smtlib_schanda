(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEFC30342)))
;; x should be Float32(0xEFC30342 [Rational(-120707030737618003370881056768), -120707030737618003370881056768.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80698CD6)))
;; y should be Float32(0x80698CD6 [Rational(-3458667, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0004DD9D)))
;; z should be Float32(0x0004DD9D [Rational(318877, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01100001 #b01000001100111100111110))))
(check-sat)
(exit)
