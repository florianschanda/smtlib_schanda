(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0048A4A0)))
;; x should be Float32(0x0048A4A0 [Rational(148773, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000111 #b00011101011101000100111)))
;; y should be Float32(0x038EBA27 [Rational(9353767, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (_ +zero 8 24)))
(check-sat)
(exit)
