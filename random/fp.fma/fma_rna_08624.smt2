(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x007FFFFF)))
;; x should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCDE9F991)))
;; y should be Float32(0xCDE9F991 [Rational(-490680864), -490680864.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00110011 #b10001101100000010101101)))
;; z should be Float32(0x99C6C0AD [Rational(-13025453, 633825300114114700748351602688), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00110011 #b10001101100000010110001))))
(check-sat)
(exit)
