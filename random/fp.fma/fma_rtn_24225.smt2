(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00001100001100111101100)))
;; x should be Float32(0xC98619EC [Rational(-2197115, 2), -1098557.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b10001000100100110011000)))
;; y should be Float32(0x49444998 [Rational(1607987, 2), 803993.500000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
