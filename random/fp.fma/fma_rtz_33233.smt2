(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101001 #b11010010010000101100100)))
;; x should be Float32(0xB4E92164 [Rational(-3819609, 8796093022208), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A800BCB)))
;; y should be Float32(0x4A800BCB [Rational(8391627, 2), 4195813.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AC7FC91)))
;; z should be Float32(0x4AC7FC91 [Rational(13106321, 2), 6553160.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AC7FC8D))))
(check-sat)
(exit)
