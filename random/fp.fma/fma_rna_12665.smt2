(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xAD3D7322)))
;; x should be Float32(0xAD3D7322 [Rational(-6207889, 576460752303423488), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b11001110111011010101100)))
;; y should be Float32(0x49E776AC [Rational(3792299, 2), 1896149.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA044CFE)))
;; z should be Float32(0xCA044CFE [Rational(-4335231, 2), -2167615.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA044CFE))))
(check-sat)
(exit)
