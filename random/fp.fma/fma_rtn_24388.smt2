(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b01001010110001010111000)))
;; x should be Float32(0xC92562B8 [Rational(-1354839, 2), -677419.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9C51CEC)))
;; y should be Float32(0xC9C51CEC [Rational(-3229499, 2), -1614749.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA8D46D5)))
;; z should be Float32(0xCA8D46D5 [Rational(-9258709, 2), -4629354.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x537EAF07))))
(check-sat)
(exit)
