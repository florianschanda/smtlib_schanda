(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB7FDA1)))
;; x should be Float32(0xCAB7FDA1 [Rational(-12058017, 2), -6029008.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x558B6F77)))
;; y should be Float32(0x558B6F77 [Rational(19163856764928), 19163856764928.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b01000101111001100001000)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
