(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000001 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010110 #b11010101101111011011111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b00011000 #b11010101101111011011111)))
(check-sat)
(exit)
