(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001010 #b00010011100110001101111)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11011100 #b11000000100010100100110)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA7F8F9A)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010100 #b11111111000111110011010))))
(check-sat)
(exit)
