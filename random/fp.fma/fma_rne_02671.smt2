(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x60F17EF1)))
;; x should be Float32(0x60F17EF1 [Rational(139212887940067753984), 139212887940067753984.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111101 #b11110011001000111010011)))
;; y should be Float32(0x7EF991D3 [Rational(165867466791678986430822166423782031360), 165867466791678986430822166423782031360.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01011110111010100111101)))
;; z should be Float32(0xCAAF753D [Rational(-11498813, 2), -5749406.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x7F800000)))
(check-sat)
(exit)
