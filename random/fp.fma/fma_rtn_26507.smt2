(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDB85FA8F)))
;; x should be Float32(0xDB85FA8F [Rational(-75423327979569152), -75423327979569152.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10111000000000010011000)))
;; y should be Float32(0x005C0098 [Rational(753683, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49EF41C4)))
;; z should be Float32(0x49EF41C4 [Rational(3919985, 2), 1959992.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10010011 #b11011110100000111000011)))
(check-sat)
(exit)
