(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b01011100000001101000100)))
;; y should be Float32(0xC9AE0344 [Rational(-2851025, 2), -1425512.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11001011 #b11110011011000011110100)))
;; z should be Float32(0xE5F9B0F4 [Rational(-147391682905557474213888), -147391682905557474213888.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11001011 #b11110011011000011110011))))
(check-sat)
(exit)
