(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11011111001110111010100)))
;; x should be Float32(0xC9EF9DD4 [Rational(-3925877, 2), -1962938.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111100100101100000111)))
;; y should be Float32(0x807E4B07 [Rational(-8276743, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAB18B47)))
;; z should be Float32(0xCAB18B47 [Rational(-11635527, 2), -5817763.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCAB18B46))))
(check-sat)
(exit)
