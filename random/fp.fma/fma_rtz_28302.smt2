(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +oo 8 24)))
;; x should be Float32(+oo)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0E4614F2)))
;; y should be Float32(0x0E4614F2 [Rational(6490745, 2658455991569831745807614120560689152), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b00000110010111000101110)))
;; z should be Float32(0xCA032E2E [Rational(-4298519, 2), -2149259.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ +oo 8 24))))
(check-sat)
(exit)
