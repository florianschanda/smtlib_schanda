(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDECED905)))
;; x should be Float32(0xDECED905 [Rational(-7452474969658753024), -7452474969658753024.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8035FC0D)))
;; y should be Float32(0x8035FC0D [Rational(-3537933, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00111110 #b01011100111101001011000))))
(check-sat)
(exit)
