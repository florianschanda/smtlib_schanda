(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10100011000111100000001)))
;; x should be Float32(0xCAD18F01 [Rational(-13733633, 2), -6866816.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00101000 #b10100010010101111101011)))
;; y should be Float32(0x14512BEB [Rational(13708267, 1298074214633706907132624082305024), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b00000000000000000000000)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFF800000))))
(check-sat)
(exit)
