(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00100101 #b10110011001001001111101)))
;; x should be Float32(0x92D9927D [Rational(-14258813, 10384593717069655257060992658440192), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00001111001011111111101)))
;; y should be Float32(0x000797FD [Rational(497661, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00010100100010010110001)))
;; z should be Float32(0xCA8A44B1 [Rational(-9061553, 2), -4530776.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA8A44B1))))
(check-sat)
(exit)
