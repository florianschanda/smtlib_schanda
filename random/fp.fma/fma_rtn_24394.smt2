(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11001000010001101011001)))
;; x should be Float32(0xCAE42359 [Rational(-14951257, 2), -7475628.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11011011010011100010001)))
;; y should be Float32(0xCAEDA711 [Rational(-15574801, 2), -7787400.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11000010 #b10010010011000111000100)))
;; z should be Float32(0xE14931C4 [Rational(-231961346876182102016), -231961346876182102016.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xE14931C1))))
(check-sat)
(exit)
