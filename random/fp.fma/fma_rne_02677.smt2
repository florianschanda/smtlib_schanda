(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3CAB2311)))
;; x should be Float32(0x3CAB2311 [Rational(11215633, 536870912), 0.020891])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x41A73DC2)))
;; y should be Float32(0x41A73DC2 [Rational(5480161, 262144), 20.905155])

(declare-const z Float32)
(assert (= z (fp #b1 #b00011100 #b01001000111110010101100)))
;; z should be Float32(0x8E247CAC [Rational(-2694955, 1329227995784915872903807060280344576), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x3EDF9A52)))
(check-sat)
(exit)
