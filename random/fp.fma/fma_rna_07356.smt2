(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x496537E8)))
;; x should be Float32(0x496537E8 [Rational(1877757, 2), 938878.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7B4083D1)))
;; y should be Float32(0x7B4083D1 [Rational(999594551182730754749949435814346752), 999594551182730754749949435814346752.000000])

(declare-const z Float32)
(assert (= z (_ +oo 8 24)))
;; z should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x7F800000)))
(check-sat)
(exit)
