(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10011010101011010111111)))
;; x should be Float32(0x4ACD56BF [Rational(13457087, 2), 6728543.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11000111010011101110001)))
;; y should be Float32(0x0063A771 [Rational(6530929, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xF3EEA74B)))
;; z should be Float32(0xF3EEA74B [Rational(-37816154688943096141857203159040), -37816154688943096141857203159040.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xF3EEA74A)))
(check-sat)
(exit)
