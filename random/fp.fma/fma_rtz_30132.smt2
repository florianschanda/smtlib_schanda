(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5DB18BEC)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11001101010010100111010)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00001110 #b00111110000100001101100)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b00111101 #b00011100110000010101110)))
(check-sat)
(exit)
