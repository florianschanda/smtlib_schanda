(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100101 #b11110110111010111100110)))
;; x should be Float32(0x32FB75E6 [Rational(8239859, 281474976710656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x005B0BB7)))
;; y should be Float32(0x005B0BB7 [Rational(5966775, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x95CD17BD)))
;; z should be Float32(0x95CD17BD [Rational(-13440957, 162259276829213363391578010288128), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x95CD17BC))))
(check-sat)
(exit)
