(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x01335B76)))
;; x should be Float32(0x01335B76 [Rational(5877179, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11100001000100011001010)))
;; y should be Float32(0xCA7088CA [Rational(-7881829, 2), -3940914.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xEED11DDB)))
;; z should be Float32(0xEED11DDB [Rational(-32359230049844846030923759616), -32359230049844846030923759616.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11011101 #b10100010001110111011011)))
(check-sat)
(exit)
