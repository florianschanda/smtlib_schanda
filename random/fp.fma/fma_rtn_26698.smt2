(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101111 #b11011000111001000111100)))
;; x should be Float32(0x97EC723C [Rational(-3873935, 2535301200456458802993406410752), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b01010000011100100000001)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (_ +oo 8 24)))
;; z should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
