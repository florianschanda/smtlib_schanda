(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001100 #b00001110000110110110101)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10000000 #b10010100001101100101111)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A80B177)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10010100 #b11111111011100001110010)))
(check-sat)
(exit)
