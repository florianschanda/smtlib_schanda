(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8033D04D)))
;; x should be Float32(0x8033D04D [Rational(-3395661, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (_ NaN 8 24)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b01101010 #b01001010001100110010101)))
;; z should be Float32(0x35251995 [Rational(10819989, 17592186044416), 0.000001])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
