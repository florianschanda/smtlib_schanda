(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEE064F00)))
;; x should be Float32(0xEE064F00 [Rational(-10391624113952448728480612352), -10391624113952448728480612352.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b00000000000000000000000)))
;; y should be Float32(+oo)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7F800000)))
;; z should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
