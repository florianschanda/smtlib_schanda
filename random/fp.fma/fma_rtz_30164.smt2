(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000010001101111010)))
;; x should be Float32(0x0080237A [Rational(4198845, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00110100100101000000010)))
;; y should be Float32(0xCA1A4A02 [Rational(-5055745, 2), -2527872.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00000010111010010111011)))
;; z should be Float32(0xCA8174BB [Rational(-8484027, 2), -4242013.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA8174BB))))
(check-sat)
(exit)
