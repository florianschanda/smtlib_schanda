(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4EF7CC12)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00011001111111100101110)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00000011011100110011011)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10110011 #b00010000111101010011010)))
(check-sat)
(exit)
