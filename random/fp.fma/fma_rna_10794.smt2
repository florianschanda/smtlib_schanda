(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b01101101101010001101110)))
;; x should be Float32(0xCA36D46E [Rational(-5990967, 2), -2995483.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10011001100001111101010)))
;; y should be Float32(0x804CC3EA [Rational(-2515445, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01010010100101011010011)))
;; z should be Float32(0x4AA94AD3 [Rational(11094739, 2), 5547369.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x4AA94AD3)))
(check-sat)
(exit)
