(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC9545B)))
;; x should be Float32(0xCAC9545B [Rational(-13194331, 2), -6597165.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AFC75F1)))
;; y should be Float32(0x4AFC75F1 [Rational(16545265, 2), 8272632.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11011110100001100101000)))
;; z should be Float32(0x006F4328 [Rational(911461, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xD6468BCB)))
(check-sat)
(exit)
