(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x669B14DB)))
;; x should be Float32(0x669B14DB [Rational(366175760169681752686592), 366175760169681752686592.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b00110100011010101110100)))
;; y should be Float32(0xC99A3574 [Rational(-2526557, 2), -1263278.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA330DF2)))
;; z should be Float32(0xCA330DF2 [Rational(-5867257, 2), -2933628.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xF0BAD5DB))))
(check-sat)
(exit)
