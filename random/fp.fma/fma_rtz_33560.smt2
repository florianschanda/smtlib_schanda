(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101100 #b01101010101101010001011)))
;; x should be Float32(0xF6355A8B [Rational(-919572422825900326020606045716480), -919572422825900326020606045716480.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFFFD5E49)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
