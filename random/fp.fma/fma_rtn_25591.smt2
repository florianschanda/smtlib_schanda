(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE8A06EF5)))
;; y should be Float32(0xE8A06EF5 [Rational(-6061003465742324617707520), -6061003465742324617707520.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10001011 #b01010111100100110100011)))
;; z should be Float32(0xC5ABC9A3 [Rational(-11258275, 2048), -5497.204590])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10001011 #b01010111100100110100011)))
(check-sat)
(exit)
