(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000010011110110010001)))
;; x should be Float32(0x00013D91 [Rational(81297, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010000 #b10011010011011111100000)))
;; y should be Float32(0xC84D37E0 [Rational(-420287, 2), -210143.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b01010001000011011001100)))
;; z should be Float32(0xC9A886CC [Rational(-2761139, 2), -1380569.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC9A886CC))))
(check-sat)
(exit)
