(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01101011001001001111011)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01000001011101010110011)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b01100001110110000011000)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xC930EC18)))
(check-sat)
(exit)
