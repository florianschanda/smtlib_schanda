(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11001101 #b00011010100000000001101)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAE977FD)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00011101100111100101101)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11100100 #b00000001101000110000000)))
(check-sat)
(exit)
