(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFBA01245)))
;; x should be Float32(0xFBA01245 [Rational(-1662276094963303269743608835461873664), -1662276094963303269743608835461873664.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA4D5976)))
;; y should be Float32(0xCA4D5976 [Rational(-6728891, 2), -3364445.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b11100001101000000111100)))
;; z should be Float32(0x49F0D03C [Rational(3945487, 2), 1972743.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
