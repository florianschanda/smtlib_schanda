(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110010 #b10000011011111101011001)))
;; x should be Float32(0xF941BF59 [Rational(-62874642827373935680212846371667968), -62874642827373935680212846371667968.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA8059DB)))
;; y should be Float32(0xCA8059DB [Rational(-8411611, 2), -4205805.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01010010011111001001001)))
;; z should be Float32(0x00293E49 [Rational(2702921, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x7F7FFFFF)))
(check-sat)
(exit)
