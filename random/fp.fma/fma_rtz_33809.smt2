(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x801B0EDF)))
;; x should be Float32(0x801B0EDF [Rational(-1773279, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF7FFFFF)))
;; y should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4EB8FA3A)))
;; z should be Float32(0x4EB8FA3A [Rational(1551703296), 1551703296.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10011101 #b01110001111101000111010))))
(check-sat)
(exit)
