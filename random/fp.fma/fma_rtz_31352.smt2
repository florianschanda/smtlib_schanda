(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00110101100001101111101)))
;; x should be Float32(0xCA9AC37D [Rational(-10142589, 2), -5071294.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11110010 #b11101001111010010100001)))
;; y should be Float32(0xF974F4A1 [Rational(-79492631072497109093952299638718464), -79492631072497109093952299638718464.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b00001101110000100111100)))
;; z should be Float32(0x4986E13C [Rational(2209871, 2), 1104935.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
