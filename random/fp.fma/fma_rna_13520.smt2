(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b00000010100001001100111)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111111111011110100110)))
;; y should be Float32(0x001FF7A6 [Rational(1047507, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b01001101000000011011100)))
;; z should be Float32(0xC9A680DC [Rational(-2727991, 2), -1363995.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
