(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8C6E8BAA)))
;; x should be Float32(0x8C6E8BAA [Rational(-7816661, 42535295865117307932921825928971026432), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00011101111111010001011)))
;; y should be Float32(0x800EFE8B [Rational(-982667, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b10010100011100110001010)))
;; z should be Float32(0x4A4A398A [Rational(6626501, 2), 3313250.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10010100 #b10010100011100110001010)))
(check-sat)
(exit)
