(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABB43E9)))
;; x should be Float32(0x4ABB43E9 [Rational(12272617, 2), 6136308.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00101000 #b00010001101010010000100)))
;; y should be Float32(0x9408D484 [Rational(-2241825, 324518553658426726783156020576256), -0.000000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x9F482F11)))
(check-sat)
(exit)
