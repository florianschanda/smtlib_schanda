(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00001110100110000110110)))
;; x should be Float32(0x00074C36 [Rational(239131, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x5AEC2E98)))
;; y should be Float32(0x5AEC2E98 [Rational(33239662436810752), 33239662436810752.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x19D772B9)))
(check-sat)
(exit)
