(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111111 #b11011110110011111010001)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00010110011001111010000)))
;; y should be Float32(0x000B33D0 [Rational(45885, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x3F4F016B)))
;; z should be Float32(0x3F4F016B [Rational(13566315, 16777216), 0.808615])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
