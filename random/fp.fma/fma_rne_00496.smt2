(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A9E19A3)))
;; x should be Float32(0x4A9E19A3 [Rational(10361251, 2), 5180625.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x08D63249)))
;; y should be Float32(0x08D63249 [Rational(14037577, 10889035741470030830827987437816582766592), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49A11BDC)))
;; z should be Float32(0x49A11BDC [Rational(2639607, 2), 1319803.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10010011 #b01000010001101111011100)))
(check-sat)
(exit)
