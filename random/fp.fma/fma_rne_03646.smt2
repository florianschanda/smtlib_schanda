(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10100010000000011100110)))
;; x should be Float32(0xCA5100E6 [Rational(-6848627, 2), -3424313.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01000010100111111100110)))
;; y should be Float32(0x4A214FE6 [Rational(5285875, 2), 2642937.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10111111 #b11001100001010111110000)))
;; z should be Float32(0xDFE615F0 [Rational(-33158842972050030592), -33158842972050030592.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10111111 #b11001100001010111110100))))
(check-sat)
(exit)
