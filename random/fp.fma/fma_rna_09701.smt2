(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4F0A7FB5)))
;; x should be Float32(0x4F0A7FB5 [Rational(2323625216), 2323625216.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01101011 #b10001101111001011100101)))
;; y should be Float32(0xB5C6F2E5 [Rational(-13038309, 8796093022208), -0.000001])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA77630E)))
;; z should be Float32(0xCA77630E [Rational(-8106375, 2), -4053187.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA7798DF))))
(check-sat)
(exit)
