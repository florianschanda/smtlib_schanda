(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001101 #b11101110101100000010111)))
;; x should be Float32(0x86F75817 [Rational(-16209943, 174224571863520493293247799005065324265472), -0.000000])

(declare-const y Float32)
(assert (= y (_ -oo 8 24)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC982EDB4)))
;; z should be Float32(0xC982EDB4 [Rational(-2145133, 2), -1072566.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (_ +oo 8 24))))
(check-sat)
(exit)
