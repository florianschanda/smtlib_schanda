(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011010 #b11100101111110010001010)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11011011101111001110010)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10101001011101001101011)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x8054BA6B)))
(check-sat)
(exit)
