(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00101001 #b10101010001111110110000)))
;; x should be Float32(0x14D51FB0 [Rational(872955, 40564819207303340847894502572032), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0010D058)))
;; y should be Float32(0x0010D058 [Rational(137739, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x00000000)))
(check-sat)
(exit)
