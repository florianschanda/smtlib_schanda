(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE654A7E1)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b00001000111010011001000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00110111 #b00001100110001010001100)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b11011111 #b10111000000111100100101)))
(check-sat)
(exit)
