(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xADD073F7)))
;; x should be Float32(0xADD073F7 [Rational(-13661175, 576460752303423488), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEE9400C0)))
;; y should be Float32(0xEE9400C0 [Rational(-22902344073961890571574312960), -22902344073961890571574312960.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10110010 #b01011011001001100101000)))
;; z should be Float32(0xD92D9328 [Rational(-3053560686182400), -3053560686182400.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x5CEFAC28)))
(check-sat)
(exit)
