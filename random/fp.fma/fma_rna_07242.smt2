(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF5A893)))
;; x should be Float32(0x4AF5A893 [Rational(16099475, 2), 8049737.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AE5D99D)))
;; y should be Float32(0x4AE5D99D [Rational(15063453, 2), 7531726.500000])

(declare-const z Float32)
(assert (= z (_ +oo 8 24)))
;; z should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x7F800000)))
(check-sat)
(exit)
