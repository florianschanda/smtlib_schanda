(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11011100100001001001100)))
;; x should be Float32(0x806E424C [Rational(-1806483, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01110001 #b11101001110011010000100)))
;; y should be Float32(0xB8F4E684 [Rational(-4012449, 34359738368), -0.000117])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807CB64C)))
;; z should be Float32(0x807CB64C [Rational(-2043283, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x807CB300))))
(check-sat)
(exit)
