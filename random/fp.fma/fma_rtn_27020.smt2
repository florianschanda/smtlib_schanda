(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10001110110110000110000)))
;; x should be Float32(0x80476C30 [Rational(-292547, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9E3E1410)))
;; y should be Float32(0x9E3E1410 [Rational(-778561, 77371252455336267181195264), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10010001010100111101111)))
;; z should be Float32(0x4AC8A9EF [Rational(13150703, 2), 6575351.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10010101 #b10010001010100111101111)))
(check-sat)
(exit)
