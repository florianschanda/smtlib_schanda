(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x006515EB)))
;; x should be Float32(0x006515EB [Rational(6624747, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAD42DD1)))
;; y should be Float32(0xCAD42DD1 [Rational(-13905361, 2), -6952680.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10101100 #b01001101001100111011011)))
;; z should be Float32(0x562699DB [Rational(45794933604352), 45794933604352.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x562699DB)))
(check-sat)
(exit)
