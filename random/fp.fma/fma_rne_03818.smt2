(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA292CC2)))
;; x should be Float32(0xCA292CC2 [Rational(-5543521, 2), -2771760.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA3EE36A)))
;; y should be Float32(0xCA3EE36A [Rational(-6255029, 2), -3127514.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x806CF782)))
;; z should be Float32(0x806CF782 [Rational(-3570625, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x54FC4B01)))
(check-sat)
(exit)
