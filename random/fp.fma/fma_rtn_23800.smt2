(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000010000111010111000)))
;; x should be Float32(0x00010EB8 [Rational(8663, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11000011010100011111010)))
;; y should be Float32(0x8061A8FA [Rational(-3200125, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (_ -oo 8 24)))
(check-sat)
(exit)
