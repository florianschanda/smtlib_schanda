(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10111111010000101100111)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4ADA4A7B)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11000011011010101001111)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10101100 #b01111101011000010000010)))
(check-sat)
(exit)
