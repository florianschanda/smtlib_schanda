(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5F83EC9D)))
;; x should be Float32(0x5F83EC9D [Rational(19012291073595670528), 19012291073595670528.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC8094CA0)))
;; y should be Float32(0xC8094CA0 [Rational(-281189, 2), -140594.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8023B03C)))
;; z should be Float32(0x8023B03C [Rational(-584719, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xE80D8239))))
(check-sat)
(exit)
