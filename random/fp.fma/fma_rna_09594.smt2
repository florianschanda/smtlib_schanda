(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111101 #b11010000000101011100110)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA327DFE)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10010001101111101111111)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10010011 #b01000011100100111000111)))
(check-sat)
(exit)
