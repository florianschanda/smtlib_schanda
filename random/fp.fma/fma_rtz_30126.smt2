(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x17D9EA0C)))
;; x should be Float32(0x17D9EA0C [Rational(3570307, 2535301200456458802993406410752), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11100010101100100010111)))
;; y should be Float32(0x00715917 [Rational(7428375, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00010101001010001011111)))
;; z should be Float32(0xCA8A945F [Rational(-9081951, 2), -4540975.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA8A945E))))
(check-sat)
(exit)
