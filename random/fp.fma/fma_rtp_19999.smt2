(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10010110111000101110010)))
;; x should be Float32(0x804B7172 [Rational(-2472121, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x3DB8A593)))
;; y should be Float32(0x3DB8A593 [Rational(12101011, 134217728), 0.090160])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA39E61A)))
;; z should be Float32(0xCA39E61A [Rational(-6091533, 2), -3045766.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010100 #b01110011110011000011010))))
(check-sat)
(exit)
