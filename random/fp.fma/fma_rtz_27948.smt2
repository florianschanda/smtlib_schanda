(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AAB1A7F)))
;; x should be Float32(0x4AAB1A7F [Rational(11213439, 2), 5606719.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x706874BB)))
;; y should be Float32(0x706874BB [Rational(287766563024638598208280330240), 287766563024638598208280330240.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x800272EB)))
;; z should be Float32(0x800272EB [Rational(-160491, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x7B9B5E08))))
(check-sat)
(exit)
