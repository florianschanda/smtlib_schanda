(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b11000101001000011101100)))
;; y should be Float32(0x49E290EC [Rational(3712059, 2), 1856029.500000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
