(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0036C952)))
;; x should be Float32(0x0036C952 [Rational(1795241, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8928F932)))
;; y should be Float32(0x8928F932 [Rational(-5536921, 2722258935367507707706996859454145691648), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00100000110000101001010)))
;; z should be Float32(0x0010614A [Rational(536741, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b00000000 #b00100000110000101001010)))
(check-sat)
(exit)
