(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x42EE9372)))
;; y should be Float32(0x42EE9372 [Rational(7817657, 65536), 119.287979])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x53F11D61)))
;; z should be Float32(0x53F11D61 [Rational(2071160029184), 2071160029184.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10100111 #b11100010001110101100010)))
(check-sat)
(exit)
