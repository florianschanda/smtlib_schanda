(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x812CDEF0)))
;; x should be Float32(0x812CDEF0 [Rational(-708079, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00010110 #b01011000010110001000111)))
;; y should be Float32(0x8B2C2C47 [Rational(-11283527, 340282366920938463463374607431768211456), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000000000)))
(check-sat)
(exit)
