(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111111 #b01000010111110011011111)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00101100010000010010101)))
;; y should be Float32(0x4A962095 [Rational(9838741, 2), 4919370.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A954F27)))
;; z should be Float32(0x4A954F27 [Rational(9785127, 2), 4892563.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
