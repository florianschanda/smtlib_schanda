(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ADA9119)))
;; x should be Float32(0x4ADA9119 [Rational(14323993, 2), 7161996.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80443108)))
;; y should be Float32(0x80443108 [Rational(-558625, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA0444DE)))
;; z should be Float32(0xCA0444DE [Rational(-4334191, 2), -2167095.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xCA0444DF)))
(check-sat)
(exit)
