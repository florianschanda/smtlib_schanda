(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC1D71CE0)))
;; x should be Float32(0xC1D71CE0 [Rational(-440551, 16384), -26.889099])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x77C65EC3)))
;; y should be Float32(0x77C65EC3 [Rational(8046849796752573507927500436537344), 8046849796752573507927500436537344.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8B313F45)))
;; z should be Float32(0x8B313F45 [Rational(-11616069, 340282366920938463463374607431768211456), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b11110100 #b01001101010111111110101)))
(check-sat)
(exit)
