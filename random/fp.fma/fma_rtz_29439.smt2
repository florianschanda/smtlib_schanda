(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAAD2861)))
;; y should be Float32(0xCAAD2861 [Rational(-11348065, 2), -5674032.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x6449E3A9)))
;; z should be Float32(0x6449E3A9 [Rational(14896800807733174468608), 14896800807733174468608.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11001000 #b10010011110001110101000))))
(check-sat)
(exit)
