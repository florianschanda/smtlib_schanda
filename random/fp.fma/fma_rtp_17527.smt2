(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC3CC4F)))
;; x should be Float32(0xCAC3CC4F [Rational(-12831823, 2), -6415911.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11001000011111010100011)))
;; y should be Float32(0xCAE43EA3 [Rational(-14958243, 2), -7479121.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0033A9BB)))
;; z should be Float32(0x0033A9BB [Rational(3385787, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10101100 #b01011101001000111011111)))
(check-sat)
(exit)
