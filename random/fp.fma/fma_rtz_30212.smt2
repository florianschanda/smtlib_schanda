(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111000 #b00100011010100100111101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01010010101101010011000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b01110111 #b10110011010100110110101)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b01111001 #b00101001000100100001000))))
(check-sat)
(exit)
