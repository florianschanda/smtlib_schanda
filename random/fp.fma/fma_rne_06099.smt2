(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101011 #b00010001010101001100101)))
(declare-const y Float32)
(assert (= y (fp #b1 #b01111000 #b10001010111010101010011)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b00011110001110101100100)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
