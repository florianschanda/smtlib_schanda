(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00621120)))
;; x should be Float32(0x00621120 [Rational(200841, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7796364D)))
;; y should be Float32(0x7796364D [Rational(6093327183338554162784861748723712), 6093327183338554162784861748723712.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01111101 #b00010011001000110100001)))
;; z should be Float32(0x3E8991A1 [Rational(9015713, 33554432), 0.268689])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x3E8998D2))))
(check-sat)
(exit)
