(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA82111BD)))
;; x should be Float32(0xA82111BD [Rational(-10555837, 1180591620717411303424), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01111000101111011001001)))
;; y should be Float32(0x4ABC5EC9 [Rational(12345033, 2), 6172516.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC973F918)))
;; z should be Float32(0xC973F918 [Rational(-1998627, 2), -999313.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xC973F918)))
(check-sat)
(exit)
