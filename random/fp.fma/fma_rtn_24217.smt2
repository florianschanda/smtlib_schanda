(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA592D2A)))
;; x should be Float32(0xCA592D2A [Rational(-7116437, 2), -3558218.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01001001111101101101010)))
;; y should be Float32(0x4A24FB6A [Rational(5406133, 2), 2703066.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAB788EF)))
;; z should be Float32(0xCAB788EF [Rational(-12028143, 2), -6014071.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xD50BF63E)))
(check-sat)
(exit)
