(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101111 #b10001010110110000101111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00010111 #b00010110001010011101010)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xEB30B3FB)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11010110 #b01100001011001111111010)))
(check-sat)
(exit)
