(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01100110 #b00101011100011111011100)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b11100010101000100010000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b00000101000111111101110)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x4A028FEE)))
(check-sat)
(exit)
