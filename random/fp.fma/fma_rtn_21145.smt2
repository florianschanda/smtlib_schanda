(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB9D9EB)))
;; x should be Float32(0x4AB9D9EB [Rational(12179947, 2), 6089973.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01001010011001000100010)))
;; y should be Float32(0xCA253222 [Rational(-5413137, 2), -2706568.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xE1869DAF)))
;; z should be Float32(0xE1869DAF [Rational(-310403250782744870912), -310403250782744870912.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b11000011 #b00001101001110110110000))))
(check-sat)
(exit)
