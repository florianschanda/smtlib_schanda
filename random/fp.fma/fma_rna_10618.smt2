(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b01111100000011100111000)))
;; x should be Float32(0xC93E0738 [Rational(-1556711, 2), -778355.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00110101 #b01001001011110000111110)))
;; y should be Float32(0x1AA4BC3E [Rational(5398047, 79228162514264337593543950336), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xA47490B6)))
(check-sat)
(exit)
