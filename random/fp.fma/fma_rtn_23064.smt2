(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000000000000000000001)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11110110 #b00110101000001011100110)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11111010 #b10100000011011011010100)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b11111010 #b10100000011011011010101))))
(check-sat)
(exit)
