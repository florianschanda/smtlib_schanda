(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100010011111100011101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111001011001001010100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00101101 #b10000110100010011011011)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00101101 #b10000110100010011011011))))
(check-sat)
(exit)
