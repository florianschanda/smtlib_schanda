(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE262D502)))
;; x should be Float32(0xE262D502 [Rational(-1046078247784597684224), -1046078247784597684224.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00001100100110001110001)))
;; y should be Float32(0x00064C71 [Rational(412785, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xA132972D)))
(check-sat)
(exit)
