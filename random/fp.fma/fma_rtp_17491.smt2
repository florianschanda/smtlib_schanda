(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b11101101101010100110000)))
;; x should be Float32(0xC8F6D530 [Rational(-1011027, 2), -505513.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x007D7064)))
;; y should be Float32(0x007D7064 [Rational(2055193, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA9FD4DB)))
;; z should be Float32(0xCA9FD4DB [Rational(-10474715, 2), -5237357.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA9FD4DB))))
(check-sat)
(exit)
