(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC386A35B)))
;; x should be Float32(0xC386A35B [Rational(-8823643, 32768), -269.276215])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b10111000110010000010110)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x74F8595A)))
;; z should be Float32(0x74F8595A [Rational(157409898181883366007529030025216), 157409898181883366007529030025216.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
