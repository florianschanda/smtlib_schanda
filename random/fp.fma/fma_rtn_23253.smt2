(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00100001 #b10011010111011100110011)))
;; x should be Float32(0x10CD7733 [Rational(13465395, 166153499473114484112975882535043072), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b00011011001001000001011)))
;; y should be Float32(0x488D920B [Rational(9277963, 32), 289936.343750])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b00110011 #b11000110011111110111011))))
(check-sat)
(exit)
