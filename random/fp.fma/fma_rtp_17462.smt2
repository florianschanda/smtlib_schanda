(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10001000011110100011110)))
;; x should be Float32(0xCA443D1E [Rational(-6430351, 2), -3215175.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1A885231)))
;; y should be Float32(0x1A885231 [Rational(8933937, 158456325028528675187087900672), 0.000000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xA550FEF2))))
(check-sat)
(exit)
