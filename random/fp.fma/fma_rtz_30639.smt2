(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA60C186)))
;; z should be Float32(0xCA60C186 [Rational(-7364803, 2), -3682401.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010100 #b11000001100000110000110))))
(check-sat)
(exit)
