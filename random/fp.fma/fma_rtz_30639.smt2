(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x006F7A49)))
;; x should be Float32(0x006F7A49 [Rational(7305801, 713623846352979940529142984724747568191373312), 0.000000])

