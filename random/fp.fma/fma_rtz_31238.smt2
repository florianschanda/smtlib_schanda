(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b01001100110001011101110)))
;; x should be Float32(0xCA2662EE [Rational(-5452151, 2), -2726075.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAC903A7)))
;; y should be Float32(0xCAC903A7 [Rational(-13173671, 2), -6586835.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x494F8328)))
;; z should be Float32(0x494F8328 [Rational(1699941, 2), 849970.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x5582A60C)))
(check-sat)
(exit)
