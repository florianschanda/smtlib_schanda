(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA19F8BB0)))
;; x should be Float32(0xA19F8BB0 [Rational(-653499, 604462909807314587353088), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00010110110110100101001)))
;; y should be Float32(0xCA8B6D29 [Rational(-9137449, 2), -4568724.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11011111011000110110010)))
;; z should be Float32(0x806FB1B2 [Rational(-3659993, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x2CADC9C2)))
(check-sat)
(exit)
