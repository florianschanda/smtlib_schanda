(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC99CD77C)))
;; x should be Float32(0xC99CD77C [Rational(-2569695, 2), -1284847.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A1E9D26)))
;; y should be Float32(0x4A1E9D26 [Rational(5197459, 2), 2598729.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x42320BFC)))
;; z should be Float32(0x42320BFC [Rational(2917119, 65536), 44.511703])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xD4425A8C)))
(check-sat)
(exit)
