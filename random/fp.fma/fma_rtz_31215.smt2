(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA71FAAE)))
;; x should be Float32(0xCA71FAAE [Rational(-7929175, 2), -3964587.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11011111001011100001110)))
;; y should be Float32(0x006F970E [Rational(3656583, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x99A48BC9)))
;; z should be Float32(0x99A48BC9 [Rational(-10783689, 633825300114114700748351602688), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x99A48BC9))))
(check-sat)
(exit)
