(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1BDCAACB)))
;; x should be Float32(0x1BDCAACB [Rational(14461643, 39614081257132168796771975168), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10100111 #b10010001100001110100110)))
;; y should be Float32(0x53C8C3A6 [Rational(1724551790592), 1724551790592.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80460CAB)))
;; z should be Float32(0x80460CAB [Rational(-4590763, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x302D0E13)))
(check-sat)
(exit)
