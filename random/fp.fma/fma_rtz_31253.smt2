(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC99F7AD4)))
;; x should be Float32(0xC99F7AD4 [Rational(-2612917, 2), -1306458.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10111100111100010010101)))
;; y should be Float32(0xCADE7895 [Rational(-14579861, 2), -7289930.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01010111 #b11011000000100101101111)))
;; z should be Float32(0xABEC096F [Rational(-15468911, 9223372036854775808), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10101010 #b00010101001011110100010))))
(check-sat)
(exit)
