(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110000 #b11101101011100010001111)))
;; x should be Float32(0x1876B88F [Rational(16169103, 5070602400912917605986812821504), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80056844)))
;; y should be Float32(0x80056844 [Rational(-88593, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00800000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00000000 #b11111111111111111111111))))
(check-sat)
(exit)
