(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x13C3F522)))
;; x should be Float32(0x13C3F522 [Rational(6421137, 1298074214633706907132624082305024), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE5C9CFFB)))
;; y should be Float32(0xE5C9CFFB [Rational(-119129028192020010631168), -119129028192020010631168.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00205804)))
;; z should be Float32(0x00205804 [Rational(529921, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b01110100 #b00110100111101010101011)))
(check-sat)
(exit)
