(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x005676DE)))
;; x should be Float32(0x005676DE [Rational(2833263, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00001001101100101110011)))
;; y should be Float32(0x8004D973 [Rational(-317811, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A0831AE)))
;; z should be Float32(0x4A0831AE [Rational(4462807, 2), 2231403.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010100 #b00010000011000110101110))))
(check-sat)
(exit)
