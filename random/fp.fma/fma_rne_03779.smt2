(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC94D9268)))
;; x should be Float32(0xC94D9268 [Rational(-1684045, 2), -842022.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x003F52D8)))
;; y should be Float32(0x003F52D8 [Rational(518747, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xADEF46FB)))
;; z should be Float32(0xADEF46FB [Rational(-15681275, 576460752303423488), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b01011011 #b11011110100011011111011))))
(check-sat)
(exit)
