(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A8C04A5)))
;; x should be Float32(0x4A8C04A5 [Rational(9176229, 2), 4588114.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x96F1A6D7)))
;; y should be Float32(0x96F1A6D7 [Rational(-15836887, 40564819207303340847894502572032), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01001101100001110101011)))
;; z should be Float32(0x8026C3AB [Rational(-2540459, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b01000100 #b00001000010101110011111))))
(check-sat)
(exit)
