(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000001 #b01000001000101100111001)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49699108)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11001111010111110111010)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00010101 #b00100100111100110100100)))
(check-sat)
(exit)
