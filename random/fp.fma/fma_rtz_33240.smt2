(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01100010 #b00010100000100001010101)))
;; x should be Float32(0xB10A0855 [Rational(-9046101, 4503599627370496), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A067BC2)))
;; y should be Float32(0x4A067BC2 [Rational(4406753, 2), 2203376.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01010111011100101011010)))
;; z should be Float32(0x002BB95A [Rational(1432749, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b01110111 #b00100010000011000101110))))
(check-sat)
(exit)
