(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABC6C8F)))
;; x should be Float32(0x4ABC6C8F [Rational(12348559, 2), 6174279.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA86865D)))
;; y should be Float32(0xCA86865D [Rational(-8816221, 2), -4408110.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x806539E9)))
;; z should be Float32(0x806539E9 [Rational(-6633961, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b10101011 #b10001100000011101110000)))
(check-sat)
(exit)
