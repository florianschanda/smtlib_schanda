(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10010011000111100101111)))
;; x should be Float32(0xCAC98F2F [Rational(-13209391, 2), -6604695.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11000010000110010011001)))
;; y should be Float32(0x00610C99 [Rational(6360217, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AF46503)))
;; z should be Float32(0x4AF46503 [Rational(16016643, 2), 8008321.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10010101 #b11101000110010100000010))))
(check-sat)
(exit)
