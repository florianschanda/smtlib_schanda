(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11110101011101110011111)))
;; x should be Float32(0xCAFABB9F [Rational(-16432031, 2), -8216015.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x007AD4D6)))
;; y should be Float32(0x007AD4D6 [Rational(4024939, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01110100101001100101100)))
;; z should be Float32(0x803A532C [Rational(-955595, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8BF09BBE))))
(check-sat)
(exit)
