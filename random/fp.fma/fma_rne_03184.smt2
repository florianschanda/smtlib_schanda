(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01010110001100011010001)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b11101001100110011110100)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010011 #b11101001100110011110100))))
(check-sat)
(exit)
