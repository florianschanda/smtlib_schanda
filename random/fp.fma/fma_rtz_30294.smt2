(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x456AEC4A)))
;; x should be Float32(0x456AEC4A [Rational(7697957, 2048), 3758.768066])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01011101110101100011100)))
;; y should be Float32(0x802EEB1C [Rational(-768711, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11010100 #b10001011000111100000000)))
;; z should be Float32(0x6A458F00 [Rational(59708421217783076670668800), 59708421217783076670668800.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11010100 #b10001011000111011111111))))
(check-sat)
(exit)
