(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x92B5DDDA)))
;; x should be Float32(0x92B5DDDA [Rational(-5959405, 5192296858534827628530496329220096), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010111 #b01111100001000110000000)))
;; y should be Float32(0x4BBE1180 [Rational(24912640), 24912640.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01111100100000001011011)))
;; z should be Float32(0x4ABE405B [Rational(12468315, 2), 6234157.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10010101 #b01111100100000001011011)))
(check-sat)
(exit)
