(assert (= y (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AEB7437)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x4AEB7436)))
(check-sat)
(exit)
