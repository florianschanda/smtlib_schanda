(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0CC1E08D)))
;; x should be Float32(0x0CC1E08D [Rational(12705933, 42535295865117307932921825928971026432), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA57CAE2)))
;; y should be Float32(0xCA57CAE2 [Rational(-7071089, 2), -3535544.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11110011 #b00001011100011000001011)))
;; z should be Float32(0xF985C60B [Rational(-86824033849555345280776387065544704), -86824033849555345280776387065544704.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xF985C60B))))
(check-sat)
(exit)
