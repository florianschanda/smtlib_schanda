(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC09CB7)))
;; y should be Float32(0x4AC09CB7 [Rational(12623031, 2), 6311515.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01001110101011000000000)))
;; z should be Float32(0x80275600 [Rational(-5035, 1393796574908163946345982392040522594123776), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x80275600))))
(check-sat)
(exit)
