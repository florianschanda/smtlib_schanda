(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11111111111111111111111)))
;; x should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010001 #b10010101000010000010000)))
;; y should be Float32(0xC8CA8410 [Rational(-829505, 2), -414752.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01101010 #b11110011010010001101110)))
;; z should be Float32(0x3579A46E [Rational(8180279, 8796093022208), 0.000001])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b01101010 #b11110011010010001101101))))
(check-sat)
(exit)
