(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49DA3EBD)))
;; y should be Float32(0x49DA3EBD [Rational(14302909, 8), 1787863.625000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC95D31A8)))
;; z should be Float32(0xC95D31A8 [Rational(-1812021, 2), -906010.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010010 #b10111010011000110100111))))
(check-sat)
(exit)
