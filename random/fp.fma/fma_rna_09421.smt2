(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x389823D3)))
;; x should be Float32(0x389823D3 [Rational(9970643, 137438953472), 0.000073])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AD6D4A5)))
;; y should be Float32(0x4AD6D4A5 [Rational(14079141, 2), 7039570.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10000111 #b11111110101100010100100))))
(check-sat)
(exit)
