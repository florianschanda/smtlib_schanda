(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A11937A)))
;; x should be Float32(0x4A11937A [Rational(4770237, 2), 2385118.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x394929BA)))
;; y should be Float32(0x394929BA [Rational(6591709, 34359738368), 0.000192])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x43E4C90A)))
(check-sat)
(exit)
