(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCABBC595)))
;; x should be Float32(0xCABBC595 [Rational(-12305813, 2), -6152906.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01001000101011001001011)))
;; y should be Float32(0x0024564B [Rational(2381387, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01101001000001111010111)))
;; z should be Float32(0x003483D7 [Rational(3441623, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8AD5389F))))
(check-sat)
(exit)
