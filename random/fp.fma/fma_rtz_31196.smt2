(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9E26054)))
;; x should be Float32(0xC9E26054 [Rational(-3708949, 2), -1854474.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x65879A21)))
;; y should be Float32(0x65879A21 [Rational(80045331459419578302464), 80045331459419578302464.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11101110 #b10111111111000111110110)))
;; z should be Float32(0xF75FF1F6 [Rational(-4542147462092676260787187424952320), -4542147462092676260787187424952320.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xF75FF3D5)))
(check-sat)
(exit)
