(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01110111101101110000110)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b10110101011011111001010)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010100 #b10110101011011111001010))))
(check-sat)
(exit)
