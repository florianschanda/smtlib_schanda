(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x08778D03)))
;; x should be Float32(0x08778D03 [Rational(16223491, 21778071482940061661655974875633165533184), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b11001110001101111110011)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b11110001111100101001010)))
;; z should be Float32(0x4A78F94A [Rational(8158373, 2), 4079186.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
