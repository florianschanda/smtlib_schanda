(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x71E45BE5)))
;; x should be Float32(0x71E45BE5 [Rational(2261557629144837890332872933376), 2261557629144837890332872933376.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00010100100111001111011)))
;; y should be Float32(0xCA8A4E7B [Rational(-9064059, 2), -4532029.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A958E0F)))
;; z should be Float32(0x4A958E0F [Rational(9801231, 2), 4900615.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111001 #b11101101011111100010110))))
(check-sat)
(exit)
