(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00000000)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x17783263)))
;; y should be Float32(0x17783263 [Rational(16265827, 20282409603651670423947251286016), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01000101110100001110101)))
;; z should be Float32(0xCAA2E875 [Rational(-10676341, 2), -5338170.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCAA2E875))))
(check-sat)
(exit)
