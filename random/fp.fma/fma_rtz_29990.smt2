(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0DAC1507)))
;; x should be Float32(0x0DAC1507 [Rational(11277575, 10633823966279326983230456482242756608), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b11010111110101001010100)))
;; y should be Float32(0x49EBEA54 [Rational(3865237, 2), 1932618.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11100111 #b11010111000101111001100)))
;; z should be Float32(0x73EB8BCC [Rational(37323766456168975366774425911296), 37323766456168975366774425911296.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x73EB8BCC)))
(check-sat)
(exit)
