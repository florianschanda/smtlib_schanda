(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x62F98797)))
;; x should be Float32(0x62F98797 [Rational(2301504776135140966400), 2301504776135140966400.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7FE553D8)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x71F32107)))
;; z should be Float32(0x71F32107 [Rational(2407833119846384465612915081216), 2407833119846384465612915081216.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
