(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000010 #b01110011000010000100010)))
;; x should be Float32(0x21398422 [Rational(6078993, 9671406556917033397649408), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111001101011011001101)))
;; y should be Float32(0x001CD6CD [Rational(1889997, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x26C82BDC)))
;; z should be Float32(0x26C82BDC [Rational(3279607, 2361183241434822606848), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x26C82BDC)))
(check-sat)
(exit)
