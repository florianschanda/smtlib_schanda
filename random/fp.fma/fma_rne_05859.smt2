(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10001101 #b01011101011111011010000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00001111 #b01011101011111011001111)))
(check-sat)
(exit)
