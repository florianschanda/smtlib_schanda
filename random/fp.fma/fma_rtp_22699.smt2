(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101010 #b01101001010000001010111)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11110111100001111111000)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x004AE9F5)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b01101100 #b01011101010011010011101)))
(check-sat)
(exit)
