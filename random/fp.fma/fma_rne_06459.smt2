(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x801A1C6A)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11110110 #b01010011010100001111101)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01100000011101110000111)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b01110110 #b00010100110111110001011)))
(check-sat)
(exit)
