(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11011010 #b11011011001111010011110)))
;; x should be Float32(0xED6D9E9E [Rational(-4596231483816386631332528128), -4596231483816386631332528128.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000001 #b00000000000000000000000)))
;; y should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111011 #b00010011011000011000100)))
;; z should be Float32(0xFD89B0C4 [Rational(-22877756877739739997230753534200250368), -22877756877739739997230753534200250368.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b11111011 #b00010011011000011000011)))
(check-sat)
(exit)
