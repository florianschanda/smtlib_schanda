(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0924DE68)))
;; x should be Float32(0x0924DE68 [Rational(1350605, 680564733841876926926749214863536422912), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC92939E8)))
;; y should be Float32(0xC92939E8 [Rational(-1386301, 2), -693150.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01100010 #b00011000001010011100100)))
;; z should be Float32(0x310C14E4 [Rational(2295097, 1125899906842624), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x310C14E4)))
(check-sat)
(exit)
