(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9E14F80)))
;; x should be Float32(0xC9E14F80 [Rational(-1845744), -1845744.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9A6C884)))
;; y should be Float32(0xC9A6C884 [Rational(-2732577, 2), -1366288.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00100010 #b10001011010001110011110)))
;; z should be Float32(0x1145A39E [Rational(6476239, 41538374868278621028243970633760768), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x5412CA07)))
(check-sat)
(exit)
