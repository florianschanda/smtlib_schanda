(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x24CB85F4)))
;; x should be Float32(0x24CB85F4 [Rational(3334525, 37778931862957161709568), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD1C42898)))
;; y should be Float32(0xD1C42898 [Rational(-105311830016), -105311830016.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xDD869A39)))
;; z should be Float32(0xDD869A39 [Rational(-1212390927457320960), -1212390927457320960.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10111011 #b00001101001101000111001)))
(check-sat)
(exit)
