(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10011100 #b11000000100010111011110)))
;; x should be Float32(0xCE6045DE [Rational(-940668800), -940668800.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00100010 #b00111111111000100000100)))
;; y should be Float32(0x911FF104 [Rational(-2620481, 20769187434139310514121985316880384), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0A69E2AF)))
;; z should be Float32(0x0A69E2AF [Rational(15327919, 1361129467683753853853498429727072845824), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01000000 #b00011000001111010001011))))
(check-sat)
(exit)
