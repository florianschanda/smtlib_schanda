(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110100 #b11001001010101110100000)))
;; x should be Float32(0xDA64ABA0 [Rational(-16091249593286656), -16091249593286656.000000])

(declare-const y Float32)
(assert (= y (_ +zero 8 24)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00101011010000100101100)))
;; z should be Float32(0x0015A12C [Rational(354379, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x0015A12C)))
(check-sat)
(exit)
