(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49FD9CF4)))
;; y should be Float32(0x49FD9CF4 [Rational(4155197, 2), 2077598.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49C6589C)))
;; z should be Float32(0x49C6589C [Rational(3249703, 2), 1624851.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10010011 #b10001100101100010011100)))
(check-sat)
(exit)
