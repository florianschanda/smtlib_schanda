(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00111010 #b01111100000101010001100)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00010000 #b01100001100100110101010)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00000000 #b00000000000000000000010))))
(check-sat)
(exit)
