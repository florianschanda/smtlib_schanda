(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD7BF565D)))
;; x should be Float32(0xD7BF565D [Rational(-420755296747520), -420755296747520.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11010101001111111111011)))
;; y should be Float32(0xCAEA9FFB [Rational(-15376379, 2), -7688189.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x005DD8F6)))
;; z should be Float32(0x005DD8F6 [Rational(3075195, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b11000110 #b01011110101110010000100)))
(check-sat)
(exit)
