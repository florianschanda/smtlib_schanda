(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB02F1B)))
;; x should be Float32(0x4AB02F1B [Rational(11546395, 2), 5773197.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01000000 #b01111000111010111011111)))
;; y should be Float32(0x203C75DF [Rational(12350943, 77371252455336267181195264), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x2B81B3B7))))
(check-sat)
(exit)
