(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01001011 #b10000001011000100100100)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x804B8255)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b11001101101101001011000)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10010010 #b11001101101101001011000)))
(check-sat)
(exit)
