(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001001 #b01010010011011011111000)))
;; x should be Float32(0x04A936F8 [Rational(1386207, 348449143727040986586495598010130648530944), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8043B59D)))
;; y should be Float32(0x8043B59D [Rational(-4437405, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b10101010110001010011011)))
;; z should be Float32(0xCAD5629B [Rational(-13984411, 2), -6992205.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010101 #b10101010110001010011011))))
(check-sat)
(exit)
