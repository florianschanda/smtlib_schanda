(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFF835BD8)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b00111110 #b00101111010001101010101)))
;; y should be Float32(0x9F17A355 [Rational(-9937749, 309485009821345068724781056), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b01011110111111011111010)))
;; z should be Float32(0xCA2F7EFA [Rational(-5750653, 2), -2875326.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
