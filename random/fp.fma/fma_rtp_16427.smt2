(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111000 #b10001000111010001001111)))
;; x should be Float32(0x3C44744F [Rational(12874831, 1073741824), 0.011991])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC9A4E79C)))
;; z should be Float32(0xC9A4E79C [Rational(-2701799, 2), -1350899.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10010011 #b01001001110011110011100)))
(check-sat)
(exit)
