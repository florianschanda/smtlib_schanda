(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001011 #b10100010111100000010000)))
;; x should be Float32(0x85D17810 [Rational(-857985, 43556142965880123323311949751266331066368), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001001 #b11000110100001010101011)))
;; y should be Float32(0x44E342AB [Rational(14893739, 8192), 1818.083374])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x8B39F403)))
(check-sat)
(exit)
