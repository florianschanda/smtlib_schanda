(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110011 #b00011101101011101110000)))
;; x should be Float32(0xF98ED770 [Rational(-92709468191366528749786201837273088), -92709468191366528749786201837273088.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8033A249)))
;; y should be Float32(0x8033A249 [Rational(-3383881, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x61795A32)))
;; z should be Float32(0x61795A32 [Rational(287483658222870462464), 287483658222870462464.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11000010 #b11110010101101000110010)))
(check-sat)
(exit)
