(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110101 #b11001110100110101110011)))
;; x should be Float32(0xDAE74D73 [Rational(-32552937968369664), -32552937968369664.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x001617A1)))
;; y should be Float32(0x001617A1 [Rational(1447841, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x9A9FB00A)))
(check-sat)
(exit)
