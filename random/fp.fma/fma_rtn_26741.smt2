(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802F5445)))
;; x should be Float32(0x802F5445 [Rational(-3101765, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49FB7C5C)))
;; y should be Float32(0x49FB7C5C [Rational(4120343, 2), 2060171.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x342E8459)))
;; z should be Float32(0x342E8459 [Rational(11437145, 70368744177664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b01101000 #b01011101000010001011000))))
(check-sat)
(exit)
