(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11101111011100110010100)))
;; x should be Float32(0xC9F7B994 [Rational(-4058725, 2), -2029362.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010011 #b10001011010111110001011)))
;; y should be Float32(0x69C5AF8B [Rational(29873420587001297619648512), 29873420587001297619648512.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x063F45AD)))
;; z should be Float32(0x063F45AD [Rational(12535213, 348449143727040986586495598010130648530944), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b11101000 #b01111110100101110101101))))
(check-sat)
(exit)
