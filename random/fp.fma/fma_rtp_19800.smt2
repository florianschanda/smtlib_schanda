(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB20F60DF)))
;; x should be Float32(0xB20F60DF [Rational(-9396447, 1125899906842624), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010010 #b00111010001110011011101)))
;; y should be Float32(0xE91D1CDD [Rational(-11871103541966088769110016), -11871103541966088769110016.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b10100000111101001101100)))
;; z should be Float32(0x49D07A6C [Rational(3415707, 2), 1707853.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10110111 #b01011111111110100100111))))
(check-sat)
(exit)
