(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10101001101111011110011)))
;; x should be Float32(0xCAD4DEF3 [Rational(-13950707, 2), -6975353.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01110111010110111110111)))
;; y should be Float32(0xCABBADF7 [Rational(-12299767, 2), -6149883.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x50BFF186)))
;; z should be Float32(0x50BFF186 [Rational(25762213888), 25762213888.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x561C2782))))
(check-sat)
(exit)
