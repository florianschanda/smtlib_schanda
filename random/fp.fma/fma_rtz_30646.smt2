(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0012C127)))
;; x should be Float32(0x0012C127 [Rational(1229095, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB03E1A72)))
;; y should be Float32(0xB03E1A72 [Rational(-6229305, 9007199254740992), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80507EB0)))
;; z should be Float32(0x80507EB0 [Rational(-329707, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x80507EB0))))
(check-sat)
(exit)
