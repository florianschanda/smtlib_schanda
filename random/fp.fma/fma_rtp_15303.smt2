(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x007FFFFF)))
;; x should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x3AA243F1)))
;; y should be Float32(0x3AA243F1 [Rational(10634225, 8589934592), 0.001238])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7373A7A5)))
;; z should be Float32(0x7373A7A5 [Rational(19304326960366635075522044887040), 19304326960366635075522044887040.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b11100110 #b11100111010011110100110))))
(check-sat)
(exit)
