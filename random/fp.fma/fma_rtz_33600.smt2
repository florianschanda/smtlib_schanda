(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100111010001110110010)))
;; x should be Float32(0x8033A3B2 [Rational(-1692121, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A80A0BB)))
;; y should be Float32(0x4A80A0BB [Rational(8429755, 2), 4214877.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x5822F10E)))
;; z should be Float32(0x5822F10E [Rational(716624822796288), 716624822796288.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x5822F10D)))
(check-sat)
(exit)
