(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01010100 #b01001000100000100101110)))
;; x should be Float32(0xAA24412E [Rational(-5382295, 36893488147419103232), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x862422E7)))
;; y should be Float32(0x862422E7 [Rational(-10756839, 348449143727040986586495598010130648530944), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b00100101100101010101110)))
;; z should be Float32(0xCA12CAAE [Rational(-4810071, 2), -2405035.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xCA12CAAD)))
(check-sat)
(exit)
