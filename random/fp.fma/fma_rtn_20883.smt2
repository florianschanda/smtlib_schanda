(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCE0C8320)))
;; y should be Float32(0xCE0C8320 [Rational(-589350912), -589350912.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A5D83AA)))
;; z should be Float32(0x4A5D83AA [Rational(7258581, 2), 3629290.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10010100 #b10111011000001110101010)))
(check-sat)
(exit)
