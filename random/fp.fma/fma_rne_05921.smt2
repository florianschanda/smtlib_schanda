(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00111101 #b00010110011001111111010)))
;; x should be Float32(0x9E8B33FA [Rational(-4561405, 309485009821345068724781056), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7B91B5BE)))
;; y should be Float32(0x7B91B5BE [Rational(1513138427953377337415598387140493312), 1513138427953377337415598387140493312.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b00000000000000000000000))))
(check-sat)
(exit)
