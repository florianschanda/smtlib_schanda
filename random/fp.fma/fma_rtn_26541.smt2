(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010010 #b10001110100011001001011)))
;; x should be Float32(0x8947464B [Rational(-13059659, 5444517870735015415413993718908291383296), -0.000000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xB4061D62)))
;; z should be Float32(0xB4061D62 [Rational(-4394673, 35184372088832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b01101000 #b00001100001110101100010)))
(check-sat)
(exit)
