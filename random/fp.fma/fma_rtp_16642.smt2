(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01100101000101011101101)))
;; x should be Float32(0x00328AED [Rational(3312365, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC56AEB)))
;; y should be Float32(0x4AC56AEB [Rational(12937963, 2), 6468981.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11000110 #b00101100010001110001110)))
;; z should be Float32(0xE316238E [Rational(-2769573596294453133312), -2769573596294453133312.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11000110 #b00101100010001110001101)))
(check-sat)
(exit)
