(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0030100E)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10111010110110110001110)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00101001 #b00100101101110010110100)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b00101001 #b00100101101110010101001)))
(check-sat)
(exit)
