(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101011 #b11010011001100000000011)))
;; x should be Float32(0x95E99803 [Rational(-15308803, 162259276829213363391578010288128), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001111 #b11011100100110101000000)))
;; y should be Float32(0x47EE4D40 [Rational(244021, 2), 122010.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00111100 #b10110010111000111010000)))
(check-sat)
(exit)
