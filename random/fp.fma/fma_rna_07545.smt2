(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01011011111110001000110)))
;; x should be Float32(0x4A2DFC46 [Rational(5701155, 2), 2850577.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11001010011011000100110)))
;; y should be Float32(0x80653626 [Rational(-3316499, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49D54674)))
;; z should be Float32(0x49D54674 [Rational(3494301, 2), 1747150.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010011 #b10101010100011001110100))))
(check-sat)
(exit)
