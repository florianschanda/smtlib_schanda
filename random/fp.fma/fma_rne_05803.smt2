(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB0CFEA93)))
;; x should be Float32(0xB0CFEA93 [Rational(-13626003, 9007199254740992), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00111100001110110000011)))
;; y should be Float32(0x4A9E1D83 [Rational(10362243, 2), 5181121.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00101110 #b10000101010100010000011)))
;; z should be Float32(0x1742A883 [Rational(12757123, 20282409603651670423947251286016), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b01111000 #b00000000110101010111111)))
(check-sat)
(exit)
