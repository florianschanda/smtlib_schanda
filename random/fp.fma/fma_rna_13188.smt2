(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11011111000110110110100)))
;; x should be Float32(0x806F8DB4 [Rational(-1827693, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01001110001001000101011)))
;; y should be Float32(0xCAA7122B [Rational(-10949163, 2), -5474581.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00110000101110001111011)))
;; z should be Float32(0x4A985C7B [Rational(9985147, 2), 4992573.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010101 #b00110000101110001111011))))
(check-sat)
(exit)
