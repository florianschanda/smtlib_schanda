(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100000101011010011011)))
;; x should be Float32(0x8030569B [Rational(-3167899, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111100 #b10010111111100111001000)))
;; y should be Float32(0xFE4BF9C8 [Rational(-67782555386008456153265429068285739008), -67782555386008456153265429068285739008.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7FD92715)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
