(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x629AFA1D)))
;; x should be Float32(0x629AFA1D [Rational(1429410574317538770944), 1429410574317538770944.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800E62C8)))
;; y should be Float32(0x800E62C8 [Rational(-117849, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AEA9829)))
;; z should be Float32(0x4AEA9829 [Rational(15374377, 2), 7687188.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10010101 #b11010101001100000101000)))
(check-sat)
(exit)
