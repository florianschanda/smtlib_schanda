(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01010101101011111110011)))
;; x should be Float32(0x002AD7F3 [Rational(2807795, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10110001 #b11100100001111101010000)))
;; y should be Float32(0x58F21F50 [Rational(2129730400681984), 2129730400681984.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b11101101110111100000001)))
;; z should be Float32(0xCAF6EF01 [Rational(-16183041, 2), -8091520.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10010101 #b11101101110111100000001))))
(check-sat)
(exit)
