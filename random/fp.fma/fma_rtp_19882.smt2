(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8064434B)))
;; x should be Float32(0x8064434B [Rational(-6570827, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC11A75)))
;; y should be Float32(0x4AC11A75 [Rational(12655221, 2), 6327610.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00100111 #b11111001001111100011110)))
;; z should be Float32(0x13FC9F1E [Rational(8277903, 1298074214633706907132624082305024), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x13FC9E87))))
(check-sat)
(exit)
