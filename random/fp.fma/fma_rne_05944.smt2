(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10100001 #b00111111100110010101001)))
;; x should be Float32(0xD09FCCA9 [Rational(-21447919616), -21447919616.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x001C59AE)))
;; y should be Float32(0x001C59AE [Rational(928983, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x908D92EA)))
(check-sat)
(exit)
