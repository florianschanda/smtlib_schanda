(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00100110 #b11101101101001111001111)))
;; x should be Float32(0x9376D3CF [Rational(-16176079, 5192296858534827628530496329220096), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b10000011011000010010110)))
;; y should be Float32(0xCA41B096 [Rational(-6346827, 2), -3173413.500000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xFF800000)))
(check-sat)
(exit)
