(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7FB1274B)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00111110011011010111011)))
;; y should be Float32(0x801F36BB [Rational(-2045627, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10011111010111001110001)))
;; z should be Float32(0x004FAE71 [Rational(5222001, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
