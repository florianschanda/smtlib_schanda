(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x03BCF372)))
;; x should be Float32(0x03BCF372 [Rational(6191545, 5575186299632655785383929568162090376495104), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x04050572)))
;; y should be Float32(0x04050572 [Rational(4358841, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xFF800000)))
(check-sat)
(exit)
