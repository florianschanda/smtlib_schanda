(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00110011101100101110011)))
;; x should be Float32(0x4A99D973 [Rational(10082675, 2), 5041337.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFFAB87DF)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x489151D0)))
;; z should be Float32(0x489151D0 [Rational(595229, 2), 297614.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
