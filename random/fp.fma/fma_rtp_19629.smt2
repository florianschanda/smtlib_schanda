(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01100010 #b10011011001010110000001)))
;; x should be Float32(0xB14D9581 [Rational(-13473153, 4503599627370496), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10111100 #b00001111010101010000110)))
;; y should be Float32(0x5E07AA86 [Rational(2443943318929801216), 2443943318929801216.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4ABA6A7D)))
;; z should be Float32(0x4ABA6A7D [Rational(12216957, 2), 6108478.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10011111 #b10110011011011011110101))))
(check-sat)
(exit)
