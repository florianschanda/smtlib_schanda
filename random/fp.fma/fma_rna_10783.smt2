(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01001011111010010100100)))
;; x should be Float32(0xC9A5F4A4 [Rational(-2719017, 2), -1359508.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10101010 #b00011000110000110111010)))
;; y should be Float32(0xD50C61BA [Rational(-9646960017408), -9646960017408.000000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x5F360248))))
(check-sat)
(exit)
