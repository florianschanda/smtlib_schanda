(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4809E120)))
;; x should be Float32(0x4809E120 [Rational(282377, 2), 141188.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10101101001111000110010)))
;; y should be Float32(0x80569E32 [Rational(-2838297, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA15A142)))
;; z should be Float32(0xCA15A142 [Rational(-4903073, 2), -2451536.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA15A142))))
(check-sat)
(exit)
