(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01010000101010011000101)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8014AA06)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b10001001010101110011001)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010101 #b10001001010101110011001))))
(check-sat)
(exit)
