(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A580642)))
;; x should be Float32(0x4A580642 [Rational(7078689, 2), 3539344.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2CB21F04)))
;; y should be Float32(0x2CB21F04 [Rational(2918337, 576460752303423488), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01000000011110011101001)))
;; z should be Float32(0x80203CE9 [Rational(-2112745, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01101111 #b00101100100111010000111))))
(check-sat)
(exit)
