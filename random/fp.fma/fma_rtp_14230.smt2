(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11001101011001010010101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b01110101 #b10001100010101011010000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00011101110111110110110)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x463294B4))))
(check-sat)
(exit)
