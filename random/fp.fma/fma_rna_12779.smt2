(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9BE8E539)))
;; x should be Float32(0x9BE8E539 [Rational(-15263033, 39614081257132168796771975168), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2F972CAD)))
;; y should be Float32(0x2F972CAD [Rational(9907373, 36028797018963968), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC9999FAC)))
;; z should be Float32(0xC9999FAC [Rational(-2516971, 2), -1258485.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xC9999FAC)))
(check-sat)
(exit)
