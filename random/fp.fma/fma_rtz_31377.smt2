(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10001010101101100001101)))
;; x should be Float32(0xCAC55B0D [Rational(-12933901, 2), -6466950.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x804A86B4)))
;; y should be Float32(0x804A86B4 [Rational(-1221037, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10110001 #b00000101000011001111011)))
;; z should be Float32(0x5882867B [Rational(1148112806608896), 1148112806608896.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10110001 #b00000101000011001111011)))
(check-sat)
(exit)
