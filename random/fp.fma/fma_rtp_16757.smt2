(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0024E152)))
;; x should be Float32(0x0024E152 [Rational(1208489, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001110 #b01100100110101011110110)))
;; y should be Float32(0x47326AF6 [Rational(5846395, 128), 45674.960938])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11010110011001100110001)))
;; z should be Float32(0x806B3331 [Rational(-7025457, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b00001110 #b10011011001110101000101))))
(check-sat)
(exit)
