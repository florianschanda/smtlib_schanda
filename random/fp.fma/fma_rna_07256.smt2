(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01111001101010000001011)))
;; x should be Float32(0x4ABCD40B [Rational(12375051, 2), 6187525.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10111110001001100101110)))
;; y should be Float32(0x4A5F132E [Rational(7309719, 2), 3654859.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xB4884EBD)))
;; z should be Float32(0xB4884EBD [Rational(-8933053, 35184372088832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x55A48ADB))))
(check-sat)
(exit)
