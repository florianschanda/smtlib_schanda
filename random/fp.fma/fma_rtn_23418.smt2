(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001001 #b00011110110011011101011)))
;; x should be Float32(0x648F66EB [Rational(21162403390014892277760), 21162403390014892277760.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x989D3872)))
;; y should be Float32(0x989D3872 [Rational(-5151801, 1267650600228229401496703205376), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b01111011 #b01100000010001101111001))))
(check-sat)
(exit)
