(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6DEE6E2F)))
(declare-const y Float32)
(assert (= y (fp #b0 #b01110011 #b01010100000100011101010)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b11101110001101011110101)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b11010000 #b00111100101110101111001))))
(check-sat)
(exit)
