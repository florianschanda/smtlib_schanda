(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01010011 #b11010001010000100110001)))
;; x should be Float32(0xA9E8A131 [Rational(-15245617, 147573952589676412928), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00101110101100100101100)))
;; y should be Float32(0x0017592C [Rational(382539, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b00000111111000110001111)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
