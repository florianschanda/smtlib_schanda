(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00111001110110001111110)))
;; x should be Float32(0x4A1CEC7E [Rational(5142079, 2), 2571039.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0032965A)))
;; y should be Float32(0x0032965A [Rational(1657645, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00011010 #b00000100110010100100111)))
;; z should be Float32(0x8D026527 [Rational(-8545575, 21267647932558653966460912964485513216), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00011001 #b11111010000100110110111))))
(check-sat)
(exit)
