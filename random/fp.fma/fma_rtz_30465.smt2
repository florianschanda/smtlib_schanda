(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10011011100001101010011)))
;; x should be Float32(0x004DC353 [Rational(5096275, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010111 #b11110000100010010010100)))
;; y should be Float32(0x4BF84494 [Rational(32540968), 32540968.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x42D17C2B)))
;; z should be Float32(0x42D17C2B [Rational(13728811, 131072), 104.742516])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10000101 #b10100010111110000101011))))
(check-sat)
(exit)
