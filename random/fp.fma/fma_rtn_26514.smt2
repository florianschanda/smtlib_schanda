(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010010 #b00101000001110011111110)))
;; x should be Float32(0xE9141CFE [Rational(-11191120814842511884288000), -11191120814842511884288000.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00746088)))
;; y should be Float32(0x00746088 [Rational(953361, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10111001110110010110010)))
;; z should be Float32(0x005CECB2 [Rational(3044953, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b01010100 #b00001101010100111111010)))
(check-sat)
(exit)
