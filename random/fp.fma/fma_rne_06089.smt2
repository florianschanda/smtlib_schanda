(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101011 #b11101111101001010010010)))
;; x should be Float32(0xF5F7D292 [Rational(-628304783880174002748706139406336), -628304783880174002748706139406336.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000001 #b01100000000111100111001)))
;; y should be Float32(0xC0B00F39 [Rational(-11538233, 2097152), -5.501858])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10010011000010000101001)))
;; z should be Float32(0x00498429 [Rational(4817961, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x772A6F81))))
(check-sat)
(exit)
