(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10011011000011011101101)))
;; x should be Float32(0x004D86ED [Rational(5080813, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11011010100110011110111)))
;; y should be Float32(0x806D4CF7 [Rational(-7163127, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11001011 #b11001101001101011011111)))
;; z should be Float32(0x65E69ADF [Rational(136125144811355016003584), 136125144811355016003584.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b11001011 #b11001101001101011011110)))
(check-sat)
(exit)
