(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011011 #b00000010110110101101000)))
;; x should be Float32(0x2D816D68 [Rational(1060269, 72057594037927936), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01000110000011000100100)))
;; y should be Float32(0x00230624 [Rational(573833, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8031B825)))
;; z should be Float32(0x8031B825 [Rational(-3258405, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00000000 #b01100011011100000100101)))
(check-sat)
(exit)
