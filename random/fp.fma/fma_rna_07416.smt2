(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A8122C5)))
;; x should be Float32(0x4A8122C5 [Rational(8463045, 2), 4231522.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9DDB1AC)))
;; y should be Float32(0xC9DDB1AC [Rational(-3632235, 2), -1816117.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10101001 #b10111111010100101001000)))
(check-sat)
(exit)
