(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00111001111101111111111)))
;; x should be Float32(0xCA9CFBFF [Rational(-10288127, 2), -5144063.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01101001000111110001110)))
;; y should be Float32(0xCA348F8E [Rational(-5916615, 2), -2958307.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x6CE64520)))
;; z should be Float32(0x6CE64520 [Rational(2227034976755944595262537728), 2227034976755944595262537728.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b11011001 #b11001100100010100100000)))
(check-sat)
(exit)
