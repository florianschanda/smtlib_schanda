(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A999A5B)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b10101100101001110011110)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10001010 #b00100100101011100010100)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10101011 #b00000001001100100110100)))
(check-sat)
(exit)
