(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2E059AC1)))
;; x should be Float32(0x2E059AC1 [Rational(8755905, 288230376151711744), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x807F0614)))
;; y should be Float32(0x807F0614 [Rational(-2081157, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00000000 #b00000000000000000000001))))
(check-sat)
(exit)
