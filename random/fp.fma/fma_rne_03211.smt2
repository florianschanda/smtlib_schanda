(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0032EBCE)))
;; x should be Float32(0x0032EBCE [Rational(1668583, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010011 #b10100110110101001110010)))
;; y should be Float32(0xA9D36A72 [Rational(-6927673, 73786976294838206464), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7FCCE197)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
