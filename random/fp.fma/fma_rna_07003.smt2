(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b01101100 #b10010110010111010010100)))
;; y should be Float32(0x364B2E94 [Rational(3328933, 1099511627776), 0.000003])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00001101001111111111001)))
;; z should be Float32(0xCA869FF9 [Rational(-8822777, 2), -4411388.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10010101 #b00001101001111111111001))))
(check-sat)
(exit)
