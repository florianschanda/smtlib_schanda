(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC753DB)))
;; x should be Float32(0xCAC753DB [Rational(-13063131, 2), -6531565.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00001110 #b00110110011010010101010)))
;; y should be Float32(0x071B34AA [Rational(5085781, 43556142965880123323311949751266331066368), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00111010110010001000011)))
;; z should be Float32(0xCA9D6443 [Rational(-10314819, 2), -5157409.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xCA9D6443)))
(check-sat)
(exit)
