(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AFF9A53)))
;; x should be Float32(0x4AFF9A53 [Rational(16751187, 2), 8375593.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x498E910C)))
;; y should be Float32(0x498E910C [Rational(2335811, 2), 1167905.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A873DB1)))
;; z should be Float32(0x4A873DB1 [Rational(8863153, 2), 4431576.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x550E5871)))
(check-sat)
(exit)
