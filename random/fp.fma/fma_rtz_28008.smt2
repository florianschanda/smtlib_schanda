(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49CDAF3C)))
;; x should be Float32(0x49CDAF3C [Rational(3369935, 2), 1684967.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF800000)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01100010010001001001001)))
;; z should be Float32(0x4AB12249 [Rational(11608649, 2), 5804324.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (_ -oo 8 24)))
(check-sat)
(exit)
