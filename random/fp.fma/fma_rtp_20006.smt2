(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80660A80)))
;; x should be Float32(0x80660A80 [Rational(-52245, 5575186299632655785383929568162090376495104), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01100010 #b11010101010010010110011)))
;; y should be Float32(0x316AA4B3 [Rational(15377587, 4503599627370496), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807F205B)))
;; z should be Float32(0x807F205B [Rational(-8331355, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x807F205B)))
(check-sat)
(exit)
