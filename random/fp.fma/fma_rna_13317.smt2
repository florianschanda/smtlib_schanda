(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x800ED404)))
;; x should be Float32(0x800ED404 [Rational(-242945, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCED78F04)))
;; y should be Float32(0xCED78F04 [Rational(-1808237056), -1808237056.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC61DC50E)))
;; z should be Float32(0xC61DC50E [Rational(-5169799, 512), -10097.263672])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10001100 #b00111011100010100001110)))
(check-sat)
(exit)
