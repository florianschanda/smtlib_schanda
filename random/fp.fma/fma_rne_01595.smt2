(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11111111111111111111111)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10011000 #b11101100011101100000000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11000110101110010001000)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b00011010 #b11101100011101011111110)))
(check-sat)
(exit)
