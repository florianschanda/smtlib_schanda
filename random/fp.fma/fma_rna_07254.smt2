(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49A3D47C)))
;; x should be Float32(0x49A3D47C [Rational(2684191, 2), 1342095.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49177C58)))
;; y should be Float32(0x49177C58 [Rational(1240971, 2), 620485.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80800000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10100110 #b10000011110001111010001)))
(check-sat)
(exit)
