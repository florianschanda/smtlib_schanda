(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80160E75)))
;; x should be Float32(0x80160E75 [Rational(-1445493, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00111101101111101110001)))
;; y should be Float32(0x801EDF71 [Rational(-2023281, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00101010 #b11110000101111001001001)))
;; z should be Float32(0x15785E49 [Rational(16277065, 324518553658426726783156020576256), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x15785E49)))
(check-sat)
(exit)
