(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111111 #b01111111101000100110001)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b10011101 #b00100001110010100001101)))
;; y should be Float32(0xCE90E50D [Rational(-1215465088), -1215465088.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01111111 #b01001011011100101011101)))
;; z should be Float32(0xBFA5B95D [Rational(-10860893, 8388608), -1.294719])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
