(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10011101000011111011001)))
;; x should be Float32(0xCACE87D9 [Rational(-13535193, 2), -6767596.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x801E1DB1)))
;; y should be Float32(0x801E1DB1 [Rational(-1973681, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111101100111000101100)))
;; z should be Float32(0x007ECE2C [Rational(2077579, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0AC25F05))))
(check-sat)
(exit)
