(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10001100011000100101001)))
;; x should be Float32(0x00463129 [Rational(4600105, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x5704F261)))
;; y should be Float32(0x5704F261 [Rational(146176544342016), 146176544342016.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01011011 #b11000010100110010111011)))
;; z should be Float32(0xADE14CBB [Rational(-14765243, 576460752303423488), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xADE14CBB)))
(check-sat)
(exit)
