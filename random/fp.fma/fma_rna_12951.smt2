(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101100 #b11101011010101100010110)))
;; x should be Float32(0x9675AB16 [Rational(-8050059, 40564819207303340847894502572032), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA317D40E)))
;; y should be Float32(0xA317D40E [Rational(-4975111, 604462909807314587353088), -0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFF800000))))
(check-sat)
(exit)
