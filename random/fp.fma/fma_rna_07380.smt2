(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00001010111100000110010)))
;; x should be Float32(0x4A057832 [Rational(4373529, 2), 2186764.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101000110011100010011)))
;; y should be Float32(0x00546713 [Rational(5531411, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b01101101100101111011010)))
;; z should be Float32(0x00B6CBDA [Rational(5989869, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0AB004C2))))
(check-sat)
(exit)
