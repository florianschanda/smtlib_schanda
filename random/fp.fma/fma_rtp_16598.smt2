(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000100 #b10000000100101001100000)))
;; x should be Float32(0x02404A60 [Rational(393811, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b00001000011001110001011)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAA4E95B)))
;; z should be Float32(0xCAA4E95B [Rational(-10807643, 2), -5403821.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
