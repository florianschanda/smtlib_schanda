(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b10001110111101110111110)))
;; y should be Float32(0xCA477BBE [Rational(-6536671, 2), -3268335.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00110101101111010101101)))
;; z should be Float32(0x4A9ADEAD [Rational(10149549, 2), 5074774.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010101 #b00110101101111010101101))))
(check-sat)
(exit)
