(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEED41B1E)))
;; x should be Float32(0xEED41B1E [Rational(-32821802375124617823363530752), -32821802375124617823363530752.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000001 #b00000000000000000000000)))
;; y should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11011011 #b00001111000000101001100)))
;; z should be Float32(0xED87814C [Rational(-5242098332058071191802347520), -5242098332058071191802347520.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11011011 #b00001111000000101001011)))
(check-sat)
(exit)
