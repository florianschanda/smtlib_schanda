(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0019FBFE)))
;; x should be Float32(0x0019FBFE [Rational(851455, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11100111110110110111001)))
;; y should be Float32(0x8073EDB9 [Rational(-7597497, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01110111101010110000101)))
;; z should be Float32(0x4ABBD585 [Rational(12309893, 2), 6154946.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4ABBD584))))
(check-sat)
(exit)
