(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE8E75996)))
;; x should be Float32(0xE8E75996 [Rational(-8740153811236431069184000), -8740153811236431069184000.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7FF2B1B4)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x800BFC8B)))
;; z should be Float32(0x800BFC8B [Rational(-785547, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
