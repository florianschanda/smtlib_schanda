(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10001110011101100100001)))
;; x should be Float32(0xCAC73B21 [Rational(-13056801, 2), -6528400.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEA87EFB0)))
;; y should be Float32(0xEA87EFB0 [Rational(-82168438932168878336245760), -82168438932168878336245760.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
