(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00011011100100111100010)))
;; x should be Float32(0xCA0DC9E2 [Rational(-4646129, 2), -2323064.500000])

(declare-const y Float32)
(assert (= y (_ NaN 8 24)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b1 #b00111101 #b00110010100110011100011)))
;; z should be Float32(0x9E994CE3 [Rational(-10046691, 618970019642690137449562112), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
