(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x222999EE)))
;; x should be Float32(0x222999EE [Rational(5557495, 2417851639229258349412352), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC91AFC28)))
;; y should be Float32(0xC91AFC28 [Rational(-1269637, 2), -634818.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11100101001000010010101)))
;; z should be Float32(0x80729095 [Rational(-7508117, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xABCD5B4E)))
(check-sat)
(exit)
