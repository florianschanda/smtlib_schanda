(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10001001010110100000001)))
;; x should be Float32(0x4AC4AD01 [Rational(12889345, 2), 6444672.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AFAA59D)))
;; y should be Float32(0x4AFAA59D [Rational(16426397, 2), 8213198.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11110001 #b01110101111101011111111)))
;; z should be Float32(0x78BAFAFF [Rational(30339313164682249809960796367618048), 30339313164682249809960796367618048.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x78BAFAFF))))
(check-sat)
(exit)
