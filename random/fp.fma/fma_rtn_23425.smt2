(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010011 #b00000011101100100100101)))
;; x should be Float32(0x0981D925 [Rational(8509733, 2722258935367507707706996859454145691648), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01100000 #b00110100010010011111101)))
;; y should be Float32(0xB01A24FD [Rational(-10102013, 18014398509481984), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xA6034BDD)))
;; z should be Float32(0xA6034BDD [Rational(-8604637, 18889465931478580854784), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b01001100 #b00000110100101111011110)))
(check-sat)
(exit)
