(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11011110 #b00101011101111010011011)))
;; y should be Float32(0x6F15DE9B [Rational(46382379962139707711785271296), 46382379962139707711785271296.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00001001 #b11100001111110111011110)))
;; z should be Float32(0x04F0FDDE [Rational(7896815, 1393796574908163946345982392040522594123776), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b01100000 #b00101011101111010011010))))
(check-sat)
(exit)
