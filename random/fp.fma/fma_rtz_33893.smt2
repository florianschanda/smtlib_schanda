(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00110010110000001010000)))
;; x should be Float32(0x80196050 [Rational(-103941, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11001000 #b11110010110011111101000)))
;; y should be Float32(0xE47967E8 [Rational(-18402906034936727207936), -18402906034936727207936.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b01001000 #b10001011100011111010100)))
(check-sat)
(exit)
