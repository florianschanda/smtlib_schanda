(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7FD898EA)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49F5792C)))
;; y should be Float32(0x49F5792C [Rational(4021835, 2), 2010917.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x285874A8)))
;; z should be Float32(0x285874A8 [Rational(1773205, 147573952589676412928), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
