(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01101011 #b11100110011101101010111)))
;; x should be Float32(0x35F33B57 [Rational(15940439, 8796093022208), 0.000002])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00011011011000111001101)))
;; y should be Float32(0x800DB1CD [Rational(-897485, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xFFD18AE7)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
