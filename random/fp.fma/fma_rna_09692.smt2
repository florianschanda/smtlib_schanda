(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011010 #b11000110001010111111001)))
;; x should be Float32(0x2D6315F9 [Rational(14882297, 1152921504606846976), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9D67F85A)))
;; y should be Float32(0x9D67F85A [Rational(-7601197, 2475880078570760549798248448), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b00000110110111011001010)))
;; z should be Float32(0x4A036ECA [Rational(4306789, 2), 2153394.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010100 #b00000110110111011001010))))
(check-sat)
(exit)
