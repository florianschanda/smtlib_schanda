(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x528B0F9C)))
;; x should be Float32(0x528B0F9C [Rational(298631168000), 298631168000.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD43A2BEB)))
;; y should be Float32(0xD43A2BEB [Rational(-3198402953216), -3198402953216.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10101110 #b10111101000100111101100)))
;; z should be Float32(0xD75E89EC [Rational(-244683951308800), -244683951308800.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11001110 #b10010100100001001100101)))
(check-sat)
(exit)
