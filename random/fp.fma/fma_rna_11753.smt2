(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b00100101101110100011100)))
;; y should be Float32(0xC992DD1C [Rational(-2406215, 2), -1203107.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b00101101011000110111000)))
;; z should be Float32(0xC916B1B8 [Rational(-1234487, 2), -617243.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC916B1B8))))
(check-sat)
(exit)
