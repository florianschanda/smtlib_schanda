(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00011011011001100101001)))
;; x should be Float32(0x000DB329 [Rational(897833, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4ABD2879)))
;; y should be Float32(0x4ABD2879 [Rational(12396665, 2), 6198332.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7D9C4798)))
;; z should be Float32(0x7D9C4798 [Rational(25966412918207825498565500828162981888), 25966412918207825498565500828162981888.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11111011 #b00111000100011110011000)))
(check-sat)
(exit)
