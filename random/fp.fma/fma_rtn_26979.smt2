(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01110101100010001001010)))
;; x should be Float32(0x803AC44A [Rational(-1925669, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111111111111111111111)))
;; y should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00101001001110000011000)))
;; z should be Float32(0x80149C18 [Rational(-168835, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00000000 #b00101001001110000011000)))
(check-sat)
(exit)
