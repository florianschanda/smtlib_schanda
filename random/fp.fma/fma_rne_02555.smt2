(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10110001 #b10001001111011011001100)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB5EA69)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00110000100011100001100)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11001000 #b00010111111011011011001)))
(check-sat)
(exit)
