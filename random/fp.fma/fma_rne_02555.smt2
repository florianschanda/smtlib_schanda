(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000011 #b10001100010101111101110)))
;; x should be Float32(0x41C62BEE [Rational(6493687, 262144), 24.771450])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01110101010010100010010)))
;; y should be Float32(0x4A3AA512 [Rational(6115977, 2), 3057988.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111001010100101010)))
;; z should be Float32(0x007F952A [Rational(4180629, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10011001 #b00100000111101110110011))))
(check-sat)
(exit)
