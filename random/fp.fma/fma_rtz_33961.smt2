(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ NaN 8 24)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b10001111 #b10110100010110111000000)))
;; y should be Float32(0x47DA2DC0 [Rational(223415, 2), 111707.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x769B13C8)))
;; z should be Float32(0x769B13C8 [Rational(1572670360327872103569923120300032), 1572670360327872103569923120300032.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
