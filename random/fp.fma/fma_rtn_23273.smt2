(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001101 #b01100100010111001001010)))
;; x should be Float32(0x46B22E4A [Rational(5838629, 256), 22807.144531])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x005C0096)))
;; y should be Float32(0x005C0096 [Rational(3014731, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x92068D88)))
;; z should be Float32(0x92068D88 [Rational(-1102257, 2596148429267413814265248164610048), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00100100 #b00001101000110110000100))))
(check-sat)
(exit)
