(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1C08C80D)))
;; x should be Float32(0x1C08C80D [Rational(8964109, 19807040628566084398385987584), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x803F1888)))
;; y should be Float32(0x803F1888 [Rational(-516881, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b11001101101000110011100)))
;; z should be Float32(0x49E6D19C [Rational(3781735, 2), 1890867.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x49E6D19C))))
(check-sat)
(exit)
