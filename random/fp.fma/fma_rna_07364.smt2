(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AC37F75)))
;; x should be Float32(0x4AC37F75 [Rational(12812149, 2), 6406074.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4C142C6F)))
;; y should be Float32(0x4C142C6F [Rational(38842812), 38842812.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC998DB2C)))
;; z should be Float32(0xC998DB2C [Rational(-2504395, 2), -1252197.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10101110 #b11000100100111100111101)))
(check-sat)
(exit)
