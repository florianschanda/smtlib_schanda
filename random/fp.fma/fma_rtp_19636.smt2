(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10100000 #b01111111100101100000001)))
;; x should be Float32(0xD03FCB01 [Rational(-12871009280), -12871009280.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00111010 #b00010001101010000111011)))
;; y should be Float32(0x1D08D43B [Rational(8967227, 4951760157141521099596496896), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111011111011010101110)))
;; z should be Float32(0x007DF6AE [Rational(4127575, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b01011011 #b10011010000010110110001)))
(check-sat)
(exit)
