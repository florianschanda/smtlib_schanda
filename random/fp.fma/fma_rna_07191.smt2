(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80328ED8)))
;; y should be Float32(0x80328ED8 [Rational(-414171, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11100011101100101000100)))
;; z should be Float32(0x0071D944 [Rational(1865297, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b00000000 #b11100011101100101000100))))
(check-sat)
(exit)
