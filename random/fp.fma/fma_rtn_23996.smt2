(assert (= y ((_ to_fp 8 24) #x00038447)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x804F3A7A)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00000000 #b10011110011101001111010)))
(check-sat)
(exit)
