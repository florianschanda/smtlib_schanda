(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x67A5A2D8)))
;; y should be Float32(0x67A5A2D8 [Rational(1564388813307489200111616), 1564388813307489200111616.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00182E74)))
;; z should be Float32(0x00182E74 [Rational(396189, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xA8A5A2D6)))
(check-sat)
(exit)
