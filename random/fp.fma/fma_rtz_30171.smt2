(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0DA5EB36)))
;; x should be Float32(0x0DA5EB36 [Rational(5436827, 5316911983139663491615228241121378304), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11011111011101111101101)))
;; y should be Float32(0xCAEFBBED [Rational(-15711213, 2), -7855606.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00111101011000101111100)))
;; z should be Float32(0x801EB17C [Rational(-502879, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x991B6063)))
(check-sat)
(exit)
