(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b00101011011010011111000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b01101001 #b10011100001100110010110)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00607CC4)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b01111100 #b11100010000110100101100)))
(check-sat)
(exit)
