(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00110110111100100011001)))
;; x should be Float32(0x4A9B7919 [Rational(10189081, 2), 5094540.500000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b10001110 #b11001011110010010000000)))
;; z should be Float32(0x4765E480 [Rational(117705, 2), 58852.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x4765E480)))
(check-sat)
(exit)
