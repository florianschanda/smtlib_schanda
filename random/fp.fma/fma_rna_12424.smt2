(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80000001)))
;; x should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x56A301C2)))
;; y should be Float32(0x56A301C2 [Rational(89613972537344), 89613972537344.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x89C4053F)))
;; z should be Float32(0x89C4053F [Rational(-12846399, 2722258935367507707706996859454145691648), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8C2921EC))))
(check-sat)
(exit)
