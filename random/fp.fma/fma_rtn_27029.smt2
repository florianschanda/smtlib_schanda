(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01110111101000110111111)))
;; x should be Float32(0x803BD1BF [Rational(-3920319, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01111101 #b11000001101111000100111)))
;; y should be Float32(0xBEE0DE27 [Rational(-14736935, 33554432), -0.439195])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b11101001111011101110011)))
;; z should be Float32(0xCAF4F773 [Rational(-16054131, 2), -8027065.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xCAF4F773)))
(check-sat)
(exit)
