(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01010101011111101100101)))
;; x should be Float32(0x802ABF65 [Rational(-2801509, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10011011010111110111101)))
;; y should be Float32(0x804DAFBD [Rational(-5091261, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00001101 #b01011001001000101110011)))
;; z should be Float32(0x86AC9173 [Rational(-11309427, 174224571863520493293247799005065324265472), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b00001101 #b01011001001000101110010)))
(check-sat)
(exit)
