(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807A0DA5)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01110010000010100110001)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00111001 #b11111010111001011000011)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b00111001 #b11111010111001011000010))))
(check-sat)
(exit)
