(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4ACF76E3)))
;; y should be Float32(0x4ACF76E3 [Rational(13596387, 2), 6798193.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01101100011000110100101)))
;; z should be Float32(0xCAB631A5 [Rational(-11940261, 2), -5970130.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xCAB631A5)))
(check-sat)
(exit)
