(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010000 #b01100001001011000001000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b1 #b01001110 #b11010010100100001100110)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b01001110 #b11010010100100001100110))))
(check-sat)
(exit)
