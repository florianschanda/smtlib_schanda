(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF5B48C5D)))
;; x should be Float32(0xF5B48C5D [Rational(-457744306689401383583124098646016), -457744306689401383583124098646016.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x000C1805)))
;; y should be Float32(0x000C1805 [Rational(792581, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b01101010 #b00010000111100001010001))))
(check-sat)
(exit)
