(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010100 #b11111101100100000101010)))
;; x should be Float32(0x8A7EC82A [Rational(-8348693, 680564733841876926926749214863536422912), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11001101001111110000010)))
;; y should be Float32(0xCA669F82 [Rational(-7557057, 2), -3778528.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC99F655C)))
;; z should be Float32(0xC99F655C [Rational(-2611543, 2), -1305771.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10010011 #b00111110110010101011100))))
(check-sat)
(exit)
