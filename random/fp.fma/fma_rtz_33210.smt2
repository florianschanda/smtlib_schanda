(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b00011000100000110000100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10100011 #b01110010111110001001101)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
