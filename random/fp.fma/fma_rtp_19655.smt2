(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDD07E0B1)))
;; x should be Float32(0xDD07E0B1 [Rational(-611938762716348416), -611938762716348416.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x005F8C93)))
;; y should be Float32(0x005F8C93 [Rational(6261907, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00101011101010101110010)))
;; z should be Float32(0x0015D572 [Rational(715449, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x9DCADBFA))))
(check-sat)
(exit)
