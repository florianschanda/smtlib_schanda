(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01011010011111011010011)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00010100 #b01100100000011011100000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00110111 #b01111101001100101101110)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x9BBE996D)))
(check-sat)
(exit)
