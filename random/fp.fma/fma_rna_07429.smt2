(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11100101001110000010011)))
;; x should be Float32(0x4AF29C13 [Rational(15899667, 2), 7949833.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAD36C4B)))
;; y should be Float32(0xCAD36C4B [Rational(-13855819, 2), -6927909.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b01011100011000111101100)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
