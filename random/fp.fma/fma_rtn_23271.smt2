(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11000001 #b01001010110100100010101)))
;; x should be Float32(0x60A56915 [Rational(95352647828455292928), 95352647828455292928.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x005ECB9B)))
;; y should be Float32(0x005ECB9B [Rational(6212507, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b01000010 #b11101010000000010010000)))
(check-sat)
(exit)
