(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x494A1EA8)))
;; x should be Float32(0x494A1EA8 [Rational(1655765, 2), 827882.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4B4C0AA8)))
;; y should be Float32(0x4B4C0AA8 [Rational(13372072), 13372072.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00101000 #b11001101110000010110101)))
;; z should be Float32(0x9466E0B5 [Rational(-15130805, 1298074214633706907132624082305024), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x552118D8))))
(check-sat)
(exit)
