(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AE78D73)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b11101010000111011101100)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x007C71FE)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x555DA7DE)))
(check-sat)
(exit)
