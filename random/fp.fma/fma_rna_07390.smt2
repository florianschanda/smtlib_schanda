(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01110100011111100110110)))
;; x should be Float32(0x4A3A3F36 [Rational(6102939, 2), 3051469.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01101111111111010111011)))
;; y should be Float32(0x0037FEBB [Rational(3669691, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10101011000111110000000)))
;; z should be Float32(0x80558F80 [Rational(-43807, 5575186299632655785383929568162090376495104), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0AA2F398))))
(check-sat)
(exit)
