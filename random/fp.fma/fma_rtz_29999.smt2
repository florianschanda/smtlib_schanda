(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x29A85EF9)))
;; x should be Float32(0x29A85EF9 [Rational(11034361, 147573952589676412928), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A852309)))
;; y should be Float32(0x4A852309 [Rational(8725257, 2), 4362628.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xDDA29DDD)))
;; z should be Float32(0xDDA29DDD [Rational(-1464720599694704640), -1464720599694704640.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10111011 #b01000101001110111011100))))
(check-sat)
(exit)
