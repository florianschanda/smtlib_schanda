(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00110001110010100101000)))
;; x should be Float32(0x8018E528 [Rational(-203941, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111011010111010000111)))
;; y should be Float32(0x007DAE87 [Rational(8236679, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11000001 #b10100010100100100101110)))
;; z should be Float32(0x60D1492E [Rational(120645083238093553664), 120645083238093553664.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11000001 #b10100010100100100101101))))
(check-sat)
(exit)
