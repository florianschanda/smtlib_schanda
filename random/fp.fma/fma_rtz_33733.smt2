(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11001011111010110100101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00100001111011100000101)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00011000 #b01001000000000001100100)))
