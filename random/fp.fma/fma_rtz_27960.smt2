(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010000 #b00100101110000111100000)))
;; x should be Float32(0x4812E1E0 [Rational(300815, 2), 150407.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00100011011011000101111)))
;; y should be Float32(0x0011B62F [Rational(1160751, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b10001111111001011111110)))
;; z should be Float32(0xCA47F2FE [Rational(-6551935, 2), -3275967.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010100 #b10001111111001011111101))))
(check-sat)
(exit)
