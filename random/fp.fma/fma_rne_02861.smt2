(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000001 #b10101100010101101001111)))
;; x should be Float32(0x20D62B4F [Rational(14035791, 38685626227668133590597632), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8019D12A)))
;; y should be Float32(0x8019D12A [Rational(-845973, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAC2EC3D)))
;; z should be Float32(0xCAC2EC3D [Rational(-12774461, 2), -6387230.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xCAC2EC3D)))
(check-sat)
(exit)
