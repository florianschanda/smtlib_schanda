(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFC26D8EF)))
;; x should be Float32(0xFC26D8EF [Rational(-3465284858088044225297279415729782784), -3465284858088044225297279415729782784.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00011000100101100101011)))
;; y should be Float32(0xCA8C4B2B [Rational(-9194283, 2), -4597141.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8075D340)))
;; z should be Float32(0x8075D340 [Rational(-120653, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
