(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b01111011001000110111000)))
;; x should be Float32(0xC93D91B8 [Rational(-1552951, 2), -776475.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b10001011100011010111000)))
;; y should be Float32(0xC945C6B8 [Rational(-1620183, 2), -810091.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11000100010010011010010)))
;; z should be Float32(0x806224D2 [Rational(-3215977, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10100110 #b00100100111010001001001))))
(check-sat)
(exit)
