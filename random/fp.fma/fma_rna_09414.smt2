(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000101 #b01110011010110100100010)))
;; x should be Float32(0x42B9AD22 [Rational(6084241, 65536), 92.838150])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00100101111011100010101)))
;; y should be Float32(0x4A92F715 [Rational(9631509, 2), 4815754.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x000F8697)))
;; z should be Float32(0x000F8697 [Rational(1017495, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4DD52FE5))))
(check-sat)
(exit)
