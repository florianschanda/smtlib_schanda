(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10111110001110111001100)))
;; x should be Float32(0x005F1DCC [Rational(1558387, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11110101011101010101100)))
;; y should be Float32(0xC9FABAAC [Rational(-4107947, 2), -2053973.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010001 #b00000001101111101110101)))
;; z should be Float32(0xC880DF75 [Rational(-8445813, 32), -263931.656250])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10010001 #b00000001101111101110101)))
(check-sat)
(exit)
