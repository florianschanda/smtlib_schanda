(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB6027D)))
;; x should be Float32(0xCAB6027D [Rational(-11928189, 2), -5964094.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xBFAF5BF7)))
;; y should be Float32(0xBFAF5BF7 [Rational(-11492343, 8388608), -1.369994])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11000010110111010110011)))
;; z should be Float32(0x80616EB3 [Rational(-6385331, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010101 #b11110010101101000101100))))
(check-sat)
(exit)
