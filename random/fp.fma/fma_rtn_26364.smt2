(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFEF9D2C7)))
;; x should be Float32(0xFEF9D2C7 [Rational(-166036094745123746418726863870973968384), -166036094745123746418726863870973968384.000000])

(declare-const y Float32)
(assert (= y (_ +zero 8 24)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b1 #b10010001 #b00011101001010000010000)))
;; z should be Float32(0xC88E9410 [Rational(-584001, 2), -292000.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC88E9410))))
(check-sat)
(exit)
