(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x805CEC79)))
;; x should be Float32(0x805CEC79 [Rational(-6089849, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A33995A)))
;; y should be Float32(0x4A33995A [Rational(5885101, 2), 2942550.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x27317B2F)))
;; z should be Float32(0x27317B2F [Rational(11631407, 4722366482869645213696), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b01001110 #b01100010111101100101111)))
(check-sat)
(exit)
