(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8027A239)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b10000110010001111110000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b01010110 #b00000011111011001101010)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b01010110 #b00000011111011001101010))))
(check-sat)
(exit)
