(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01001101101101111001001)))
;; x should be Float32(0xCAA6DBC9 [Rational(-10935241, 2), -5467620.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00001101 #b11011111001101111001001)))
;; y should be Float32(0x86EF9BC9 [Rational(-15702985, 174224571863520493293247799005065324265472), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10001001010100101111101)))
;; z should be Float32(0x0044A97D [Rational(4499837, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x121C2CBB))))
(check-sat)
(exit)
