(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE997083D)))
;; y should be Float32(0xE997083D [Rational(-22823337868132557353123840), -22823337868132557353123840.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A9D57AD)))
;; z should be Float32(0x4A9D57AD [Rational(10311597, 2), 5155798.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4A9D57AD))))
(check-sat)
(exit)
