(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000101100111010110101)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8066B6F2)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b10111010111000010000010)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010100 #b10111010111000010000010))))
(check-sat)
(exit)
