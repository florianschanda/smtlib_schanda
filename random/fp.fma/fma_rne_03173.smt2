(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01000010001111001000110)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000001 #b00000000000000000000000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b01101010011111010010100)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
