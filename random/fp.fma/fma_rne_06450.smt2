(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01101011101010011110111)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEC8DAF36)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11101001011001111010110)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b01011001 #b11011100101100100100000))))
(check-sat)
(exit)
