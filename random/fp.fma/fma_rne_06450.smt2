(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802E42B1)))
;; x should be Float32(0x802E42B1 [Rational(-3031729, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xBBE0D83D)))
;; y should be Float32(0xBBE0D83D [Rational(-14735421, 2147483648), -0.006862])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11011011011010100100101)))
;; z should be Float32(0x006DB525 [Rational(7189797, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b00000000 #b11011100000011001101000))))
(check-sat)
(exit)
