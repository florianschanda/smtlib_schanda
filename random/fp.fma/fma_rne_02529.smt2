(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00100101 #b01011000000101011010011)))
;; x should be Float32(0x12AC0AD3 [Rational(11274963, 10384593717069655257060992658440192), 0.000000])

(declare-const y Float32)
(assert (= y (_ +zero 8 24)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b10001100 #b10011100110101000000000)))
;; z should be Float32(0x464E6A00 [Rational(26421, 2), 13210.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10001100 #b10011100110101000000000)))
(check-sat)
(exit)
