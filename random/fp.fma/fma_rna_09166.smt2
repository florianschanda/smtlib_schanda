(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00000001)))
;; x should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1C14F4EA)))
;; y should be Float32(0x1C14F4EA [Rational(4881013, 9903520314283042199192993792), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11110100 #b00011000001100000010110)))
;; z should be Float32(0x7A0C1816 [Rational(181852520261234705474967819522146304), 181852520261234705474967819522146304.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b11110100 #b00011000001100000010110))))
(check-sat)
(exit)
