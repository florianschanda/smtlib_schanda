(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00011101 #b01010101001110111001111)))
;; x should be Float32(0x0EAA9DCF [Rational(11181519, 2658455991569831745807614120560689152), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9735BE8)))
;; y should be Float32(0xC9735BE8 [Rational(-1993597, 2), -996798.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10011000111001100010101)))
;; z should be Float32(0x4ACC7315 [Rational(13398805, 2), 6699402.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x4ACC7314)))
(check-sat)
(exit)
