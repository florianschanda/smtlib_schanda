(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101101 #b10000101101000001100000)))
;; x should be Float32(0xF6C2D060 [Rational(-1975648315736166946795910735069184), -1975648315736166946795910735069184.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00000000000000000000001)))
;; y should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010010 #b00100000011101011101000)))
;; z should be Float32(0x49103AE8 [Rational(1181533, 2), 590766.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10010010 #b00100000011101011101000)))
(check-sat)
(exit)
