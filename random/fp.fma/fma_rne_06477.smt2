(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00101110011010001001101)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x806F8F49)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00101000 #b01011111101100111101100)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00101000 #b01011111101100111101100)))
(check-sat)
(exit)
