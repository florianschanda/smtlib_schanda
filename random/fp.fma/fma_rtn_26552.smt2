(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01011111 #b00110100010001011100100)))
;; x should be Float32(0xAF9A22E4 [Rational(-2525369, 9007199254740992), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b10000111001010101010110)))
;; y should be Float32(0xCA439556 [Rational(-6408875, 2), -3204437.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00110100100000000000010)))
;; z should be Float32(0x001A4002 [Rational(860161, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x3A6B84FB)))
(check-sat)
(exit)
