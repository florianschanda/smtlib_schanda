(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC901D058)))
;; x should be Float32(0xC901D058 [Rational(-1063435, 2), -531717.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01110001 #b00110100001010110101011)))
;; y should be Float32(0x389A15AB [Rational(10098091, 137438953472), 0.000073])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8015D1DF)))
;; z should be Float32(0x8015D1DF [Rational(-1429983, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xC21C44A3)))
(check-sat)
(exit)
