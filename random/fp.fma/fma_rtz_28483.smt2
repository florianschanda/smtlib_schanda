(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +oo 8 24)))
;; x should be Float32(+oo)

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11011110011011110110101)))
;; y should be Float32(0x806F37B5 [Rational(-7288757, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11010010110110100001111)))
;; z should be Float32(0x4AE96D0F [Rational(15297807, 2), 7648903.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
