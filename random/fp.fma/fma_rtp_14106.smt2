(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF1E581)))
;; x should be Float32(0x4AF1E581 [Rational(15852929, 2), 7926464.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB1D33F)))
;; y should be Float32(0x4AB1D33F [Rational(11653951, 2), 5826975.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01001011 #b00011011000010001110110)))
;; z should be Float32(0x258D8476 [Rational(4637243, 18889465931478580854784), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x5628074A)))
(check-sat)
(exit)
