(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11101101 #b11010110110110110110001)))
;; x should be Float32(0x76EB6DB1 [Rational(2387528452709467870251232314523648), 2387528452709467870251232314523648.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD6F20033)))
;; y should be Float32(0xD6F20033 [Rational(-133041334779904), -133041334779904.000000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b00000000000000000000000))))
(check-sat)
(exit)
