(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00011011001110010110111)))
;; x should be Float32(0xCA8D9CB7 [Rational(-9280695, 2), -4640347.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAA002BD)))
;; y should be Float32(0xCAA002BD [Rational(-10486461, 2), -5243230.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xF77D5618)))
;; z should be Float32(0xF77D5618 [Rational(-5138270679340335062573348749836288), -5138270679340335062573348749836288.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xF77D5618)))
(check-sat)
(exit)
