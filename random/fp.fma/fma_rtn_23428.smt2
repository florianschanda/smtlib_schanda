(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4523810C)))
;; x should be Float32(0x4523810C [Rational(2678851, 1024), 2616.065430])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8074D204)))
;; y should be Float32(0x8074D204 [Rational(-1913985, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00001100 #b00101010011100100111000))))
(check-sat)
(exit)
