(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10101110011110101100111)))
;; y should be Float32(0x4AD73D67 [Rational(14105959, 2), 7052979.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11010101011110100000101)))
;; z should be Float32(0x806ABD05 [Rational(-6995205, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8BD73D66))))
(check-sat)
(exit)
