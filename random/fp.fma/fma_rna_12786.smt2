(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01110011 #b10101001111100011010011)))
;; x should be Float32(0xB9D4F8D3 [Rational(-13957331, 34359738368), -0.000406])

(declare-const y Float32)
(assert (= y (fp #b0 #b01110101 #b11000101011110111000001)))
;; y should be Float32(0x3AE2BDC1 [Rational(14859713, 8589934592), 0.001730])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8005E421)))
;; z should be Float32(0x8005E421 [Rational(-386081, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b01101010 #b01111001010000110000111)))
(check-sat)
(exit)
