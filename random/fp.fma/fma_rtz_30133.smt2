(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000110 #b01100011101010010100011)))
;; x should be Float32(0x2331D4A3 [Rational(11654307, 1208925819614629174706176), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01110011011000011111111)))
;; y should be Float32(0x0039B0FF [Rational(3780863, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x803BFD12)))
;; z should be Float32(0x803BFD12 [Rational(-1965705, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x803BFD11))))
(check-sat)
(exit)
