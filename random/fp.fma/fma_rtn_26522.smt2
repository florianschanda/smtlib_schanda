(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC08DEA61)))
;; x should be Float32(0xC08DEA61 [Rational(-9300577, 2097152), -4.434861])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x002D7DAA)))
;; y should be Float32(0x002D7DAA [Rational(1490645, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x87DA58FB)))
;; z should be Float32(0x87DA58FB [Rational(-14309627, 43556142965880123323311949751266331066368), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00001111 #b10110100101110000100010)))
(check-sat)
(exit)
