(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111110 #b10100001010101110000011)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10001010000010000110101)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00010001000011011110101)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x4A8886F5)))
(check-sat)
(exit)
