(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x38B7E920)))
;; x should be Float32(0x38B7E920 [Rational(376649, 4294967296), 0.000088])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111010 #b10101000111010000000111)))
;; y should be Float32(0x7D547407 [Rational(17649923969928714413888100124023848960), 17649923969928714413888100124023848960.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10010010010010000010000)))
;; z should be Float32(0x80492410 [Rational(-299585, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x7698A069)))
(check-sat)
(exit)
