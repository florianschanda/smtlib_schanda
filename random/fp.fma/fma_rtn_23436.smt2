(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x23FD6063)))
;; x should be Float32(0x23FD6063 [Rational(16605283, 604462909807314587353088), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80008485)))
;; y should be Float32(0x80008485 [Rational(-33925, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10001110111000111101110)))
;; z should be Float32(0x004771EE [Rational(2341111, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b00000000 #b10001110111000111101101))))
(check-sat)
(exit)
