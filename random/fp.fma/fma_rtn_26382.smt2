(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x85CF942D)))
;; x should be Float32(0x85CF942D [Rational(-13603885, 696898287454081973172991196020261297061888), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01100111110010111100100)))
;; y should be Float32(0x49B3E5E4 [Rational(2947449, 2), 1473724.500000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x9011DF04)))
(check-sat)
(exit)
