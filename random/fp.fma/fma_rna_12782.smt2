(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF4B1FED4)))
;; x should be Float32(0xF4B1FED4 [Rational(-112818001998345341623187290456064), -112818001998345341623187290456064.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11000001 #b00101100100000010111110)))
;; y should be Float32(0x609640BE [Rational(86614899291263598592), 86614899291263598592.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
