(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11100011111100011001111)))
;; x should be Float32(0x0071F8CF [Rational(7469263, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF3B9BD85)))
;; y should be Float32(0xF3B9BD85 [Rational(-29431727038258290836949700706304), -29431727038258290836949700706304.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xF261B126)))
;; z should be Float32(0xF261B126 [Rational(-4470290337907249847905078476800), -4470290337907249847905078476800.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11100100 #b11000011011000100100110)))
(check-sat)
(exit)
