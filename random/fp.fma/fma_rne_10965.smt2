(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00011010001110001011000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10100000 #b01011000110010011001010)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00011111 #b00011010100001101001001)))
(check-sat)
(exit)
