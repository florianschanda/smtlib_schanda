(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000111100110010010110)))
;; x should be Float32(0x8003CC96 [Rational(-124491, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00100000 #b00010000011001101011101)))
;; y should be Float32(0x9008335D [Rational(-8926045, 332306998946228968225951765070086144), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A4D5106)))
;; z should be Float32(0x4A4D5106 [Rational(6727811, 2), 3363905.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4A4D5106))))
(check-sat)
(exit)
