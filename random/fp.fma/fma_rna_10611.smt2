(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10000101010010100111101)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0B9DECE9)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11011010110101011010011)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b00101101 #b11100000010011011110000)))
(check-sat)
(exit)
