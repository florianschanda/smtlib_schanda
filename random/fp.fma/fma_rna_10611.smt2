(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00110000111111011010100)))
;; x should be Float32(0xC9987ED4 [Rational(-2498485, 2), -1249242.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10000111 #b01111000000010011110100)))
;; y should be Float32(0x43BC04F4 [Rational(3080509, 8192), 376.038696])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00110101001101111111111)))
;; z should be Float32(0x001A9BFF [Rational(1743871, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10011011 #b11000000000000000101110)))
(check-sat)
(exit)
