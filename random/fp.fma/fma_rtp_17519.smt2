(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAD6664B)))
;; x should be Float32(0xCAD6664B [Rational(-14050891, 2), -7025445.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9FEFD14)))
;; y should be Float32(0xC9FEFD14 [Rational(-4177733, 2), -2088866.500000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10101010 #b10101011000110101110011)))
(check-sat)
(exit)
