(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x803161A9)))
;; x should be Float32(0x803161A9 [Rational(-3236265, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01000010111001111011010)))
;; y should be Float32(0x002173DA [Rational(1096173, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10000011 #b10111001101101010001111)))
;; z should be Float32(0x41DCDA8F [Rational(14473871, 524288), 27.606718])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x41DCDA8F))))
(check-sat)
(exit)
