(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x007CC7D0)))
;; x should be Float32(0x007CC7D0 [Rational(511101, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC0BD11)))
;; y should be Float32(0x4AC0BD11 [Rational(12631313, 2), 6315656.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b00000000000000000000000)))
;; z should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ +oo 8 24))))
(check-sat)
(exit)
