(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC3C3E8DA)))
;; x should be Float32(0xC3C3E8DA [Rational(-6419565, 16384), -391.819153])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11011110010001000001111)))
;; y should be Float32(0x806F220F [Rational(-7283215, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0065C4BD)))
;; z should be Float32(0x0065C4BD [Rational(6669501, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x04AA7DD3)))
(check-sat)
(exit)
