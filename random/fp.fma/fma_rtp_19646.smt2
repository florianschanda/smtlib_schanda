(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA2A71742)))
;; x should be Float32(0xA2A71742 [Rational(-5475233, 1208925819614629174706176), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10111011 #b01101001001000101101010)))
;; y should be Float32(0x5DB4916A [Rational(1626412168335327232), 1626412168335327232.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
