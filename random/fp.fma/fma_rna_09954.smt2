(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00020E0D)))
;; x should be Float32(0x00020E0D [Rational(134669, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA4692F2)))
;; y should be Float32(0xCA4692F2 [Rational(-6506873, 2), -3253436.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xBB562EE9)))
;; z should be Float32(0xBB562EE9 [Rational(-14036713, 4294967296), -0.003268])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xBB562EE9)))
(check-sat)
(exit)
