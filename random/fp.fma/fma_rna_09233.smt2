(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000000000000000000001)))
;; x should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9C28384)))
;; y should be Float32(0xC9C28384 [Rational(-3186913, 2), -1593456.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8045CC84)))
;; z should be Float32(0x8045CC84 [Rational(-1143585, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00000000 #b10111100001110011110101))))
(check-sat)
(exit)
