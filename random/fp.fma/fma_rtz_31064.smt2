(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCACAC98D)))
;; x should be Float32(0xCACAC98D [Rational(-13289869, 2), -6644934.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00000000)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11100111100011001000000)))
;; z should be Float32(0x8073C640 [Rational(-118553, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8073C640))))
(check-sat)
(exit)
