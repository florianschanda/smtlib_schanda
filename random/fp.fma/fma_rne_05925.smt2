(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB08A62F4)))
;; x should be Float32(0xB08A62F4 [Rational(-2267325, 2251799813685248), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0AEEB54D)))
;; y should be Float32(0x0AEEB54D [Rational(15643981, 680564733841876926926749214863536422912), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00000000 #b00000000100000010000110))))
(check-sat)
(exit)
