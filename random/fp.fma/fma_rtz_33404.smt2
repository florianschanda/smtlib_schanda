(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xED395AEC)))
;; x should be Float32(0xED395AEC [Rational(-3585290288700256973504905216), -3585290288700256973504905216.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC964D468)))
;; y should be Float32(0xC964D468 [Rational(-1874573, 2), -937286.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b10111110011011001111110)))
;; z should be Float32(0x4A5F367E [Rational(7314239, 2), 3657119.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11101110 #b01001011010111011000100))))
(check-sat)
(exit)
