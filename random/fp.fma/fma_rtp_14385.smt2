(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ADC8D55)))
;; x should be Float32(0x4ADC8D55 [Rational(14454101, 2), 7227050.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xECA7D46F)))
;; y should be Float32(0xECA7D46F [Rational(-1623150409268828949771714560), -1623150409268828949771714560.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A825C07)))
;; z should be Float32(0x4A825C07 [Rational(8543239, 2), 4271619.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11110000 #b00100001001011100110111)))
(check-sat)
(exit)
