(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11011010011000111000010)))
(declare-const z Float32)
(assert (= z (fp #b1 #b01110010 #b00111011111001110100111)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b11111111 #b00000000000000000000000))))
(check-sat)
(exit)
