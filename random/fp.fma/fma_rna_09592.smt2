(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x548EE943)))
;; x should be Float32(0x548EE943 [Rational(4910390706176), 4910390706176.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC99BEDF4)))
;; y should be Float32(0xC99BEDF4 [Rational(-2554749, 2), -1277374.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10111101 #b01011100001100000100011))))
(check-sat)
(exit)
