(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01001100101001010100101)))
;; x should be Float32(0x002652A5 [Rational(2511525, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11101010011011111111110)))
;; y should be Float32(0x007537FE [Rational(3841023, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x2413BD25)))
;; z should be Float32(0x2413BD25 [Rational(9682213, 302231454903657293676544), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b01001000 #b00100111011110100100101)))
(check-sat)
(exit)
