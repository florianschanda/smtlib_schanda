(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01000011 #b00101100101101001111110)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11001000 #b11001010010100110000011)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01110011101001111111101)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10001101 #b00001101001011101010100)))
(check-sat)
(exit)
