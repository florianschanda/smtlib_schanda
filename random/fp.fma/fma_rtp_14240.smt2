(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x496B0C88)))
;; x should be Float32(0x496B0C88 [Rational(1925521, 2), 962760.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00336010)))
;; y should be Float32(0x00336010 [Rational(210433, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01110000100000000011111)))
;; z should be Float32(0x0038401F [Rational(3686431, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b00010011 #b01111001010111011011000))))
(check-sat)
(exit)
