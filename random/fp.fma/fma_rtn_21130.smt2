(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00010000110111011101001)))
;; x should be Float32(0x4A886EE9 [Rational(8941289, 2), 4470644.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9DA8BD4)))
;; y should be Float32(0xC9DA8BD4 [Rational(-3580661, 2), -1790330.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A9A4D19)))
;; z should be Float32(0x4A9A4D19 [Rational(10112281, 2), 5056140.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10101001 #b11010001111000111100110))))
(check-sat)
(exit)
