(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111011101000110000000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00100000100011111010101)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10010001000101100110101)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x4AC88B34)))
(check-sat)
(exit)
