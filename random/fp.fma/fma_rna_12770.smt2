(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCBBA9EA0)))
;; x should be Float32(0xCBBA9EA0 [Rational(-24460608), -24460608.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0ADFBD3F)))
;; y should be Float32(0x0ADFBD3F [Rational(14662975, 680564733841876926926749214863536422912), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b10101010111001110100110)))
;; z should be Float32(0x4A5573A6 [Rational(6994387, 2), 3497193.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4A5573A6))))
(check-sat)
(exit)
