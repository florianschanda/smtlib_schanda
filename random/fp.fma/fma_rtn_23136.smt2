(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01001000 #b00110110001010011100101)))
;; x should be Float32(0x241B14E5 [Rational(10163429, 302231454903657293676544), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01011100100001010111100)))
;; y should be Float32(0x49AE42BC [Rational(2855087, 2), 1427543.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111110 #b11111111111111111111111)))
;; z should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
