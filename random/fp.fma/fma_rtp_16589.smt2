(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11110100 #b00000100000000110100101)))
;; x should be Float32(0x7A0201A5 [Rational(168757986666486524248772851200425984), 168757986666486524248772851200425984.000000])

(declare-const y Float32)
(assert (= y (_ NaN 8 24)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01100001001111011011011)))
;; z should be Float32(0x4AB09EDB [Rational(11575003, 2), 5787501.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
