(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A842407)))
;; x should be Float32(0x4A842407 [Rational(8659975, 2), 4329987.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01011011101010001000111)))
;; y should be Float32(0x002DD447 [Rational(3003463, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x97506393)))
;; z should be Float32(0x97506393 [Rational(-13656979, 20282409603651670423947251286016), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00101110 #b10100000110001110010011))))
(check-sat)
(exit)
