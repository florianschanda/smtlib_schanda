(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00266806)))
;; x should be Float32(0x00266806 [Rational(1258499, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10100111100101010110011)))
;; y should be Float32(0x4AD3CAB3 [Rational(13879987, 2), 6939993.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC745FE36)))
;; z should be Float32(0xC745FE36 [Rational(-6487835, 128), -50686.210938])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10001110 #b10001011111111000110110))))
(check-sat)
(exit)
