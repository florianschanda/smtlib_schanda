(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80351D16)))
;; x should be Float32(0x80351D16 [Rational(-1740427, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b11100100011000011100001)))
;; z should be Float32(0xCAF230E1 [Rational(-15872225, 2), -7936112.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010101 #b11100100011000011100001))))
(check-sat)
(exit)
