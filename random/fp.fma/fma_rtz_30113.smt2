(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4FD2DA6A)))
;; x should be Float32(0x4FD2DA6A [Rational(7075058688), 7075058688.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x167D481F)))
;; y should be Float32(0x167D481F [Rational(16599071, 81129638414606681695789005144064), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xF547AC39)))
;; z should be Float32(0xF547AC39 [Rational(-253115274732793481068781867368448), -253115274732793481068781867368448.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11101010 #b10001111010110000111000))))
(check-sat)
(exit)
