(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8060ADA3)))
;; x should be Float32(0x8060ADA3 [Rational(-6335907, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10000111110001010100010)))
;; y should be Float32(0x0043E2A2 [Rational(2224465, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b11100000001010111110100)))
;; z should be Float32(0xC9F015F4 [Rational(-3933565, 2), -1966782.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10010011 #b11100000001010111110101)))
(check-sat)
(exit)
