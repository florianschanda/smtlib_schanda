(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8F325885)))
;; x should be Float32(0x8F325885 [Rational(-11688069, 1329227995784915872903807060280344576), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01111011011000000011010)))
;; y should be Float32(0x4A3DB01A [Rational(6215693, 2), 3107846.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b10111000100011110100011)))
;; z should be Float32(0xCADC47A3 [Rational(-14436259, 2), -7218129.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xCADC47A3)))
(check-sat)
(exit)
