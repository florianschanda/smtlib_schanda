(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB3489F73)))
;; x should be Float32(0xB3489F73 [Rational(-13148019, 281474976710656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01010001100100110001100)))
;; y should be Float32(0x49A8C98C [Rational(2765411, 2), 1382705.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xF2BD4187)))
;; z should be Float32(0xF2BD4187 [Rational(-7497201222909997604792751357952), -7497201222909997604792751357952.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11100101 #b01111010100000110000111)))
(check-sat)
(exit)
