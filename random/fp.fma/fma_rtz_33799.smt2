(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x801290D6)))
;; x should be Float32(0x801290D6 [Rational(-608363, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF800000)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z (fp #b1 #b00111001 #b11101101000110001101010)))
;; z should be Float32(0x9CF68C6A [Rational(-8078901, 4951760157141521099596496896), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
