(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10000101110100100100011)))
;; x should be Float32(0x4AC2E923 [Rational(12773667, 2), 6386833.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01100000 #b11111000011001111111111)))
;; y should be Float32(0x307C33FF [Rational(16528383, 18014398509481984), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10001100 #b11011101010011100010100)))
;; z should be Float32(0xC66EA714 [Rational(-3910085, 256), -15273.769531])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10001100 #b11011101010011100001110))))
(check-sat)
(exit)
