(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80599DA1)))
;; x should be Float32(0x80599DA1 [Rational(-5873057, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF800000)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z (fp #b1 #b00101011 #b11010100000010100011101)))
;; z should be Float32(0x95EA051D [Rational(-15336733, 162259276829213363391578010288128), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
