(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AE67FEB)))
;; x should be Float32(0x4AE67FEB [Rational(15106027, 2), 7553013.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111010110000010110100)))
;; y should be Float32(0x807D60B4 [Rational(-2054189, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00010111 #b11000011100011100010000))))
(check-sat)
(exit)
