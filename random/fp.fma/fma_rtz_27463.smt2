(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000000000000000000000)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00010000110111100100101)))
;; y should be Float32(0x4A886F25 [Rational(8941349, 2), 4470674.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x309D6DA7)))
;; z should be Float32(0x309D6DA7 [Rational(10317223, 9007199254740992), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b01100001 #b00111010110110110100111)))
(check-sat)
(exit)
