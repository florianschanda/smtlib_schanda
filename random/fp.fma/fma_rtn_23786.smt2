(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10011010100101001100000)))
;; x should be Float32(0x004D4A60 [Rational(158291, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010011 #b10000000101011000100110)))
;; y should be Float32(0xA9C05626 [Rational(-6302483, 73786976294838206464), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xF21FE174)))
;; z should be Float32(0xF21FE174 [Rational(-3166763050593226903705207439360), -3166763050593226903705207439360.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b11100100 #b00111111110000101110101)))
(check-sat)
(exit)
