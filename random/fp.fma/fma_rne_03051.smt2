(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00110110001110101010010)))
;; x should be Float32(0x001B1D52 [Rational(888489, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11001010110011011000111)))
;; y should be Float32(0x006566C7 [Rational(6645447, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA93F629)))
;; z should be Float32(0xCA93F629 [Rational(-9696809, 2), -4848404.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xCA93F629)))
(check-sat)
(exit)
