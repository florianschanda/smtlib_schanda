(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80130397)))
;; x should be Float32(0x80130397 [Rational(-1246103, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0068ABC6)))
;; y should be Float32(0x0068ABC6 [Rational(3429859, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC9C62FD4)))
;; z should be Float32(0xC9C62FD4 [Rational(-3247093, 2), -1623546.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010011 #b10001100010111111010100))))
(check-sat)
(exit)
