(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7F8DEE0B)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11111101000101110010100)))
;; y should be Float32(0xC9FE8B94 [Rational(-4170469, 2), -2085234.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111110110110100110)))
;; z should be Float32(0x807FEDA6 [Rational(-4191955, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
