(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8007FB50)))
;; x should be Float32(0x8007FB50 [Rational(-32693, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAB1C21F)))
;; y should be Float32(0xCAB1C21F [Rational(-11649567, 2), -5824783.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01001110 #b10011111100000010110110)))
;; z should be Float32(0x274FC0B6 [Rational(6807643, 2361183241434822606848), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01001110 #b10011111100000010110111))))
(check-sat)
(exit)
