(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807CFD29)))
;; x should be Float32(0x807CFD29 [Rational(-8191273, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01101101101101000010101)))
;; y should be Float32(0x0036DA15 [Rational(3594773, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x85F6703D)))
;; z should be Float32(0x85F6703D [Rational(-16150589, 696898287454081973172991196020261297061888), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b00001011 #b11101100111000000111101))))
(check-sat)
(exit)
