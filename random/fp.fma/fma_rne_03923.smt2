(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAFD06D9)))
;; x should be Float32(0xCAFD06D9 [Rational(-16582361, 2), -8291180.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10001111 #b10001101101101001101100)))
;; y should be Float32(0xC7C6DA6C [Rational(-3258011, 32), -101812.843750])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000001000000011111001)))
;; z should be Float32(0x000080F9 [Rational(33017, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10100110 #b10001001000101100101110))))
(check-sat)
(exit)
