(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0001774B)))
;; x should be Float32(0x0001774B [Rational(96075, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00000111011011100011110)))
;; y should be Float32(0x4A03B71E [Rational(4316047, 2), 2158023.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80398A57)))
;; z should be Float32(0x80398A57 [Rational(-3770967, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b00001111 #b10000010001011011101101)))
(check-sat)
(exit)
