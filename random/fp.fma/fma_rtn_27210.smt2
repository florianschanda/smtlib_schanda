(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFFBC188D)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b01100100 #b11111011001111010111001)))
;; y should be Float32(0x327D9EB9 [Rational(16621241, 1125899906842624), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b00101010010000010101100)))
;; z should be Float32(0x499520AC [Rational(2443307, 2), 1221653.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
