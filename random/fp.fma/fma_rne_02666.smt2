(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111001 #b01010011010101111101001)))
;; x should be Float32(0x7CA9ABE9 [Rational(7047879370371809027638465242788265984), 7047879370371809027638465242788265984.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2085A305)))
;; y should be Float32(0x2085A305 [Rational(8758021, 38685626227668133590597632), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10111011 #b01100010010010010111000))))
(check-sat)
(exit)
