(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11001010010001011000001)))
;; x should be Float32(0x006522C1 [Rational(6628033, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00100010101011011010011)))
;; y should be Float32(0x4A9156D3 [Rational(9524947, 2), 4762473.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01010011100101010010110)))
;; z should be Float32(0x0029CA96 [Rational(1369419, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00010110 #b11001011010101111110011)))
(check-sat)
(exit)
