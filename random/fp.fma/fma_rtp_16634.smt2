(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x004B7F70)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b10101011001001001010100)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00111101110111011100110)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00010100 #b11110111111000011000011)))
(check-sat)
(exit)
