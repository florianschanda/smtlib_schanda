(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4D96C39E)))
;; x should be Float32(0x4D96C39E [Rational(316175296), 316175296.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00110110101010110100100)))
;; y should be Float32(0x499B55A4 [Rational(2545001, 2), 1272500.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA9DFEB3)))
;; z should be Float32(0xCA9DFEB3 [Rational(-10354355, 2), -5177177.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x57B6F5C0))))
(check-sat)
(exit)
