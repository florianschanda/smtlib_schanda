(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01101000 #b10100100011000101110010)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01100011111100111110101)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC9B8FFDC)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10010011 #b01110001111111111010011)))
(check-sat)
(exit)
