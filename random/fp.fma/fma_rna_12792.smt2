(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFA0938A4)))
;; x should be Float32(0xFA0938A4 [Rational(-178123369493932054500946096095756288), -178123369493932054500946096095756288.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10000011111110010001110)))
;; y should be Float32(0x0041FC8E [Rational(2162247, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x007FFFFF)))
;; z should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xBA8D7B06))))
(check-sat)
(exit)
