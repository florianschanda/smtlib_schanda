(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49E93C94)))
;; x should be Float32(0x49E93C94 [Rational(3821349, 2), 1910674.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00101111001101010110010)))
;; y should be Float32(0x80179AB2 [Rational(-773465, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x89AC0B0F))))
(check-sat)
(exit)
