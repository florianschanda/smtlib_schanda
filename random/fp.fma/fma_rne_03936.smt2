(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA5790DA)))
;; x should be Float32(0xCA5790DA [Rational(-7063661, 2), -3531830.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10100100011000100110110)))
;; y should be Float32(0x80523136 [Rational(-2693275, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (_ +oo 8 24)))
;; z should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (_ +oo 8 24))))
(check-sat)
(exit)
