(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB52B024C)))
;; y should be Float32(0xB52B024C [Rational(-2801811, 4398046511104), -0.000001])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10100011111101100101101)))
;; z should be Float32(0x8051FB2D [Rational(-5372717, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00000000 #b10100011111101100101000))))
(check-sat)
(exit)
