(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00269012)))
;; x should be Float32(0x00269012 [Rational(1263625, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01011111011110010100011)))
;; y should be Float32(0xCAAFBCA3 [Rational(-11517091, 2), -5758545.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0030E522)))
;; z should be Float32(0x0030E522 [Rational(1602193, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b00010101 #b10100111100011100110010)))
(check-sat)
(exit)
