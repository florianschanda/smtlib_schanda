(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00111001100011110101010)))
;; x should be Float32(0xCA1CC7AA [Rational(-5137365, 2), -2568682.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD03BA8D2)))
;; y should be Float32(0xD03BA8D2 [Rational(-12593612800), -12593612800.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xEE7A66A4)))
;; z should be Float32(0xEE7A66A4 [Rational(-19373834339260037494707585024), -19373834339260037494707585024.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11011100 #b11110100110011010100100))))
(check-sat)
(exit)
