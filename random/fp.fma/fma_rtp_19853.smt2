(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101101 #b11011110110101100000100)))
;; x should be Float32(0xB6EF6B04 [Rational(-3922625, 549755813888), -0.000007])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFFD9B108)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b1 #b01101010 #b00101110010010010000100)))
;; z should be Float32(0xB5172484 [Rational(-2476321, 4398046511104), -0.000001])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
