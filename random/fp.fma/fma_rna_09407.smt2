(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1752A32E)))
;; x should be Float32(0x1752A32E [Rational(6902167, 10141204801825835211973625643008), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A57B142)))
;; y should be Float32(0x4A57B142 [Rational(7067809, 2), 3533904.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AA3E8C7)))
;; z should be Float32(0x4AA3E8C7 [Rational(10741959, 2), 5370979.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AA3E8C7))))
(check-sat)
(exit)
