(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b10110011000000001111000)))
;; y should be Float32(0x49598078 [Rational(1781775, 2), 890887.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x499205DC)))
;; z should be Float32(0x499205DC [Rational(2392439, 2), 1196219.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10010011 #b00100100000010111011100))))
(check-sat)
(exit)
