(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00001011001101111101011)))
;; x should be Float32(0x80059BEB [Rational(-367595, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x061D8770)))
;; y should be Float32(0x061D8770 [Rational(645239, 21778071482940061661655974875633165533184), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x802CB1DC)))
;; z should be Float32(0x802CB1DC [Rational(-732279, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b00000000 #b01011001011000111011100)))
(check-sat)
(exit)
