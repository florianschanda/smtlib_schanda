(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010000 #b11111101000010010001011)))
;; x should be Float32(0x087E848B [Rational(16680075, 21778071482940061661655974875633165533184), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10011001 #b10110010110110010010001)))
;; y should be Float32(0x4CD96C91 [Rational(113992840), 113992840.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4B9807C9)))
;; z should be Float32(0x4B9807C9 [Rational(19926930), 19926930.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10010111 #b00110000000011111001001))))
(check-sat)
(exit)
