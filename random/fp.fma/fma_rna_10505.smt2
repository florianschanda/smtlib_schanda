(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA9CED11)))
;; x should be Float32(0xCA9CED11 [Rational(-10284305, 2), -5142152.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00011110000001111111100)))
;; y should be Float32(0x498F03FC [Rational(2343167, 2), 1171583.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x9F48F157)))
;; z should be Float32(0x9F48F157 [Rational(-13168983, 309485009821345068724781056), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10101001 #b01011110101010110111011))))
(check-sat)
(exit)
