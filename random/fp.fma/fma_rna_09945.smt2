(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0063EB04)))
;; x should be Float32(0x0063EB04 [Rational(1637057, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAC45CE9)))
;; y should be Float32(0xCAC45CE9 [Rational(-12868841, 2), -6434420.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10101001 #b11100011101101011010110)))
;; z should be Float32(0x54F1DAD6 [Rational(8310068609024), 8310068609024.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x54F1DAD6)))
(check-sat)
(exit)
