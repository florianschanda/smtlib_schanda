(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AE4A9C1)))
;; x should be Float32(0x4AE4A9C1 [Rational(14985665, 2), 7492832.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010100 #b01110101110111101111010)))
;; y should be Float32(0x6A3AEF7A [Rational(56497774635125966066679808), 56497774635125966066679808.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x75A6F93E))))
(check-sat)
(exit)
