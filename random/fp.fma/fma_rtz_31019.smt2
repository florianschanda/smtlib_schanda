(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8054FDC0)))
;; y should be Float32(0x8054FDC0 [Rational(-87031, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b00000010000110110110110)))
;; z should be Float32(0xCA010DB6 [Rational(-4228827, 2), -2114413.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA010DB6))))
(check-sat)
(exit)
