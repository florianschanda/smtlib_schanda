(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001111 #b00010010010011011000000)))
;; x should be Float32(0x478926C0 [Rational(140443, 2), 70221.500000])

(declare-const y Float32)
(assert (= y (_ NaN 8 24)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00100000000000100111000)))
;; z should be Float32(0x80100138 [Rational(-131111, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
