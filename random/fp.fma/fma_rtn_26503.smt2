(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9DF38C90)))
;; x should be Float32(0x9DF38C90 [Rational(-997577, 154742504910672534362390528), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010111 #b10000101010001011010101)))
;; y should be Float32(0x4BC2A2D5 [Rational(25511338), 25511338.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10000111 #b00100101111010111100011)))
;; z should be Float32(0xC392F5E3 [Rational(-9631203, 32768), -293.920990])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10000111 #b00100101111010111100100))))
(check-sat)
(exit)
