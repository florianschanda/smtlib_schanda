(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000101 #b11000111111110111001001)))
;; y should be Float32(0xE2E3FDC9 [Rational(-2102849026246991413248), -2102849026246991413248.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11101000011111011010011)))
;; z should be Float32(0x80743ED3 [Rational(-7618259, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b01000111 #b11000111111110111001001)))
(check-sat)
(exit)
