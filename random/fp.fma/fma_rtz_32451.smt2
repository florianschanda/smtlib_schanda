(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00100011 #b10100001111101010011111)))
;; y should be Float32(0x91D0FA9F [Rational(-13695647, 41538374868278621028243970633760768), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80298067)))
;; z should be Float32(0x80298067 [Rational(-2719847, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00000000 #b01010011000000001100110)))
(check-sat)
(exit)
