(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111110 #b11111111111111111111111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10100101000110100001111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10000101 #b01111111100000010001100)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10000101 #b10001001110100101011101))))
(check-sat)
(exit)
