(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10011110001001110100001)))
;; x should be Float32(0x4ACF13A1 [Rational(13570977, 2), 6785488.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2924E13B)))
;; y should be Float32(0x2924E13B [Rational(10805563, 295147905179352825856), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x693E0045)))
;; z should be Float32(0x693E0045 [Rational(14356073659507539322077184), 14356073659507539322077184.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b11010010 #b01111100000000001000101))))
(check-sat)
(exit)
