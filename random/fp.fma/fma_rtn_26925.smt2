(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01000010100111110010010)))
;; x should be Float32(0x80214F92 [Rational(-1091529, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (_ -oo 8 24)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b01001011011111110101100)))
;; z should be Float32(0x49A5BFAC [Rational(2715627, 2), 1357813.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
