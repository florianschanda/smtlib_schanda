(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7C4C5D54)))
;; x should be Float32(0x7C4C5D54 [Rational(4244485913599582559096024812889309184), 4244485913599582559096024812889309184.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x94F5FDE4)))
;; y should be Float32(0x94F5FDE4 [Rational(-4030329, 162259276829213363391578010288128), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80800000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD1C45FFF))))
(check-sat)
(exit)
