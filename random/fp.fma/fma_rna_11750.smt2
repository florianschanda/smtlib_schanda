(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01110001100111100100011)))
;; y should be Float32(0xCAB8CF23 [Rational(-12111651, 2), -6055825.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7ED9CC7A)))
;; z should be Float32(0x7ED9CC7A [Rational(144752089049219747380069037448326283264), 144752089049219747380069037448326283264.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b11111101 #b10110011100110001111010)))
(check-sat)
(exit)
