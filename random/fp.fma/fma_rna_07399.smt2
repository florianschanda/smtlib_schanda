(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00000011111010000101110)))
;; x should be Float32(0x4A01F42E [Rational(4258327, 2), 2129163.500000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7B7B3DB0)))
;; z should be Float32(0x7B7B3DB0 [Rational(1304517682634666997180431824697950208), 1304517682634666997180431824697950208.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b11110110 #b11110110011110110110000))))
(check-sat)
(exit)
