(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49AF6F94)))
;; x should be Float32(0x49AF6F94 [Rational(2874341, 2), 1437170.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1B45FE4F)))
;; y should be Float32(0x1B45FE4F [Rational(12975695, 79228162514264337593543950336), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00101111001101000100010)))
;; z should be Float32(0x80179A22 [Rational(-773393, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x2587AF24)))
(check-sat)
(exit)
