(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01111010 #b10000101110101001001111)))
;; x should be Float32(0xBD42EA4F [Rational(-12773967, 268435456), -0.047587])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111000110100100001100)))
;; y should be Float32(0x001C690C [Rational(465475, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x80815A19))))
(check-sat)
(exit)
