(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEB251DDA)))
;; x should be Float32(0xEB251DDA [Rational(-199613730254625102219968512), -199613730254625102219968512.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10000100000110001110011)))
;; y should be Float32(0x80420C73 [Rational(-4328563, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b01100000101010111001100)))
;; z should be Float32(0x49B055CC [Rational(2889075, 2), 1444537.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x49B055CC)))
(check-sat)
(exit)
