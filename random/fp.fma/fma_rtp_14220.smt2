(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00001000000011110011111)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00111010 #b00001101011101000110010)))
(declare-const z Float32)
(assert (= z (fp #b0 #b01110011 #b10011111001110000100000)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x39CF9C21)))
(check-sat)
(exit)
