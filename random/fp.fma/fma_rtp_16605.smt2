(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001000 #b00001111111001001000001)))
;; x should be Float32(0x0407F241 [Rational(8909377, 5575186299632655785383929568162090376495104), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b01011000011101111101010)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8031E8EB)))
;; z should be Float32(0x8031E8EB [Rational(-3270891, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
