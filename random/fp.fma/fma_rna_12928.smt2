(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001100 #b10110110110000101000011)))
;; x should be Float32(0x865B6143 [Rational(-14377283, 348449143727040986586495598010130648530944), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80000001)))
;; y should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10001110 #b10101100111000001100101)))
;; z should be Float32(0x47567065 [Rational(14053477, 256), 54896.394531])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10001110 #b10101100111000001100101)))
(check-sat)
(exit)
