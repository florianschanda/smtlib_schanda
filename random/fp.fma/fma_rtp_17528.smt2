(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAF6C295)))
;; x should be Float32(0xCAF6C295 [Rational(-16171669, 2), -8085834.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11100000011111010111010)))
;; y should be Float32(0xCA703EBA [Rational(-7872349, 2), -3936174.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x55E792E3))))
(check-sat)
(exit)
