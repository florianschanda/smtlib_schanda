(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11001100 #b11011010011110111011110)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01001110 #b11011010011110111011101))))
(check-sat)
(exit)
