(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10101010110110111110111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10011110 #b01100000111011101001100)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10100001011011010100001)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x5A931F10))))
(check-sat)
(exit)
