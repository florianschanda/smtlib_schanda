(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b01001000010100000001000)))
;; x should be Float32(0xC9242808 [Rational(-1344769, 2), -672384.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010101 #b01110111010100101101111)))
;; y should be Float32(0xAABBA96F [Rational(-12298607, 36893488147419103232), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b01010110011101100110100)))
;; z should be Float32(0x49AB3B34 [Rational(2805453, 2), 1402726.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10010011 #b01010110011101100110101)))
(check-sat)
(exit)
