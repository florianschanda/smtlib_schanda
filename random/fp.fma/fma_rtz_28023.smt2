(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01100110101001101110110)))
;; x should be Float32(0x4A335376 [Rational(5876155, 2), 2938077.500000])

(declare-const y Float32)
(assert (= y (_ -oo 8 24)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z (fp #b1 #b10110110 #b00111001110110111000001)))
;; z should be Float32(0xDB1CEDC1 [Rational(-44171509551333376), -44171509551333376.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
