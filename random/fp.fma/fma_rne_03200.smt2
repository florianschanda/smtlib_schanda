(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01011011100111100110001)))
;; x should be Float32(0x002DCF31 [Rational(3002161, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10011000 #b01010011010010101110111)))
;; y should be Float32(0xCC29A577 [Rational(-44471772), -44471772.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00001010 #b11110101100011110100110)))
;; z should be Float32(0x057AC7A6 [Rational(8217555, 696898287454081973172991196020261297061888), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x8C72D72E)))
(check-sat)
(exit)
