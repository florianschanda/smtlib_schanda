(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7C35F506)))
;; x should be Float32(0x7C35F506 [Rational(3779101588466694182415649893670453248), 3779101588466694182415649893670453248.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b01110000001011111011100)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x806D5AF8)))
;; z should be Float32(0x806D5AF8 [Rational(-895839, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
