(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11000110 #b10100011111110100001110)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00101001 #b11100110010011001000111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10000110001000001100011)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b01110001 #b10001110111001010011010)))
(check-sat)
(exit)
