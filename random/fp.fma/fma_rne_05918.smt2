(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9C480537)))
;; x should be Float32(0x9C480537 [Rational(-13108535, 19807040628566084398385987584), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x42A13E75)))
;; y should be Float32(0x42A13E75 [Rational(10567285, 131072), 80.621986])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00100000101011110111000)))
;; z should be Float32(0x001057B8 [Rational(133879, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00111110 #b11110111111100000101001))))
(check-sat)
(exit)
