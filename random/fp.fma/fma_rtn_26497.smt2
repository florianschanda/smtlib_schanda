(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x89355168)))
;; x should be Float32(0x89355168 [Rational(-1485357, 680564733841876926926749214863536422912), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01010110 #b10101011001100111000101)))
;; y should be Float32(0x2B5599C5 [Rational(13998533, 18446744073709551616), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA9E3039)))
;; z should be Float32(0xCA9E3039 [Rational(-10367033, 2), -5183516.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10010101 #b00111100011000000111010)))
(check-sat)
(exit)
