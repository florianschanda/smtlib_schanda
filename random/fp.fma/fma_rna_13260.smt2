(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10101010101001011101100)))
;; x should be Float32(0x805552EC [Rational(-1397947, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111111111111111111111)))
;; y should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xB9944B21)))
;; z should be Float32(0xB9944B21 [Rational(-9718561, 34359738368), -0.000283])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b01110011 #b00101000100101100100001))))
(check-sat)
(exit)
