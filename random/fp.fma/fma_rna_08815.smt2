(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2F76CAF2)))
;; y should be Float32(0x2F76CAF2 [Rational(8086905, 36028797018963968), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x804B10D1)))
;; z should be Float32(0x804B10D1 [Rational(-4919505, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b00000000 #b10010110001000011010001)))
(check-sat)
(exit)
