(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11010100011010111111001)))
;; x should be Float32(0x806A35F9 [Rational(-6960633, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b01010100101110100101101)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00001111111110100001001)))
;; z should be Float32(0x4A87FD09 [Rational(8912137, 2), 4456068.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
