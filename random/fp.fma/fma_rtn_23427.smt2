(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000010 #b00001000110011000100110)))
;; x should be Float32(0x21046626 [Rational(4338451, 9671406556917033397649408), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xBFCB78A2)))
;; y should be Float32(0xBFCB78A2 [Rational(-6667345, 4194304), -1.589619])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b11100110111101011001001)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
