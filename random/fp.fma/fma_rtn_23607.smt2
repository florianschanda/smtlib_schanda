(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00001011000100110100110)))
;; x should be Float32(0x000589A6 [Rational(181459, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1BC6664A)))
;; y should be Float32(0x1BC6664A [Rational(6501157, 19807040628566084398385987584), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11010011011010100001110)))
;; z should be Float32(0x0069B50E [Rational(3463815, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b00000000 #b11010011011010100001110)))
(check-sat)
(exit)
