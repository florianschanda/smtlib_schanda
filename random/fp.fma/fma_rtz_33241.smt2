(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01011010 #b11111001111011001000000)))
;; x should be Float32(0xAD7CF640 [Rational(-259033, 18014398509481984), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11100111001100010111111)))
;; y should be Float32(0x4AF398BF [Rational(15964351, 2), 7982175.500000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xB8F0B4AD))))
(check-sat)
(exit)
