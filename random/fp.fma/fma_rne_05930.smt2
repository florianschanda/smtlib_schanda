(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDD956022)))
;; x should be Float32(0xDD956022 [Rational(-1345455061601353728), -1345455061601353728.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00447B60)))
;; y should be Float32(0x00447B60 [Rational(140251, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x48855690)))
;; z should be Float32(0x48855690 [Rational(546153, 2), 273076.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x48855690))))
(check-sat)
(exit)
