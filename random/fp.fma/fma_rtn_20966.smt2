(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A9FE4DD)))
;; x should be Float32(0x4A9FE4DD [Rational(10478813, 2), 5239406.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00100000000100111100100)))
;; y should be Float32(0x499009E4 [Rational(2359929, 2), 1179964.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11001000100111111111011)))
;; z should be Float32(0x00644FFB [Rational(6574075, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x54B3EDD3)))
(check-sat)
(exit)
