(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A0E91F6)))
;; x should be Float32(0x4A0E91F6 [Rational(4671739, 2), 2335869.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01010110 #b11111001111110001001100)))
;; y should be Float32(0x2B7CFC4C [Rational(4144915, 4611686018427387904), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00001100 #b01111001110010100101010)))
;; z should be Float32(0x063CE52A [Rational(6189717, 174224571863520493293247799005065324265472), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b01101100 #b00011001110010000110000))))
(check-sat)
(exit)
