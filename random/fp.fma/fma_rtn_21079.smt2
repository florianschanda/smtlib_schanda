(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10010100010101110111010)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10011101 #b11100110011110001000111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b11101100 #b00100001111011001011100)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x7610F65C)))
(check-sat)
(exit)
