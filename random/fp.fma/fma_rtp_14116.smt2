(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A00FAB2)))
;; x should be Float32(0x4A00FAB2 [Rational(4226393, 2), 2113196.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A6A0092)))
;; y should be Float32(0x4A6A0092 [Rational(7667785, 2), 3833892.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01111101010111000010101)))
;; z should be Float32(0x803EAE15 [Rational(-4107797, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10101001 #b11010111100101011100001))))
(check-sat)
(exit)
