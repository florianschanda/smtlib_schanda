(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE5CB4EB1)))
;; x should be Float32(0xE5CB4EB1 [Rational(-120011499531804004581376), -120011499531804004581376.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10010011000111101100001)))
;; y should be Float32(0xCAC98F61 [Rational(-13209441, 2), -6604720.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00010001 #b01010010111010011111110)))
;; z should be Float32(0x88A974FE [Rational(-5552767, 5444517870735015415413993718908291383296), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b11100010 #b01000000001001010100111))))
(check-sat)
(exit)
