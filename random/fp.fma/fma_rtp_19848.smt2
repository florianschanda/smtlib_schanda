(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEE900E17)))
;; x should be Float32(0xEE900E17 [Rational(-22291437495088700353327136768), -22291437495088700353327136768.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7FF8E0C6)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xFF800000)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
