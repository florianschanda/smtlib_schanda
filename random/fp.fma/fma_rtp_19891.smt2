(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80488B5C)))
;; x should be Float32(0x80488B5C [Rational(-1188567, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01110001001010010110001)))
;; y should be Float32(0x4AB894B1 [Rational(12096689, 2), 6048344.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10110011 #b10111111001011110101011)))
;; z should be Float32(0xD9DF97AB [Rational(-7866960062709760), -7866960062709760.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD9DF97AB))))
(check-sat)
(exit)
