(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A5A109A)))
;; x should be Float32(0x4A5A109A [Rational(7145549, 2), 3572774.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD9145A7B)))
;; y should be Float32(0xD9145A7B [Rational(-2609861305040896), -2609861305040896.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b11001101000110111010001)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
