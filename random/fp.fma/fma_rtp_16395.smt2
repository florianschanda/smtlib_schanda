(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110100 #b11011100111000110010010)))
;; x should be Float32(0x1A6E7192 [Rational(7813321, 158456325028528675187087900672), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x39C1FC4D)))
;; y should be Float32(0x39C1FC4D [Rational(12713037, 34359738368), 0.000370])

(declare-const z Float32)
(assert (= z (fp #b1 #b10001000 #b10010100100100001100111)))
;; z should be Float32(0xC44A4867 [Rational(-13256807, 16384), -809.131287])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10001000 #b10010100100100001100110)))
(check-sat)
(exit)
