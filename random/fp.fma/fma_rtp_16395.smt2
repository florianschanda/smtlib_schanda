(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01010101 #b11010101100111001101001)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x373241C4)))
(declare-const z Float32)
(assert (= z (fp #b1 #b01110110 #b01001011100111000011010)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b01110110 #b01001011100111000011001))))
(check-sat)
(exit)
