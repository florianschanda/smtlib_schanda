(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A91C59B)))
;; x should be Float32(0x4A91C59B [Rational(9553307, 2), 4776653.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD58109E1)))
;; y should be Float32(0xD58109E1 [Rational(-17734928695296), -17734928695296.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b11000001 #b00100101111010001100110))))
(check-sat)
(exit)
