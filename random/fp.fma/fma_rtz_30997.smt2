(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEFC4AD59)))
;; y should be Float32(0xEFC4AD59 [Rational(-121737252764787879431410548736), -121737252764787879431410548736.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x64D08AB7)))
;; z should be Float32(0x64D08AB7 [Rational(30775345801836470730752), 30775345801836470730752.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b11001001 #b10100001000101010110111)))
(check-sat)
(exit)
