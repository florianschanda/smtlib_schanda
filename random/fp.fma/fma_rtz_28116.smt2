(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A06A612)))
;; x should be Float32(0x4A06A612 [Rational(4412169, 2), 2206084.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9A25EECB)))
;; y should be Float32(0x9A25EECB [Rational(-10874571, 316912650057057350374175801344), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80800000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b01001001 #b01011101000110101000101)))
(check-sat)
(exit)
