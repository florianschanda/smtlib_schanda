(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11100001001110111100101)))
;; x should be Float32(0x4AF09DE5 [Rational(15769061, 2), 7884530.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9AD8DBBE)))
;; y should be Float32(0x9AD8DBBE [Rational(-7106015, 79228162514264337593543950336), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x2892375B)))
;; z should be Float32(0x2892375B [Rational(9582427, 590295810358705651712), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b01010001 #b00010111101100010111100)))
(check-sat)
(exit)
