(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AA5DC2F)))
;; x should be Float32(0x4AA5DC2F [Rational(10869807, 2), 5434903.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC81C3B60)))
;; y should be Float32(0xC81C3B60 [Rational(-319963, 2), -159981.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD34A7149))))
(check-sat)
(exit)
