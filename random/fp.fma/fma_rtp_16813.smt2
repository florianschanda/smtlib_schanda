(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11110100101110011010010)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01110001010011011001110)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xBD1D8843)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b01111010 #b00111011000100001000011))))
(check-sat)
(exit)
