(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD35ECBF2)))
;; x should be Float32(0xD35ECBF2 [Rational(-956904374272), -956904374272.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A8915F7)))
;; y should be Float32(0x4A8915F7 [Rational(8984055, 2), 4492027.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00110011 #b11101101000001101100100)))
;; z should be Float32(0x19F68364 [Rational(4038873, 158456325028528675187087900672), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xDE6E9C84))))
(check-sat)
(exit)
