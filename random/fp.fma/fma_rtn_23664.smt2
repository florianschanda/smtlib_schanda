(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01101010110111100011101)))
;; x should be Float32(0x00356F1D [Rational(3501853, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10011011110100001011100)))
;; y should be Float32(0xC9CDE85C [Rational(-3373591, 2), -1686795.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10111100000110001110101)))
;; z should be Float32(0x005E0C75 [Rational(6163573, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00010100 #b01010111110100111011101))))
(check-sat)
(exit)
