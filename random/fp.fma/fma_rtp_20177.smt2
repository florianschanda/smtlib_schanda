(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00111100111011101011000)))
;; x should be Float32(0x801E7758 [Rational(-249579, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xAC816E0A)))
;; y should be Float32(0xAC816E0A [Rational(-4241157, 1152921504606846976), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00001011010110100111011)))
;; z should be Float32(0x8005AD3B [Rational(-372027, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8005AD3A))))
(check-sat)
(exit)
