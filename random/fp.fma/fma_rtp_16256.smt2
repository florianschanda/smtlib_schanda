(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x22064DD6)))
;; x should be Float32(0x22064DD6 [Rational(4400875, 2417851639229258349412352), 0.000000])

(declare-const y Float32)
(assert (= y (_ +zero 8 24)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b11010100111000110001000)))
;; z should be Float32(0xC96A7188 [Rational(-1920561, 2), -960280.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010010 #b11010100111000110001000))))
(check-sat)
(exit)
