(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11110000 #b10000110101010000010110)))
;; x should be Float32(0x78435416 [Rational(15846927388133960026476334551138304), 15846927388133960026476334551138304.000000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11110101011100111100001)))
;; z should be Float32(0x4AFAB9E1 [Rational(16431585, 2), 8215792.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x4AFAB9E1)))
(check-sat)
(exit)
