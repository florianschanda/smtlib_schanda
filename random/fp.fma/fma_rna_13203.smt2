(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80041F09)))
;; x should be Float32(0x80041F09 [Rational(-270089, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010000 #b10010101011010101100000)))
;; y should be Float32(0xC84AB560 [Rational(-415147, 2), -207573.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11001011 #b01010000100011110101101)))
;; z should be Float32(0xE5A847AD [Rational(-99334969239387791949824), -99334969239387791949824.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11001011 #b01010000100011110101101)))
(check-sat)
(exit)
