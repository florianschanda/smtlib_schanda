(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4CB28FED)))
;; x should be Float32(0x4CB28FED [Rational(93618024), 93618024.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA99DE15)))
;; y should be Float32(0xCA99DE15 [Rational(-10083861, 2), -5041930.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10001000 #b00000100110101000010100)))
;; z should be Float32(0xC4026A14 [Rational(-2136709, 4096), -521.657471])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xD7D6A5D8)))
(check-sat)
(exit)
