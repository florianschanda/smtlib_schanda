(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00011111 #b11011100000111010101100)))
;; x should be Float32(0x8FEE0EAC [Rational(-3900331, 166153499473114484112975882535043072), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAA92355)))
;; y should be Float32(0xCAA92355 [Rational(-11084629, 2), -5542314.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01010101 #b01110001011111010010110)))
;; z should be Float32(0xAAB8BE96 [Rational(-6053707, 18446744073709551616), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b01010101 #b01110001011111010010110)))
(check-sat)
(exit)
