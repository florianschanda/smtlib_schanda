(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10111010 #b10111100011110110111001)))
;; x should be Float32(0xDD5E3DB9 [Rational(-1000884953728155648), -1000884953728155648.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC941466E)))
;; y should be Float32(0xC941466E [Rational(-6333239, 8), -791654.875000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x592CDD18)))
;; z should be Float32(0x592CDD18 [Rational(3041049446449152), 3041049446449152.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x6727C9AD))))
(check-sat)
(exit)
