(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x99CCB1BA)))
;; x should be Float32(0x99CCB1BA [Rational(-6707421, 316912650057057350374175801344), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80521C8E)))
;; y should be Float32(0x80521C8E [Rational(-2690631, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11000001101110100011100)))
;; z should be Float32(0x8060DD1C [Rational(-1587015, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b00000000 #b11000001101110100011011))))
(check-sat)
(exit)
