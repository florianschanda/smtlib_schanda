(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111100111000110001111)))
;; x should be Float32(0x807E718F [Rational(-8286607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1AB11AE4)))
;; y should be Float32(0x1AB11AE4 [Rational(2901689, 39614081257132168796771975168), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x667AF252)))
;; z should be Float32(0x667AF252 [Rational(296265410376490031251456), 296265410376490031251456.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x667AF251)))
(check-sat)
(exit)
