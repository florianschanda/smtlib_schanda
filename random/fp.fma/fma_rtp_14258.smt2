(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00000010100110101001110)))
;; x should be Float32(0x4A014D4E [Rational(4236967, 2), 2118483.500000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b10011001 #b11100110110100011100110)))
;; z should be Float32(0x4CF368E6 [Rational(127616816), 127616816.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10011001 #b11100110110100011100110))))
(check-sat)
(exit)
