(assert (= y (fp #b0 #b00000000 #b10001001101001000011000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00101001001111101010011)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x4A949F53)))
(check-sat)
(exit)
