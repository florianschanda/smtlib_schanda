(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11101011101010010011110)))
;; x should be Float32(0xCA75D49E [Rational(-8055375, 2), -4027687.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8061C488)))
;; y should be Float32(0x8061C488 [Rational(-800913, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11101010110011111111101)))
;; z should be Float32(0x4AF567FD [Rational(16082941, 2), 8041470.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10010101 #b11101010110011111111101)))
(check-sat)
(exit)
