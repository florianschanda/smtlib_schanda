(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11110110010011011111010)))
;; x should be Float32(0x007B26FA [Rational(4035453, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x001965A5)))
;; y should be Float32(0x001965A5 [Rational(1664421, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xA6432717)))
;; z should be Float32(0xA6432717 [Rational(-12789527, 18889465931478580854784), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b01001100 #b10000110010011100010110))))
(check-sat)
(exit)
