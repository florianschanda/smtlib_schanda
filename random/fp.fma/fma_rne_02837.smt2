(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010010 #b10011111001111111100111)))
;; x should be Float32(0x094F9FE7 [Rational(13606887, 5444517870735015415413993718908291383296), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10100001 #b00100011001000011101111)))
;; y should be Float32(0xD09190EF [Rational(-19537557504), -19537557504.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00110100 #b11011000001111000111011))))
(check-sat)
(exit)
