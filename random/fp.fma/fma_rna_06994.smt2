(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00110110 #b10100011000100100111100)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00110010001001100000001)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010101 #b00110010001001100000001))))
(check-sat)
(exit)
