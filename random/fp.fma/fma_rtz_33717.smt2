(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8040126E)))
;; x should be Float32(0x8040126E [Rational(-2099511, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10101010 #b11000100100111001101010)))
;; y should be Float32(0x55624E6A [Rational(15551650856960), 15551650856960.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00001010000011010100011)))
;; z should be Float32(0xCA8506A3 [Rational(-8717987, 2), -4358993.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010101 #b00001010000011010100011))))
(check-sat)
(exit)
