(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10011101 #b10100011011100101001110)))
;; y should be Float32(0x4ED1B94E [Rational(1759291136), 1759291136.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x41EFDF17)))
;; z should be Float32(0x41EFDF17 [Rational(15720215, 524288), 29.983931])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10000011 #b11011111101111100010111)))
(check-sat)
(exit)
