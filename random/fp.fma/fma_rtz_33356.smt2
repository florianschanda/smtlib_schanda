(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFA14BA35)))
;; x should be Float32(0xFA14BA35 [Rational(-193059165585511738932815025823285248), -193059165585511738932815025823285248.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01001010 #b00101000100011000110001)))
;; y should be Float32(0x25144631 [Rational(9717297, 75557863725914323419136), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAB4B233)))
;; z should be Float32(0xCAB4B233 [Rational(-11842099, 2), -5921049.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xDFAC48DC)))
(check-sat)
(exit)
