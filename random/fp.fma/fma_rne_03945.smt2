(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9E1C17C)))
;; x should be Float32(0xC9E1C17C [Rational(-3698783, 2), -1849391.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80122377)))
;; y should be Float32(0x80122377 [Rational(-1188727, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xFF800000)))
(check-sat)
(exit)
