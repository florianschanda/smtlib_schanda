(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01010100001101111110110)))
;; x should be Float32(0x002A1BF6 [Rational(1379835, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1DBB632F)))
;; y should be Float32(0x1DBB632F [Rational(12280623, 2475880078570760549798248448), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01111001 #b10011010110011010100000)))
;; z should be Float32(0xBCCD66A0 [Rational(-420661, 16777216), -0.025073])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xBCCD66A0)))
(check-sat)
(exit)
