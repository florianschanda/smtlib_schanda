(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAF21DA5)))
;; x should be Float32(0xCAF21DA5 [Rational(-15867301, 2), -7933650.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0005F742)))
;; y should be Float32(0x0005F742 [Rational(195489, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA37D21A)))
;; z should be Float32(0xCA37D21A [Rational(-6023437, 2), -3011718.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA37D21A))))
(check-sat)
(exit)
