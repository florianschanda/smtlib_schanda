(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0AF4F6C1)))
;; x should be Float32(0x0AF4F6C1 [Rational(16053953, 680564733841876926926749214863536422912), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111001 #b01110001100010110010011)))
;; y should be Float32(0x7CB8C593 [Rational(7675119225617738076646041472324337664), 7675119225617738076646041472324337664.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0024FAC3)))
;; z should be Float32(0x0024FAC3 [Rational(2423491, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10010000 #b01100001100111001101001)))
(check-sat)
(exit)
