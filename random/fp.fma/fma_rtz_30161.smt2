(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10111001 #b11011011011111000111100)))
;; x should be Float32(0x5CEDBE3C [Rational(535349875101925376), 535349875101925376.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAECE119)))
;; y should be Float32(0xCAECE119 [Rational(-15524121, 2), -7762060.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x3896A9B1)))
;; z should be Float32(0x3896A9B1 [Rational(9873841, 137438953472), 0.000072])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xE85BFC6A))))
(check-sat)
(exit)
