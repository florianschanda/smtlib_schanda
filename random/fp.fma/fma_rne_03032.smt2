(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10001101010100110001000)))
;; x should be Float32(0x0046A988 [Rational(578865, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x074FFD68)))
;; y should be Float32(0x074FFD68 [Rational(1703853, 10889035741470030830827987437816582766592), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAC60BF9)))
;; z should be Float32(0xCAC60BF9 [Rational(-12979193, 2), -6489596.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010101 #b10001100000101111111001))))
(check-sat)
(exit)
