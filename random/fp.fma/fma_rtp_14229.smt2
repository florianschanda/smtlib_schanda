(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4820D520)))
;; x should be Float32(0x4820D520 [Rational(329385, 2), 164692.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00001101 #b10011000100101010000110)))
;; y should be Float32(0x06CC4A86 [Rational(6694211, 87112285931760246646623899502532662132736), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x84A818B9)))
;; z should be Float32(0x84A818B9 [Rational(-11016377, 2787593149816327892691964784081045188247552), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x0F8058A5)))
(check-sat)
(exit)
