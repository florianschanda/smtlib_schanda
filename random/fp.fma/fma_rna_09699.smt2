(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3634DED8)))
;; x should be Float32(0x3634DED8 [Rational(1481691, 549755813888), 0.000003])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB5D12BF2)))
;; y should be Float32(0xB5D12BF2 [Rational(-6854137, 4398046511104), -0.000002])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00401FEB)))
;; z should be Float32(0x00401FEB [Rational(4202475, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xAC93C8FB)))
(check-sat)
(exit)
