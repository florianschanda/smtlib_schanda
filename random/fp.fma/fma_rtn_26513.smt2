(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA558EE9B)))
;; x should be Float32(0xA558EE9B [Rational(-14216859, 75557863725914323419136), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101101001001100100110)))
;; y should be Float32(0x00569326 [Rational(2836883, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10011000 #b11101101011010111000110)))
;; z should be Float32(0x4C76B5C6 [Rational(64673560), 64673560.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x4C76B5C5)))
(check-sat)
(exit)
