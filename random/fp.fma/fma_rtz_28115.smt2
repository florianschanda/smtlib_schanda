(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A94413D)))
;; x should be Float32(0x4A94413D [Rational(9716029, 2), 4858014.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00110000 #b01100110110001101011110)))
;; y should be Float32(0x9833635E [Rational(-5878191, 2535301200456458802993406410752), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xA34FC652)))
(check-sat)
(exit)
