(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCBAFDC5F)))
;; x should be Float32(0xCBAFDC5F [Rational(-23050430), -23050430.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80000000)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b11001000 #b11001011000011001111110)))
;; z should be Float32(0x6465867E [Rational(16935982305310540824576), 16935982305310540824576.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b11001000 #b11001011000011001111110)))
(check-sat)
(exit)
