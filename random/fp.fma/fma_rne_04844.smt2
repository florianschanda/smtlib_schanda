(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x6CF33D2C)))
;; y should be Float32(0x6CF33D2C [Rational(2352462801428393448255258624), 2352462801428393448255258624.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x805A75D7)))
;; z should be Float32(0x805A75D7 [Rational(-5928407, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b01011011 #b11100110011110100101010)))
(check-sat)
(exit)
