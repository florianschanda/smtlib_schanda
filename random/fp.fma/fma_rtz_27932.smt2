(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49238978)))
;; x should be Float32(0x49238978 [Rational(1339695, 2), 669847.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10111111 #b00000101011111101100000)))
;; y should be Float32(0x5F82BF60 [Rational(18842708997197266944), 18842708997197266944.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49C23E9C)))
;; z should be Float32(0x49C23E9C [Rational(3182503, 2), 1591251.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x69270C1F))))
(check-sat)
(exit)
