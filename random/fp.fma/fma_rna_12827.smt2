(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110111 #b01001001000101101101111)))
;; x should be Float32(0xDBA48B6F [Rational(-92630410076356608), -92630410076356608.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01010000001100110001111)))
;; y should be Float32(0xCAA8198F [Rational(-11016591, 2), -5508295.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AF65561)))
;; z should be Float32(0x4AF65561 [Rational(16143713, 2), 8071856.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b11001101 #b10110000001011111011101)))
(check-sat)
(exit)
