(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49836F4C)))
;; x should be Float32(0x49836F4C [Rational(2153427, 2), 1076713.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10100011011000100010011)))
;; y should be Float32(0x0051B113 [Rational(5353747, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b11100000101100000011011)))
;; z should be Float32(0xCAF0581B [Rational(-15751195, 2), -7875597.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10010101 #b11100000101100000011011)))
(check-sat)
(exit)
