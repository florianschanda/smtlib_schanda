(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b01011010001101001110100)))
;; x should be Float32(0x49AD1A74 [Rational(2836125, 2), 1418062.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01110011100001111010011)))
;; y should be Float32(0x0039C3D3 [Rational(3785683, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x2534EDEA)))
;; z should be Float32(0x2534EDEA [Rational(5928693, 37778931862957161709568), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x2534EDEA))))
(check-sat)
(exit)
