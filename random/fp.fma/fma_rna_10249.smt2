(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x07369C12)))
;; y should be Float32(0x07369C12 [Rational(5983753, 43556142965880123323311949751266331066368), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x38550E09)))
;; z should be Float32(0x38550E09 [Rational(13962761, 274877906944), 0.000051])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b01110000 #b10101010000111000001001))))
(check-sat)
(exit)
