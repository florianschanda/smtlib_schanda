(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A3F934E)))
;; x should be Float32(0x4A3F934E [Rational(6277543, 2), 3138771.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x004DC0A2)))
;; y should be Float32(0x004DC0A2 [Rational(2547793, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01010111000010001100111)))
;; z should be Float32(0x4AAB8467 [Rational(11240551, 2), 5620275.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010101 #b01010111000010001100111))))
(check-sat)
(exit)
