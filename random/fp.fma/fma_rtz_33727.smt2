(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11011000000010101100110)))
;; x should be Float32(0x806C0566 [Rational(-3539635, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00323E48)))
;; y should be Float32(0x00323E48 [Rational(411593, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10001111000101100010011)))
;; z should be Float32(0x4AC78B13 [Rational(13077267, 2), 6538633.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AC78B12))))
(check-sat)
(exit)
