(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AAF7A23)))
;; x should be Float32(0x4AAF7A23 [Rational(11500067, 2), 5750033.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC938B368)))
;; y should be Float32(0xC938B368 [Rational(-1513069, 2), -756534.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00100001 #b00101010110011001000111)))
;; z should be Float32(0x90956647 [Rational(-9791047, 166153499473114484112975882535043072), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10101000 #b11111010011010110000110))))
(check-sat)
(exit)
