(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC7B4ACC0)))
;; x should be Float32(0xC7B4ACC0 [Rational(-185011, 2), -92505.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC888E2F0)))
;; y should be Float32(0xC888E2F0 [Rational(-560687, 2), -280343.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xD224FE78)))
;; z should be Float32(0xD224FE78 [Rational(-177160978432), -177160978432.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b10100100 #b00011001101011101111100)))
(check-sat)
(exit)
