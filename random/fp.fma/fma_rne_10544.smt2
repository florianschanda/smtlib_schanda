(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011100 #b11100110001100001011000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10110100 #b00111111100011101000100)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11001000011000010110001)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x6917B957)))
(check-sat)
(exit)
