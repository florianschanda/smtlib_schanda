(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001000 #b10011110011001001011110)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b11111101001111010111011)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
