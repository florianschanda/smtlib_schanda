(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11011001 #b10111110101010101011010)))
(declare-const y Float32)
(assert (= y (fp #b0 #b01000000 #b01000011101010110010000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11010100 #b00010010001000011011110)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xEA0910DD)))
(check-sat)
(exit)
