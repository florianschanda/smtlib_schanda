(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010011 #b11000110111000111111010)))
;; x should be Float32(0xE9E371FA [Rational(-34370550018887939994419200), -34370550018887939994419200.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9F6B35C8)))
;; y should be Float32(0x9F6B35C8 [Rational(-1926841, 38685626227668133590597632), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xB511A353)))
;; z should be Float32(0xB511A353 [Rational(-9544531, 17592186044416), -0.000001])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x49D0F968))))
(check-sat)
(exit)
