(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2847231D)))
;; x should be Float32(0x2847231D [Rational(13050653, 1180591620717411303424), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10111110110101001000101)))
;; y should be Float32(0x805F6A45 [Rational(-6253125, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA89C875)))
;; z should be Float32(0xCA89C875 [Rational(-9029749, 2), -4514874.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010101 #b00010011100100001110101))))
(check-sat)
(exit)
