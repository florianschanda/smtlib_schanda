(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AAFEFDD)))
;; x should be Float32(0x4AAFEFDD [Rational(11530205, 2), 5765102.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC3CCC0C6)))
;; y should be Float32(0xC3CCC0C6 [Rational(-6709347, 16384), -409.506042])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x801EFC9E)))
;; z should be Float32(0x801EFC9E [Rational(-1015375, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCF0CB7A1))))
(check-sat)
(exit)
