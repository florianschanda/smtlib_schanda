(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x269AE3B7)))
;; y should be Float32(0x269AE3B7 [Rational(10150839, 9444732965739290427392), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11000001 #b01110011100011000100001)))
;; z should be Float32(0xE0B9C621 [Rational(-107091385810312757248), -107091385810312757248.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11000001 #b01110011100011000100001)))
(check-sat)
(exit)
