(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x139F1278)))
;; x should be Float32(0x139F1278 [Rational(1303119, 324518553658426726783156020576256), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010000 #b11100011010011001100000)))
;; y should be Float32(0x4871A660 [Rational(494899, 2), 247449.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x5E9E35C1)))
;; z should be Float32(0x5E9E35C1 [Rational(5700115118751219712), 5700115118751219712.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10111101 #b00111100011010111000010)))
(check-sat)
(exit)
