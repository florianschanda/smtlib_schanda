(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100000101110101011000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b01010101 #b00100001111100011101100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00110101001110010110001)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA9A9CB0))))
(check-sat)
(exit)
