(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802A8680)))
;; x should be Float32(0x802A8680 [Rational(-21773, 5575186299632655785383929568162090376495104), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFA9EF956)))
;; y should be Float32(0xFA9EF956 [Rational(-412720018630894128988207165183361024), -412720018630894128988207165183361024.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA00C82E)))
;; z should be Float32(0xCA00C82E [Rational(-4219927, 2), -2109963.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA00C82D))))
(check-sat)
(exit)
