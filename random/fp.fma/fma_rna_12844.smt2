(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8FCFBD13)))
;; x should be Float32(0x8FCFBD13 [Rational(-13614355, 664613997892457936451903530140172288), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00100111101111001100110)))
;; y should be Float32(0xCA13DE66 [Rational(-4845363, 2), -2422681.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b00101001101110110100101)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
