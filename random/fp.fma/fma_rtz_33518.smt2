(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC908D1A6)))
;; x should be Float32(0xC908D1A6 [Rational(-4483283, 8), -560410.375000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE3EBFEDF)))
;; y should be Float32(0xE3EBFEDF [Rational(-8706700510254369603584), -8706700510254369603584.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b11001100011011100110100)))
;; z should be Float32(0x49E63734 [Rational(3771853, 2), 1885926.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11011010 #b11111000100000101010101))))
(check-sat)
(exit)
