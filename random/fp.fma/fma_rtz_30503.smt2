(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00001001111010001101010)))
;; x should be Float32(0x0004F46A [Rational(162357, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b11100100 #b00010010110010000011001)))
;; z should be Float32(0x72096419 [Rational(2721309247145459780729341739008), 2721309247145459780729341739008.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x72096419)))
(check-sat)
(exit)
