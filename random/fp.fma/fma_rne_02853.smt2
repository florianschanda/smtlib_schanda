(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x05E1F6BE)))
;; x should be Float32(0x05E1F6BE [Rational(7404383, 348449143727040986586495598010130648530944), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01101111011111001011100)))
;; y should be Float32(0x8037BE5C [Rational(-913303, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (_ +oo 8 24)))
;; z should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
