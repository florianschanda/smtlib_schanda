(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000110 #b11010010101110110110100)))
;; x should be Float32(0x03695DB4 [Rational(3823469, 5575186299632655785383929568162090376495104), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01001011001110000001110)))
;; y should be Float32(0xCA259C0E [Rational(-5426695, 2), -2713347.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA9DE817)))
;; z should be Float32(0xCA9DE817 [Rational(-10348567, 2), -5174283.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10010101 #b00111011110100000010111)))
(check-sat)
(exit)
