(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A95DB3F)))
;; y should be Float32(0x4A95DB3F [Rational(9820991, 2), 4910495.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11110010101111101101000)))
;; z should be Float32(0x80795F68 [Rational(-994285, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00010111 #b00101011101101101000000))))
(check-sat)
(exit)
