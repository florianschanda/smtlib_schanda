(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b11000011001100100010110)))
;; x should be Float32(0x4A619916 [Rational(7392395, 2), 3696197.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001110 #b10011110101111001100001)))
;; y should be Float32(0x474F5E61 [Rational(13590113, 256), 53086.378906])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10100100 #b01101101011110111110100)))
(check-sat)
(exit)
