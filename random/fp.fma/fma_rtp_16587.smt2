(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01101100 #b00000100110110110100111)))
;; x should be Float32(0x36026DA7 [Rational(8547751, 4398046511104), 0.000002])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00110011110011111000001)))
;; y should be Float32(0x8019E7C1 [Rational(-1697729, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
