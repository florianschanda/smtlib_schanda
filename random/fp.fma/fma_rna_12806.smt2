(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xAAF40247)))
;; x should be Float32(0xAAF40247 [Rational(-15991367, 36893488147419103232), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01100010111101100000011)))
;; y should be Float32(0x00317B03 [Rational(3242755, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b01001110001011100010010)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
