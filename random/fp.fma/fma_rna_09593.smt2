(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1AC63ABA)))
;; x should be Float32(0x1AC63ABA [Rational(6495581, 79228162514264337593543950336), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA3C5CC6)))
;; y should be Float32(0xCA3C5CC6 [Rational(-6172259, 2), -3086129.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x85FA4D0C)))
;; z should be Float32(0x85FA4D0C [Rational(-4100931, 174224571863520493293247799005065324265472), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xA591DAF7)))
(check-sat)
(exit)
