(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10010000111110001100011)))
;; x should be Float32(0xCAC87C63 [Rational(-13139043, 2), -6569521.500000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010010 #b00001110101000111101000)))
;; z should be Float32(0x490751E8 [Rational(1108541, 2), 554270.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10010010 #b00001110101000111101000))))
(check-sat)
(exit)
