(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11001100010001000110100)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11110100 #b01011100001000011010000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b01110110 #b00010101110001111101000)))
(check-sat)
(exit)
