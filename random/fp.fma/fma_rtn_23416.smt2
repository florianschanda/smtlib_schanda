(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1DD9E7A0)))
;; x should be Float32(0x1DD9E7A0 [Rational(446269, 77371252455336267181195264), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11101100 #b01010111100010001100011)))
;; y should be Float32(0xF62BC463 [Rational(-870962850273300941456279584374784), -870962850273300941456279584374784.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00101011 #b10100001000111101010100)))
;; z should be Float32(0x15D08F54 [Rational(3417045, 40564819207303340847894502572032), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10101001 #b00100100011010011100010)))
(check-sat)
(exit)
