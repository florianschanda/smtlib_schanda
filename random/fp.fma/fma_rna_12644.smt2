(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11000001 #b01101100110101101101000)))
;; x should be Float32(0xE0B66B68 [Rational(-105157714292961705984), -105157714292961705984.000000])

(declare-const y Float32)
(assert (= y (_ +zero 8 24)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10110010011000111011100)))
;; z should be Float32(0x005931DC [Rational(1461367, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b00000000 #b10110010011000111011100))))
(check-sat)
(exit)
