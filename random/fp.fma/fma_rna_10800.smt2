(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCADB2C89)))
;; x should be Float32(0xCADB2C89 [Rational(-14363785, 2), -7181892.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00001101010111010100101)))
;; y should be Float32(0x8006AEA5 [Rational(-437925, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00010100 #b01010000011111001111100)))
;; z should be Float32(0x0A283E7C [Rational(2756511, 340282366920938463463374607431768211456), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0A81E3C4))))
(check-sat)
(exit)
