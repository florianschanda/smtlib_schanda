(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49EFDDBC)))
;; x should be Float32(0x49EFDDBC [Rational(3929967, 2), 1964983.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10001001001110011111010)))
;; y should be Float32(0x80449CFA [Rational(-2248317, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000110 #b10110010011001010000110)))
;; z should be Float32(0x83593286 [Rational(-7117123, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00010101 #b00000001001010110101001))))
(check-sat)
(exit)
