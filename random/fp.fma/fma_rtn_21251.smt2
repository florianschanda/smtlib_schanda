(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b00001000011101111000100)))
;; x should be Float32(0x49843BC4 [Rational(2166513, 2), 1083256.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x97FD3367)))
;; y should be Float32(0x97FD3367 [Rational(-16593767, 10141204801825835211973625643008), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x000DBE72)))
;; z should be Float32(0x000DBE72 [Rational(450361, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xA202C99E))))
(check-sat)
(exit)
