(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00198172)))
;; x should be Float32(0x00198172 [Rational(835769, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAFABEED)))
;; y should be Float32(0xCAFABEED [Rational(-16432877, 2), -8216438.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x806C61F5)))
;; z should be Float32(0x806C61F5 [Rational(-7102965, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8AC7DB7F))))
(check-sat)
(exit)
