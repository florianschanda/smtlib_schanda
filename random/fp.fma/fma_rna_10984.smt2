(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b00000000000000000000000)))
;; x should be Float32(-oo)

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101110110101111110000)))
;; y should be Float32(0x00576BF0 [Rational(358079, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010010 #b00111000010011000011000)))
;; z should be Float32(0x491C2618 [Rational(1279171, 2), 639585.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
