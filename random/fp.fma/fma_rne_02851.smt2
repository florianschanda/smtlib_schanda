(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x781E1A0A)))
;; x should be Float32(0x781E1A0A [Rational(12826734977809732052847140374839296), 12826734977809732052847140374839296.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805E63C0)))
;; y should be Float32(0x805E63C0 [Rational(-96655, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xB8E92CA9))))
(check-sat)
(exit)
