(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AD8CE33)))
;; x should be Float32(0x4AD8CE33 [Rational(14208563, 2), 7104281.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49716E08)))
;; y should be Float32(0x49716E08 [Rational(1977793, 2), 988896.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11101000 #b10101001011110101010111)))
;; z should be Float32(0xF454BD57 [Rational(-67419873185706321042234002112512), -67419873185706321042234002112512.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xF454BD57))))
(check-sat)
(exit)
