(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AD81EBD)))
;; x should be Float32(0x4AD81EBD [Rational(14163645, 2), 7081822.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805C7AD4)))
;; y should be Float32(0x805C7AD4 [Rational(-1515189, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC94B82B8)))
;; z should be Float32(0xC94B82B8 [Rational(-1667159, 2), -833579.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10010010 #b10010111000001010111000)))
(check-sat)
(exit)
