(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010010 #b00011010010110101101111)))
;; x should be Float32(0xE90D2D6F [Rational(-10667068415374001839276032), -10667068415374001839276032.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11001000001101000001011)))
;; y should be Float32(0xCAE41A0B [Rational(-14948875, 2), -7474437.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x747B95A7))))
(check-sat)
(exit)
