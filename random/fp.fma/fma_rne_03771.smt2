(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA5148A2)))
;; x should be Float32(0xCA5148A2 [Rational(-6857809, 2), -3428904.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01011100101110011010001)))
;; y should be Float32(0x002E5CD1 [Rational(3038417, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00345BEC)))
;; z should be Float32(0x00345BEC [Rational(857851, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8A979BB5))))
(check-sat)
(exit)
