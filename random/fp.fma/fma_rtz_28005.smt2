(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10010100111101111101011)))
;; x should be Float32(0x4ACA7BEB [Rational(13269995, 2), 6634997.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10100001000011101110100)))
;; y should be Float32(0xC9D08774 [Rational(-3416541, 2), -1708270.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8022719F)))
;; z should be Float32(0x8022719F [Rational(-2257311, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD524EFD2))))
(check-sat)
(exit)
