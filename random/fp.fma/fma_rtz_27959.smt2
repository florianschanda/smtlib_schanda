(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x47BE5AC0)))
;; x should be Float32(0x47BE5AC0 [Rational(194923, 2), 97461.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10011100101101001111000)))
;; y should be Float32(0x004E5A78 [Rational(641871, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00010000 #b11010010000101110101110))))
(check-sat)
(exit)
