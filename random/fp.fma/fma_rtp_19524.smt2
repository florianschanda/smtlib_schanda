(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101000 #b10010010101011110000100)))
;; x should be Float32(0xB4495784 [Rational(-3298785, 17592186044416), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AD1DB59)))
;; y should be Float32(0x4AD1DB59 [Rational(13753177, 2), 6876588.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b11011001101110001100010)))
;; z should be Float32(0xCA6CDC62 [Rational(-7761457, 2), -3880728.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010100 #b11011001101110001100111))))
(check-sat)
(exit)
