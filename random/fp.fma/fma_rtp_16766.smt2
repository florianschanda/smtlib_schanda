(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00100011001001111000100)))
;; x should be Float32(0x001193C4 [Rational(287985, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11110101110011101010110)))
;; y should be Float32(0x007AE756 [Rational(4027307, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x160851D5)))
;; z should be Float32(0x160851D5 [Rational(8933845, 81129638414606681695789005144064), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00101100 #b00010000101000111010110)))
(check-sat)
(exit)
