(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0007078E)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101010001101101011010)))
(declare-const z Float32)
(assert (= z (fp #b0 #b11110101 #b00110110000010111010110)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b11110101 #b00110110000010111010111)))
(check-sat)
(exit)
