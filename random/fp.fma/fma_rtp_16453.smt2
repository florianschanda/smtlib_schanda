(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6201A796)))
;; x should be Float32(0x6201A796 [Rational(597926456239843180544), 597926456239843180544.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11000000111100011101100)))
;; y should be Float32(0xC9E078EC [Rational(-3677755, 2), -1838877.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01110101000011010101000)))
;; z should be Float32(0x803A86A8 [Rational(-479445, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11011000 #b11000110101111111000010)))
(check-sat)
(exit)
