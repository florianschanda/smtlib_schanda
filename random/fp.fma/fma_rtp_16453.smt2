(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5212ACFB)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11010011111011110010101)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11011000101010100100100)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10111011 #b00001100000110101001010)))
(check-sat)
(exit)
