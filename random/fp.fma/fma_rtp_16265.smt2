(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110100 #b11011110001111100010101)))
;; x should be Float32(0x1A6F1F15 [Rational(15671061, 316912650057057350374175801344), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10110110110011100100011)))
;; y should be Float32(0x4ADB6723 [Rational(14378787, 2), 7189393.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01001011 #b10011001110111111101110))))
(check-sat)
(exit)
