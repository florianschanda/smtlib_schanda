(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFF91A7DC)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10100011100101101100011)))
;; y should be Float32(0x0051CB63 [Rational(5360483, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807B6C0B)))
;; z should be Float32(0x807B6C0B [Rational(-8088587, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
