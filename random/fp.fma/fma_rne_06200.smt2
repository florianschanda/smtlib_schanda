(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00101000000111111011110)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11111110 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b01111110 #b01000000111111011101111))))
(check-sat)
(exit)
