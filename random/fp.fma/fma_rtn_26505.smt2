(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD233B9AB)))
;; x should be Float32(0xD233B9AB [Rational(-192978534400), -192978534400.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11000110 #b10001111110001111011100)))
;; y should be Float32(0x6347E3DC [Rational(3687321069009686757376), 3687321069009686757376.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
