(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110011 #b11001010110001101111110)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805D8122)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10100001001000010000101)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00110101 #b01001111001000110110101)))
(check-sat)
(exit)
