(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0032F4F4)))
;; x should be Float32(0x0032F4F4 [Rational(834877, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01010110100001101101010)))
;; y should be Float32(0xCA2B436A [Rational(-5611957, 2), -2805978.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC9D01AA4)))
;; z should be Float32(0xC9D01AA4 [Rational(-3409577, 2), -1704788.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC9D01AA4))))
(check-sat)
(exit)
