(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AA2D87B)))
;; x should be Float32(0x4AA2D87B [Rational(10672251, 2), 5336125.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x804FFDCD)))
;; y should be Float32(0x804FFDCD [Rational(-5242317, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xD2FA077D)))
;; z should be Float32(0xD2FA077D [Rational(-536933728256), -536933728256.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10100101 #b11110100000011101111101)))
(check-sat)
(exit)
