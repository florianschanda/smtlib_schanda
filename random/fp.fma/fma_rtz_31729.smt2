(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -oo 8 24)))
;; x should be Float32(-oo)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x863F9E6B)))
;; y should be Float32(0x863F9E6B [Rational(-12557931, 348449143727040986586495598010130648530944), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10111010100011110001001)))
;; z should be Float32(0x805D4789 [Rational(-6113161, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x7F800000))))
(check-sat)
(exit)
