(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x004D3310)))
;; x should be Float32(0x004D3310 [Rational(316209, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11100111000110000011101)))
;; y should be Float32(0x00738C1D [Rational(7572509, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AF876A1)))
;; z should be Float32(0x4AF876A1 [Rational(16283297, 2), 8141648.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010101 #b11110000111011010100010))))
(check-sat)
(exit)
