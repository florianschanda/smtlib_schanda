(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010110 #b10011001001101111101111)))
;; x should be Float32(0xCB4C9BEF [Rational(-13409263), -13409263.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAEDE8F5)))
;; y should be Float32(0xCAEDE8F5 [Rational(-15591669, 2), -7795834.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11010111000011001001110)))
;; z should be Float32(0x006B864E [Rational(3523367, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x56BE268D))))
(check-sat)
(exit)
