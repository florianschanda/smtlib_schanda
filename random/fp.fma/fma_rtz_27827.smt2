(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A8F11CB)))
;; x should be Float32(0x4A8F11CB [Rational(9376203, 2), 4688101.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00010011111111000100010)))
;; y should be Float32(0x4A09FE22 [Rational(4521745, 2), 2260872.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b11000111001001111010100)))
;; z should be Float32(0xC9E393D4 [Rational(-3728629, 2), -1864314.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x551A3D16))))
(check-sat)
(exit)
