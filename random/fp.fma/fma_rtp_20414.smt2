(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ NaN 8 24)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11011001101011001010011)))
;; y should be Float32(0xCAECD653 [Rational(-15521363, 2), -7760681.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x74959889)))
;; z should be Float32(0x74959889 [Rational(94817628471644143648649200533504), 94817628471644143648649200533504.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
