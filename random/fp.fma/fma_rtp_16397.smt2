(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111011 #b10100010000100101111111)))
;; x should be Float32(0x1DD1097F [Rational(13699455, 2475880078570760549798248448), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x5D149848)))
;; y should be Float32(0x5D149848 [Rational(669211704931909632), 669211704931909632.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xFF937AEE)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
