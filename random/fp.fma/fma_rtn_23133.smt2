(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000111 #b00100100000010100101001)))
;; x should be Float32(0x03920529 [Rational(9569577, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10001010100010110010010)))
;; y should be Float32(0x4A454592 [Rational(6464201, 2), 3232100.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b00011100 #b11000010000101101001110))))
(check-sat)
(exit)
