(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x48AA73D0)))
;; x should be Float32(0x48AA73D0 [Rational(698173, 2), 349086.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8018A4E4)))
;; y should be Float32(0x8018A4E4 [Rational(-403769, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xB253692A)))
;; z should be Float32(0xB253692A [Rational(-6927509, 562949953421312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b01100100 #b10100110110100100101010)))
(check-sat)
(exit)
