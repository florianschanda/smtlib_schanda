(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01001011010110011001000)))
;; x should be Float32(0x0025ACC8 [Rational(308633, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9AF06A30)))
;; y should be Float32(0x9AF06A30 [Rational(-984739, 9903520314283042199192993792), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8056DDA0)))
;; z should be Float32(0x8056DDA0 [Rational(-177901, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00000000 #b10101101101110110100000))))
(check-sat)
(exit)
