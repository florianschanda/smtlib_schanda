(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11100111 #b01010110111101010110000)))
;; x should be Float32(0x73AB7AB0 [Rational(27171971464163316003230374166528), 27171971464163316003230374166528.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00101001 #b11111000011001010100111)))
;; y should be Float32(0x94FC32A7 [Rational(-16528039, 649037107316853453566312041152512), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xC928EEB3)))
(check-sat)
(exit)
