(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AAFC289)))
;; x should be Float32(0x4AAFC289 [Rational(11518601, 2), 5759300.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10111101101101010110101)))
;; y should be Float32(0xCADEDAB5 [Rational(-14604981, 2), -7302490.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A240102)))
;; z should be Float32(0x4A240102 [Rational(5374081, 2), 2687040.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xD61900DA)))
(check-sat)
(exit)
