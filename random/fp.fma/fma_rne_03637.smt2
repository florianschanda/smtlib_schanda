(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11001111101000011100011)))
;; x should be Float32(0xCAE7D0E3 [Rational(-15192291, 2), -7596145.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b10101000010110110100100)))
;; y should be Float32(0x49D42DA4 [Rational(3476329, 2), 1738164.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x38A6A7F8)))
;; z should be Float32(0x38A6A7F8 [Rational(1365247, 17179869184), 0.000079])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD5402250))))
(check-sat)
(exit)
