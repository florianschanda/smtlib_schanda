(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x18E840A7)))
;; x should be Float32(0x18E840A7 [Rational(15220903, 2535301200456458802993406410752), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC53E593A)))
;; y should be Float32(0xC53E593A [Rational(-6237341, 2048), -3045.576660])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b00111101 #b01011001011000011101111)))
(check-sat)
(exit)
