(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01001000100010001010011)))
;; x should be Float32(0x4AA44453 [Rational(10765395, 2), 5382697.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCACA88E9)))
;; y should be Float32(0xCACA88E9 [Rational(-13273321, 2), -6636660.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b01000100101010111001010)))
;; z should be Float32(0x4A2255CA [Rational(5319397, 2), 2659698.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10101100 #b00000011111010111000010))))
(check-sat)
(exit)
