(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001101 #b01001111111000100000101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01001111 #b01001111111000100000101))))
(check-sat)
(exit)
