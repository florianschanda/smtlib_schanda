(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x63BA7751)))
;; x should be Float32(0x63BA7751 [Rational(6879384101747207176192), 6879384101747207176192.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010100 #b10001100101110010011011)))
;; y should be Float32(0xAA465C9B [Rational(-12999835, 73786976294838206464), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10011101 #b00100000111101110111100)))
(check-sat)
(exit)
