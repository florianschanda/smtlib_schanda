(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001110 #b01101110110011100000101)))
;; x should be Float32(0x67376705 [Rational(866093441292707347562496), 866093441292707347562496.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80000000)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11001001111010101101010)))
;; z should be Float32(0x8064F56A [Rational(-3308213, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8064F56A))))
(check-sat)
(exit)
