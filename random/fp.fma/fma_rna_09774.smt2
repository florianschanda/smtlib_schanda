(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x006B8D32)))
;; x should be Float32(0x006B8D32 [Rational(3524249, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00000000100111000111010)))
;; y should be Float32(0x4A004E3A [Rational(4204317, 2), 2102158.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01110100 #b11111011000101101110010)))
;; z should be Float32(0x3A7D8B72 [Rational(8308153, 8589934592), 0.000967])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x3A7D8B72)))
(check-sat)
(exit)
