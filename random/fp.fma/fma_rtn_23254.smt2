(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5EB09BEC)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10000000 #b10010110000001000101000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11010100 #b01001100110110101000011)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b11010100 #b01001100110110100111111))))
(check-sat)
(exit)
