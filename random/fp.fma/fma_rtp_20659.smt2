(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10000100100111100001101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b01110000 #b10010100010001101110011)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00111101010100111010010)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x001EAAA4)))
(check-sat)
(exit)
