(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11000100 #b01010001011101100011100)))
;; x should be Float32(0x6228BB1C [Rational(778133913941911273472), 778133913941911273472.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b11100011000010011010011)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x18769FF0)))
;; z should be Float32(0x18769FF0 [Rational(1010175, 316912650057057350374175801344), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
