(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x20D22294)))
;; x should be Float32(0x20D22294 [Rational(3442853, 9671406556917033397649408), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11110010 #b11011100110101110011101)))
;; y should be Float32(0xF96E6B9D [Rational(-77371831811274652739163916790136832), -77371831811274652739163916790136832.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xDAC3B47A)))
(check-sat)
(exit)
