(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10110110100010000100001)))
;; x should be Float32(0xCADB4421 [Rational(-14369825, 2), -7184912.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A19DA6E)))
;; y should be Float32(0x4A19DA6E [Rational(5041463, 2), 2520731.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8074567E)))
;; z should be Float32(0x8074567E [Rational(-3812159, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD583C6CE))))
(check-sat)
(exit)
