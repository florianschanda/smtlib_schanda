(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00100100 #b00111110111001111101111)))
;; x should be Float32(0x121F73EF [Rational(10449903, 20769187434139310514121985316880384), 0.000000])

(declare-const y Float32)
(assert (= y (_ +oo 8 24)))
;; y should be Float32(+oo)

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01100001100000101101110)))
;; z should be Float32(0x0030C16E [Rational(1597623, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
