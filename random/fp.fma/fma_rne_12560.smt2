(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11000000 #b00000100001111010100100)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00101011 #b00000100001111010100100)))
(check-sat)
(exit)
