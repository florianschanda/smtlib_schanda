(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001C4075)))
;; x should be Float32(0x001C4075 [Rational(1851509, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01010101101110001101110)))
;; y should be Float32(0x4A2ADC6E [Rational(5598775, 2), 2799387.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10001001100011110011100)))
;; z should be Float32(0x0044C79C [Rational(1126887, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b00010100 #b00101101101100100010010))))
(check-sat)
(exit)
