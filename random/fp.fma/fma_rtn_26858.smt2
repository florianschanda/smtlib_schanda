(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11001100001011100110000)))
;; x should be Float32(0x80661730 [Rational(-418163, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001011 #b00110010000111110111010)))
;; y should be Float32(0x45990FBA [Rational(5015517, 1024), 4897.965820])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b01101011001110101111100)))
;; z should be Float32(0xC9B59D7C [Rational(-2975583, 2), -1487791.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10010011 #b01101011001110101111101)))
(check-sat)
(exit)
