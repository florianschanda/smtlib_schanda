(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01001011 #b10111001101000000101011)))
;; x should be Float32(0x25DCD02B [Rational(14471211, 37778931862957161709568), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10100001000010011111001)))
;; y should be Float32(0xCAD084F9 [Rational(-13665529, 2), -6832764.500000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xB133DBD5))))
(check-sat)
(exit)
