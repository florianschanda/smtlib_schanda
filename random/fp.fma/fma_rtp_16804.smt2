(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01101000101110100000111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01001110010001111001011)))
(declare-const z Float32)
(assert (= z (fp #b0 #b11000110 #b01001100110101011011111)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x63266ADF)))
(check-sat)
(exit)
