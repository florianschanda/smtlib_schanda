(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101000 #b01000000000101101100000)))
;; x should be Float32(0x94200B60 [Rational(-327771, 40564819207303340847894502572032), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10110101101101010101100)))
;; y should be Float32(0x005ADAAC [Rational(1488555, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8019518A)))
;; z should be Float32(0x8019518A [Rational(-829637, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b00000000 #b00110010101000110001010))))
(check-sat)
(exit)
