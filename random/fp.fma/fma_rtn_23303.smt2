(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x37D457B0)))
;; x should be Float32(0x37D457B0 [Rational(869755, 34359738368), 0.000025])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11110011011001110100010)))
;; y should be Float32(0xCA79B3A2 [Rational(-8182225, 2), -4091112.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x006EC9D8)))
;; z should be Float32(0x006EC9D8 [Rational(907579, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10000101 #b10011110001111001001010))))
(check-sat)
(exit)
