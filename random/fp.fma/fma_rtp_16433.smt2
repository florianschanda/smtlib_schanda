(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2DAA274D)))
;; x should be Float32(0x2DAA274D [Rational(11151181, 576460752303423488), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000000)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z (fp #b1 #b10110010 #b10011101010000000000101)))
;; z should be Float32(0xD94EA005 [Rational(-3634986783604736), -3634986783604736.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10110010 #b10011101010000000000101))))
(check-sat)
(exit)
