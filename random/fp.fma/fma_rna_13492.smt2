(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111111 #b11000010101110011110011)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b11111010 #b10110001101001001100011)))
;; y should be Float32(0x7D58D263 [Rational(18012852336774056491536606251722997760), 18012852336774056491536606251722997760.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b11001000110101000001010)))
;; z should be Float32(0x4A646A0A [Rational(7484677, 2), 3742338.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
