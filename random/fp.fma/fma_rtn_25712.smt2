(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00111011111011110101010)))
(declare-const y Float32)
(assert (= y (fp #b1 #b01011010 #b01110001101001110101110)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00000001 #b00000000000000000000000))))
(check-sat)
(exit)
