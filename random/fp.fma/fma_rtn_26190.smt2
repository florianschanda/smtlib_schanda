(assert (= y (fp #b1 #b10010100 #b00000100000010101010010)))
(declare-const z Float32)
(assert (= z (fp #b0 #b11001011 #b11110011011011110001110)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b11001011 #b11110011011011110001110)))
(check-sat)
(exit)
