(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA36A626)))
;; x should be Float32(0xCA36A626 [Rational(-5985043, 2), -2992521.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0494133E)))
;; y should be Float32(0x0494133E [Rational(4852127, 1393796574908163946345982392040522594123776), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01010111 #b11110000011110010111110)))
;; z should be Float32(0xABF83CBE [Rational(-8134239, 4611686018427387904), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xABF83CBE))))
(check-sat)
(exit)
