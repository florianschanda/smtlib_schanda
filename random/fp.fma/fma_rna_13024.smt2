(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10010000001101101101110)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b01001101000100111110000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11000111001101001010010)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x893BA25D)))
(check-sat)
(exit)
