(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00001000010101000100010)))
;; x should be Float32(0x80042A22 [Rational(-136465, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10000100010110011110110)))
;; y should be Float32(0x4A422CF6 [Rational(6362747, 2), 3181373.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x002C1D44)))
;; z should be Float32(0x002C1D44 [Rational(722769, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x88CA2A14))))
(check-sat)
(exit)
