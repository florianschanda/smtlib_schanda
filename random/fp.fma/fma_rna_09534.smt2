(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x18E1B36B)))
;; x should be Float32(0x18E1B36B [Rational(14791531, 2535301200456458802993406410752), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11100001 #b11101001010010010111111)))
;; y should be Float32(0x70F4A4BF [Rational(605708065622585792445873651712), 605708065622585792445873651712.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80800000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x4A57B041)))
(check-sat)
(exit)
