(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10000001110011000111100)))
;; y should be Float32(0x8040E63C [Rational(-1063311, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4ACBCC6B)))
;; z should be Float32(0x4ACBCC6B [Rational(13356139, 2), 6678069.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4ACBCC6A))))
(check-sat)
(exit)
