(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB9FDD5)))
;; x should be Float32(0x4AB9FDD5 [Rational(12189141, 2), 6094570.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80061B07)))
;; y should be Float32(0x80061B07 [Rational(-400135, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x898DF2BB))))
(check-sat)
(exit)
