(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10101001 #b00100001010111101111100)))
;; x should be Float32(0xD490AF7C [Rational(-4971355439104), -4971355439104.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11000000100101011001110)))
;; y should be Float32(0x00604ACE [Rational(3155303, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x801CCBF6)))
;; z should be Float32(0x801CCBF6 [Rational(-943611, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x9559B056)))
(check-sat)
(exit)
