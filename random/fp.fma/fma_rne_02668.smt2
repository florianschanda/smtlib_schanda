(declare-const y Float32)
(assert (= y (fp #b0 #b00000110 #b00111110001001101100111)))
;; y should be Float32(0x031F1367 [Rational(10425191, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01111011 #b11111111001001000001000)))
;; z should be Float32(0x3DFF9208 [Rational(2093633, 16777216), 0.124790])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
