(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABF1CA7)))
;; x should be Float32(0x4ABF1CA7 [Rational(12524711, 2), 6262355.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01100101 #b10010101001100101001001)))
;; y should be Float32(0xB2CA9949 [Rational(-13277513, 562949953421312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00001001110111010111011)))
;; z should be Float32(0xCA84EEBB [Rational(-8711867, 2), -4355933.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010101 #b00001001110111010111011))))
(check-sat)
(exit)
