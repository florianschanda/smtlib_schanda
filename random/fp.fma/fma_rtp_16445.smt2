(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111100 #b10011101111000010000010)))
;; x should be Float32(0x3E4EF082 [Rational(6780993, 33554432), 0.202089])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b01101110011010101001100)))
;; y should be Float32(0xC9B7354C [Rational(-3001683, 2), -1500841.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC8941902))))
(check-sat)
(exit)
