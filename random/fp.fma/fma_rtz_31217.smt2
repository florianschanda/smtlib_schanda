(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b01101100111011101010000)))
;; x should be Float32(0xC8B67750 [Rational(-747381, 2), -373690.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10001000100011011011001)))
;; y should be Float32(0x004446D9 [Rational(4474585, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b10101011110100010111001)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
