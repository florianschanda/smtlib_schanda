(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11100100100010001001111)))
;; x should be Float32(0x8072444F [Rational(-7488591, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4B373B5F)))
;; y should be Float32(0x4B373B5F [Rational(12008287), 12008287.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x007C22BB)))
;; z should be Float32(0x007C22BB [Rational(8135355, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b00011000 #b01000111001001010101000)))
(check-sat)
(exit)
