(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x735B9701)))
;; x should be Float32(0x735B9701 [Rational(17397701036032732652992741769216), 17397701036032732652992741769216.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10011010001100001011011)))
;; y should be Float32(0x004D185B [Rational(5052507, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x3AB17A79)))
;; z should be Float32(0x3AB17A79 [Rational(11631225, 8589934592), 0.001354])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b01110101 #b01100010111111010011011))))
(check-sat)
(exit)
