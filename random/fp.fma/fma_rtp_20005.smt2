(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11001000001101110001111)))
;; x should be Float32(0x80641B8F [Rational(-6560655, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7DDC863B)))
;; y should be Float32(0x7DDC863B [Rational(36640890439236471798701956582202146816), 36640890439236471798701956582202146816.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00100010 #b00011010010111001000001)))
;; z should be Float32(0x910D2E41 [Rational(-9252417, 83076749736557242056487941267521536), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xBEAC7858)))
(check-sat)
(exit)
