(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x704B5C70)))
;; x should be Float32(0x704B5C70 [Rational(251748828296734704941869826048), 251748828296734704941869826048.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01111001101101000010110)))
;; y should be Float32(0x003CDA16 [Rational(1993995, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11001010011111001011001)))
;; z should be Float32(0x00653E59 [Rational(6635097, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b01100001 #b10000010101101110100010)))
(check-sat)
(exit)
