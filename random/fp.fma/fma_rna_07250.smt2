(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AEE7D59)))
;; x should be Float32(0x4AEE7D59 [Rational(15629657, 2), 7814828.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01101101111000100101010)))
;; y should be Float32(0x4A36F12A [Rational(5994645, 2), 2997322.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01000111000011000001101)))
;; z should be Float32(0xCAA3860D [Rational(-10716685, 2), -5358342.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10101011 #b01010100110110111000110)))
(check-sat)
(exit)
