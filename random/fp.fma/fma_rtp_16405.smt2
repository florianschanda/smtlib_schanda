(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6174AE02)))
;; x should be Float32(0x6174AE02 [Rational(282096508643605217280), 282096508643605217280.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10100000101111101010100)))
;; y should be Float32(0x00505F54 [Rational(1316821, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0A22D946)))
;; z should be Float32(0x0A22D946 [Rational(5336227, 680564733841876926926749214863536422912), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x2219A2FB))))
(check-sat)
(exit)
