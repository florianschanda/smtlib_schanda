(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A62CF82)))
;; x should be Float32(0x4A62CF82 [Rational(7432129, 2), 3716064.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00111000111001001000100)))
;; y should be Float32(0x499C7244 [Rational(2563217, 2), 1281608.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80391B8C)))
;; z should be Float32(0x80391B8C [Rational(-935651, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x548A9BAF))))
(check-sat)
(exit)
