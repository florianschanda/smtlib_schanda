(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10011100010011101111010)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b00100101100000001101000)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x805984AE)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10100111 #b11011000101101011101001))))
(check-sat)
(exit)
