(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE6DA2BA8)))
;; x should be Float32(0xE6DA2BA8 [Rational(-515140604468275269599232), -515140604468275269599232.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000001 #b11111110110011110111001)))
;; y should be Float32(0xC0FF67B9 [Rational(-16738233, 2097152), -7.981411])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x65C790C4)))
;; z should be Float32(0x65C790C4 [Rational(117802673065763125854208), 117802673065763125854208.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x685FE667)))
(check-sat)
(exit)
