(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b11111000101110000010110)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01010000100100111001011)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x802618E3)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00010101 #b00111101101110010001010)))
(check-sat)
(exit)
