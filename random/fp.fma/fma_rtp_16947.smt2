(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0035DB88)))
;; x should be Float32(0x0035DB88 [Rational(441201, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8072AE46)))
;; y should be Float32(0x8072AE46 [Rational(-3757859, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01100000001110101101110)))
;; z should be Float32(0x80301D6E [Rational(-1576631, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b00000000 #b01100000001110101101110)))
(check-sat)
(exit)
