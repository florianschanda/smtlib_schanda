(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10100111 #b10111110011110010111010)))
;; x should be Float32(0xD3DF3CBA [Rational(-1917593059328), -1917593059328.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A8B782F)))
;; y should be Float32(0x4A8B782F [Rational(9140271, 2), 4570135.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCEC12F02)))
;; z should be Float32(0xCEC12F02 [Rational(-1620541696), -1620541696.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xDEF33D8C))))
(check-sat)
(exit)
