(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFFE66786)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b11111010 #b01100011101100110000001)))
;; y should be Float32(0xFD31D981 [Rational(-14775168756441939885303103535529852928), -14775168756441939885303103535529852928.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11100110010110010000100)))
;; z should be Float32(0x80732C84 [Rational(-1887009, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
