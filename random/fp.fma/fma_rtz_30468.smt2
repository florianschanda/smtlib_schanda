(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0041A0AB)))
;; x should be Float32(0x0041A0AB [Rational(4300971, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x239E6BC4)))
;; y should be Float32(0x239E6BC4 [Rational(2595569, 151115727451828646838272), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b11110100000011000100010)))
;; z should be Float32(0xCA7A0622 [Rational(-8192785, 2), -4096392.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10010100 #b11110100000011000100001)))
(check-sat)
(exit)
