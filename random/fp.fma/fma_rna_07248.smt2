(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ACFDFBF)))
;; x should be Float32(0x4ACFDFBF [Rational(13623231, 2), 6811615.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01001001001110000111011)))
;; y should be Float32(0x4AA49C3B [Rational(10787899, 2), 5393949.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00249002)))
;; z should be Float32(0x00249002 [Rational(1198081, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10101100 #b00001011010101000110011)))
(check-sat)
(exit)
