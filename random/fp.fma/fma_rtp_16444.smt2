(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6165F9FA)))
;; x should be Float32(0x6165F9FA [Rational(265144818908694315008), 265144818908694315008.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b00010011011111001011000)))
;; y should be Float32(0xC909BE58 [Rational(-1128395, 2), -564197.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11001110100001010101011)))
;; z should be Float32(0x006742AB [Rational(6767275, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xEAF77B8A))))
(check-sat)
(exit)
