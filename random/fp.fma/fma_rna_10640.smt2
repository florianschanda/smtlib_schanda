(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01011011101001011010001)))
;; x should be Float32(0xCAADD2D1 [Rational(-11391697, 2), -5695848.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x000CEF5E)))
;; y should be Float32(0x000CEF5E [Rational(423855, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
