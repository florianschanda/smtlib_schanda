(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010101 #b10001000011000010011011)))
;; x should be Float32(0xEAC4309B [Rational(-118589496740488243096584192), -118589496740488243096584192.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x803A0F8E)))
;; y should be Float32(0x803A0F8E [Rational(-1902535, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x2B31FBBB))))
(check-sat)
(exit)
