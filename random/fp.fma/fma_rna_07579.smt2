(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00011000101111010101011)))
;; x should be Float32(0x4A8C5EAB [Rational(9199275, 2), 4599637.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b01110110101110000100010)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b1 #b01111000 #b00100010110000010100100)))
;; z should be Float32(0xBC1160A4 [Rational(-2381865, 268435456), -0.008873])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
