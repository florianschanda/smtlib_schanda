(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAABAA5F)))
;; x should be Float32(0xCAABAA5F [Rational(-11250271, 2), -5625135.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7953BD5A)))
;; y should be Float32(0x7953BD5A [Rational(68713446443785317445027760932126720), 68713446443785317445027760932126720.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8072CB13)))
;; z should be Float32(0x8072CB13 [Rational(-7523091, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
