(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFF9B96B5)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b11000111 #b10011110001100010000101)))
;; y should be Float32(0xE3CF1885 [Rational(-7640485683373379944448), -7640485683373379944448.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xA92DC6FC)))
;; z should be Float32(0xA92DC6FC [Rational(-2847167, 73786976294838206464), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
