(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11110001111001101111100)))
;; x should be Float32(0xC9F8F37C [Rational(-4078815, 2), -2039407.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11101110000111010100101)))
;; y should be Float32(0x80770EA5 [Rational(-7802533, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b00010101 #b11001111000111011010001))))
(check-sat)
(exit)
