(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11001010000001111011100)))
;; x should be Float32(0xC9E503DC [Rational(-3752183, 2), -1876091.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00000000000000000000000)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11001010000010011001101)))
;; z should be Float32(0x4AE504CD [Rational(15008973, 2), 7504486.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AE504CD))))
(check-sat)
(exit)
