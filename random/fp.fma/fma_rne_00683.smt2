(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01100011101001010111010)))
;; x should be Float32(0x4A31D2BA [Rational(5826909, 2), 2913454.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00010110 #b10000111000111101111101)))
;; y should be Float32(0x8B438F7D [Rational(-12816253, 340282366920938463463374607431768211456), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111011101011010010111)))
;; z should be Float32(0x807DD697 [Rational(-8246935, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00101100 #b00001111101011100101111)))
(check-sat)
(exit)
