(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8002E210)))
;; z should be Float32(0x8002E210 [Rational(-11809, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
