(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0A92E499)))
;; x should be Float32(0x0A92E499 [Rational(9626777, 680564733841876926926749214863536422912), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111111111111111111111)))
;; y should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00001011011110011101011)))
;; z should be Float32(0x8005BCEB [Rational(-376043, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8005BCEC))))
(check-sat)
(exit)
