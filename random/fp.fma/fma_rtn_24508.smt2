(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11101010111101010111001)))
;; x should be Float32(0xCAF57AB9 [Rational(-16087737, 2), -8043868.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00010101 #b01110100110101100001101)))
;; y should be Float32(0x8ABA6B0D [Rational(-12217101, 680564733841876926926749214863536422912), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x9D256890)))
;; z should be Float32(0x9D256890 [Rational(-677513, 309485009821345068724781056), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x9D2565C5)))
(check-sat)
(exit)
