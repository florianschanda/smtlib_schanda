(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111111 #b10111011101000000110010)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b11111110 #b01000111010011010011101)))
;; y should be Float32(0x7F23A69D [Rational(217529268929765841981913172934798606336), 217529268929765841981913172934798606336.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8D115FB6)))
;; z should be Float32(0x8D115FB6 [Rational(-4763611, 10633823966279326983230456482242756608), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
