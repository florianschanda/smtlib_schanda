(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE5CB84F8)))
;; x should be Float32(0xE5CB84F8 [Rational(-120136654565448630665216), -120136654565448630665216.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x334BC2B7)))
;; y should be Float32(0x334BC2B7 [Rational(13353655, 281474976710656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10110011 #b01000011111110100111100)))
(check-sat)
(exit)
