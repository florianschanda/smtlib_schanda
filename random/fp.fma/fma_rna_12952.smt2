(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDA083DF3)))
;; x should be Float32(0xDA083DF3 [Rational(-9587177679749120), -9587177679749120.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111101 #b00011111111110001011011)))
;; y should be Float32(0xFE8FFC5B [Rational(-95694953952433839344821336947459883008), -95694953952433839344821336947459883008.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111110 #b11111111111111111111111)))
;; z should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
