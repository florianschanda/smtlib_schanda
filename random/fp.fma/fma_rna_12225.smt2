(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB67FD269)))
;; y should be Float32(0xB67FD269 [Rational(-16765545, 4398046511104), -0.000004])

(declare-const z Float32)
(assert (= z (fp #b0 #b11100011 #b11100000001111000000011)))
;; z should be Float32(0x71F01E03 [Rational(2378005897561942527299976953856), 2378005897561942527299976953856.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x71F01E03))))
(check-sat)
(exit)
