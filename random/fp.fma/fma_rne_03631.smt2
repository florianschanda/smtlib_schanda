(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCACD56BF)))
;; x should be Float32(0xCACD56BF [Rational(-13457087, 2), -6728543.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10010011111011001011110)))
;; y should be Float32(0x4A49F65E [Rational(6617903, 2), 3308951.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10100100111111110011111)))
;; z should be Float32(0x4AD27F9F [Rational(13795231, 2), 6897615.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD5A1FEB5))))
(check-sat)
(exit)
