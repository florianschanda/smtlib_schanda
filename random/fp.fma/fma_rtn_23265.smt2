(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010110 #b10100011011000100001011)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10100111001001010010011)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11000111100111101101101)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x0C08E8EC)))
(check-sat)
(exit)
