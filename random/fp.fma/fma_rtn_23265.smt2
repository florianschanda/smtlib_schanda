(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x44C1EB3F)))
;; x should be Float32(0x44C1EB3F [Rational(12708671, 8192), 1551.351440])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x006BD660)))
;; y should be Float32(0x006BD660 [Rational(220851, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11010001000000000010100)))
;; z should be Float32(0x00688014 [Rational(1712133, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x05A3798D))))
(check-sat)
(exit)
