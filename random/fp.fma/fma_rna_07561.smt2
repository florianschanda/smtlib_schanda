(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A42D67E)))
;; x should be Float32(0x4A42D67E [Rational(6384447, 2), 3192223.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01101001001001001000100)))
;; y should be Float32(0x80349244 [Rational(-861329, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8028DDB9)))
;; z should be Float32(0x8028DDB9 [Rational(-2678201, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x8AA00B91)))
(check-sat)
(exit)
