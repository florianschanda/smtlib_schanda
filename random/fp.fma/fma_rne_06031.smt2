(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10000111 #b11011100111001100101011)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b01110110 #b11001101101000011111010)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b01110110 #b11001101101000011111010))))
(check-sat)
(exit)
