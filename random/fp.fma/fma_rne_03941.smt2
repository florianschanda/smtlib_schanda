(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00110010000101000110111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11110101101101111000000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00010011 #b11111100001110101001111)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x0BA2C64E)))
(check-sat)
(exit)
