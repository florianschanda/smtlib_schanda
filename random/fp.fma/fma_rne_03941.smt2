(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9F1DC9C)))
;; x should be Float32(0xC9F1DC9C [Rational(-3962663, 2), -1981331.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800B19E9)))
;; y should be Float32(0x800B19E9 [Rational(-727529, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01101110 #b11010011010011000110100)))
;; z should be Float32(0x3769A634 [Rational(3828109, 274877906944), 0.000014])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x3769A634))))
(check-sat)
(exit)
