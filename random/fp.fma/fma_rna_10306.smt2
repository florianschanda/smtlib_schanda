(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00100011100010100100101)))
;; y should be Float32(0xCA91C525 [Rational(-9553189, 2), -4776594.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x08762F4B)))
;; z should be Float32(0x08762F4B [Rational(16133963, 21778071482940061661655974875633165533184), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b00010000 #b11101100010111101001011))))
(check-sat)
(exit)
