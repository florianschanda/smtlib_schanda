(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x48983090)))
;; x should be Float32(0x48983090 [Rational(623369, 2), 311684.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10111100 #b11100011100010001001010)))
;; y should be Float32(0x5E71C44A [Rational(4355282655609094144), 4355282655609094144.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x678FBA69)))
(check-sat)
(exit)
