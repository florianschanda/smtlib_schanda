(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01001010 #b10111001000011110111000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01111001011010111001111)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010101 #b01111001011010111001111))))
(check-sat)
(exit)
