(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x535B6898)))
;; x should be Float32(0x535B6898 [Rational(942352629760), 942352629760.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11010100101100000110110)))
;; y should be Float32(0x4A6A5836 [Rational(7679003, 2), 3839501.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80728C5C)))
;; z should be Float32(0x80728C5C [Rational(-1876759, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x5E48D935)))
(check-sat)
(exit)
