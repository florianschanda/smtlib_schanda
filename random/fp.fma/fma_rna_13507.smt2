(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFF9CB126)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b01110010 #b11000011001010010000101)))
;; y should be Float32(0x39619485 [Rational(14783621, 68719476736), 0.000215])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x94B80BA7)))
;; z should be Float32(0x94B80BA7 [Rational(-12061607, 649037107316853453566312041152512), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
