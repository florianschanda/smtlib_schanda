(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAA672FD)))
;; x should be Float32(0xCAA672FD [Rational(-10908413, 2), -5454206.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00475A2C)))
;; y should be Float32(0x00475A2C [Rational(1169035, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00010110 #b01110011001001000010100)))
(check-sat)
(exit)
