(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x006833F8)))
;; x should be Float32(0x006833F8 [Rational(853631, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00101101100101011110100)))
;; y should be Float32(0x4996CAF4 [Rational(2470589, 2), 1235294.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00712EFF)))
;; z should be Float32(0x00712EFF [Rational(7417599, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b00010100 #b11101011000010001001101))))
(check-sat)
(exit)
