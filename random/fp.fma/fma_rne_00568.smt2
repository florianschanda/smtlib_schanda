(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01010110001101011101111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01010001111111111100001)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00011010 #b00010000010010011100000)))
