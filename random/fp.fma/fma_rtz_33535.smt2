(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFEE84624)))
;; x should be Float32(0xFEE84624 [Rational(-154372542984471825953907817414565822464), -154372542984471825953907817414565822464.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10111010 #b11111001111001111111001)))
;; y should be Float32(0xDD7CF3F9 [Rational(-1139199118455865344), -1139199118455865344.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
