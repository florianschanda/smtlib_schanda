(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00101101 #b00000001010100100010100)))
;; x should be Float32(0x1680A914 [Rational(2107973, 10141204801825835211973625643008), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01001101101101000101011)))
;; y should be Float32(0x4AA6DA2B [Rational(10934827, 2), 5467413.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b11010100001010000000100)))
;; z should be Float32(0x49EA1404 [Rational(3835137, 2), 1917568.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10010011 #b11010100001010000000100)))
(check-sat)
(exit)
