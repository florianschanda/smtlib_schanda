(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA6A62A1)))
;; x should be Float32(0xCA6A62A1 [Rational(-15360673, 4), -3840168.250000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11000110111101100100101)))
;; y should be Float32(0x4AE37B25 [Rational(14908197, 2), 7454098.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10000000 #b11001001110001101000001)))
;; z should be Float32(0x4064E341 [Rational(15000385, 4194304), 3.576370])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xD5D04635)))
(check-sat)
(exit)
