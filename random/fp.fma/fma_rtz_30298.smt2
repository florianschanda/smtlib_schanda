(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111000 #b10001011001100101010001)))
;; x should be Float32(0x1C459951 [Rational(12949841, 19807040628566084398385987584), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00101101110011101001100)))
;; y should be Float32(0x8016E74C [Rational(-375251, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFF800000))))
(check-sat)
(exit)
