(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11010001100100000010101)))
;; x should be Float32(0xCAE8C815 [Rational(-15255573, 2), -7627786.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49854A5C)))
;; y should be Float32(0x49854A5C [Rational(2183831, 2), 1091915.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x001CEC27)))
;; z should be Float32(0x001CEC27 [Rational(1895463, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xD4F26720)))
(check-sat)
(exit)
