(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x901D7A62)))
;; x should be Float32(0x901D7A62 [Rational(-5160241, 166153499473114484112975882535043072), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01100111011100010001011)))
;; y should be Float32(0x8033B88B [Rational(-3389579, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xFF800000)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ -oo 8 24)))
(check-sat)
(exit)
