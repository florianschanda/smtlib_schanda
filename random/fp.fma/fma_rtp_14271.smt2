(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x484DEDE0)))
;; x should be Float32(0x484DEDE0 [Rational(421743, 2), 210871.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA56ACB2)))
;; y should be Float32(0xCA56ACB2 [Rational(-7034457, 2), -3517228.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A958233)))
;; z should be Float32(0x4A958233 [Rational(9798195, 2), 4899097.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10100110 #b01011001010111101111001))))
(check-sat)
(exit)
