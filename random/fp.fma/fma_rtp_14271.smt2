(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00011011111000010101111)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA943FFB)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10010000001000110110111)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10101011 #b01001000110010100111010))))
(check-sat)
(exit)
