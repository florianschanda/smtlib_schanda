(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9C7163C)))
;; x should be Float32(0xC9C7163C [Rational(-3261839, 2), -1630919.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1D703CD3)))
;; y should be Float32(0x1D703CD3 [Rational(15744211, 4951760157141521099596496896), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b10100111111011000101011)))
;; z should be Float32(0xCAD3F62B [Rational(-13891115, 2), -6945557.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xCAD3F62B)))
(check-sat)
(exit)
