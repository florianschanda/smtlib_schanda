(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA5558C2)))
;; x should be Float32(0xCA5558C2 [Rational(-6990945, 2), -3495472.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11001110101011011111101)))
;; y should be Float32(0x006756FD [Rational(6772477, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00110001100010011101010)))
;; z should be Float32(0x8018C4EA [Rational(-811637, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8B2C3E6A))))
(check-sat)
(exit)
