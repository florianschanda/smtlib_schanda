(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10010001011011000010001)))
;; x should be Float32(0xCAC8B611 [Rational(-13153809, 2), -6576904.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8058DE35)))
;; y should be Float32(0x8058DE35 [Rational(-5824053, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80800000)))
;; z should be Float32(0x80800000 [Rational(-1, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00010111 #b00010110101100110011000))))
(check-sat)
(exit)
