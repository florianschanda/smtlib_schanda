(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01011011010101001111101)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010000 #b11011111111111010100000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b00010001 #b01010110011111001011010)))
(check-sat)
(exit)
