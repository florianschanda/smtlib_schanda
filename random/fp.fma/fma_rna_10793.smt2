(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE18593)))
;; x should be Float32(0xCAE18593 [Rational(-14779795, 2), -7389897.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x803B6F72)))
;; y should be Float32(0x803B6F72 [Rational(-1947577, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b00010110 #b10100010110111111011001))))
(check-sat)
(exit)
