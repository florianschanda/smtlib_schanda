(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11110101001100111101101)))
;; x should be Float32(0xCAFA99ED [Rational(-16423405, 2), -8211702.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1EB7F8BF)))
;; y should be Float32(0x1EB7F8BF [Rational(12056767, 618970019642690137449562112), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x46F11700)))
;; z should be Float32(0x46F11700 [Rational(61719, 2), 30859.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10001101 #b11100010001011100000000))))
(check-sat)
(exit)
