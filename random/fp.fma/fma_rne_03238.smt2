(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000101100000001101110)))
;; x should be Float32(0x0002C06E [Rational(90167, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b01110111100010101101001)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b11010100001011011001100)))
;; z should be Float32(0x4A6A16CC [Rational(3835315), 3835315.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
