(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AED14AB)))
;; x should be Float32(0x4AED14AB [Rational(15537323, 2), 7768661.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00001010 #b10110001101101111011011)))
;; y should be Float32(0x8558DBDB [Rational(-14212059, 1393796574908163946345982392040522594123776), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01110111000100000011001)))
;; z should be Float32(0xCABB8819 [Rational(-12290073, 2), -6145036.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCABB8819))))
(check-sat)
(exit)
