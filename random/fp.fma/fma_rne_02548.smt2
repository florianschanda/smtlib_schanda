(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100001 #b01001101111011000101101)))
;; x should be Float32(0x30A6F62D [Rational(10941997, 9007199254740992), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10001001010110110010110)))
;; y should be Float32(0x4A44AD96 [Rational(6444747, 2), 3222373.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A5ADF2E)))
;; z should be Float32(0x4A5ADF2E [Rational(7171991, 2), 3585995.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b10010100 #b10110101101111100101110))))
(check-sat)
(exit)
