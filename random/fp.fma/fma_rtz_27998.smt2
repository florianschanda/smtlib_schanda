(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A7AA742)))
;; x should be Float32(0x4A7AA742 [Rational(8213409, 2), 4106704.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA8C0ABD)))
;; y should be Float32(0xCA8C0ABD [Rational(-9177789, 2), -4588894.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC9FB8ACC)))
;; z should be Float32(0xC9FB8ACC [Rational(-4121267, 2), -2060633.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10101011 #b00010010001110111111100))))
(check-sat)
(exit)
