(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10110011 #b01100011011000010010110)))
;; x should be Float32(0x59B1B096 [Rational(6251903646171136), 6251903646171136.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4965C9C8)))
;; y should be Float32(0x4965C9C8 [Rational(1882425, 2), 941212.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10011011010011101101111)))
;; z should be Float32(0x004DA76F [Rational(5089135, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x639F7F04))))
(check-sat)
(exit)
