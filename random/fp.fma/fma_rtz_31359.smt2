(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAD9CFC1)))
;; x should be Float32(0xCAD9CFC1 [Rational(-14274497, 2), -7137248.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11110011 #b10001000101011000001100)))
;; y should be Float32(0xF9C4560C [Rational(-127429427779586303752560983333142528), -127429427779586303752560983333142528.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x003497A2)))
;; z should be Float32(0x003497A2 [Rational(1723345, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11111110 #b11111111111111111111111))))
(check-sat)
(exit)
