(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10001110101011000101001)))
;; x should be Float32(0x80475629 [Rational(-4675113, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A6C1342)))
;; y should be Float32(0x4A6C1342 [Rational(7735713, 2), 3867856.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11101101011001000001101)))
;; z should be Float32(0x0076B20D [Rational(7778829, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b00010110 #b00000111001000110010011))))
(check-sat)
(exit)
