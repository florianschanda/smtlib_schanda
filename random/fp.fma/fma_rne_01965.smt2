(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01100111111100000110000)))
;; y should be Float32(0x0033F830 [Rational(212867, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x64AEC926)))
;; z should be Float32(0x64AEC926 [Rational(25793821930209604009984), 25793821930209604009984.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b11001001 #b01011101100100100100110))))
(check-sat)
(exit)
