(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01100001101111101001100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b01011001 #b00110001100011101000100)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b01011001 #b00110001100011101000101)))
(check-sat)
(exit)
