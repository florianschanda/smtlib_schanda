(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111111 #b01100010011001001111110)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b01101001 #b01110100101010001111101)))
;; y should be Float32(0x34BA547D [Rational(12211325, 35184372088832), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x801021FA)))
;; z should be Float32(0x801021FA [Rational(-528637, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
