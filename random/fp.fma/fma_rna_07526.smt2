(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF3F479)))
;; x should be Float32(0x4AF3F479 [Rational(15987833, 2), 7993916.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9839D6D2)))
;; y should be Float32(0x9839D6D2 [Rational(-6089577, 2535301200456458802993406410752), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A94792F)))
;; z should be Float32(0x4A94792F [Rational(9730351, 2), 4865175.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10010101 #b00101000111100100101111)))
(check-sat)
(exit)
