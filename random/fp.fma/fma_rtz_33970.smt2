(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7FADF720)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01011000000110001110100)))
;; y should be Float32(0x49AC0C74 [Rational(2818845, 2), 1409422.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01110000 #b00001100111001000000010)))
;; z should be Float32(0xB8067202 [Rational(-4405505, 137438953472), -0.000032])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
