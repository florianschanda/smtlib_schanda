(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10011101110100100100101)))
;; y should be Float32(0xCACEE925 [Rational(-13560101, 2), -6780050.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x9BCF425F)))
;; z should be Float32(0x9BCF425F [Rational(-13582943, 39614081257132168796771975168), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x9BCF425F))))
(check-sat)
(exit)
