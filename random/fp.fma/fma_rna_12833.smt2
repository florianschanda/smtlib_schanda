(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xACE912D6)))
;; x should be Float32(0xACE912D6 [Rational(-7637355, 1152921504606846976), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010001 #b01101101111110000010000)))
;; y should be Float32(0xC8B6FC10 [Rational(-749505, 2), -374752.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x13EC006B)))
;; z should be Float32(0x13EC006B [Rational(15466603, 2596148429267413814265248164610048), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x362698E1))))
(check-sat)
(exit)
