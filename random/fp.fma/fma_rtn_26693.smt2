(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB28624C0)))
;; x should be Float32(0xB28624C0 [Rational(-137363, 8796093022208), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010010001110101100011)))
;; y should be Float32(0x80091D63 [Rational(-597347, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01100110 #b10011111101111110000101)))
;; z should be Float32(0xB34FDF85 [Rational(-13623173, 281474976710656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xB34FDF85)))
(check-sat)
(exit)
