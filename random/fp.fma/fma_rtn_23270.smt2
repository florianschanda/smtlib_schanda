(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11010010 #b01100101000010011010100)))
;; x should be Float32(0x693284D4 [Rational(13488503686055400793178112), 13488503686055400793178112.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00100000110101111001000)))
;; y should be Float32(0x00106BC8 [Rational(134521, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807FFFFF)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b01010001 #b01101110011011101100011))))
(check-sat)
(exit)
