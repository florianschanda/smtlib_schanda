(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A966AE5)))
;; x should be Float32(0x4A966AE5 [Rational(9857765, 2), 4928882.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11001110110101010011000)))
;; y should be Float32(0x80676A98 [Rational(-847187, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01001000111111100001111)))
;; z should be Float32(0x80247F0F [Rational(-2391823, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b00010110 #b11100110000111010001111))))
(check-sat)
(exit)
