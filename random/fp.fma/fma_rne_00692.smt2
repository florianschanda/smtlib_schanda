(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A9AE3B7)))
;; x should be Float32(0x4A9AE3B7 [Rational(10150839, 2), 5075419.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80401923)))
;; y should be Float32(0x80401923 [Rational(-4200739, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x05F1E127)))
;; z should be Float32(0x05F1E127 [Rational(15851815, 696898287454081973172991196020261297061888), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8B1B0251))))
(check-sat)
(exit)
