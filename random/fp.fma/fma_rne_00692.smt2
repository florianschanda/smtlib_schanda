(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11110011101001101000111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00001101000110100010110)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x6745682E)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11001110 #b10001010110100000101110)))
(check-sat)
(exit)
