(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11100100001000100001010)))
;; y should be Float32(0xCA72110A [Rational(-7932037, 2), -3966018.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b01000010001101111100110)))
;; z should be Float32(0x80211BE6 [Rational(-1084915, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x80211BE6))))
(check-sat)
(exit)
