(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00110101001001111100100)))
;; x should be Float32(0xC99A93E4 [Rational(-2532601, 2), -1266300.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00011100 #b01101111111011100100010)))
;; y should be Float32(0x8E37F722 [Rational(-6028177, 2658455991569831745807614120560689152), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAA075FD)))
;; z should be Float32(0xCAA075FD [Rational(-10515965, 2), -5257982.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xCAA075FD)))
(check-sat)
(exit)
