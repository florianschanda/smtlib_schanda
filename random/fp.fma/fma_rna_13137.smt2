(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10010000001010100111101)))
;; x should be Float32(0x8048153D [Rational(-4724029, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00010010 #b11011010100111101101110)))
;; y should be Float32(0x096D4F6E [Rational(7776183, 2722258935367507707706996859454145691648), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7CFF952E)))
;; z should be Float32(0x7CFF952E [Rational(10616491379622406402623792059315650560), 10616491379622406402623792059315650560.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x7CFF952E)))
(check-sat)
(exit)
