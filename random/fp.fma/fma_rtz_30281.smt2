(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011011 #b01111011001101010010011)))
;; x should be Float32(0x4DBD9A93 [Rational(397628000), 397628000.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA8AB154F)))
;; y should be Float32(0xA8AB154F [Rational(-11212111, 590295810358705651712), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807FFFFF)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b01101101 #b11111010110110000010000)))
(check-sat)
(exit)
