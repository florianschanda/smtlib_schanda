(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC92B57D8)))
;; x should be Float32(0xC92B57D8 [Rational(-1403643, 2), -701821.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01011100 #b10100110100110011111100)))
;; y should be Float32(0xAE534CFC [Rational(-3461951, 72057594037927936), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x612D654E)))
;; z should be Float32(0x612D654E [Rational(199911656049860411392), 199911656049860411392.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x612D654E)))
(check-sat)
(exit)
