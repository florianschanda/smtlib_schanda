(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10101001100001010011110)))
;; x should be Float32(0xCA54C29E [Rational(-6971727, 2), -3485863.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11000011000000001011101)))
;; y should be Float32(0x0061805D [Rational(6389853, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10101010010010000100010)))
;; z should be Float32(0x80552422 [Rational(-2789905, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00010110 #b01000100001000011011011)))
(check-sat)
(exit)
