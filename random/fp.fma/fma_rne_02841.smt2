(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x48C92281)))
;; x should be Float32(0x48C92281 [Rational(13181569, 32), 411924.031250])

(declare-const y Float32)
(assert (= y (fp #b1 #b00010011 #b00101100011001111010000)))
;; y should be Float32(0x899633D0 [Rational(-615229, 170141183460469231731687303715884105728), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00100101 #b11011000000010111011010)))
(check-sat)
(exit)
