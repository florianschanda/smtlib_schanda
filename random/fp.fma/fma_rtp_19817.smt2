(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDD51E42F)))
;; x should be Float32(0xDD51E42F [Rational(-945266570353967104), -945266570353967104.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10001110 #b00100101111010001010101)))
;; y should be Float32(0xC712F455 [Rational(-9630805, 256), -37620.332031])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b01111000101111011001010)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
