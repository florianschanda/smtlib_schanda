(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b00011110 #b11110111011001111001011)))
;; y should be Float32(0x8F7BB3CB [Rational(-16495563, 1329227995784915872903807060280344576), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01100011111111001111011)))
;; z should be Float32(0x4AB1FE7B [Rational(11665019, 2), 5832509.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010101 #b01100011111111001111011))))
(check-sat)
(exit)
