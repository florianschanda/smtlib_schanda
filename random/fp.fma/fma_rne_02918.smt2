(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00010100111100111010100)))
;; x should be Float32(0x000A79D4 [Rational(171637, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b11100101101101111001000)))
;; y should be Float32(0x4972DBC8 [Rational(1989497, 2), 994748.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC8D159D0)))
;; z should be Float32(0xC8D159D0 [Rational(-857501, 2), -428750.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xC8D159D0))))
(check-sat)
(exit)
