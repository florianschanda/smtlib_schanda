(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b11000110011010111101011)))
;; x should be Float32(0xC8E335EB [Rational(-14890475, 32), -465327.343750])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10000110110011011001001)))
;; y should be Float32(0x4AC366C9 [Rational(12805833, 2), 6402916.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x806B34A9)))
;; z should be Float32(0x806B34A9 [Rational(-7025833, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b10101000 #b01011010110110101001100)))
(check-sat)
(exit)
