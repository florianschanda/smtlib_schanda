(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11011110 #b11010100101011101100011)))
;; x should be Float32(0xEF6A5763 [Rational(-72525136358783022914674360320), -72525136358783022914674360320.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01111010001110111100001)))
;; y should be Float32(0x803D1DE1 [Rational(-4005345, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x3939B8B4)))
;; z should be Float32(0x3939B8B4 [Rational(3042861, 17179869184), 0.000177])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x3939B8CF))))
(check-sat)
(exit)
