(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101010 #b10111001010010110100001)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00010110 #b01101001001001101010011)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00010110 #b01101001001001101010011))))
(check-sat)
(exit)
