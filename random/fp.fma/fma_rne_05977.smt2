(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10011000 #b00000101011000101000000)))
;; x should be Float32(0xCC02B140 [Rational(-34260224), -34260224.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA8721E5)))
;; y should be Float32(0xCA8721E5 [Rational(-8856037, 2), -4428018.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b11101010100101001000100)))
;; z should be Float32(0xC9F54A44 [Rational(-4018833, 2), -2009416.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x5709F98D))))
(check-sat)
(exit)
