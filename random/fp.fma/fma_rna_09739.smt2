(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x68F12B91)))
;; x should be Float32(0x68F12B91 [Rational(9111151845743116054167552), 9111151845743116054167552.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b00100010110011000011000)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAA725D9)))
;; z should be Float32(0xCAA725D9 [Rational(-10954201, 2), -5477100.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xFFFFFFFF)))
(check-sat)
(exit)
