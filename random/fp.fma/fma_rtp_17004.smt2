(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49E538AC)))
;; y should be Float32(0x49E538AC [Rational(3755563, 2), 1877781.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111100101101001110001)))
;; z should be Float32(0x807E5A71 [Rational(-8280689, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b00000000 #b11111100101101001110001))))
(check-sat)
(exit)
