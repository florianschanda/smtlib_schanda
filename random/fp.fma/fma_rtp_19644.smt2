(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCD778218)))
;; x should be Float32(0xCD778218 [Rational(-259531136), -259531136.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11000110 #b11011100000111100001111)))
;; y should be Float32(0x636E0F0F [Rational(4391410175578092863488), 4391410175578092863488.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11001101 #b11011100010010010000000)))
;; z should be Float32(0xE6EE2480 [Rational(-562298264540832979746816), -562298264540832979746816.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xF1662988)))
(check-sat)
(exit)
