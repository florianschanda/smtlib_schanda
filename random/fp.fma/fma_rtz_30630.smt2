(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01101011000110001110000)))
;; x should be Float32(0x00358C70 [Rational(219335, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF9E10BCC)))
;; y should be Float32(0xF9E10BCC [Rational(-146063257777641161839861772100567040), -146063257777641161839861772100567040.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b10000010110010010001010)))
;; z should be Float32(0x4A41648A [Rational(6337093, 2), 3168546.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4A416489))))
(check-sat)
(exit)
