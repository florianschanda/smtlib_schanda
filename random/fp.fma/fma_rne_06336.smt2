(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x800EE8A7)))
;; x should be Float32(0x800EE8A7 [Rational(-977063, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01011001100010000100101)))
;; y should be Float32(0xCAACC425 [Rational(-11322405, 2), -5661202.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x004E535F)))
;; z should be Float32(0x004E535F [Rational(5133151, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b00010100 #b01000001111101111010010)))
(check-sat)
(exit)
