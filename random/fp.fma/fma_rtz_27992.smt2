(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01100010011011001111010)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10001000010010001010101)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10101011 #b00001111100011011011011))))
(check-sat)
(exit)
