(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010000 #b01000100001010111110000)))
;; x should be Float32(0x082215F0 [Rational(663903, 1361129467683753853853498429727072845824), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b00101111011100101001000)))
;; y should be Float32(0x4917B948 [Rational(1242921, 2), 621460.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x11C02080))))
(check-sat)
(exit)
