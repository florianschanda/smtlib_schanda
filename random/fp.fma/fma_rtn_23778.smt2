(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x000D10C8)))
;; x should be Float32(0x000D10C8 [Rational(107033, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01001110 #b10111001100111110100000)))
;; y should be Float32(0xA75CCFA0 [Rational(-452221, 147573952589676412928), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111010000011100100100)))
;; z should be Float32(0x007D0724 [Rational(2048457, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b00000000 #b11111010000011100100011))))
(check-sat)
(exit)
