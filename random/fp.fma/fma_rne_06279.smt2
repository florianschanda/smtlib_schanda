(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111000000110110101100)))
;; x should be Float32(0x807C0DAC [Rational(-2032491, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001000 #b11100000100000001011100)))
;; y should be Float32(0x4470405C [Rational(3936279, 4096), 961.005615])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11110001011001000110010)))
;; z should be Float32(0x0078B232 [Rational(3954969, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x85689BA9))))
(check-sat)
(exit)
