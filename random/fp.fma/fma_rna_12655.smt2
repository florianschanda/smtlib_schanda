(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF3BF1F5F)))
;; x should be Float32(0xF3BF1F5F [Rational(-30284575846963627134537919627264), -30284575846963627134537919627264.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A920925)))
;; y should be Float32(0x4A920925 [Rational(9570597, 2), 4785298.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFEDA0D70))))
(check-sat)
(exit)
