(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AC9A2D5)))
;; x should be Float32(0x4AC9A2D5 [Rational(13214421, 2), 6607210.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFF7FFFFF)))
;; y should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAD9FCCF)))
;; z should be Float32(0xCAD9FCCF [Rational(-14286031, 2), -7143015.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (_ -oo 8 24)))
(check-sat)
(exit)
