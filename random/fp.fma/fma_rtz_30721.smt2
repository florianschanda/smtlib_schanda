(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11101100110010011011010)))
;; y should be Float32(0x4A7664DA [Rational(8073837, 2), 4036918.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01111010 #b01001000101011100101011)))
;; z should be Float32(0xBD24572B [Rational(-10770219, 268435456), -0.040122])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b01111010 #b01001000101011100101011))))
(check-sat)
(exit)
