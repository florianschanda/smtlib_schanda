(assert (= y (fp #b1 #b00000000 #b00011000011000100010000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10111100 #b01101011111110011000010)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10111100 #b01101011111110011000001)))
(check-sat)
(exit)
