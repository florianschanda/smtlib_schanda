(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b11010011110001110011000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10101011 #b00111011111111000101011)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x800950E1)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10111111 #b00100000101100011001011))))
(check-sat)
(exit)
