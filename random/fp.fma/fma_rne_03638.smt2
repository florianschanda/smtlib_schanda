(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAAFEE11)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10100001001010111011001)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01101011001110100011111)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10101100 #b00011110101100001101001))))
(check-sat)
(exit)
