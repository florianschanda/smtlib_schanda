(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0546D139)))
;; x should be Float32(0x0546D139 [Rational(13029689, 1393796574908163946345982392040522594123776), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x23B0C928)))
;; y should be Float32(0x23B0C928 [Rational(1448229, 75557863725914323419136), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x00000001))))
(check-sat)
(exit)
