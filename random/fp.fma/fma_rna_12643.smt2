(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010101 #b00001100001100011001100)))
;; x should be Float32(0x8A8618CC [Rational(-2197043, 170141183460469231731687303715884105728), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00000000)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x17894692)))
;; z should be Float32(0x17894692 [Rational(4498249, 5070602400912917605986812821504), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b00101111 #b00010010100011010010010)))
(check-sat)
(exit)
