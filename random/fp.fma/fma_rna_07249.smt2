(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AC91EE3)))
;; x should be Float32(0x4AC91EE3 [Rational(13180643, 2), 6590321.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49FCE17C)))
;; y should be Float32(0x49FCE17C [Rational(4143199, 2), 2071599.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000000)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10101010 #b10001101010101110001101))))
(check-sat)
(exit)
