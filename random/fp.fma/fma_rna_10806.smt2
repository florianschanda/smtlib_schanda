(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAD8D4F1)))
;; x should be Float32(0xCAD8D4F1 [Rational(-14210289, 2), -7105144.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00011010011101101011100)))
;; y should be Float32(0x800D3B5C [Rational(-216791, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807FFFFF)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x0A335164)))
(check-sat)
(exit)
