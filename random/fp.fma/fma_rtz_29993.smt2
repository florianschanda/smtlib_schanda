(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010001 #b00001100111101011001111)))
;; x should be Float32(0x48867ACF [Rational(8813263, 32), 275414.468750])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00111111000110100111001)))
;; y should be Float32(0x4A9F8D39 [Rational(10456377, 2), 5228188.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b11010011000001110010100)))
;; z should be Float32(0xC9E98394 [Rational(-3825893, 2), -1912946.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x53A7A0DD)))
(check-sat)
(exit)
