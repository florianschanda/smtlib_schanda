(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA9098C7)))
;; x should be Float32(0xCA9098C7 [Rational(-9476295, 2), -4738147.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x801C5044)))
;; y should be Float32(0x801C5044 [Rational(-463889, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b01101000110111111101010)))
;; z should be Float32(0xCA346FEA [Rational(-5912565, 2), -2956282.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10010100 #b01101000110111111101010))))
(check-sat)
(exit)
