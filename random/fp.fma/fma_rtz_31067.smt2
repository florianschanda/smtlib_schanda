(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00110010100110011111110)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49A42DFC)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00101011100101010101111)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10101000 #b10001001010000110100101)))
(check-sat)
(exit)
