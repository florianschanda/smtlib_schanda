(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11110010110011101011011)))
;; x should be Float32(0xCAF9675B [Rational(-16344923, 2), -8172461.500000])

(declare-const y Float32)
(assert (= y (_ -oo 8 24)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z (fp #b0 #b11101000 #b00010101100110110110111)))
;; z should be Float32(0x740ACDB7 [Rational(43988608349627375216546465972224), 43988608349627375216546465972224.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (_ +oo 8 24))))
(check-sat)
(exit)
