(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A60CEEA)))
;; x should be Float32(0x4A60CEEA [Rational(7366517, 2), 3683258.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b11000000111100111010011)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00100101110100101011001)))
;; z should be Float32(0x0012E959 [Rational(1239385, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
