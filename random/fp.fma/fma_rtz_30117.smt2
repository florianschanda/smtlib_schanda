(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10111100 #b10001111011011000010100)))
;; x should be Float32(0x5E47B614 [Rational(3597677912385388544), 3597677912385388544.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0034A3BF)))
;; y should be Float32(0x0034A3BF [Rational(3449791, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11110111000100110101111)))
;; z should be Float32(0x4AFB89AF [Rational(16484783, 2), 8242391.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b10010101 #b11110111000100110101111)))
(check-sat)
(exit)
