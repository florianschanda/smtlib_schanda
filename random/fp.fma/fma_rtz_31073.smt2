(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA02386E)))
;; x should be Float32(0xCA02386E [Rational(-4267063, 2), -2133531.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AE671D3)))
;; y should be Float32(0x4AE671D3 [Rational(15102419, 2), 7551209.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x1C2BE832)))
;; z should be Float32(0x1C2BE832 [Rational(5633049, 9903520314283042199192993792), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10101010 #b11010100111000100110010)))
(check-sat)
(exit)
