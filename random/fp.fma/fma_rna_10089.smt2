(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01110101010110100110000)))
;; x should be Float32(0x003AAD30 [Rational(240339, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x807D8F7C)))
;; y should be Float32(0x807D8F7C [Rational(-2057183, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7FAC9518)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
