(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00570544)))
;; x should be Float32(0x00570544 [Rational(1425745, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (_ NaN 8 24)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11110111100011110110111)))
;; z should be Float32(0x807BC7B7 [Rational(-8112055, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
