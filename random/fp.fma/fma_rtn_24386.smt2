(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAED827B)))
;; x should be Float32(0xCAED827B [Rational(-15565435, 2), -7782717.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAF8D56D)))
;; y should be Float32(0xCAF8D56D [Rational(-16307565, 2), -8153782.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00705CE0)))
;; z should be Float32(0x00705CE0 [Rational(230119, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x5666DC69)))
(check-sat)
(exit)
