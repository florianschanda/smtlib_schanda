(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7F80E6B0)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b1 #b11111101 #b11010000011100101001000)))
;; y should be Float32(0xFEE83948 [Rational(-154339158138264215304390000238949040128), -154339158138264215304390000238949040128.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010001 #b01100011101100010110000)))
;; z should be Float32(0xC8B1D8B0 [Rational(-728459, 2), -364229.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
