(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE1F0BD)))
;; x should be Float32(0xCAE1F0BD [Rational(-14807229, 2), -7403614.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01111100101011011101010)))
;; y should be Float32(0x003E56EA [Rational(2042741, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x12A0797D)))
;; z should be Float32(0x12A0797D [Rational(10516861, 10384593717069655257060992658440192), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x12A077C5)))
(check-sat)
(exit)
