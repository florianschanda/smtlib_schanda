(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x29D12578)))
;; x should be Float32(0x29D12578 [Rational(1713327, 18446744073709551616), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00110110 #b01011100101110011001111)))
;; y should be Float32(0x9B2E5CCF [Rational(-11427023, 79228162514264337593543950336), -0.000000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b00001011 #b00011100111001101001010))))
(check-sat)
(exit)
