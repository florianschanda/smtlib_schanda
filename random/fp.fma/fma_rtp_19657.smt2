(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11011110 #b01010101111000011010101)))
;; x should be Float32(0xEF2AF0D5 [Rational(-52903599730397023919572779008), -52903599730397023919572779008.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01010001111111011110100)))
;; y should be Float32(0x0028FEF4 [Rational(671677, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b01110100001010001000010)))
;; z should be Float32(0xCA3A1442 [Rational(-6097441, 2), -3048720.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010100 #b01110100001010001000010))))
(check-sat)
(exit)
