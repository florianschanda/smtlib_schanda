(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010011 #b10101110110101001011011)))
;; x should be Float32(0x89D76A5B [Rational(-14117467, 2722258935367507707706996859454145691648), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1D86E53B)))
;; y should be Float32(0x1D86E53B [Rational(8840507, 2475880078570760549798248448), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC8F47070)))
;; z should be Float32(0xC8F47070 [Rational(-1001223, 2), -500611.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xC8F47070)))
(check-sat)
(exit)
