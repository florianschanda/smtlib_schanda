(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10010101011100100011100)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00011101 #b01011010100010100010000)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0079C448)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b00000000 #b11110011100010001001000)))
(check-sat)
(exit)
