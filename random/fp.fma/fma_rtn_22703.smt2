(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE8DEE2CF)))
;; y should be Float32(0xE8DEE2CF [Rational(-8420393914237483577507840), -8420393914237483577507840.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xD2967D97)))
;; z should be Float32(0xD2967D97 [Rational(-323176071168), -323176071168.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xD2967D98))))
(check-sat)
(exit)
