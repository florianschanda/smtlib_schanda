(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x005EED31)))
;; x should be Float32(0x005EED31 [Rational(6221105, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10110010111011000000111)))
;; y should be Float32(0x4AD97607 [Rational(14251527, 2), 7125763.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x800897BD)))
;; z should be Float32(0x800897BD [Rational(-563133, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b00010111 #b01000010100010110100101)))
(check-sat)
(exit)
