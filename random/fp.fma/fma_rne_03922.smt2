(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA5F1D32)))
;; x should be Float32(0xCA5F1D32 [Rational(-7311001, 2), -3655500.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01100101 #b00010011100110001011101)))
;; y should be Float32(0xB289CC5D [Rational(-9030749, 562949953421312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x34555313)))
;; z should be Float32(0x34555313 [Rational(13980435, 70368744177664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x3D7031AD))))
(check-sat)
(exit)
