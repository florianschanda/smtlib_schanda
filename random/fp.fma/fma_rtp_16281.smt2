(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100001 #b11010010011100100110001)))
;; x should be Float32(0x30E93931 [Rational(15284529, 9007199254740992), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB0F2A7)))
;; y should be Float32(0x4AB0F2A7 [Rational(11596455, 2), 5798227.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01001101 #b00111100101110110010011)))
;; z should be Float32(0xA69E5D93 [Rational(-10378643, 9444732965739290427392), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x3C213462)))
(check-sat)
(exit)
