(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802810CA)))
;; x should be Float32(0x802810CA [Rational(-1312869, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000001)))
;; y should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xA0E50DF9)))
;; z should be Float32(0xA0E50DF9 [Rational(-15011321, 38685626227668133590597632), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xA0E50DF8))))
(check-sat)
(exit)
