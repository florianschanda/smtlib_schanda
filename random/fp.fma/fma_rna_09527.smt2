(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100100 #b10111111101011000100001)))
;; x should be Float32(0x325FD621 [Rational(14669345, 1125899906842624), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01111111 #b00011101111111011010101)))
;; y should be Float32(0x3F8EFED5 [Rational(9371349, 8388608), 1.117152])

(declare-const z Float32)
(assert (= z (fp #b0 #b10000111 #b11100000001100001000010)))
;; z should be Float32(0x43F01842 [Rational(7867425, 16384), 480.189514])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x43F01842)))
(check-sat)
(exit)
