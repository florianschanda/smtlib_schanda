(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010000 #b11101100101100101001111)))
;; x should be Float32(0x0876594F [Rational(16144719, 21778071482940061661655974875633165533184), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10111111000000101010110)))
;; y should be Float32(0x4A5F8156 [Rational(7323819, 2), 3661909.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b00100110 #b10101110001010001000010))))
(check-sat)
(exit)
