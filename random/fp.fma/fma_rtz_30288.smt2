(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2A07AEE3)))
;; x should be Float32(0x2A07AEE3 [Rational(8892131, 73786976294838206464), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10101011111010110001010)))
;; y should be Float32(0x8055F58A [Rational(-2816709, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b01011011101101001110110)))
;; z should be Float32(0x4A2DDA76 [Rational(5696827, 2), 2848413.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010100 #b01011011101101001110101))))
(check-sat)
(exit)
