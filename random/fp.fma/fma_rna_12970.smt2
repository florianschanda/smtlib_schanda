(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111000 #b00011101111111011010010)))
;; x should be Float32(0xFC0EFED2 [Rational(-2969898095461604172199630899221889024), -2969898095461604172199630899221889024.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8072FCF3)))
;; y should be Float32(0x8072FCF3 [Rational(-7535859, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b11111111 #b00000000000000000000000))))
(check-sat)
(exit)
