(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00011000010100011011110)))
;; x should be Float32(0x4A0C28DE [Rational(4592751, 2), 2296375.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11110100 #b11100010001110001110111)))
;; y should be Float32(0xFA711C77 [Rational(-312980219631783725675973442527035392), -312980219631783725675973442527035392.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11010000000001100000000)))
;; z should be Float32(0x00680300 [Rational(26627, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
