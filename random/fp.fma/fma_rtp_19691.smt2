(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x93B0CEEE)))
;; x should be Float32(0x93B0CEEE [Rational(-5793655, 1298074214633706907132624082305024), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAF44CB1)))
;; y should be Float32(0xCAF44CB1 [Rational(-16010417, 2), -8005208.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00111110 #b01010001011101000110011)))
(check-sat)
(exit)
