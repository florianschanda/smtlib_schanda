(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011011 #b11011111000101101000100)))
;; x should be Float32(0x2DEF8B44 [Rational(3924689, 144115188075855872), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00010100 #b00010100010100010110111)))
;; y should be Float32(0x8A0A28B7 [Rational(-9054391, 1361129467683753853853498429727072845824), -0.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
