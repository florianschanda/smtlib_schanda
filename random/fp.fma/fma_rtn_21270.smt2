(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b11100111111100011000010)))
;; x should be Float32(0x4A73F8C2 [Rational(7994465, 2), 3997232.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00011001001000000000011)))
;; y should be Float32(0x800C9003 [Rational(-823299, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x004240A9)))
;; z should be Float32(0x004240A9 [Rational(4341929, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x89BF8E6E)))
(check-sat)
(exit)
