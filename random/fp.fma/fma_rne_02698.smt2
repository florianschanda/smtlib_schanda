(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00100110 #b01110111111111010110101)))
;; x should be Float32(0x133BFEB5 [Rational(12320437, 5192296858534827628530496329220096), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x006AF40A)))
;; y should be Float32(0x006AF40A [Rational(3504645, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7FDB800E)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
