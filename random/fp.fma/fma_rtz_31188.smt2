(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11011000011110011101100)))
;; x should be Float32(0xC9EC3CEC [Rational(-3870523, 2), -1935261.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x51962727)))
;; y should be Float32(0x51962727 [Rational(80612745216), 80612745216.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x006546F7)))
;; z should be Float32(0x006546F7 [Rational(6637303, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10111000 #b00010101000111111010011))))
(check-sat)
(exit)
