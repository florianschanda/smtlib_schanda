(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000000 #b00001111011110110100100)))
;; x should be Float32(0x4007BDA4 [Rational(2223977, 1048576), 2.120950])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF7F5B912)))
;; y should be Float32(0xF7F5B912 [Rational(-9967706267379949881066238483890176), -9967706267379949881066238483890176.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA2BD646)))
;; z should be Float32(0xCA2BD646 [Rational(-5630755, 2), -2815377.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xF8824A9F))))
(check-sat)
(exit)
