(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9F2C4DE5)))
;; x should be Float32(0x9F2C4DE5 [Rational(-11292133, 309485009821345068724781056), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00100011011011011011001)))
;; y should be Float32(0x4A91B6D9 [Rational(9549529, 2), 4774764.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00499CFD)))
;; z should be Float32(0x00499CFD [Rational(4824317, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xAA442660)))
(check-sat)
(exit)
