(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA8FD331)))
;; x should be Float32(0xCA8FD331 [Rational(-9425713, 2), -4712856.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10010100011000101001110)))
;; y should be Float32(0x004A314E [Rational(2431143, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01000011 #b10100010110100101010101)))
;; z should be Float32(0xA1D16955 [Rational(-13723989, 9671406556917033397649408), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #xA1D16955)))
(check-sat)
(exit)
