(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10111001 #b10011000111110101110100)))
;; x should be Float32(0xDCCC7D74 [Rational(-460470659349217280), -460470659349217280.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80000001)))
;; y should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00101101 #b00110001110111110100111)))
;; z should be Float32(0x9698EFA7 [Rational(-10022823, 40564819207303340847894502572032), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00101101 #b00110001000100101101000)))
(check-sat)
(exit)
