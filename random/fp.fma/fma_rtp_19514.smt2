(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11011111 #b10110000100011011000000)))
;; x should be Float32(0xEFD846C0 [Rational(-133868587246296539717326340096), -133868587246296539717326340096.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b00010011011100010110000)))
;; y should be Float32(0x4889B8B0 [Rational(564107, 2), 282053.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11110001 #b11010001011001111001000))))
(check-sat)
(exit)
