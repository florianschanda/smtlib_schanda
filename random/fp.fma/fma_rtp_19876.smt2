(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x803A1BB2)))
;; x should be Float32(0x803A1BB2 [Rational(-1904089, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A90597F)))
;; y should be Float32(0x4A90597F [Rational(9460095, 2), 4730047.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A6B0E6A)))
;; z should be Float32(0x4A6B0E6A [Rational(7702325, 2), 3851162.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010100 #b11010110000111001101010))))
(check-sat)
(exit)
