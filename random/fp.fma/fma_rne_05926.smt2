(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9617E6A4)))
;; x should be Float32(0x9617E6A4 [Rational(-2488745, 20282409603651670423947251286016), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11000100 #b11111011001000000111111)))
;; y should be Float32(0x627D903F [Rational(1169355069278377738240), 1169355069278377738240.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11010100 #b10101111000000011111011)))
;; z should be Float32(0xEA5780FB [Rational(-65132036064928772061659136), -65132036064928772061659136.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11010100 #b10101111000000011111011)))
(check-sat)
(exit)
