(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01110010000101111100110)))
;; x should be Float32(0x00390BE6 [Rational(1869299, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A9F77A1)))
;; y should be Float32(0x4A9F77A1 [Rational(10450849, 2), 5225424.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11111000110110111001011)))
;; z should be Float32(0x4AFC6DCB [Rational(16543179, 2), 8271589.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010101 #b11111000110110111001011))))
(check-sat)
(exit)
