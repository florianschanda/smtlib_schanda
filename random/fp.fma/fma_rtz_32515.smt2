(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10100000 #b00000001110111110111101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11000001 #b10011101001111101010011)))
(declare-const z Float32)
(assert (= z (fp #b1 #b01000110 #b11001110011010011110000)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11100010 #b10100000010001001010001))))
(check-sat)
(exit)
