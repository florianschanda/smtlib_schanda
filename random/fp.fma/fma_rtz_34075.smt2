(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFFF61954)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b01011011 #b01010000101010001100001)))
;; y should be Float32(0x2DA85461 [Rational(11031649, 576460752303423488), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x5F098EBB)))
;; z should be Float32(0x5F098EBB [Rational(9912065438563434496), 9912065438563434496.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
