(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x29C63ECB)))
;; x should be Float32(0x29C63ECB [Rational(12992203, 147573952589676412928), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x32BDE9C9)))
;; y should be Float32(0x32BDE9C9 [Rational(12446153, 562949953421312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49E95774)))
;; z should be Float32(0x49E95774 [Rational(3823069, 2), 1911534.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10010011 #b11010010101011101110100)))
(check-sat)
(exit)
