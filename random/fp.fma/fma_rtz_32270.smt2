(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4D730BED)))
;; y should be Float32(0x4D730BED [Rational(254852816), 254852816.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00010100 #b00110100001100101011001)))
;; z should be Float32(0x0A1A1959 [Rational(10099033, 1361129467683753853853498429727072845824), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8E7271D1))))
(check-sat)
(exit)
