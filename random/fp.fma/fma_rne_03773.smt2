(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA4FF772)))
;; x should be Float32(0xCA4FF772 [Rational(-6814649, 2), -3407324.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x005586FD)))
;; y should be Float32(0x005586FD [Rational(5605117, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01011101100100100100001)))
;; z should be Float32(0xCAAEC921 [Rational(-11454753, 2), -5727376.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b10010101 #b01011101100100100100001)))
(check-sat)
(exit)
