(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00000001)))
;; x should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000011 #b01101001010111011111101)))
;; y should be Float32(0xE1B4AEFD [Rational(-416627895974928318464), -416627895974928318464.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7734ACA3)))
;; z should be Float32(0x7734ACA3 [Rational(3664511418666355021622796930252800), 3664511418666355021622796930252800.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b11101110 #b01101001010110010100011)))
(check-sat)
(exit)
