(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11010100000101011001111)))
;; x should be Float32(0x006A0ACF [Rational(6949583, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA15D0FA)))
;; y should be Float32(0xCA15D0FA [Rational(-4909181, 2), -2454590.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x1F9D974B)))
;; z should be Float32(0x1F9D974B [Rational(10327883, 154742504910672534362390528), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00111111 #b00111011001011101001010))))
(check-sat)
(exit)
