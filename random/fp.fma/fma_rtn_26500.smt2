(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001001 #b10100111001100111101011)))
;; x should be Float32(0x84D399EB [Rational(-13867499, 2787593149816327892691964784081045188247552), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x606A28F6)))
;; y should be Float32(0x606A28F6 [Rational(67492025935215984640), 67492025935215984640.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xA5C18C8D))))
(check-sat)
(exit)
