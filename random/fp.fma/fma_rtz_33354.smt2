(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x887FE0DF)))
;; x should be Float32(0x887FE0DF [Rational(-16769247, 21778071482940061661655974875633165533184), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10000001 #b11101101011100100001001)))
;; y should be Float32(0x40F6B909 [Rational(16169225, 2097152), 7.710087])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00778F33)))
;; z should be Float32(0x00778F33 [Rational(7835443, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00010011 #b11101101001101011101010)))
(check-sat)
(exit)
