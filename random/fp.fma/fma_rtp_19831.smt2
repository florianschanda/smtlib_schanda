(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10101011 #b10111111011010100111110)))
;; x should be Float32(0xD5DFB53E [Rational(-30746190282752), -30746190282752.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x803F0C3B)))
;; y should be Float32(0x803F0C3B [Rational(-4131899, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00101100 #b10111000110000100101010)))
(check-sat)
(exit)
