(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
;; x should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01101100011111001110011)))
;; y should be Float32(0xCAB63E73 [Rational(-11943539, 2), -5971769.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00418DCF)))
;; z should be Float32(0x00418DCF [Rational(4296143, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x009CAD09))))
(check-sat)
(exit)
