(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA85B22B)))
;; y should be Float32(0xCA85B22B [Rational(-8761899, 2), -4380949.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xE6269F67)))
;; z should be Float32(0xE6269F67 [Rational(-196713322599066707689472), -196713322599066707689472.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xE6269F67))))
(check-sat)
(exit)
