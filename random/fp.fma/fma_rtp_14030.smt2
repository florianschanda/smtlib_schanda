(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x88D98D9E)))
;; y should be Float32(0x88D98D9E [Rational(-7128783, 5444517870735015415413993718908291383296), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10001011 #b00100000011101010111111)))
;; z should be Float32(0x45903ABF [Rational(9452223, 2048), 4615.343262])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x45903ABF))))
(check-sat)
(exit)
