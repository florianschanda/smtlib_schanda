(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11000100 #b00111000011111101101001)))
;; x should be Float32(0xE21C3F69 [Rational(-720565314698908532736), -720565314698908532736.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10011011 #b00011010101010111000100)))
;; y should be Float32(0x4D8D55C4 [Rational(296401024), 296401024.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00001001111100010001111)))
;; z should be Float32(0x8004F88F [Rational(-325775, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11100000 #b01011001000011010001011))))
(check-sat)
(exit)
