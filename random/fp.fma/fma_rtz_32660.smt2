(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001000 #b11100100110110110100000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11000010000000100100100)))
(declare-const z Float32)
(assert (= z (fp #b0 #b01100111 #b10011000001010000000101)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b01100111 #b10011000001010000000101))))
(check-sat)
(exit)
