(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80529784)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0055BE8E)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10100011 #b11011001000100100000000)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x51EC8900)))
(check-sat)
(exit)
