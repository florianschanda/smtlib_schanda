(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA8E9513)))
;; x should be Float32(0xCA8E9513 [Rational(-9344275, 2), -4672137.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x6F5C58AA)))
;; y should be Float32(0x6F5C58AA [Rational(68193890435124090326512304128), 68193890435124090326512304128.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01110111 #b10011011111000100000100)))
;; z should be Float32(0xBBCDF104 [Rational(-3374145, 536870912), -0.006285])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b11110100 #b11101010111001011111101)))
(check-sat)
(exit)
