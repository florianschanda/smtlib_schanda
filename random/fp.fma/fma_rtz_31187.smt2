(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB7F851)))
;; x should be Float32(0xCAB7F851 [Rational(-12056657, 2), -6028328.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b10001001110111010000111)))
;; y should be Float32(0x49C4EE87 [Rational(12906119, 8), 1613264.875000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00111110 #b01101101110101001010000)))
;; z should be Float32(0x1F36EA50 [Rational(749221, 19342813113834066795298816), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b10101010 #b00011011000010110000111)))
(check-sat)
(exit)
