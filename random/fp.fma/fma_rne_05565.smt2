(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const y Float32)
(assert (= y (fp #b0 #b11101111 #b11011000111010010001100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00010000 #b11110101001100000001101)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b01011010 #b11011000111010010001100)))
(check-sat)
(exit)
