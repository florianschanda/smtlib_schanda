(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01100101010001110001001)))
;; x should be Float32(0x0032A389 [Rational(3318665, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A8A0F89)))
;; y should be Float32(0x4A8A0F89 [Rational(9047945, 2), 4523972.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00011010 #b11010101111110001011010)))
;; z should be Float32(0x8D6AFC5A [Rational(-7700013, 10633823966279326983230456482242756608), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8D64288C))))
(check-sat)
(exit)
