(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10111100 #b11010011110000101010000)))
;; y should be Float32(0xDE69E150 [Rational(-4213209810381832192), -4213209810381832192.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01011110011110110001111)))
;; z should be Float32(0x4AAF3D8F [Rational(11484559, 2), 5742279.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010101 #b01011110011110110001111))))
(check-sat)
(exit)
