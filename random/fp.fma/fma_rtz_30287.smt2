(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0B1D00C5)))
;; x should be Float32(0x0B1D00C5 [Rational(10289349, 340282366920938463463374607431768211456), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11100111110110111000001)))
;; y should be Float32(0x8073EDC1 [Rational(-7597505, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ -zero 8 24))))
(check-sat)
(exit)
