(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001010 #b01100011011101110001011)))
;; x should be Float32(0x0531BB8B [Rational(11647883, 1393796574908163946345982392040522594123776), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11101101001011000000100)))
;; y should be Float32(0xC9F69604 [Rational(-4040065, 2), -2020032.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b00011111 #b01010110011001001011110))))
(check-sat)
(exit)
