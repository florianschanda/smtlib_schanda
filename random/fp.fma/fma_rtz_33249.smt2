(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110011 #b00011011010001111000011)))
;; x should be Float32(0xD98DA3C3 [Rational(-4983503703769088), -4983503703769088.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC153BB)))
;; y should be Float32(0x4AC153BB [Rational(12669883, 2), 6334941.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x803D0714)))
;; z should be Float32(0x803D0714 [Rational(-999877, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xE4D5ED93))))
(check-sat)
(exit)
