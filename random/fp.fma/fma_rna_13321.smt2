(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00001010100011000111010)))
;; x should be Float32(0x8005463A [Rational(-172829, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8071C186)))
;; y should be Float32(0x8071C186 [Rational(-3727555, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10011000110001001001001)))
;; z should be Float32(0x4ACC6249 [Rational(13394505, 2), 6697252.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010101 #b10011000110001001001001))))
(check-sat)
(exit)
