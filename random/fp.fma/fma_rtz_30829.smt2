(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10011101 #b11011111000111111101100)))
;; y should be Float32(0x4EEF8FEC [Rational(2009593344), 2009593344.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b10000100111101110101001)))
;; z should be Float32(0xCAC27BA9 [Rational(-12745641, 2), -6372820.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010101 #b10000100111101110101001))))
(check-sat)
(exit)
