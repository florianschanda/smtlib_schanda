(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10100000110011100111011)))
;; y should be Float32(0x4AD0673B [Rational(13657915, 2), 6828957.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x993DCB62)))
;; z should be Float32(0x993DCB62 [Rational(-6219185, 633825300114114700748351602688), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00110010 #b01111011100101101100010)))
(check-sat)
(exit)
