(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x90FB14B2)))
;; x should be Float32(0x90FB14B2 [Rational(-8227417, 83076749736557242056487941267521536), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111111010000000110101)))
;; y should be Float32(0x807FA035 [Rational(-8364085, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xEBCC79C8)))
;; z should be Float32(0xEBCC79C8 [Rational(-494391925789252641242480640), -494391925789252641242480640.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xEBCC79C8)))
(check-sat)
(exit)
