(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10001111 #b01000100111100100011000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10101010 #b11110111110111001011000)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10101010 #b11110111110111001010111))))
(check-sat)
(exit)
