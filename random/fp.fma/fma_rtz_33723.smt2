(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8074C6C6)))
;; x should be Float32(0x8074C6C6 [Rational(-3826531, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010000 #b01010111111010101110000)))
;; y should be Float32(0x682BF570 [Rational(3248208765277201678467072), 3248208765277201678467072.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b01100000 #b00000100001100101101001)))
;; z should be Float32(0xB0021969 [Rational(-8526185, 18014398509481984), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b01100000 #b00000100001101111011100)))
(check-sat)
(exit)
