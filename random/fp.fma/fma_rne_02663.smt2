(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x058F42BF)))
;; x should be Float32(0x058F42BF [Rational(9388735, 696898287454081973172991196020261297061888), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11000000010000011010011)))
;; y should be Float32(0x4AE020D3 [Rational(14688467, 2), 7344233.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b00000000000000000000000)))
;; z should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x7F800000)))
(check-sat)
(exit)
