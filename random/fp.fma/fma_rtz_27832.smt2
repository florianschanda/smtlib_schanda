(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010001 #b10000111011011001110000)))
;; x should be Float32(0x48C3B670 [Rational(801639, 2), 400819.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b10110100010011001010000)))
;; y should be Float32(0x48DA2650 [Rational(893541, 2), 446770.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x5226C6A5))))
(check-sat)
(exit)
