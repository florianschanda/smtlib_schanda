(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA6529A2)))
;; x should be Float32(0xCA6529A2 [Rational(-7509201, 2), -3754600.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01100101011100101100001)))
;; y should be Float32(0x8032B961 [Rational(-3324257, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b10011011111010111101100)))
;; z should be Float32(0x49CDF5EC [Rational(3374459, 2), 1687229.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x49CDF5EC))))
(check-sat)
(exit)
