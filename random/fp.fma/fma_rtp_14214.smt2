(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00011010100000101100011)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x21F86BF3)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00011111011010100010011)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010101 #b00011111011010100010100))))
(check-sat)
(exit)
