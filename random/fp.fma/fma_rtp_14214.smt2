(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ACA5C49)))
;; x should be Float32(0x4ACA5C49 [Rational(13261897, 2), 6630948.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01100000 #b11010010001111011000110)))
;; y should be Float32(0x30691EC6 [Rational(7638883, 9007199254740992), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b11000010101100011101001)))
;; z should be Float32(0x4AE158E9 [Rational(14768361, 2), 7384180.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x4AE158EA)))
(check-sat)
(exit)
