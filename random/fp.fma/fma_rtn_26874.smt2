(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01110101110011110001110)))
;; x should be Float32(0x803AE78E [Rational(-1930183, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0029ED3B)))
;; y should be Float32(0x0029ED3B [Rational(2747707, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11010011 #b01000111001101110011010)))
;; z should be Float32(0x69A39B9A [Rational(24723714525077087719522304), 24723714525077087719522304.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b11010011 #b01000111001101110011001))))
(check-sat)
(exit)
