(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x004337FD)))
;; x should be Float32(0x004337FD [Rational(4405245, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x48E78290)))
;; y should be Float32(0x48E78290 [Rational(948265, 2), 474132.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11000011 #b00000110111100110011001)))
;; z should be Float32(0x61837999 [Rational(303160688525747159040), 303160688525747159040.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11000011 #b00000110111100110011001))))
(check-sat)
(exit)
