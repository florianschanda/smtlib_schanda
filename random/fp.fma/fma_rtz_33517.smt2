(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9CDAB575)))
;; x should be Float32(0x9CDAB575 [Rational(-14333301, 9903520314283042199192993792), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB0B6F243)))
;; y should be Float32(0xB0B6F243 [Rational(-11989571, 9007199254740992), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b00011100 #b00111000100101111111001)))
(check-sat)
(exit)
