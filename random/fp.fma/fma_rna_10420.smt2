(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10101011 #b01110100010110001001101)))
;; y should be Float32(0xD5BA2C4D [Rational(-25587429146624), -25587429146624.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00001000 #b01000101111010101101111)))
;; z should be Float32(0x0422F56F [Rational(10679663, 5575186299632655785383929568162090376495104), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0422F56F))))
(check-sat)
(exit)
