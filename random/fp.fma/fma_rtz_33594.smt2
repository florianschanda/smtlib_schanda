(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10011101011101010000011)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10100111010100000011001)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A1B9BDA)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010100 #b00110111001101111011001))))
(check-sat)
(exit)
