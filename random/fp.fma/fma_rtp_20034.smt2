(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10110110011110010101111)))
;; x should be Float32(0x805B3CAF [Rational(-5979311, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x70E7141F)))
;; z should be Float32(0x70E7141F [Rational(572122897427871779343361376256), 572122897427871779343361376256.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b11100001 #b11001110001010000011111))))
(check-sat)
(exit)
