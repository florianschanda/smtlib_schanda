(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA98D351)))
;; x should be Float32(0xCA98D351 [Rational(-10015569, 2), -5007784.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11100100 #b01001100011101100100110)))
;; y should be Float32(0x72263B26 [Rational(3292545133032121188872924168192), 3292545133032121188872924168192.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01101110 #b11100101010011111100011)))
;; z should be Float32(0x3772A7E3 [Rational(15902691, 1099511627776), 0.000014])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFD4678AB))))
(check-sat)
(exit)
