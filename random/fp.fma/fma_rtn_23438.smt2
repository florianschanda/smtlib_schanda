(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6FA8D42C)))
;; x should be Float32(0x6FA8D42C [Rational(104499963415739038390380658688), 104499963415739038390380658688.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x803BFE5D)))
;; y should be Float32(0x803BFE5D [Rational(-3931741, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCAAA531D)))
;; z should be Float32(0xCAAA531D [Rational(-11162397, 2), -5581198.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCAAA531E))))
(check-sat)
(exit)
