(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x07643EF8)))
;; x should be Float32(0x07643EF8 [Rational(1869791, 10889035741470030830827987437816582766592), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00100111100010100111111)))
;; y should be Float32(0x0013C53F [Rational(1295679, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b01110110 #b10110111100100001101111)))
;; z should be Float32(0x3B5BC86F [Rational(14403695, 4294967296), 0.003354])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b01110110 #b10110111100100001101111))))
(check-sat)
(exit)
