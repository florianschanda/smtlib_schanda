(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80691821)))
;; x should be Float32(0x80691821 [Rational(-6887457, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA882ED5)))
;; y should be Float32(0xCA882ED5 [Rational(-8924885, 2), -4462442.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xDE479B4F)))
;; z should be Float32(0xDE479B4F [Rational(-3595794174089101312), -3595794174089101312.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10111100 #b10001111001101101001111)))
(check-sat)
(exit)
