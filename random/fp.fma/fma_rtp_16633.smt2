(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11000110101100000001100)))
;; x should be Float32(0x0063580C [Rational(1627651, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4ACCBA8F)))
;; y should be Float32(0x4ACCBA8F [Rational(13417103, 2), 6708551.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11101101 #b11000010101000000000000)))
;; z should be Float32(0x76E15000 [Rational(2284940206911383496197807527690240), 2284940206911383496197807527690240.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b11101101 #b11000010101000000000001))))
(check-sat)
(exit)
