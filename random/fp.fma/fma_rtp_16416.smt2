(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x383F1A17)))
;; x should be Float32(0x383F1A17 [Rational(12524055, 274877906944), 0.000046])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00668510)))
;; y should be Float32(0x00668510 [Rational(419921, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
