(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001001 #b11001110011101111110000)))
;; x should be Float32(0x84E73BF0 [Rational(-947135, 174224571863520493293247799005065324265472), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b10110101000001010110010)))
;; y should be Float32(0xCA5A82B2 [Rational(-7160153, 2), -3580076.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0FC55F17))))
(check-sat)
(exit)
