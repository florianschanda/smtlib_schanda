(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010000 #b10001111101011001101101)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b00010010 #b10001111101011010101100))))
(check-sat)
(exit)
