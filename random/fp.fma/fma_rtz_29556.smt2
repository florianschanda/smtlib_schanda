(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB31606ED)))
;; y should be Float32(0xB31606ED [Rational(-9832173, 281474976710656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b01110101001001000011011)))
;; z should be Float32(0xCABA921B [Rational(-12227099, 2), -6113549.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b10010101 #b01110101001001000011011))))
(check-sat)
(exit)
