(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01110011000000101100110)))
;; x should be Float32(0x4A398166 [Rational(6078643, 2), 3039321.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x001505B7)))
;; y should be Float32(0x001505B7 [Rational(1377719, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10001111111110111101010)))
;; z should be Float32(0x8047FDEA [Rational(-2359029, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b00010011 #b11100111011110000000110)))
(check-sat)
(exit)
