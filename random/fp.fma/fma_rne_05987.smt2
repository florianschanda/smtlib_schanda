(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF25E8C93)))
;; x should be Float32(0xF25E8C93 [Rational(-4408039422909288651469227032576), -4408039422909288651469227032576.000000])

(declare-const y Float32)
(assert (= y (_ -oo 8 24)))
;; y should be Float32(-oo)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b01001010101100101100111)))
;; z should be Float32(0x4AA55967 [Rational(10836327, 2), 5418163.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
