(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01000001010101101010111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11000110001010010001011)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11111111111111111111111)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b00010101 #b11001111101010001110010)))
(check-sat)
(exit)
