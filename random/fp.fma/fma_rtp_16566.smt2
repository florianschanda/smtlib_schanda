(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001010 #b11110111010011001011101)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11111000 #b00011001011101111101011)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010001 #b11100100001001101110111)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111110 #b11111111111111111111111))))
(check-sat)
(exit)
