(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000011 #b10010001111010010100111)))
;; x should be Float32(0x21C8F4A7 [Rational(13169831, 9671406556917033397649408), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFFE9620A)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8025BBC9)))
;; z should be Float32(0x8025BBC9 [Rational(-2472905, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
