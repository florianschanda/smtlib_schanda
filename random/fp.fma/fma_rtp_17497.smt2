(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10111101101011001111111)))
;; x should be Float32(0xCADED67F [Rational(-14603903, 2), -7301951.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01001010100101011101011)))
;; y should be Float32(0x00254AEB [Rational(2444011, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x9474223F)))
;; z should be Float32(0x9474223F [Rational(-15999551, 1298074214633706907132624082305024), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x9474225F))))
(check-sat)
(exit)
