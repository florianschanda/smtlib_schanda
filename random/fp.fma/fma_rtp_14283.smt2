(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00111101001110110000111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00001011111101011101001)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b10101011 #b01001100000011010011010)))
(check-sat)
(exit)
