(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111111 #b00111010000100000111011)))
;; x should be Float32(0x3F9D083B [Rational(10291259, 8388608), 1.226814])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA2E493ED)))
;; y should be Float32(0xA2E493ED [Rational(-14980077, 2417851639229258349412352), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b01000110 #b00011000011011000010010))))
(check-sat)
(exit)
