(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC894A9F0)))
;; x should be Float32(0xC894A9F0 [Rational(-608927, 2), -304463.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10000001 #b00100101011010011101101)))
;; y should be Float32(0x4092B4ED [Rational(9614573, 2097152), 4.584586])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x1E44CA1B)))
;; z should be Float32(0x1E44CA1B [Rational(12896795, 1237940039285380274899124224), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b10010011 #b01010100110001111111000))))
(check-sat)
(exit)
