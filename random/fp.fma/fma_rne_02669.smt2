(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x41C69D7A)))
;; x should be Float32(0x41C69D7A [Rational(6508221, 262144), 24.826893])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x68E0B9F0)))
;; y should be Float32(0x68E0B9F0 [Rational(8489920269112047180972032), 8489920269112047180972032.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x006186DB)))
;; z should be Float32(0x006186DB [Rational(6391515, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b11010110 #b01011100101101000001101)))
(check-sat)
(exit)
