(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA10D25A)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00010101010100101101001)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b01111111011011000100110)))
(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10101010 #b00111001110001001111001))))
(check-sat)
(exit)
