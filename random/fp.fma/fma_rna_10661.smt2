(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10100010100011011100110)))
;; x should be Float32(0xCA5146E6 [Rational(-6857587, 2), -3428793.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b00001011100101011001000)))
;; y should be Float32(0xC905CAC8 [Rational(-1096025, 2), -548012.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AB593CB)))
;; z should be Float32(0x4AB593CB [Rational(11899851, 2), 5949925.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x53DABF63))))
(check-sat)
(exit)
