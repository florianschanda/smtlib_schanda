(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10110110 #b11100001011111110110001)))
;; x should be Float32(0x5B70BFB1 [Rational(67764761340674048), 67764761340674048.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805A79CB)))
;; y should be Float32(0x805A79CB [Rational(-5929419, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00010101 #b11101001110101000100110)))
;; z should be Float32(0x0AF4EA26 [Rational(8025363, 340282366920938463463374607431768211456), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00111000 #b01010100010101111011100))))
(check-sat)
(exit)
