(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01010111111101001010011)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00001100 #b11110010011111000010100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b01101110000110001111010)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xCA370C7A)))
(check-sat)
(exit)
