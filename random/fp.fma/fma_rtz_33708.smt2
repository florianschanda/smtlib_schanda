(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11100010101100100110100)))
;; x should be Float32(0x80715934 [Rational(-1857101, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00100110 #b10101001011011100000100)))
;; y should be Float32(0x1354B704 [Rational(3485121, 1298074214633706907132624082305024), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49E68F04)))
;; z should be Float32(0x49E68F04 [Rational(3777473, 2), 1888736.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x49E68F03))))
(check-sat)
(exit)
