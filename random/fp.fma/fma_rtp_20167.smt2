(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11001100000011110010010)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11001011 #b10000110011000110101111)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x1B467820)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b01001101 #b00110111001011100100101))))
(check-sat)
(exit)
