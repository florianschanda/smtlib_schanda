(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11110011111000000001100)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b00101101100010100111000)))
(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b11101000011101001111001)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
