(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x72E69340)))
;; x should be Float32(0x72E69340 [Rational(9134024522988495353942416293888), 9134024522988495353942416293888.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x806C04A8)))
;; y should be Float32(0x806C04A8 [Rational(-884885, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
