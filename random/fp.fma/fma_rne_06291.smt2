(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80178F7A)))
;; x should be Float32(0x80178F7A [Rational(-772029, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0074F686)))
;; y should be Float32(0x0074F686 [Rational(3832643, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b01000000110100101000100)))
;; z should be Float32(0x49A06944 [Rational(2628177, 2), 1314088.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10010011 #b01000000110100101000100)))
(check-sat)
(exit)
