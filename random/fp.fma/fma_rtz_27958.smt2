(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A4DF95E)))
;; x should be Float32(0x4A4DF95E [Rational(6749359, 2), 3374679.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x000F010B)))
;; y should be Float32(0x000F010B [Rational(983307, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11101111100010100110000)))
;; z should be Float32(0x0077C530 [Rational(490579, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00010011 #b10000010010011101010011))))
(check-sat)
(exit)
