(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101011 #b10011100111101001000000)))
;; x should be Float32(0x95CE7A40 [Rational(-211433, 2535301200456458802993406410752), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10000010010010010100111)))
;; y should be Float32(0xCAC124A7 [Rational(-12657831, 2), -6328915.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x807FFFFF)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b01000010 #b00110111100011110111010)))
(check-sat)
(exit)
