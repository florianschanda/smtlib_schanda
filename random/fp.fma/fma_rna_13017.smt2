(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x801D7F08)))
;; x should be Float32(0x801D7F08 [Rational(-241633, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01010000011101100001100)))
;; y should be Float32(0x49A83B0C [Rational(2756291, 2), 1378145.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A940C03)))
;; z should be Float32(0x4A940C03 [Rational(9702403, 2), 4851201.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x4A940C03)))
(check-sat)
(exit)
