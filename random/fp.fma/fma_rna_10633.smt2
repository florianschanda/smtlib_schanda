(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA7744CE)))
;; x should be Float32(0xCA7744CE [Rational(-8102503, 2), -4051251.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00011001011101000110110)))
;; y should be Float32(0x000CBA36 [Rational(417051, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ -oo 8 24)))
(check-sat)
(exit)
