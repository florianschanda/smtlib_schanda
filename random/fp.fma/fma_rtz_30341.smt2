(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11100000101100110001010)))
;; x should be Float32(0x0070598A [Rational(3681477, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (_ +zero 8 24)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b1 #b00001010 #b10000011000111001101000)))
;; z should be Float32(0x85418E68 [Rational(-1585613, 174224571863520493293247799005065324265472), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b00001010 #b10000011000111001101000))))
(check-sat)
(exit)
