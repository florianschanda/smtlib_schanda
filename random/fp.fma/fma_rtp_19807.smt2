(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10101101 #b10010001000001011001111)))
(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB0F130DD)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11000101010100000111010)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010000 #b01111001110100101101011))))
(check-sat)
(exit)
