(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80362A30)))
;; x should be Float32(0x80362A30 [Rational(-221859, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01001011101110101010011)))
;; y should be Float32(0xCAA5DD53 [Rational(-10870099, 2), -5435049.500000])

(declare-const z Float32)
(assert (= z (_ +zero 8 24)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x0B0C6014)))
(check-sat)
(exit)
