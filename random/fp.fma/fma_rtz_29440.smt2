(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA067796)))
;; y should be Float32(0xCA067796 [Rational(-4406219, 2), -2203109.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x0001D9F5)))
;; z should be Float32(0x0001D9F5 [Rational(121333, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x8B067795))))
(check-sat)
(exit)
