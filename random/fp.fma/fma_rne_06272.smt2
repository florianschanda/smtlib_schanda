(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10100110101101000011100)))
;; x should be Float32(0x80535A1C [Rational(-1365639, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x23A8E822)))
;; y should be Float32(0x23A8E822 [Rational(5534737, 302231454903657293676544), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AB48F8B)))
;; z should be Float32(0x4AB48F8B [Rational(11833227, 2), 5916613.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4AB48F8B))))
(check-sat)
(exit)
