(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1631842A)))
;; x should be Float32(0x1631842A [Rational(5816853, 40564819207303340847894502572032), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10110110001110100010001)))
;; y should be Float32(0x005B1D11 [Rational(5971217, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4AEDF54D)))
;; z should be Float32(0x4AEDF54D [Rational(15594829, 2), 7797414.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10010101 #b11011011111010101001101))))
(check-sat)
(exit)
