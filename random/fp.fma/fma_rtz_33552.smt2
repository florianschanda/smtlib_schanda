(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD20FE86B)))
;; x should be Float32(0xD20FE86B [Rational(-154519912448), -154519912448.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11101011010100100010101)))
;; y should be Float32(0x8075A915 [Rational(-7710997, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00110110 #b01011000010100000111000)))
;; z should be Float32(0x9B2C2838 [Rational(-1410311, 9903520314283042199192993792), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b1 #b00110110 #b01011000010011110110011)))
(check-sat)
(exit)
