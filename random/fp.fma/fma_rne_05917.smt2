(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC5D00598)))
;; x should be Float32(0xC5D00598 [Rational(-1704115, 256), -6656.699219])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x39EE80A6)))
;; y should be Float32(0x39EE80A6 [Rational(7815251, 17179869184), 0.000455])

(declare-const z Float32)
(assert (= z (fp #b0 #b10101010 #b01101011001011011010011)))
;; z should be Float32(0x553596D3 [Rational(12478711857152), 12478711857152.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b0 #b10101010 #b01101011001011011010011)))
(check-sat)
(exit)
