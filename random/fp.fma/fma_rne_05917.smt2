(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110010 #b11110101100001010010001)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10101100 #b11010001001010100110010)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10001011 #b00000000011101010010101)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11100000 #b11000111101001001111000)))
(check-sat)
(exit)
