(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF818CD54)))
;; y should be Float32(0xF818CD54 [Rational(-12396776119245212346529726347608064), -12396776119245212346529726347608064.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b01010101001100000101000)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFFFFFFFF))))
(check-sat)
(exit)
