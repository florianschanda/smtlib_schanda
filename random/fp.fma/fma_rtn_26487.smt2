(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01100100 #b11100000101110010100111)))
;; x should be Float32(0xB2705CA7 [Rational(-15752359, 1125899906842624), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01001100 #b10100010100011001010100)))
;; y should be Float32(0x26514654 [Rational(3428757, 4722366482869645213696), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000000)))
;; z should be Float32(+zero)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b00110010 #b10001000111110110101101)))
(check-sat)
(exit)
