(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111111111111111111111)))
(declare-const y Float32)
(assert (= y (fp #b1 #b01001111 #b01011001011011111110000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b00011000110011100011100)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010011 #b00011000110011100011100))))
(check-sat)
(exit)
