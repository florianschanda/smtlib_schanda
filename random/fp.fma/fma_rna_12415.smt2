(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
;; x should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1EF5F0A6)))
;; y should be Float32(0x1EF5F0A6 [Rational(8058963, 309485009821345068724781056), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x74841593)))
;; z should be Float32(0x74841593 [Rational(83718354793476993274237629235200), 83718354793476993274237629235200.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x74841593))))
(check-sat)
(exit)
