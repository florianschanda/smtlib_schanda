(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01001011 #b11110101110111001100111)))
;; x should be Float32(0xA5FAEE67 [Rational(-16445031, 37778931862957161709568), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A6D189A)))
;; y should be Float32(0x4A6D189A [Rational(7769165, 2), 3884582.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x805B548B)))
;; z should be Float32(0x805B548B [Rational(-5985419, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b01100001 #b11010000110011011010011))))
(check-sat)
(exit)
