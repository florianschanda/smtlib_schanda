(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49A05AF4)))
;; x should be Float32(0x49A05AF4 [Rational(2627261, 2), 1313630.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10101110101110010101011)))
;; y should be Float32(0x4AD75CAB [Rational(14113963, 2), 7056981.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11111111 #b01100011001110001000110)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
