(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ -zero 8 24)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01000011010101110010111)))
;; y should be Float32(0x0021AB97 [Rational(2206615, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11010110 #b11111100111100011110110)))
;; z should be Float32(0xEB7E78F6 [Rational(-307638380059102300350709760), -307638380059102300350709760.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xEB7E78F6))))
(check-sat)
(exit)
