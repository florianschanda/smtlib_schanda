(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111111 #b10011100000101011010011)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b00000000000000000000000)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00011110010001001110011)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
