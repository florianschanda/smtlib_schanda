(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11010000 #b10110101100101110001110)))
(declare-const y Float32)
(assert (= y (fp #b1 #b11111110 #b11111111111111111111111)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010110 #b01101110100001110000110)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
