(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00011001001000111000010)))
;; x should be Float32(0xCA0C91C2 [Rational(-4606177, 2), -2303088.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8015C4C4)))
;; y should be Float32(0x8015C4C4 [Rational(-356657, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
