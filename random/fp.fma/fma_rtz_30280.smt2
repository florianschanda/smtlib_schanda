(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110010 #b10101010010111010111000)))
;; x should be Float32(0x19552EB8 [Rational(1746391, 158456325028528675187087900672), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00010111 #b01000010100000100101111)))
;; y should be Float32(0x8BA1412F [Rational(-10567983, 170141183460469231731687303715884105728), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xFF7FFFFF)))
;; z should be Float32(0xFF7FFFFF [Rational(-340282346638528859811704183484516925440), -340282346638528859811704183484516925440.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFF7FFFFF))))
(check-sat)
(exit)
