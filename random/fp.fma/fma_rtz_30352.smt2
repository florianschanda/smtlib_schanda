(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001AC64D)))
;; x should be Float32(0x001AC64D [Rational(1754701, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01011010110100011110110)))
;; y should be Float32(0x4A2D68F6 [Rational(5682299, 2), 2841149.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10110000010100100100010)))
;; z should be Float32(0x00582922 [Rational(2888849, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x0A1117ED))))
(check-sat)
(exit)
