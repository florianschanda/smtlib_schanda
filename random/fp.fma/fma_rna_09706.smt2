(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5F00164B)))
;; x should be Float32(0x5F00164B [Rational(9229646949714493440), 9229646949714493440.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8173C4EB)))
;; y should be Float32(0x8173C4EB [Rational(-15975659, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b01000001 #b11100111110111101100000)))
(check-sat)
(exit)
