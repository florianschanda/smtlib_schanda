(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC689E1)))
;; x should be Float32(0xCAC689E1 [Rational(-13011425, 2), -6505712.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010111 #b01011000000010111110010)))
;; y should be Float32(0xCBAC05F2 [Rational(-22547428), -22547428.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCABE9E27)))
;; z should be Float32(0xCABE9E27 [Rational(-12492327, 2), -6246163.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10101110 #b00001010110100100111111))))
(check-sat)
(exit)
