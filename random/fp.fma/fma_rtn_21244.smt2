(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x499C5D04)))
;; x should be Float32(0x499C5D04 [Rational(2561857, 2), 1280928.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA003373D)))
;; y should be Float32(0xA003373D [Rational(-8599357, 77371252455336267181195264), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x49565D18)))
;; z should be Float32(0x49565D18 [Rational(1756067, 2), 878033.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x49565D17)))
(check-sat)
(exit)
