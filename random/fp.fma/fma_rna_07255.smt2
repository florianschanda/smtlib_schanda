(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A94A6CF)))
;; x should be Float32(0x4A94A6CF [Rational(9742031, 2), 4871015.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11101100100001000000001)))
;; y should be Float32(0x4AF64201 [Rational(16138753, 2), 8069376.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result ((_ to_fp 8 24) #x560EFE9F))))
(check-sat)
(exit)
