(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8E77544D)))
;; x should be Float32(0x8E77544D [Rational(-16208973, 5316911983139663491615228241121378304), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10111101100000001001110)))
;; y should be Float32(0x805EC04E [Rational(-3104807, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00101001110010101001101)))
;; z should be Float32(0xCA94E54D [Rational(-9758029, 2), -4879014.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xCA94E54C)))
(check-sat)
(exit)
