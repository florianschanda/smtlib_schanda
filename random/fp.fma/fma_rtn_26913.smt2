(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8040D40D)))
;; x should be Float32(0x8040D40D [Rational(-4248589, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAD77DDB)))
;; y should be Float32(0xCAD77DDB [Rational(-14122459, 2), -7061229.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00458565)))
;; z should be Float32(0x00458565 [Rational(4556133, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x0B5A47D9)))
(check-sat)
(exit)
