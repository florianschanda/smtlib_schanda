(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC8F845F0)))
;; y should be Float32(0xC8F845F0 [Rational(-1016927, 2), -508463.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x750C18E9)))
;; z should be Float32(0x750C18E9 [Rational(177594433151179036053492069302272), 177594433151179036053492069302272.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b11101010 #b00011000001100011101001))))
(check-sat)
(exit)
