(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001011 #b00011101101010010111101)))
;; x should be Float32(0x458ED4BD [Rational(9360573, 2048), 4570.592285])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10111111101101010001110)))
;; y should be Float32(0x4A5FDA8E [Rational(7335239, 2), 3667619.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x803976F1)))
;; z should be Float32(0x803976F1 [Rational(-3766001, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b0 #b10100000 #b11110011100101010000010)))
(check-sat)
(exit)
