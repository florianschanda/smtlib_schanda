(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8059E982)))
;; x should be Float32(0x8059E982 [Rational(-2946241, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80581FA9)))
;; y should be Float32(0x80581FA9 [Rational(-5775273, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (_ -oo 8 24)))
;; z should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (_ -oo 8 24)))
(check-sat)
(exit)
