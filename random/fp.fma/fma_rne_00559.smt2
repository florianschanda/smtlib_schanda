(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A3D1D92)))
;; x should be Float32(0x4A3D1D92 [Rational(6196937, 2), 3098468.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10010101100001011011100)))
;; y should be Float32(0xC9CAC2DC [Rational(-3322039, 2), -1661019.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x063AE89C)))
;; z should be Float32(0x063AE89C [Rational(3062311, 87112285931760246646623899502532662132736), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10101001 #b00101011100100101001000))))
(check-sat)
(exit)
