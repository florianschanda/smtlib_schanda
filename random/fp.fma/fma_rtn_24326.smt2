(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00011011111100101111100)))
;; x should be Float32(0xC98DF97C [Rational(-2326111, 2), -1163055.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7414D8F5)))
;; y should be Float32(0x7414D8F5 [Rational(47171652004233352331347441483776), 47171652004233352331347441483776.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000001 #b00000000000000000000000)))
;; z should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result ((_ to_fp 8 24) #xFE25191D))))
(check-sat)
(exit)
