(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10101111111011111001111)))
;; x should be Float32(0xCAD7F7CF [Rational(-14153679, 2), -7076839.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x550DD472)))
;; y should be Float32(0x550DD472 [Rational(9746474074112), 9746474074112.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b01111110010101010001110)))
;; z should be Float32(0x4A3F2A8E [Rational(6264135, 2), 3132067.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11000000 #b11011110100110101101101)))
(check-sat)
(exit)
