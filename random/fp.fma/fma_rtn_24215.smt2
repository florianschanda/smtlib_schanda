(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b10000110110110010000100)))
;; x should be Float32(0xC9C36C84 [Rational(-3201825, 2), -1600912.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11100111101010010100011)))
;; y should be Float32(0x4AF3D4A3 [Rational(15979683, 2), 7989841.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b11110001100110110101100)))
;; z should be Float32(0x0078CDAC [Rational(1979243, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xD53A2254)))
(check-sat)
(exit)
