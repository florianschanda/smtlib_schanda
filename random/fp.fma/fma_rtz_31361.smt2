(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01111000111111111011100)))
;; x should be Float32(0xC9BC7FDC [Rational(-3088375, 2), -1544187.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF24F02D0)))
;; y should be Float32(0xF24F02D0 [Rational(-4100275016760710103717346541568), -4100275016760710103717346541568.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b10110000001110010101000)))
;; z should be Float32(0xC9581CA8 [Rational(-1770389, 2), -885194.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b11111001 #b00110000110110101110101))))
(check-sat)
(exit)
