(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001101 #b01000000000111100000000)))
;; x should be Float32(0x46A00F00 [Rational(40975, 2), 20487.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0069C973)))
;; y should be Float32(0x0069C973 [Rational(6932851, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00001111 #b00001000100100000110101))))
(check-sat)
(exit)
