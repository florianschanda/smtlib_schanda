(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10011110101000110001010)))
;; x should be Float32(0x4A4F518A [Rational(6793413, 2), 3396706.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01110101 #b01001000110010000010000)))
;; y should be Float32(0x3AA46410 [Rational(673345, 536870912), 0.001254])

(declare-const z Float32)
(assert (= z (_ NaN 8 24)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
