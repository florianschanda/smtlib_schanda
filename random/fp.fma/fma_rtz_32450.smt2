(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF99E218F)))
;; y should be Float32(0xF99E218F [Rational(-102632944099082851279010569511763968), -102632944099082851279010569511763968.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00101000 #b10011001000000001011000)))
;; z should be Float32(0x944C8058 [Rational(-1675275, 162259276829213363391578010288128), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x3A9E218D))))
(check-sat)
(exit)
