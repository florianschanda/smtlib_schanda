(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB1DFB314)))
;; x should be Float32(0xB1DFB314 [Rational(-3665093, 562949953421312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010101 #b00100010101101100010101)))
;; y should be Float32(0xEA915B15 [Rational(-87862183287843957973712896), -87862183287843957973712896.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC4E08383)))
;; z should be Float32(0xC4E08383 [Rational(-14713731, 8192), -1796.109741])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10111001 #b11111100000100000001010))))
(check-sat)
(exit)
