(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x15235C6B)))
;; x should be Float32(0x15235C6B [Rational(10706027, 324518553658426726783156020576256), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC28AFFBA)))
;; y should be Float32(0xC28AFFBA [Rational(-4554717, 65536), -69.499466])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b10110011011110010000100)))
;; z should be Float32(0x49D9BC84 [Rational(3567393, 2), 1783696.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b0 #b10010011 #b10110011011110010000011)))
(check-sat)
(exit)
