(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11100110111010000110001)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00001001111001000001111)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b00010111 #b11100110111010000110000))))
(check-sat)
(exit)
