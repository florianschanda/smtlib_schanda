(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010101 #b10111011100001000101110)))
;; x should be Float32(0x8ADDC22E [Rational(-7266583, 340282366920938463463374607431768211456), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD7B67CC2)))
;; y should be Float32(0xD7B67CC2 [Rational(-401293893959680), -401293893959680.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xFFE7AA50)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
