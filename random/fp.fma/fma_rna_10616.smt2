(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01000100111000110101011)))
;; x should be Float32(0xCAA271AB [Rational(-10645931, 2), -5322965.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01111001 #b00111110100111011100110)))
;; y should be Float32(0x3C9F4EE6 [Rational(5220211, 268435456), 0.019447])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b1 #b10001111 #b10010100010110101010100))))
(check-sat)
(exit)
