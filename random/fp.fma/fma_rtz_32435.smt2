(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00111100 #b01011000000011000011001)))
;; y should be Float32(0x9E2C0619 [Rational(-11273753, 1237940039285380274899124224), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00000110001100111110011)))
;; z should be Float32(0x4A8319F3 [Rational(8591859, 2), 4295929.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010101 #b00000110001100111110011))))
(check-sat)
(exit)
