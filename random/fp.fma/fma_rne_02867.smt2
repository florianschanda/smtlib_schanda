(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x44772D42)))
;; x should be Float32(0x44772D42 [Rational(8099489, 8192), 988.707153])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x802DED81)))
;; y should be Float32(0x802DED81 [Rational(-3009921, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xB6C1D3D9)))
;; z should be Float32(0xB6C1D3D9 [Rational(-12702681, 2199023255552), -0.000006])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b01101101 #b10000011101001111011001)))
(check-sat)
(exit)
