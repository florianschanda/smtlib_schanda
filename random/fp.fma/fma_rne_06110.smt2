(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001100 #b01110111010011111000010)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010001010011111101001)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b10101101011101000110001)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b1 #b10010101 #b10101101011101000110001))))
(check-sat)
(exit)
