(assert (= z ((_ to_fp 8 24) #x4A8C01A7)))
;; z should be Float32(0x4A8C01A7 [Rational(9175463, 2), 4587731.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
