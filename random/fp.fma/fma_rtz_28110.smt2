(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01101101110011001011011)))
;; x should be Float32(0x4AB6E65B [Rational(11986523, 2), 5993261.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF20CC99F)))
;; y should be Float32(0xF20CC99F [Rational(-2788585364544104086987153080320), -2788585364544104086987153080320.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x005C4DAB)))
;; z should be Float32(0x005C4DAB [Rational(6049195, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b1 #b11111010 #b10010010010110000001100))))
(check-sat)
(exit)
