(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9A622FC)))
;; x should be Float32(0xC9A622FC [Rational(-2721983, 2), -1360991.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10010000011110110110011)))
;; y should be Float32(0x80483DB3 [Rational(-4734387, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b00111010000101011111101)))
;; z should be Float32(0x801D0AFD [Rational(-1903357, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x0A3B8781)))
(check-sat)
(exit)
