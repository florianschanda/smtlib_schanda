(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0045D3F4)))
;; x should be Float32(0x0045D3F4 [Rational(1144061, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00110010 #b10110111011100011001100)))
;; y should be Float32(0x995BB8CC [Rational(-3599923, 316912650057057350374175801344), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b00100101000111010110111)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
