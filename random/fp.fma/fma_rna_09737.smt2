(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10100011 #b11010100110101111111000)))
;; x should be Float32(0x51EA6BF8 [Rational(125854220288), 125854220288.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b11101101011111000111001)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b10010000010101101101111)))
;; z should be Float32(0x00482B6F [Rational(4729711, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
