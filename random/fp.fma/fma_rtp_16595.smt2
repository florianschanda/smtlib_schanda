(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1CF668C0)))
;; x should be Float32(0x1CF668C0 [Rational(252323, 154742504910672534362390528), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b01100101010010101100000)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b0 #b01000110 #b00010010001001001011111)))
;; z should be Float32(0x2309125F [Rational(8983135, 1208925819614629174706176), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
