(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9F9BF94)))
;; x should be Float32(0xC9F9BF94 [Rational(-4091877, 2), -2045938.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00110000111110001111010)))
;; y should be Float32(0xCA187C7A [Rational(-4996669, 2), -2498334.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x8055A913)))
;; z should be Float32(0x8055A913 [Rational(-5613843, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x5494C330)))
(check-sat)
(exit)
