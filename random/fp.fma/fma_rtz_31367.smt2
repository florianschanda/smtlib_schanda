(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCADA90D1)))
;; x should be Float32(0xCADA90D1 [Rational(-14323921, 2), -7161960.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000100 #b11001001011010110111101)))
;; y should be Float32(0xE264B5BD [Rational(-1054738318024310259712), -1054738318024310259712.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC41E7561)))
;; z should be Float32(0xC41E7561 [Rational(-10384737, 16384), -633.834045])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result (fp #b0 #b11011011 #b10000110100010000100011)))
(check-sat)
(exit)
