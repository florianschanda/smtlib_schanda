(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3828F1DA)))
;; x should be Float32(0x3828F1DA [Rational(5535981, 137438953472), 0.000040])

(declare-const y Float32)
(assert (= y (fp #b1 #b01110011 #b00010111110010100100110)))
;; y should be Float32(0xB98BE526 [Rational(-4584083, 17179869184), -0.000267])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x00E552F5)))
;; z should be Float32(0x00E552F5 [Rational(15028981, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xB238A516)))
(check-sat)
(exit)
