(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9FC6B353)))
;; x should be Float32(0x9FC6B353 [Rational(-13022035, 154742504910672534362390528), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCED57DF3)))
;; y should be Float32(0xCED57DF3 [Rational(-1790900608), -1790900608.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x535A52CB)))
;; z should be Float32(0x535A52CB [Rational(937691906048), 937691906048.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x535A52CB))))
(check-sat)
(exit)
