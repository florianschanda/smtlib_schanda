(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA1E33EA)))
;; x should be Float32(0xCA1E33EA [Rational(-5183989, 2), -2591994.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49A054D4)))
;; y should be Float32(0x49A054D4 [Rational(2626869, 2), 1313434.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xBD4B7B55)))
;; z should be Float32(0xBD4B7B55 [Rational(-13335381, 268435456), -0.049678])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result (fp #b1 #b10101000 #b10001100010100110111101)))
(check-sat)
(exit)
