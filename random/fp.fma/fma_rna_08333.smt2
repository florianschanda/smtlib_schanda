(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11111111111111111111111)))
;; x should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC61A39)))
;; y should be Float32(0x4AC61A39 [Rational(12982841, 2), 6491420.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b11001101001011111100100)))
;; z should be Float32(0xC9E697E4 [Rational(-3778041, 2), -1889020.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10010011 #b11001101001011111100100)))
(check-sat)
(exit)
