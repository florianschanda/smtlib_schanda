(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ADB5DFB)))
;; x should be Float32(0x4ADB5DFB [Rational(14376443, 2), 7188221.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b11100111000101100011110)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x1947E3D8)))
;; z should be Float32(0x1947E3D8 [Rational(1637499, 158456325028528675187087900672), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
