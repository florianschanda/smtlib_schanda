(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10011100 #b11001111010111101010110)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00000000000000000000001)))
(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10100111001000001101011)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010101 #b10100111001000001101010))))
(check-sat)
(exit)
