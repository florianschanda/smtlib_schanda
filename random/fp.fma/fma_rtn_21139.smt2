(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ADAE7BD)))
;; x should be Float32(0x4ADAE7BD [Rational(14346173, 2), 7173086.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b11101000101100111001000)))
;; y should be Float32(0xC97459C8 [Rational(-2001721, 2), -1000860.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA2A57BA)))
;; z should be Float32(0xCA2A57BA [Rational(-5581789, 2), -2790894.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xD4D0F1AC)))
(check-sat)
(exit)
