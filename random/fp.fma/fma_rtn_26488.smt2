(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDE5FF402)))
;; x should be Float32(0xDE5FF402 [Rational(-4034381390949646336), -4034381390949646336.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7148FBA6)))
;; y should be Float32(0x7148FBA6 [Rational(995219620125255072462606958592), 995219620125255072462606958592.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010011 #b10100101001011000011100)))
;; z should be Float32(0x49D2961C [Rational(3450247, 2), 1725123.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xFF800000)))
(check-sat)
(exit)
