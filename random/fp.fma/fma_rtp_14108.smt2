(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A43A2CE)))
;; x should be Float32(0x4A43A2CE [Rational(6410599, 2), 3205299.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A95F8C7)))
;; y should be Float32(0x4A95F8C7 [Rational(9828551, 2), 4914275.500000])

(declare-const z Float32)
(assert (= z (_ -zero 8 24)))
;; z should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #x556537C0)))
(check-sat)
(exit)
