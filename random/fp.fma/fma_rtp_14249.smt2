(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x488E8670)))
;; x should be Float32(0x488E8670 [Rational(583783, 2), 291891.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00718A1C)))
;; y should be Float32(0x00718A1C [Rational(1860231, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11101101000010101001111)))
;; z should be Float32(0x8076854F [Rational(-7767375, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b00010010 #b11111001101100010110011)))
(check-sat)
(exit)
