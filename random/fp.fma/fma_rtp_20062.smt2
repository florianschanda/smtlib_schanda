(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11000101010010110100000)))
;; x should be Float32(0x8062A5A0 [Rational(-202029, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA8FDE4F)))
;; y should be Float32(0xCA8FDE4F [Rational(-9428559, 2), -4714279.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xE839E6D4)))
;; z should be Float32(0xE839E6D4 [Rational(-3511583306711094408511488), -3511583306711094408511488.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result ((_ to_fp 8 24) #xE839E6D3)))
(check-sat)
(exit)
