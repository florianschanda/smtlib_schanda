(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4C209F18)))
;; y should be Float32(0x4C209F18 [Rational(42105952), 42105952.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x5A903B51)))
;; z should be Float32(0x5A903B51 [Rational(20298807862362112), 20298807862362112.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x5A903B51))))
(check-sat)
(exit)
