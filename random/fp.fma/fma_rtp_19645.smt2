(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB07309BE)))
;; x should be Float32(0xB07309BE [Rational(-7963871, 9007199254740992), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01011111 #b01001110111001001110010)))
;; y should be Float32(0x2FA77272 [Rational(5486905, 18014398509481984), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x800445F4)))
;; z should be Float32(0x800445F4 [Rational(-70013, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b01000001 #b00111101111100000000001))))
(check-sat)
(exit)
