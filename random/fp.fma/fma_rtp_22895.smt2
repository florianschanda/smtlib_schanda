(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10101001011100001001110)))
(declare-const y Float32)
(assert (= y (_ +oo 8 24)))
(declare-const z Float32)
(assert (= z (fp #b1 #b11111111 #b11101000100010000011001)))
