(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49364308)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b10010101100100011110100)))
(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b01100000111110110111000)))
(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (= result (fp #b0 #b10100111 #b00100000101111111110001)))
(check-sat)
(exit)
