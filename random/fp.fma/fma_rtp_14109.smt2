(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A10AD76)))
;; x should be Float32(0x4A10AD76 [Rational(4740795, 2), 2370397.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AE4EFDD)))
;; y should be Float32(0x4AE4EFDD [Rational(15003613, 2), 7501806.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b11001110010011100101100)))
;; z should be Float32(0xC9E7272C [Rational(-3787211, 2), -1893605.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #x5581620C))))
(check-sat)
(exit)
