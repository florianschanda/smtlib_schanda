(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11001101000110111011101)))
(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01010111101000100011110)))
(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11001000001101100100000)))
(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #x0B09A90D)))
(check-sat)
(exit)
