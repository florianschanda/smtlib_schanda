(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8052E700)))
;; x should be Float32(0x8052E700 [Rational(-21223, 2787593149816327892691964784081045188247552), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805933DD)))
;; y should be Float32(0x805933DD [Rational(-5845981, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b00000000000000000000001)))
;; z should be Float32(0x00000001 [Rational(1, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (not (= result (fp #b0 #b00000000 #b00000000000000000000001))))
(check-sat)
(exit)
