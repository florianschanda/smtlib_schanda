(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFFEF0BFE)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A98BEBD)))
;; y should be Float32(0x4A98BEBD [Rational(10010301, 2), 5005150.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b00111101101110111001111)))
;; z should be Float32(0x4A9EDDCF [Rational(10411471, 2), 5205735.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
