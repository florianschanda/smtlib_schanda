(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA6F3162)))
;; y should be Float32(0xCA6F3162 [Rational(-7837873, 2), -3918936.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x803DF700)))
;; z should be Float32(0x803DF700 [Rational(-15863, 2787593149816327892691964784081045188247552), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b00010110 #b11011110011000101100011))))
(check-sat)
(exit)
