(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00011011 #b10001110110001000001001)))
;; x should be Float32(0x8DC76209 [Rational(-13066761, 10633823966279326983230456482242756608), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80000001)))
;; y should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b11111001 #b00001101100010110000010)))
;; z should be Float32(0xFC86C582 [Rational(-5598189707173705085283930218659053568), -5598189707173705085283930218659053568.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #xFC86C582)))
(check-sat)
(exit)
