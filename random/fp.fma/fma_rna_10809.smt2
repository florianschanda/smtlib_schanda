(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCABB41E3)))
;; x should be Float32(0xCABB41E3 [Rational(-12272099, 2), -6136049.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000101001001110011111)))
;; y should be Float32(0x8002939F [Rational(-168863, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xDB484C3C)))
;; z should be Float32(0xDB484C3C [Rational(-56378815923879936), -56378815923879936.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result (fp #b1 #b10110110 #b10010000100110000111100)))
(check-sat)
(exit)
