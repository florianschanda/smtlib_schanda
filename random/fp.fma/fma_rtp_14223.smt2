(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AFB7AF5)))
;; x should be Float32(0x4AFB7AF5 [Rational(16481013, 2), 8240506.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2E403886)))
;; y should be Float32(0x2E403886 [Rational(6298691, 144115188075855872), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xCA66D5DA)))
;; z should be Float32(0xCA66D5DA [Rational(-7564013, 2), -3782006.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA66D5D9))))
(check-sat)
(exit)
