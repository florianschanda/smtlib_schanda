(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10001101 #b00101010111101101100111)))
;; x should be Float32(0xC6957B67 [Rational(-9796455, 512), -19133.701172])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD4ED23D3)))
;; y should be Float32(0xD4ED23D3 [Rational(-8148066238464), -8148066238464.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80000001)))
;; z should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b0 #b10111000 #b00010100111100000101001))))
(check-sat)
(exit)
