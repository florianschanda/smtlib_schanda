(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01100010100010111100000)))
;; x should be Float32(0x003145E0 [Rational(100911, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01010111101110010001110)))
;; y should be Float32(0x002BDC8E [Rational(1437255, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010010 #b01101100010111110101000)))
;; z should be Float32(0xC9362FA8 [Rational(-1492469, 2), -746234.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b1 #b10010010 #b01101100010111110100111))))
(check-sat)
(exit)
