(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA188DA6)))
;; x should be Float32(0xCA188DA6 [Rational(-4998867, 2), -2499433.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00000000)))
;; y should be Float32(+zero)

(declare-const z Float32)
(assert (= z (fp #b0 #b10010101 #b10010010110110001110111)))
;; z should be Float32(0x4AC96C77 [Rational(13200503, 2), 6600251.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010101 #b10010010110110001110111))))
(check-sat)
(exit)
