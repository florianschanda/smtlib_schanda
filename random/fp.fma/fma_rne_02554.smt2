(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3381190E)))
;; x should be Float32(0x3381190E [Rational(4230279, 70368744177664), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A29DEA2)))
;; y should be Float32(0x4A29DEA2 [Rational(5566289, 2), 2783144.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x670C0590)))
;; z should be Float32(0x670C0590 [Rational(661233917615660339298304), 661233917615660339298304.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #x670C0590)))
(check-sat)
(exit)
