(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x805B4445)))
;; x should be Float32(0x805B4445 [Rational(-5981253, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01011010010001011110011)))
;; y should be Float32(0x4AAD22F3 [Rational(11346675, 2), 5673337.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xF1BD0969)))
;; z should be Float32(0xF1BD0969 [Rational(-1872129377186926430857709223936), -1872129377186926430857709223936.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xF1BD0969)))
(check-sat)
(exit)
