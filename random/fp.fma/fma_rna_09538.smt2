(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x170AECB0)))
;; x should be Float32(0x170AECB0 [Rational(569035, 1267650600228229401496703205376), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2B4BD519)))
;; y should be Float32(0x2B4BD519 [Rational(13358361, 18446744073709551616), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xFFCE0F7E)))
;; z should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
