(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x806B24A0)))
;; x should be Float32(0x806B24A0 [Rational(-219429, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01000011 #b01111111110111001100010)))
;; y should be Float32(0x21BFEE62 [Rational(6289201, 4835703278458516698824704), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b11111111111111111111111)))
;; z should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b00000000 #b11111111111111111111111)))
(check-sat)
(exit)
