(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA83442F)))
;; x should be Float32(0xCA83442F [Rational(-8602671, 2), -4301335.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD19E0CF1)))
;; y should be Float32(0xD19E0CF1 [Rational(-84852744192), -84852744192.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x7F7FFFFF)))
;; z should be Float32(0x7F7FFFFF [Rational(340282346638528859811704183484516925440), 340282346638528859811704183484516925440.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x7F7FFFFF)))
(check-sat)
(exit)
