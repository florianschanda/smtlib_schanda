(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0066661C)))
;; x should be Float32(0x0066661C [Rational(1677703, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0F1A3E73)))
;; y should be Float32(0x0F1A3E73 [Rational(10108531, 1329227995784915872903807060280344576), 0.000000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A64621A)))
;; z should be Float32(0x4A64621A [Rational(7483661, 2), 3741830.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result (fp #b0 #b10010100 #b11001000110001000011010))))
(check-sat)
(exit)
