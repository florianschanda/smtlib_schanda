(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01101111111111110110011)))
;; x should be Float32(0xCAB7FFB3 [Rational(-12058547, 2), -6029273.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01111110 #b00101101110101110010111)))
;; y should be Float32(0xBF16EB97 [Rational(-9890711, 16777216), -0.589532])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x002019BE)))
;; z should be Float32(0x002019BE [Rational(1051871, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (not (= result (fp #b0 #b10010100 #b10110001111001001001110))))
(check-sat)
(exit)
