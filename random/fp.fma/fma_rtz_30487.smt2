(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01100110111110111101110)))
;; x should be Float32(0x00337DEE [Rational(1687287, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x007A8493)))
;; y should be Float32(0x007A8493 [Rational(8029331, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010100 #b10000010111100110101110)))
;; z should be Float32(0xCA4179AE [Rational(-6339799, 2), -3169899.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #xCA4179AD))))
(check-sat)
(exit)
