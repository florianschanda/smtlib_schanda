(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00129C10)))
;; x should be Float32(0x00129C10 [Rational(76225, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8047A018)))
;; y should be Float32(0x8047A018 [Rational(-586755, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b11010001 #b01001001100111010011111)))
;; z should be Float32(0x68A4CE9F [Rational(6226236717018064105766912), 6226236717018064105766912.000000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x68A4CE9F)))
(check-sat)
(exit)
