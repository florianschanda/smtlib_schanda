(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11000001010001001000111)))
;; x should be Float32(0x0060A247 [Rational(6332999, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A1AB126)))
;; y should be Float32(0x4A1AB126 [Rational(5068947, 2), 2534473.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4A764606)))
;; z should be Float32(0x4A764606 [Rational(8069891, 2), 4034945.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNA x y z)))
(assert (= result ((_ to_fp 8 24) #x4A764606)))
(check-sat)
(exit)
