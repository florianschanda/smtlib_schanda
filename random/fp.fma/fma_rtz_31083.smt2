(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA0657FE)))
;; x should be Float32(0xCA0657FE [Rational(-4402175, 2), -2201087.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49F1BE94)))
;; y should be Float32(0x49F1BE94 [Rational(3960741, 2), 1980370.500000])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x80639CD1)))
;; z should be Float32(0x80639CD1 [Rational(-6528209, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (= result ((_ to_fp 8 24) #xD47DB9B2)))
(check-sat)
(exit)
