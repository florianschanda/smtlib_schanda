(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01011011000011100000011)))
;; x should be Float32(0xCAAD8703 [Rational(-11372291, 2), -5686145.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01100101 #b01110111111011010010011)))
;; y should be Float32(0x32BBF693 [Rational(12318355, 562949953421312), 0.000000])

(declare-const z Float32)
(assert (= z (fp #b0 #b10010100 #b01001000001001100011110)))
;; z should be Float32(0x4A24131E [Rational(5376399, 2), 2688199.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (fp #b0 #b10010100 #b01001000001001100011110))))
(check-sat)
(exit)
