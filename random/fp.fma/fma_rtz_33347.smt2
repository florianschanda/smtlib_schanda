(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA61D1077)))
;; x should be Float32(0xA61D1077 [Rational(-10293367, 18889465931478580854784), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10000110 #b11111011101111011111100)))
;; y should be Float32(0x437DDEFC [Rational(4159423, 16384), 253.871033])

(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #x4ABC8DC9)))
;; z should be Float32(0x4ABC8DC9 [Rational(12357065, 2), 6178532.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x4ABC8DC8))))
(check-sat)
(exit)
