(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80414E99)))
;; x should be Float32(0x80414E99 [Rational(-4279961, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A6BBCA6)))
;; y should be Float32(0x4A6BBCA6 [Rational(7724627, 2), 3862313.500000])

(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01100110110110011000001)))
;; z should be Float32(0x00336CC1 [Rational(3370177, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (not (= result (fp #b1 #b00010101 #b11100001000110100010111))))
(check-sat)
(exit)
