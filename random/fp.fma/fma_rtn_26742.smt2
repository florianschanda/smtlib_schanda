(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10000000100111011000100)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00101001100000111011110)))
(declare-const z Float32)
(assert (= z (fp #b0 #b00000000 #b01111110111111001010011)))
(declare-const result Float32)
(assert (= result (fp.fma RTN x y z)))
(assert (= result ((_ to_fp 8 24) #x8A9578EE)))
(check-sat)
(exit)
