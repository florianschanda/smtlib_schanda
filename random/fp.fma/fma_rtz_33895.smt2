(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8007578A)))
;; x should be Float32(0x8007578A [Rational(-240581, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000011 #b01111000100011011101011)))
;; y should be Float32(0xE1BC46EB [Rational(-434137258007447207936), -434137258007447207936.000000])

(declare-const z Float32)
(assert (= z (fp #b1 #b00000000 #b10001110001000101100100)))
;; z should be Float32(0x80471164 [Rational(-1164377, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.fma RTZ x y z)))
(assert (not (= result ((_ to_fp 8 24) #x20ACCA40))))
(check-sat)
(exit)
