(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10101011110110100101111)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01011111110100001110101)))
(declare-const z Float32)
(assert (= z ((_ to_fp 8 24) #xC849D0E0)))
(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result (fp #b1 #b10101100 #b00100101111111101100100)))
(check-sat)
(exit)
