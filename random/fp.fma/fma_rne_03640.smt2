(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9E325AC)))
;; x should be Float32(0xC9E325AC [Rational(-3721579, 2), -1860789.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A067C16)))
;; y should be Float32(0x4A067C16 [Rational(4406795, 2), 2203397.500000])

(declare-const z Float32)
(assert (= z (fp #b1 #b10010011 #b00001100001010100110100)))
;; z should be Float32(0xC9861534 [Rational(-2196813, 2), -1098406.500000])

(declare-const result Float32)
(assert (= result (fp.fma RNE x y z)))
(assert (= result ((_ to_fp 8 24) #xD46EA7A8)))
(check-sat)
(exit)
