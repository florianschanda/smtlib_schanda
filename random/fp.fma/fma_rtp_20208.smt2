(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80098E3C)))
;; x should be Float32(0x80098E3C [Rational(-156559, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b00001000111100111010100)))
;; y should be Float32(NaN)

(declare-const z Float32)
(assert (= z (fp #b1 #b10010101 #b00110101011110011001001)))
;; z should be Float32(0xCA9ABCC9 [Rational(-10140873, 2), -5070436.500000])

(declare-const result Float32)
(assert (= result (fp.fma RTP x y z)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
