(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC999280C)))
;; x should be Float32(0xC999280C [Rational(-2509315, 2), -1254657.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AE8DDA7)))
;; y should be Float32(0x4AE8DDA7 [Rational(15261095, 2), 7630547.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b1 #b10101010 #b00010110101000011100110)))
(check-sat)
(exit)
