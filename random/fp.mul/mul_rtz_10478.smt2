(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01110010100011110001011)))
;; x should be Float32(0x4AB9478B [Rational(12142475, 2), 6071237.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11010011101000011010001)))
;; y should be Float32(0xCAE9D0D1 [Rational(-15323345, 2), -7661672.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xD629393E))))
(check-sat)
(exit)
