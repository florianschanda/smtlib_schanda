(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110011 #b00100001110001001111111)))
;; x should be Float32(0xF990E27F [Rational(-94035579272009970949384741285003264), -94035579272009970949384741285003264.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11011000011100000001110)))
;; y should be Float32(0x006C380E [Rational(3546119, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #xBA74FD1C)))
(check-sat)
(exit)
