(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b10000001101001001001000)))
;; x should be Float32(0xC940D248 [Rational(-1579593, 2), -789796.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB454462A)))
;; y should be Float32(0xB454462A [Rational(-6955797, 35184372088832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x3E1FE2FD))))
(check-sat)
(exit)
