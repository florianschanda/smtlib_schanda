(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB03117)))
;; x should be Float32(0x4AB03117 [Rational(11546903, 2), 5773451.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01111001110100010010010)))
;; y should be Float32(0x803CE892 [Rational(-1995849, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b1 #b00010110 #b01001111010111001001001)))
(check-sat)
(exit)
