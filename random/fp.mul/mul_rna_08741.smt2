(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001001 #b00011010101001101001111)))
;; x should be Float32(0x048D534F [Rational(9261903, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80665737)))
;; y should be Float32(0x80665737 [Rational(-6706999, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result ((_ to_fp 8 24) #x80000000)))
(check-sat)
(exit)
