(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10100100 #b10010010101011001000110)))
;; x should be Float32(0x52495646 [Rational(216183963648), 216183963648.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00010010 #b00011110011101101010011)))
;; y should be Float32(0x090F3B53 [Rational(9386835, 5444517870735015415413993718908291383296), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b0 #b00110111 #b11000010100101110110010)))
(check-sat)
(exit)
