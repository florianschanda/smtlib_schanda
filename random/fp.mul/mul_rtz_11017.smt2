(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01000101100010111101010)))
;; x should be Float32(0x8022C5EA [Rational(-1139445, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11110011 #b00000100101110111100101)))
;; y should be Float32(0x79825DE5 [Rational(84612874868985370448962567341604864), 84612874868985370448962567341604864.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xB98DAA0E))))
(check-sat)
(exit)
