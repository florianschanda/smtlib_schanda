(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00011101100010110100100)))
;; x should be Float32(0x800EC5A4 [Rational(-242025, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10100011110000101100010)))
;; y should be Float32(0x4A51E162 [Rational(6877361, 2), 3438680.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b1 #b00010011 #b10000011100010111000100))))
(check-sat)
(exit)
