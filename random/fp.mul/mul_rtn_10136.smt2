(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9A5F1BC)))
;; x should be Float32(0xC9A5F1BC [Rational(-2718831, 2), -1359415.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00011001010010010100111)))
;; y should be Float32(0xCA8CA4A7 [Rational(-9217191, 2), -4608595.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b0 #b10101001 #b01101100101010111011011)))
(check-sat)
(exit)
