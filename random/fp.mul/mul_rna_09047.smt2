(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11000101 #b01111110101100110001100)))
;; x should be Float32(0xE2BF598C [Rational(-1764890325222319718400), -1764890325222319718400.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10110000 #b01001011010011110111100)))
;; y should be Float32(0xD825A7BC [Rational(-728559328952320), -728559328952320.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b11110110 #b11101111010010000101110))))
(check-sat)
(exit)
