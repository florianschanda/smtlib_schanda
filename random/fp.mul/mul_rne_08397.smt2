(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9560E6E0)))
;; x should be Float32(0x9560E6E0 [Rational(-460599, 10141204801825835211973625643008), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE7873EE8)))
;; y should be Float32(0xE7873EE8 [Rational(-1277359781363577790660608), -1277359781363577790660608.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b0 #b01111010 #b11011011010001000001000))))
(check-sat)
(exit)
