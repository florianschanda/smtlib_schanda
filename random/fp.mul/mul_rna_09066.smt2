(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10001101010000001000110)))
;; x should be Float32(0x8046A046 [Rational(-2314275, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x218C9B81)))
;; y should be Float32(0x218C9B81 [Rational(9214849, 9671406556917033397649408), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000000000)))
(check-sat)
(exit)
