(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB40ABF)))
;; x should be Float32(0x4AB40ABF [Rational(11799231, 2), 5899615.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x001F24C5)))
;; y should be Float32(0x001F24C5 [Rational(2041029, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b0 #b00010101 #b01011110011100101001010)))
(check-sat)
(exit)
