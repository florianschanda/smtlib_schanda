(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11110100110110101101001)))
;; x should be Float32(0x4AFA6D69 [Rational(16412009, 2), 8206004.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4ADF9203)))
;; y should be Float32(0x4ADF9203 [Rational(14651907, 2), 7325953.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b0 #b10101100 #b10110101011010000100100))))
(check-sat)
(exit)
