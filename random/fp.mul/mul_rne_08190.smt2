(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB54BEB)))
;; x should be Float32(0xCAB54BEB [Rational(-11881451, 2), -5940725.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0017898C)))
;; y should be Float32(0x0017898C [Rational(385635, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x8A8559D7))))
(check-sat)
(exit)
