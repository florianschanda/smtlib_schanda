(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAAAAE35)))
;; x should be Float32(0xCAAAAE35 [Rational(-11185717, 2), -5592858.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x6CEFC2D2)))
;; y should be Float32(0x6CEFC2D2 [Rational(2318826270414628503456579584), 2318826270414628503456579584.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b1 #b11110000 #b00111111101101010000111)))
(check-sat)
(exit)
