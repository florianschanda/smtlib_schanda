(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01011110001000011001011)))
;; x should be Float32(0x4AAF10CB [Rational(11473099, 2), 5736549.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01010101000000100111001)))
;; y should be Float32(0x002A8139 [Rational(2785593, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x0AE888FD))))
(check-sat)
(exit)
