(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01011011 #b10101001000111110110000)))
;; x should be Float32(0xADD48FB0 [Rational(-870651, 36028797018963968), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80722E34)))
;; y should be Float32(0x80722E34 [Rational(-1870733, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x00000000))))
(check-sat)
(exit)
