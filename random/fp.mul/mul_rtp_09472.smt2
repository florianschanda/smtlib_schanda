(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b10001011000111010010100)))
;; x should be Float32(0xC9C58E94 [Rational(-3236773, 2), -1618386.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AAB9605)))
;; y should be Float32(0x4AAB9605 [Rational(11245061, 2), 5622530.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b1 #b10101010 #b00001000110101000000010)))
(check-sat)
(exit)
