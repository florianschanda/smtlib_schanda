(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11110101111111001101001)))
;; x should be Float32(0x4AFAFE69 [Rational(16449129, 2), 8224564.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00226A78)))
;; y should be Float32(0x00226A78 [Rational(281935, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b0 #b00010110 #b00001101111100010110011)))
(check-sat)
(exit)
