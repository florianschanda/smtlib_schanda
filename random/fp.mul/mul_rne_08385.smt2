(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01111100 #b01001101011010001101100)))
;; x should be Float32(0xBE26B46C [Rational(-2731291, 16777216), -0.162798])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b11001011000110011011000)))
;; y should be Float32(0xC9658CD8 [Rational(-1880475, 2), -940237.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x48157B1C))))
(check-sat)
(exit)
