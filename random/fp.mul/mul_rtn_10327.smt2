(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000100 #b10001100101000010101100)))
;; x should be Float32(0x824650AC [Rational(-3249195, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2576BB5D)))
;; y should be Float32(0x2576BB5D [Rational(16169821, 75557863725914323419136), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x80000001))))
(check-sat)
(exit)
