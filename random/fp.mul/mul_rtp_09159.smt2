(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10011001110110100110011)))
;; x should be Float32(0x4ACCED33 [Rational(13430067, 2), 6715033.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10001010111000110110101)))
;; y should be Float32(0x4AC571B5 [Rational(12939701, 2), 6469850.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x561E0D8E))))
(check-sat)
(exit)
