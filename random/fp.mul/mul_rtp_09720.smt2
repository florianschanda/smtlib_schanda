(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8000EBB7)))
;; x should be Float32(0x8000EBB7 [Rational(-60343, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000011 #b01100010000101001011010)))
;; y should be Float32(0x01B10A5A [Rational(5801261, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000000000)))
(check-sat)
(exit)
