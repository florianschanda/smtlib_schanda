(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA0BCA395)))
;; x should be Float32(0xA0BCA395 [Rational(-12362645, 38685626227668133590597632), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x58AD7D06)))
;; y should be Float32(0x58AD7D06 [Rational(1526019865444352), 1526019865444352.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b1 #b01110011 #b11111111010110101011000))))
(check-sat)
(exit)
