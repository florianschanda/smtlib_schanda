(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE783B7)))
;; x should be Float32(0xCAE783B7 [Rational(-15172535, 2), -7586267.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0000925C)))
;; y should be Float32(0x0000925C [Rational(9367, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x88045C51))))
(check-sat)
(exit)
