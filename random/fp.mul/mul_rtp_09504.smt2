(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA53814E)))
;; x should be Float32(0xCA53814E [Rational(-6930599, 2), -3465299.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010000100011100011011)))
;; y should be Float32(0x8008471B [Rational(-542491, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #x095AD934)))
(check-sat)
(exit)
