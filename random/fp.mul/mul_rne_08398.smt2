(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010010 #b10100011110011101110000)))
;; x should be Float32(0x8951E770 [Rational(-859767, 340282366920938463463374607431768211456), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA06BBD5C)))
;; y should be Float32(0xA06BBD5C [Rational(-3862359, 19342813113834066795298816), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (_ +zero 8 24)))
(check-sat)
(exit)
