(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11011111111111101110111)))
;; x should be Float32(0x806FFF77 [Rational(-7339895, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001111 #b00010011101010111000000)))
;; y should be Float32(0x4789D5C0 [Rational(141143, 2), 70571.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b1 #b00010000 #b11100010011010011101000)))
(check-sat)
(exit)
