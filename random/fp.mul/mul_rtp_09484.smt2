(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10011111001110001111110)))
;; x should be Float32(0xCA4F9C7E [Rational(-6803007, 2), -3401503.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x71FDA689)))
;; y should be Float32(0x71FDA689 [Rational(2512033156322063487096483479552), 2512033156322063487096483479552.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #xFCCDB4B7)))
(check-sat)
(exit)
