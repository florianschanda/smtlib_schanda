(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x073362FD)))
;; x should be Float32(0x073362FD [Rational(11756285, 87112285931760246646623899502532662132736), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01111001 #b00010101010111011111100)))
;; y should be Float32(0x3C8AAEFC [Rational(2272191, 134217728), 0.016929])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b00001000 #b10000100101101111110100))))
(check-sat)
(exit)
