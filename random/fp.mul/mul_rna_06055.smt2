(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10111010 #b10101001100010011001001)))
(declare-const y Float32)
(assert (= y (fp #b0 #b01101111 #b01100111101111010001100)))
(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b1 #b10101011 #b00101010111111010010000)))
(check-sat)
(exit)
