(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA042FA6)))
;; x should be Float32(0xCA042FA6 [Rational(-4331475, 2), -2165737.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA6AF9DA0)))
;; y should be Float32(0xA6AF9DA0 [Rational(-359661, 295147905179352825856), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x31355BEC)))
(check-sat)
(exit)
