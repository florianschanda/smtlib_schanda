(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA1A13231)))
;; x should be Float32(0xA1A13231 [Rational(-10564145, 9671406556917033397649408), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111010110100111010110)))
;; y should be Float32(0x007D69D6 [Rational(4109547, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000000000)))
(check-sat)
(exit)
