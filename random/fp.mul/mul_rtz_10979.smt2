(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001111 #b11000110011100101011011)))
;; x should be Float32(0x87E3395B [Rational(-14891355, 43556142965880123323311949751266331066368), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001000 #b00110111111111011100000)))
;; y should be Float32(0x441BFEE0 [Rational(319479, 512), 623.982422])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x8C8A75F3)))
(check-sat)
(exit)
