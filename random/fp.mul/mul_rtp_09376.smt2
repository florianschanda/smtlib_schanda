(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5DCED039)))
;; x should be Float32(0x5DCED039 [Rational(1862809229891469312), 1862809229891469312.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x282C35EC)))
;; y should be Float32(0x282C35EC [Rational(2821499, 295147905179352825856), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #x468B1F77)))
(check-sat)
(exit)
