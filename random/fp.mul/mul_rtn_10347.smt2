(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC34675CE)))
;; x should be Float32(0xC34675CE [Rational(-6503143, 32768), -198.460175])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00011100110100110001110)))
;; y should be Float32(0x800E698E [Rational(-472263, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b0 #b00000101 #b01100101100010001011010)))
(check-sat)
(exit)
