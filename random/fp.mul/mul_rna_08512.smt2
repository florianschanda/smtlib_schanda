(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x488CAFD0)))
;; x should be Float32(0x488CAFD0 [Rational(576253, 2), 288126.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49F9C6E4)))
;; y should be Float32(0x49F9C6E4 [Rational(4092345, 2), 2046172.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b10100110 #b00010010100010001001111))))
(check-sat)
(exit)
