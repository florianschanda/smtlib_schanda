(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB62598F2)))
;; x should be Float32(0xB62598F2 [Rational(-5426297, 2199023255552), -0.000002])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01010100001100011010111)))
;; y should be Float32(0x802A18D7 [Rational(-2758871, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x00000007))))
(check-sat)
(exit)
