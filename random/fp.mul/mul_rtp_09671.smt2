(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101100 #b01010000010001001001101)))
;; x should be Float32(0x9628224D [Rational(-11018829, 81129638414606681695789005144064), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00001100101110000010110)))
;; y should be Float32(0x4A065C16 [Rational(4402699, 2), 2201349.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #xA0B07CDE)))
(check-sat)
(exit)
