(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x007FCEFB)))
;; x should be Float32(0x007FCEFB [Rational(8376059, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE2A80FA9)))
;; y should be Float32(0xE2A80FA9 [Rational(-1550090718782418845696), -1550090718782418845696.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b1 #b01000111 #b01001111100111101001101)))
(check-sat)
(exit)
