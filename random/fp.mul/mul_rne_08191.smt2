(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00111100100011010111100)))
;; x should be Float32(0xC99E46BC [Rational(-2593199, 2), -1296599.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00001000011011011000110)))
;; y should be Float32(0x000436C6 [Rational(138083, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x8826BE12)))
(check-sat)
(exit)
