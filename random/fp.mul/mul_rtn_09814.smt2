(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b10101111011011111111000)))
;; x should be Float32(0x4957B7F8 [Rational(1767167, 2), 883583.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x48467D20)))
;; y should be Float32(0x48467D20 [Rational(406505, 2), 203252.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b0 #b10100100 #b01001110100000110111001))))
(check-sat)
(exit)
