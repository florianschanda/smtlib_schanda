(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD577E9F6)))
;; x should be Float32(0xD577E9F6 [Rational(-17036514164736), -17036514164736.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xAEA03BC4)))
;; y should be Float32(0xAEA03BC4 [Rational(-2625265, 36028797018963968), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b0 #b10001001 #b00110110010110000011010)))
(check-sat)
(exit)
