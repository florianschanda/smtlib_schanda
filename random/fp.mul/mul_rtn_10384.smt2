(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10100000010100110111010)))
;; x should be Float32(0x805029BA [Rational(-2626781, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10110100 #b01001010111101011000001)))
;; y should be Float32(0xDA257AC1 [Rational(-11644585126133760), -11644585126133760.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x1ACF4554))))
(check-sat)
(exit)
