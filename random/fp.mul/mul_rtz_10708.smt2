(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11001100111101100111011)))
;; x should be Float32(0x00667B3B [Rational(6716219, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11001110 #b11011010001001100011001)))
;; y should be Float32(0x676D1319 [Rational(1119553146017357345325056), 1119553146017357345325056.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x283DCF75))))
(check-sat)
(exit)
