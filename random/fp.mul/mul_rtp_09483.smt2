(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA998B19)))
;; x should be Float32(0xCA998B19 [Rational(-10062617, 2), -5031308.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11101100 #b11011011110001111101100)))
;; y should be Float32(0x766DE3EC [Rational(1206247226854625433136363019960320), 1206247226854625433136363019960320.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF7FFFFF))))
(check-sat)
(exit)
