(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A9E7C81)))
;; x should be Float32(0x4A9E7C81 [Rational(10386561, 2), 5193280.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00011001 #b11101010111111010001011)))
;; y should be Float32(0x8CF57E8B [Rational(-16088715, 42535295865117307932921825928971026432), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x9817FB7E))))
(check-sat)
(exit)
