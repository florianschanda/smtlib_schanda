(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x242DCC35)))
;; x should be Float32(0x242DCC35 [Rational(11390005, 302231454903657293676544), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01000001 #b11000110111011011100000)))
;; y should be Float32(0x20E376E0 [Rational(465847, 1208925819614629174706176), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x059A6CC7))))
(check-sat)
(exit)
