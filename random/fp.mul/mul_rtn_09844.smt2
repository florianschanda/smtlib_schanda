(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11100001111010010111001)))
;; x should be Float32(0x4AF0F4B9 [Rational(15791289, 2), 7895644.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01100110 #b11001100111010010101101)))
;; y should be Float32(0xB36674AD [Rational(-15103149, 281474976710656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result ((_ to_fp 8 24) #xBED8E9B0)))
(check-sat)
(exit)
