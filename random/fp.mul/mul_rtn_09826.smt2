(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x481EE3A0)))
;; x should be Float32(0x481EE3A0 [Rational(325405, 2), 162702.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10111111 #b00100101100000110001110)))
;; y should be Float32(0x5F92C18E [Rational(21149779061387558912), 21149779061387558912.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b0 #b11010000 #b01101100010101111100101))))
(check-sat)
(exit)
