(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x39D1C38A)))
;; x should be Float32(0x39D1C38A [Rational(6873541, 17179869184), 0.000400])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB0864DB2)))
;; y should be Float32(0xB0864DB2 [Rational(-4400857, 4503599627370496), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #xAADC1807)))
(check-sat)
(exit)
