(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA1BBDD6)))
;; x should be Float32(0xCA1BBDD6 [Rational(-5103339, 2), -2551669.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00101010101100100001110)))
;; y should be Float32(0x8015590E [Rational(-699527, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b0 #b00010011 #b10011111100110000000001)))
(check-sat)
(exit)
