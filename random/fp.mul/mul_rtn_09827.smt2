(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49B7900C)))
;; x should be Float32(0x49B7900C [Rational(3007491, 2), 1503745.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11000110 #b00010011011001101000010)))
;; y should be Float32(0x6309B342 [Rational(2540120824779460575232), 2540120824779460575232.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b0 #b11011010 #b10001010111100100111110)))
(check-sat)
(exit)
