(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01001100011010101111011)))
;; x should be Float32(0x4AA6357B [Rational(10892667, 2), 5446333.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00001101101101001111001)))
;; y should be Float32(0x0006DA79 [Rational(449145, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b0 #b00010011 #b00011100110001100100000)))
(check-sat)
(exit)
