(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011111 #b01010111100111111000011)))
;; x should be Float32(0x2FABCFC3 [Rational(11259843, 36028797018963968), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10011101011000100011011)))
;; y should be Float32(0x4ACEB11B [Rational(13545755, 2), 6772877.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x3B0AB80C)))
(check-sat)
(exit)
