(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010011 #b11010010011100011111111)))
;; x should be Float32(0xE9E938FF [Rational(-35243609051623511721771008), -35243609051623511721771008.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01101011001101011100010)))
;; y should be Float32(0xCA359AE2 [Rational(-5950833, 2), -2975416.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result ((_ to_fp 8 24) #x74A57266)))
(check-sat)
(exit)
