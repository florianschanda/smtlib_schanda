(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xAF4E6E22)))
;; x should be Float32(0xAF4E6E22 [Rational(-6764305, 36028797018963968), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b01101101011100110011100)))
;; y should be Float32(0xC9B6B99C [Rational(-2993767, 2), -1496883.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result ((_ to_fp 8 24) #x399357F8)))
(check-sat)
(exit)
