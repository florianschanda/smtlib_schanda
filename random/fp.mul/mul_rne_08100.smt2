(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2ABA48BB)))
;; x should be Float32(0x2ABA48BB [Rational(12208315, 36893488147419103232), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8047A776)))
;; y should be Float32(0x8047A776 [Rational(-2347963, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (_ -zero 8 24))))
(check-sat)
(exit)
