(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x036714A2)))
;; x should be Float32(0x036714A2 [Rational(7572049, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA636296)))
;; y should be Float32(0xCA636296 [Rational(-7450955, 2), -3725477.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b1 #b00011100 #b10011010100000001001001))))
(check-sat)
(exit)
