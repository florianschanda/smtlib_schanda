(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11001011101110000100111)))
;; x should be Float32(0xCAE5DC27 [Rational(-15064103, 2), -7532051.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111101011001111011000)))
;; y should be Float32(0x007EB3D8 [Rational(1037947, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b1 #b00010111 #b11000111000011110101100)))
(check-sat)
(exit)
