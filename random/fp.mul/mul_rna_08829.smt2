(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b10001100111110101010100)))
;; x should be Float32(0xC9C67D54 [Rational(-3252053, 2), -1626026.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1C4F482F)))
;; y should be Float32(0x1C4F482F [Rational(13584431, 19807040628566084398385987584), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result ((_ to_fp 8 24) #xA6A0B74F)))
(check-sat)
(exit)
