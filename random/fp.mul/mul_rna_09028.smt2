(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA82FC6A2)))
;; x should be Float32(0xA82FC6A2 [Rational(-5759825, 590295810358705651712), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x74CFC357)))
;; y should be Float32(0x74CFC357 [Rational(131685475151313493144025035702272), 131685475151313493144025035702272.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b1 #b10111011 #b00011101010011110111101))))
(check-sat)
(exit)
