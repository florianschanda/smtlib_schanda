(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x076802B6)))
;; x should be Float32(0x076802B6 [Rational(7602523, 43556142965880123323311949751266331066368), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49EAD774)))
;; y should be Float32(0x49EAD774 [Rational(3847645, 2), 1923822.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b00100011 #b10101001101010110111110))))
(check-sat)
(exit)
