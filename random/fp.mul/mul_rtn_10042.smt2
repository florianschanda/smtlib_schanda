(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11011110 #b00101110001001111011101)))
;; x should be Float32(0x6F1713DD [Rational(46756249716588497523353583616), 46756249716588497523353583616.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80010E28)))
;; y should be Float32(0x80010E28 [Rational(-8645, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xAC9F6E8F))))
(check-sat)
(exit)
