(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x003F5AE1)))
;; x should be Float32(0x003F5AE1 [Rational(4152033, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x802B1F13)))
;; y should be Float32(0x802B1F13 [Rational(-2826003, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (_ -zero 8 24))))
(check-sat)
(exit)
