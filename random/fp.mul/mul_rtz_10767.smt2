(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAD53A27)))
;; x should be Float32(0xCAD53A27 [Rational(-13974055, 2), -6987027.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00101111001110001000100)))
;; y should be Float32(0x49979C44 [Rational(2483985, 2), 1241992.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b1 #b10101001 #b11111001000111011101010))))
(check-sat)
(exit)
