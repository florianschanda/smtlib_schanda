(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEEC1CADE)))
;; x should be Float32(0xEEC1CADE [Rational(-29987929138220475209205415936), -29987929138220475209205415936.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010001 #b00101001010010001110000)))
;; y should be Float32(0xC894A470 [Rational(-608839, 2), -304419.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x77E10B87))))
(check-sat)
(exit)
