(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011000 #b11101000010000100011011)))
;; x should be Float32(0x2C74211B [Rational(15999259, 4611686018427387904), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11111111001000010011010)))
;; y should be Float32(0x4A7F909A [Rational(8374349, 2), 4187174.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b0 #b01101110 #b11100111011011011100000))))
(check-sat)
(exit)
