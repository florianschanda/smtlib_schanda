(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01011011 #b01000011011000111010100)))
;; x should be Float32(0xADA1B1D4 [Rational(-2649205, 144115188075855872), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9008DB8)))
;; y should be Float32(0xC9008DB8 [Rational(-1053111, 2), -526555.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result ((_ to_fp 8 24) #x372264DA)))
(check-sat)
(exit)
