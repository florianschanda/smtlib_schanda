(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10000001011010000101011)))
;; x should be Float32(0x4AC0B42B [Rational(12629035, 2), 6314517.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01110011 #b11001101110000001001010)))
;; y should be Float32(0x39E6E04A [Rational(7565349, 17179869184), 0.000440])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x452DCAB4)))
(check-sat)
(exit)
