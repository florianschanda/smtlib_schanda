(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00066C91)))
;; x should be Float32(0x00066C91 [Rational(421009, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11100101 #b00010010100101000100011)))
;; y should be Float32(0xF2894A23 [Rational(-5438601233792340148711134855168), -5438601233792340148711134855168.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b1 #b01100010 #b10111000111110101110110))))
(check-sat)
(exit)
