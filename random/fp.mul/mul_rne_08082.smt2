(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1298969A)))
;; x should be Float32(0x1298969A [Rational(5000013, 5192296858534827628530496329220096), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x003B6A40)))
;; y should be Float32(0x003B6A40 [Rational(60841, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (_ +zero 8 24))))
(check-sat)
(exit)
