(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01010100 #b10100010100000101011000)))
;; x should be Float32(0xAA514158 [Rational(-1714219, 9223372036854775808), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA77AAAE)))
;; y should be Float32(0xCA77AAAE [Rational(-8115543, 2), -4057771.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b0 #b01101010 #b10010100111000110010000)))
(check-sat)
(exit)
