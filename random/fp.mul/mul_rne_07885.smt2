(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49AB852C)))
;; x should be Float32(0x49AB852C [Rational(2810187, 2), 1405093.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x006E9B74)))
;; y should be Float32(0x006E9B74 [Rational(1812189, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x0A9436C1)))
(check-sat)
(exit)
