(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b01110001010000110001110)))
;; x should be Float32(0xCA38A18E [Rational(-6049991, 2), -3024995.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA6FFDA1D)))
;; y should be Float32(0xA6FFDA1D [Rational(-16767517, 9444732965739290427392), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b0 #b01100011 #b01110001000011000111011)))
(check-sat)
(exit)
