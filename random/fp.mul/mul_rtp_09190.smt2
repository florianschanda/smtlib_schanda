(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00110111100001000100110)))
;; x should be Float32(0x4A1BC226 [Rational(5103891, 2), 2551945.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD0CF7FA3)))
;; y should be Float32(0xD0CF7FA3 [Rational(-27849988096), -27849988096.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b1 #b10110110 #b11111000111111101001010))))
(check-sat)
(exit)
