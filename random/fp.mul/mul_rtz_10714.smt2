(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00101111100101101111101)))
;; x should be Float32(0x0017CB7D [Rational(1559421, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00010011010000101000010)))
;; y should be Float32(0xCA09A142 [Rational(-4509857, 2), -2254928.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b1 #b00010011 #b10011001010111000110000))))
(check-sat)
(exit)
