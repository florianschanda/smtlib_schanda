(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001B0984)))
;; x should be Float32(0x001B0984 [Rational(442977, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFAD3FFC9)))
;; y should be Float32(0xFAD3FFC9 [Rational(-550381288230222586354948788438695936), -550381288230222586354948788438695936.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xBAB31EDC)))
(check-sat)
(exit)
