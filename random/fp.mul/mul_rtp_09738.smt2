(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8057F65F)))
;; x should be Float32(0x8057F65F [Rational(-5764703, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11110010 #b01111111011011011101111)))
;; y should be Float32(0xF93FB6EF [Rational(-62214939628678599390198003476201472), -62214939628678599390198003476201472.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #x3A03BF59)))
(check-sat)
(exit)
