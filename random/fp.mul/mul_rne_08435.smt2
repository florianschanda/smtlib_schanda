(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01011100010110000110000)))
;; x should be Float32(0x802E2C30 [Rational(-189123, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB3077D80)))
;; y should be Float32(0xB3077D80 [Rational(-69371, 2199023255552), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x00000000)))
(check-sat)
(exit)
