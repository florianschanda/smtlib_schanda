(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10010100101001100110001)))
;; x should be Float32(0x804A5331 [Rational(-4870961, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10000100000111100111011)))
;; y should be Float32(0x4AC20F3B [Rational(12717883, 2), 6358941.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b1 #b00010110 #b11000010101110111011101))))
(check-sat)
(exit)
