(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10110000 #b01001100011110110110011)))
;; x should be Float32(0x58263DB3 [Rational(731135705350144), 731135705350144.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11010001100110001101001)))
;; y should be Float32(0x4AE8CC69 [Rational(15256681, 2), 7628340.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x63972CA7))))
(check-sat)
(exit)
