(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x003B22FB)))
;; x should be Float32(0x003B22FB [Rational(3875579, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7C2EF300)))
;; y should be Float32(0x7C2EF300 [Rational(3633553115674989453109302173387194368), 3633553115674989453109302173387194368.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x3CA1A7A3)))
(check-sat)
(exit)
