(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10101111000101110111111)))
;; x should be Float32(0xCAD78BBF [Rational(-14126015, 2), -7063007.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01100100100000110101100)))
;; y should be Float32(0x49B241AC [Rational(2920555, 2), 1460277.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b10101010 #b00101100001011001110111))))
(check-sat)
(exit)
