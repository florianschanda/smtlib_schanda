(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011110 #b01101110100110001001000)))
;; x should be Float32(0x2F374C48 [Rational(1501577, 9007199254740992), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000000)))
;; y should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b1 #b00000000 #b00000000000000000000000))))
(check-sat)
(exit)
