(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA81679D)))
;; x should be Float32(0xCA81679D [Rational(-8480669, 2), -4240334.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010010 #b01010010111010110001001)))
;; y should be Float32(0xE9297589 [Rational(-12803969224831636076494848), -12803969224831636076494848.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b0 #b11101000 #b01010110101000110100000)))
(check-sat)
(exit)
