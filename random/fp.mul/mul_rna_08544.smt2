(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10101010100000001111111)))
;; x should be Float32(0x4AD5407F [Rational(13975679, 2), 6987839.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00100100011011101001001)))
;; y should be Float32(0x80123749 [Rational(-1193801, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b1 #b00010100 #b11100101100100101101010))))
(check-sat)
(exit)
