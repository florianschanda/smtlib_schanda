(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE39F0250)))
;; x should be Float32(0xE39F0250 [Rational(-5866397881812062830592), -5866397881812062830592.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x162754CA)))
;; y should be Float32(0x162754CA [Rational(5483109, 40564819207303340847894502572032), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b1 #b01110100 #b10011111101111001011000))))
(check-sat)
(exit)
