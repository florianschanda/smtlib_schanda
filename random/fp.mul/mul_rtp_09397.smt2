(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111010 #b00011100110111100010001)))
;; x should be Float32(0x3D0E6F11 [Rational(9334545, 268435456), 0.034774])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8002F3FB)))
;; y should be Float32(0x8002F3FB [Rational(-193531, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b1 #b00000000 #b00000000001101001001001)))
(check-sat)
(exit)
