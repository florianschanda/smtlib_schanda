(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10111111111110001111110)))
;; x should be Float32(0x4A5FFC7E [Rational(7339583, 2), 3669791.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A320996)))
;; y should be Float32(0x4A320996 [Rational(5833931, 2), 2916965.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b10101010 #b00110111100010111110011))))
(check-sat)
(exit)
