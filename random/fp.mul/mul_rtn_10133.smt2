(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC9E635)))
;; x should be Float32(0xCAC9E635 [Rational(-13231669, 2), -6615834.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01000010001111010011101)))
;; y should be Float32(0x00211E9D [Rational(2170525, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b1 #b00010101 #b10100001111011010001110)))
(check-sat)
(exit)
