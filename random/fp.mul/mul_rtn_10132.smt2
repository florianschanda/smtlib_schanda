(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b01010100110000101000110)))
;; x should be Float32(0xCA2A6146 [Rational(-5583011, 2), -2791505.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00101110101101011110110)))
;; y should be Float32(0x00175AF6 [Rational(765307, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b00010011 #b11110001011010001110100))))
(check-sat)
(exit)
