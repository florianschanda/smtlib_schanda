(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB39DA1)))
;; x should be Float32(0x4AB39DA1 [Rational(11771297, 2), 5885648.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010110 #b01100010000001100010100)))
;; y should be Float32(0x6B310314 [Rational(213994406106119447049666560), 213994406106119447049666560.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x7678644A))))
(check-sat)
(exit)
