(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AB9FD6D)))
;; x should be Float32(0x4AB9FD6D [Rational(12189037, 2), 6094518.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80750495)))
;; y should be Float32(0x80750495 [Rational(-7668885, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b1 #b00010111 #b01010100000100001001110)))
(check-sat)
(exit)
