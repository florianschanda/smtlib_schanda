(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10111011 #b11001001100001011110110)))
;; x should be Float32(0x5DE4C2F6 [Rational(2060501008248733696), 2060501008248733696.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01100001011011011111010)))
;; y should be Float32(0x4A30B6FA [Rational(5790589, 2), 2895294.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x689DE98B))))
(check-sat)
(exit)
