(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC469304D)))
;; x should be Float32(0xC469304D [Rational(-15282253, 16384), -932.754700])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11110110001001010001001)))
;; y should be Float32(0x007B1289 [Rational(8065673, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b1 #b00001010 #b11000000011011000101110))))
(check-sat)
(exit)
