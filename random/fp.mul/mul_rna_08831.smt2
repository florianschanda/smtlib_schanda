(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00111100000111010110110)))
;; x should be Float32(0xCA1E0EB6 [Rational(-5179227, 2), -2589613.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01011011001110100111011)))
;; y should be Float32(0x002D9D3B [Rational(2989371, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b1 #b00010100 #b11000010100110101001100))))
(check-sat)
(exit)
