(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE2244BBB)))
;; x should be Float32(0xE2244BBB [Rational(-757680745865463988224), -757680745865463988224.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00010110011111111111011)))
;; y should be Float32(0x000B3FFB [Rational(737275, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b1 #b01000010 #b11001110000101000011000))))
(check-sat)
(exit)
