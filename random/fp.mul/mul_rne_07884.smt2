(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A6B037A)))
;; x should be Float32(0x4A6B037A [Rational(7700925, 2), 3850462.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10010100111001011100001)))
;; y should be Float32(0x004A72E1 [Rational(4879073, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x0B08B0EF))))
(check-sat)
(exit)
