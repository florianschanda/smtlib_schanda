(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9EE973C)))
;; x should be Float32(0xC9EE973C [Rational(-3909071, 2), -1954535.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAAA90EF)))
;; y should be Float32(0xCAAA90EF [Rational(-11178223, 2), -5589111.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b0 #b10101010 #b00111101111011110000010))))
(check-sat)
(exit)
