(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x98582558)))
;; x should be Float32(0x98582558 [Rational(-1770667, 633825300114114700748351602688), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10000100001111011010111)))
;; y should be Float32(0xCAC21ED7 [Rational(-12721879, 2), -6360939.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b0 #b01000111 #b01000111110011001010110))))
(check-sat)
(exit)
