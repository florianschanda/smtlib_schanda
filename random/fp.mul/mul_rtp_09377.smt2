(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011000 #b11001010010110011010010)))
;; x should be Float32(0x4C652CD2 [Rational(60076872), 60076872.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0D5C73D5)))
;; y should be Float32(0x0D5C73D5 [Rational(14447573, 21267647932558653966460912964485513216), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x1A455A37))))
(check-sat)
(exit)
