(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01001101101101001010111)))
;; x should be Float32(0x4AA6DA57 [Rational(10934871, 2), 5467435.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE1A31BE3)))
;; y should be Float32(0xE1A31BE3 [Rational(-376103591734174285824), -376103591734174285824.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b1 #b11011001 #b10101001001111001100101)))
(check-sat)
(exit)
