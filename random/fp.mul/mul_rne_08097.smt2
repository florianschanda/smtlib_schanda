(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000011 #b00100101000111000000011)))
;; x should be Float32(0x41928E03 [Rational(9604611, 524288), 18.319342])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFED2B573)))
;; y should be Float32(0xFED2B573 [Rational(-140040008661665778526331128214179545088), -140040008661665778526331128214179545088.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b1 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
