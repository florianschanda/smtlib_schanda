(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01101101010111111001110)))
;; x should be Float32(0x8036AFCE [Rational(-1791975, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10110010010110011101011)))
;; y should be Float32(0xCAD92CEB [Rational(-14232811, 2), -7116405.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x0B399278))))
(check-sat)
(exit)
