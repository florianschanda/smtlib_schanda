(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7FABA159)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b11011011 #b10110001100110111101000)))
;; y should be Float32(0x6DD8CDE8 [Rational(8387210937931944947882131456), 8387210937931944947882131456.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b1 #b11111111 #b11111111111111111111111))))
(check-sat)
(exit)
