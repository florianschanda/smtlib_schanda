(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2E1C8A68)))
;; x should be Float32(0x2E1C8A68 [Rational(1282381, 36028797018963968), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101011100010000101000)))
;; y should be Float32(0x0055C428 [Rational(702597, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b00000000 #b00000000000000000000000))))
(check-sat)
(exit)
