(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11110010100111110100101)))
;; x should be Float32(0xCAF94FA5 [Rational(-16338853, 2), -8169426.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80413E9C)))
;; y should be Float32(0x80413E9C [Rational(-1068967, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b0 #b00010110 #b11111100010100011001000)))
(check-sat)
(exit)
