(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB0EBEB)))
;; x should be Float32(0xCAB0EBEB [Rational(-11594731, 2), -5797365.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11011110 #b10011110001110110111110)))
;; y should be Float32(0x6F4F1DBE [Rational(64099353131418998704686759936), 64099353131418998704686759936.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xFA8F2351))))
(check-sat)
(exit)
