(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11100010111001110011100)))
;; x should be Float32(0xC9F1739C [Rational(-3955943, 2), -1977971.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00100101000011000100011)))
;; y should be Float32(0x80128623 [Rational(-1213987, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b0 #b00010011 #b00010111100010100100100)))
(check-sat)
(exit)
