(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00010111000000101000101)))
;; x should be Float32(0x800B8145 [Rational(-753989, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11111001001100001110010)))
;; y should be Float32(0xCA7C9872 [Rational(-8277049, 2), -4138524.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b0 #b00010011 #b01101011010000110011101)))
(check-sat)
(exit)
