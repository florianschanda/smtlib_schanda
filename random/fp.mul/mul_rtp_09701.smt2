(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDB1C40CF)))
;; x should be Float32(0xDB1C40CF [Rational(-43981354169270272), -43981354169270272.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000100 #b00011111111000111101011)))
;; y should be Float32(0xE20FF1EB [Rational(-663829107330783379456), -663829107330783379456.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #x7DAFB7B9)))
(check-sat)
(exit)
