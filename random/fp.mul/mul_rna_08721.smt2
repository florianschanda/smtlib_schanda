(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01101111 #b11000110001011100011000)))
;; x should be Float32(0x37E31718 [Rational(1860323, 68719476736), 0.000027])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001111 #b11111011101010100110100)))
;; y should be Float32(0x47FDD534 [Rational(4158797, 32), 129962.406250])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b10000000 #b11000010010101011110011))))
(check-sat)
(exit)
