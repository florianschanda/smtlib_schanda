(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000110 #b11000111000011111010000)))
;; x should be Float32(0x036387D0 [Rational(931965, 1393796574908163946345982392040522594123776), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00001100000100110001001)))
;; y should be Float32(0xCA860989 [Rational(-8784265, 2), -4392132.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result ((_ to_fp 8 24) #x8E6E4321)))
(check-sat)
(exit)
