(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA990AA3)))
;; x should be Float32(0xCA990AA3 [Rational(-10029731, 2), -5014865.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10001001001111000110110)))
;; y should be Float32(0x4A449E36 [Rational(6442779, 2), 3221389.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xD56B1573))))
(check-sat)
(exit)
