(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01101000110100001110100)))
;; x should be Float32(0xC9B46874 [Rational(-2955805, 2), -1477902.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A0753C2)))
;; y should be Float32(0x4A0753C2 [Rational(4434401, 2), 2217200.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result ((_ to_fp 8 24) #xD43EBC38)))
(check-sat)
(exit)
