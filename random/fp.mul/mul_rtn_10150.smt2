(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA6E59F2)))
;; x should be Float32(0xCA6E59F2 [Rational(-7810297, 2), -3905148.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00100100101001000100011)))
;; y should be Float32(0x80125223 [Rational(-1200675, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x0A087663))))
(check-sat)
(exit)
