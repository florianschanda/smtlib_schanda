(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x42847BFD)))
;; x should be Float32(0x42847BFD [Rational(8682493, 131072), 66.242165])

(declare-const y Float32)
(assert (= y (fp #b1 #b00111010 #b00001111000001101110101)))
;; y should be Float32(0x9D078375 [Rational(-8881013, 4951760157141521099596496896), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b1 #b01000000 #b00011000100001011010101))))
(check-sat)
(exit)
