(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7FE15625)))
;; x should be Float32(NaN)

(declare-const y Float32)
(assert (= y (fp #b0 #b11001001 #b00010110011101110101011)))
;; y should be Float32(0x648B3BAB [Rational(20547175652119063560192), 20547175652119063560192.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
