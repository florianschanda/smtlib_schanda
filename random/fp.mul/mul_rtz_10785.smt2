(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00110101111101100111011)))
;; x should be Float32(0xCA9AFB3B [Rational(-10156859, 2), -5078429.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9A83DDC)))
;; y should be Float32(0xC9A83DDC [Rational(-2756471, 2), -1378235.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b0 #b10101001 #b10010111011010010100011)))
(check-sat)
(exit)
