(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A82CE85)))
;; x should be Float32(0x4A82CE85 [Rational(8572549, 2), 4286274.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10000101101011001001100)))
;; y should be Float32(0x8042D64C [Rational(-1095059, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b00010110 #b00010001001101011111000))))
(check-sat)
(exit)
