(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11011100110100001110010)))
;; x should be Float32(0xCA6E6872 [Rational(-7812153, 2), -3906076.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10110111100011000010111)))
;; y should be Float32(0xCADBC617 [Rational(-14403095, 2), -7201547.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b0 #b10101011 #b10011001010101111010011))))
(check-sat)
(exit)
