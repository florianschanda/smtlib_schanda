(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA06D656)))
;; x should be Float32(0xCA06D656 [Rational(-4418347, 2), -2209173.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01010111 #b00110011010111101100111)))
;; y should be Float32(0x2B99AF67 [Rational(10071911, 9223372036854775808), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b01101100 #b01000011110010011111001))))
(check-sat)
(exit)
