(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5BFCB68D)))
;; x should be Float32(0x5BFCB68D [Rational(142264821675458560), 142264821675458560.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01100001010010000110100)))
;; y should be Float32(0x0030A434 [Rational(796941, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x1C40114B))))
(check-sat)
(exit)
