(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2E5182AC)))
;; x should be Float32(0x2E5182AC [Rational(3432619, 72057594037927936), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b00011000110111111010100)))
;; y should be Float32(0xC98C6FD4 [Rational(-2300917, 2), -1150458.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xB865DDF6))))
(check-sat)
(exit)
