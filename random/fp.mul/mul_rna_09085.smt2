(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10000100001001000110011)))
;; x should be Float32(0x80421233 [Rational(-4330035, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8B0B4A2B)))
;; y should be Float32(0x8B0B4A2B [Rational(-9128491, 340282366920938463463374607431768211456), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (_ +zero 8 24))))
(check-sat)
(exit)
