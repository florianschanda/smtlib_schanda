(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x28895DD3)))
;; x should be Float32(0x28895DD3 [Rational(9002451, 590295810358705651712), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A245D2A)))
;; y should be Float32(0x4A245D2A [Rational(5385877, 2), 2692938.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x33306432))))
(check-sat)
(exit)
