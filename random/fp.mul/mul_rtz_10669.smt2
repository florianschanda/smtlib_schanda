(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4E7DCE97)))
;; x should be Float32(0x4E7DCE97 [Rational(1064543680), 1064543680.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11100000 #b10011111111110110100011)))
;; y should be Float32(0x704FFDA3 [Rational(257480100044470552637600694272), 257480100044470552637600694272.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b0 #b11111110 #b10011100011010110000010)))
(check-sat)
(exit)
