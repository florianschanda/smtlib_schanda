(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE3305F55)))
;; x should be Float32(0xE3305F55 [Rational(-3253496353779504644096), -3253496353779504644096.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAD07D01)))
;; y should be Float32(0xCAD07D01 [Rational(-13663489, 2), -6831744.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b0 #b11011101 #b00011111010001110010100)))
(check-sat)
(exit)
