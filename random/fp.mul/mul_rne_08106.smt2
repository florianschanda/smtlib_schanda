(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10110111100100000101100)))
;; x should be Float32(0x005BC82C [Rational(1503755, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00000000110011101000101)))
;; y should be Float32(0x4A806745 [Rational(8415045, 2), 4207522.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b0 #b00010110 #b01110000010010001110001))))
(check-sat)
(exit)
