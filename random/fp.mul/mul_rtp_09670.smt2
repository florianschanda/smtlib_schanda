(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE7BC2559)))
;; x should be Float32(0xE7BC2559 [Rational(-1776987682872179858341888), -1776987682872179858341888.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC15F17)))
;; y should be Float32(0x4AC15F17 [Rational(12672791, 2), 6336395.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xF30E1E0A))))
(check-sat)
(exit)
