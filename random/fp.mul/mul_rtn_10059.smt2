(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001B1364)))
;; x should be Float32(0x001B1364 [Rational(443609, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10110100 #b00000101100010011010111)))
;; y should be Float32(0x5A02C4D7 [Rational(9202043667349504), 9202043667349504.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b0 #b00110011 #b10111010100101010100101))))
(check-sat)
(exit)
