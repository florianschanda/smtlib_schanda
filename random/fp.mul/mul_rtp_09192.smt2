(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF32EC1)))
;; x should be Float32(0x4AF32EC1 [Rational(15937217, 2), 7968608.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x806D6181)))
;; y should be Float32(0x806D6181 [Rational(-7168385, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b1 #b00010111 #b10011111100111100001110)))
(check-sat)
(exit)
