(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01110100111100000111101)))
;; x should be Float32(0x4ABA783D [Rational(12220477, 2), 6110238.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE7968DD3)))
;; y should be Float32(0xE7968DD3 [Rational(-1421942334994363663253504), -1421942334994363663253504.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b1 #b11100101 #b10110110101001110000011)))
(check-sat)
(exit)
