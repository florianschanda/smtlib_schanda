(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01110010 #b01011011101000110111100)))
;; x should be Float32(0x392DD1BC [Rational(2847855, 17179869184), 0.000166])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC4B8B663)))
;; y should be Float32(0xC4B8B663 [Rational(-12105315, 8192), -1477.699585])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #xBE7AD52A)))
(check-sat)
(exit)
