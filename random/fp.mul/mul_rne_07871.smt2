(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A3FB46A)))
;; x should be Float32(0x4A3FB46A [Rational(6281781, 2), 3140890.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AA50E9F)))
;; y should be Float32(0x4AA50E9F [Rational(10817183, 2), 5408591.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x55773477)))
(check-sat)
(exit)
