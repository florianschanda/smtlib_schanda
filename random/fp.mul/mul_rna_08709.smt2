(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111110 #b00101001000001101000001)))
;; x should be Float32(0x1F148341 [Rational(9732929, 309485009821345068724781056), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49B1A56C)))
;; y should be Float32(0x49B1A56C [Rational(2910555, 2), 1455277.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b01010010 #b10011100001110101101110))))
(check-sat)
(exit)
