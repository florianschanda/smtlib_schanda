(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x47AC10C0)))
;; x should be Float32(0x47AC10C0 [Rational(176195, 2), 88097.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00100101 #b11001101001010110000011)))
;; y should be Float32(0x12E69583 [Rational(15111555, 10384593717069655257060992658440192), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x1B1AFB8A))))
(check-sat)
(exit)
