(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10100100000101101010111)))
;; x should be Float32(0xCAD20B57 [Rational(-13765463, 2), -6882731.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11010110100111100100001)))
;; y should be Float32(0x806B4F21 [Rational(-7032609, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result ((_ to_fp 8 24) #x0BB01753)))
(check-sat)
(exit)
