(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8DDA0809)))
;; x should be Float32(0x8DDA0809 [Rational(-14288905, 10633823966279326983230456482242756608), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10101100000110101110010)))
;; y should be Float32(0x4A560D72 [Rational(7014073, 2), 3507036.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result ((_ to_fp 8 24) #x98B64E2B)))
(check-sat)
(exit)
