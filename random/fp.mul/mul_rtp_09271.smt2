(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11111111111111111111111)))
;; x should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00010010 #b00110000010111001101100)))
;; y should be Float32(0x09182E6C [Rational(2493339, 1361129467683753853853498429727072845824), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b0 #b00000000 #b00000000000000000000001))))
(check-sat)
(exit)
