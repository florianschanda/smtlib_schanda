(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10001001101001100111110)))
;; x should be Float32(0x4A44D33E [Rational(6449567, 2), 3224783.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00010001 #b00111011100110000101000)))
;; y should be Float32(0x089DCC28 [Rational(1292677, 1361129467683753853853498429727072845824), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x1372A508))))
(check-sat)
(exit)
