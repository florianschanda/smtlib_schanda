(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11101101110101000000011)))
;; x should be Float32(0xCAF6EA03 [Rational(-16181763, 2), -8090881.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11011101111100000001101)))
;; y should be Float32(0x006EF80D [Rational(7272461, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x8BD60F99)))
(check-sat)
(exit)
