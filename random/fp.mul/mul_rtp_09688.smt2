(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01001010 #b01110101000100010010000)))
;; x should be Float32(0xA53A8890 [Rational(-764041, 4722366482869645213696), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10001100101000111111111)))
;; y should be Float32(0xCAC651FF [Rational(-12997119, 2), -6498559.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x3090815F))))
(check-sat)
(exit)
