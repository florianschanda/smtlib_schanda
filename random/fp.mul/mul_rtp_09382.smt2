(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x69C56FC6)))
;; x should be Float32(0x69C56FC6 [Rational(29835777699875884065882112), 29835777699875884065882112.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b00011010100110001111100)))
;; y should be Float32(0xC98D4C7C [Rational(-2315039, 2), -1157519.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b1 #b11100111 #b10110011111001100011001)))
(check-sat)
(exit)
