(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1809AC37)))
;; x should be Float32(0x1809AC37 [Rational(9022519, 5070602400912917605986812821504), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b01000011010101101101111)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
