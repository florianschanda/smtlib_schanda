(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11111001101010111100011)))
;; x should be Float32(0x4AFCD5E3 [Rational(16569827, 2), 8284913.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x50EF09CD)))
;; y should be Float32(0x50EF09CD [Rational(32083175424), 32083175424.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b0 #b10111000 #b11011000001010101011101))))
(check-sat)
(exit)
