(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110110 #b01011101111001101010101)))
;; x should be Float32(0xFB2EF355 [Rational(-908395013312561075746020994582577152), -908395013312561075746020994582577152.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8A4400D5)))
;; y should be Float32(0x8A4400D5 [Rational(-12845269, 1361129467683753853853498429727072845824), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (fp #b0 #b10001100 #b00001011111001011011111)))
(check-sat)
(exit)
