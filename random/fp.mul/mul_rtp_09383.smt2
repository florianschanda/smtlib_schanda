(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0C7EBA86)))
;; x should be Float32(0x0C7EBA86 [Rational(8346947, 42535295865117307932921825928971026432), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00100000101011100001001)))
;; y should be Float32(0xCA905709 [Rational(-9459465, 2), -4729732.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x978F9F85))))
(check-sat)
(exit)
