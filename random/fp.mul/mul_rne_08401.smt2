(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001010 #b11011001111101001010100)))
;; x should be Float32(0x856CFA54 [Rational(-3882645, 348449143727040986586495598010130648530944), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b00100101110100001111011)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (_ NaN 8 24)))
(check-sat)
(exit)
