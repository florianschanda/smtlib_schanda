(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0007C0D6)))
;; x should be Float32(0x0007C0D6 [Rational(254059, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01111111101111001100010)))
;; y should be Float32(0x4A3FDE62 [Rational(6287153, 2), 3143576.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b0 #b00010010 #b01110011111001101111011))))
(check-sat)
(exit)
