(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10000000001110010011101)))
;; x should be Float32(0x80401C9D [Rational(-4201629, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10000110010111010101101)))
;; y should be Float32(0x00432EAD [Rational(4402861, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x80000000))))
(check-sat)
(exit)
