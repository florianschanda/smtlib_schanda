(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB9770A2D)))
;; x should be Float32(0xB9770A2D [Rational(-16189997, 68719476736), -0.000236])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AADCCCB)))
;; y should be Float32(0x4AADCCCB [Rational(11390155, 2), 5695077.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b1 #b10001001 #b01001111011011110000001)))
(check-sat)
(exit)
