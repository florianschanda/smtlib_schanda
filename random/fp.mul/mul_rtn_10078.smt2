(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01101111111110100100010)))
;; x should be Float32(0x0037FD22 [Rational(1834641, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01111011 #b10111011101100100011101)))
;; y should be Float32(0xBDDDD91D [Rational(-14539037, 134217728), -0.108324])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b00000000 #b00001100001000010100001))))
(check-sat)
(exit)
