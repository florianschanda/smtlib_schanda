(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11110100010000111011111)))
;; x should be Float32(0x4AFA21DF [Rational(16392671, 2), 8196335.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10110101110001110110010)))
;; y should be Float32(0x805AE3B2 [Rational(-2978265, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x8BB19CC5))))
(check-sat)
(exit)
