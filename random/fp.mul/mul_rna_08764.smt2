(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01111111001101110100111)))
;; x should be Float32(0x003F9BA7 [Rational(4168615, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01111010110101000110111)))
;; y should be Float32(0xCABD6A37 [Rational(-12413495, 2), -6206747.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b1 #b00010110 #b01111000100000100111010))))
(check-sat)
(exit)
