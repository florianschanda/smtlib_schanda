(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAA84FA9)))
;; x should be Float32(0xCAA84FA9 [Rational(-11030441, 2), -5515220.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAD3AE57)))
;; y should be Float32(0xCAD3AE57 [Rational(-13872727, 2), -6936363.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b0 #b10101100 #b00010110010110001001000)))
(check-sat)
(exit)
