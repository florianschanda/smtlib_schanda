(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x44A97E78)))
;; x should be Float32(0x44A97E78 [Rational(1388495, 1024), 1355.952148])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11011001000110001001001)))
;; y should be Float32(0x4AEC8C49 [Rational(15502409, 2), 7751204.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x501C9D79))))
(check-sat)
(exit)
