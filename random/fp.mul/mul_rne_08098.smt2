(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010001 #b11111001100111010100000)))
;; x should be Float32(0x08FCCEA0 [Rational(517749, 340282366920938463463374607431768211456), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB87A9133)))
;; y should be Float32(0xB87A9133 [Rational(-16421171, 274877906944), -0.000060])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x81F7712C)))
(check-sat)
(exit)
