(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b11000000111100110010100)))
;; x should be Float32(0x49E07994 [Rational(3677797, 2), 1838898.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x3C117781)))
;; y should be Float32(0x3C117781 [Rational(9533313, 1073741824), 0.008879])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x467F1B4D))))
(check-sat)
(exit)
