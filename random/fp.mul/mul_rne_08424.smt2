(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x805CCE29)))
;; x should be Float32(0x805CCE29 [Rational(-6082089, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10011001110100110100111)))
;; y should be Float32(0xCACCE9A7 [Rational(-13429159, 2), -6714579.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b0 #b00010111 #b00101001001000111111010)))
(check-sat)
(exit)
