(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00010001111000011110101)))
;; x should be Float32(0xCA88F0F5 [Rational(-8974581, 2), -4487290.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011001 #b10111111011111000001000)))
;; y should be Float32(0x0CDFBE08 [Rational(1832897, 5316911983139663491615228241121378304), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b1 #b00101111 #b11011110101111100011001))))
(check-sat)
(exit)
