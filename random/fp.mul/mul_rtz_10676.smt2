(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11000111 #b11100000111100000101001)))
;; x should be Float32(0x63F07829 [Rational(8871754058897777754112), 8871754058897777754112.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA21E10E)))
;; y should be Float32(0xCA21E10E [Rational(-5304455, 2), -2652227.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b1 #b11011101 #b00110000000111011111000)))
(check-sat)
(exit)
