(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10100100 #b11101010000101110010011)))
;; x should be Float32(0x52750B93 [Rational(263115292672), 263115292672.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xBB14A8E1)))
;; y should be Float32(0xBB14A8E1 [Rational(-9742561, 4294967296), -0.002268])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result ((_ to_fp 8 24) #xCE0E4C58)))
(check-sat)
(exit)
