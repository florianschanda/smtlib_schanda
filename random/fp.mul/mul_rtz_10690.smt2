(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011101 #b01001001001101100100010)))
;; x should be Float32(0x4EA49B22 [Rational(1380815104), 1380815104.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8014BC59)))
;; y should be Float32(0x8014BC59 [Rational(-1358937, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b1 #b00011100 #b10101010101001110011100)))
(check-sat)
(exit)
