(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00100101010110100100101)))
;; x should be Float32(0x8012AD25 [Rational(-1223973, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8045C536)))
;; y should be Float32(0x8045C536 [Rational(-2286235, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result (_ +zero 8 24)))
(check-sat)
(exit)
