(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b01100001101110100010000)))
;; x should be Float32(0xC8B0DD10 [Rational(-724433, 2), -362216.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01110110 #b11111101111110100011101)))
;; y should be Float32(0xBB7EFD1D [Rational(-16710941, 4294967296), -0.003891])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x44B02A35))))
(check-sat)
(exit)
