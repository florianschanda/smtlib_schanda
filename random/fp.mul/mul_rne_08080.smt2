(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000011 #b00100010101000111011101)))
;; x should be Float32(0x219151DD [Rational(9523677, 9671406556917033397649408), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x12A03538)))
;; y should be Float32(0x12A03538 [Rational(1312423, 1298074214633706907132624082305024), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x00000001))))
(check-sat)
(exit)
