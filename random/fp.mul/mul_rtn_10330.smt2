(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE8BE3E77)))
;; x should be Float32(0xE8BE3E77 [Rational(-7187215237851944769814528), -7187215237851944769814528.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00118BB1)))
;; y should be Float32(0x00118BB1 [Rational(1149873, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b01010000 #b10100001001111101010110))))
(check-sat)
(exit)
