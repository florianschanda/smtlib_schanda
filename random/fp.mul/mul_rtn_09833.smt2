(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010001 #b00011101001110001010000)))
;; x should be Float32(0x488E9C50 [Rational(584133, 2), 292066.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00111111101110100001110)))
;; y should be Float32(0xCA1FDD0E [Rational(-5238407, 2), -2619203.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b10100110 #b01100100001110001110101))))
(check-sat)
(exit)
