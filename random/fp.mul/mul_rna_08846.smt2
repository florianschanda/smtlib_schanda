(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b01101010110010011001010)))
;; x should be Float32(0xCA3564CA [Rational(-5943909, 2), -2971954.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000110 #b10111001110001010000110)))
;; y should be Float32(0xC35CE286 [Rational(-7237955, 32768), -220.884857])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x4E1C8320))))
(check-sat)
(exit)
