(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00101010 #b10010110111111000110011)))
;; x should be Float32(0x154B7E33 [Rational(13336115, 324518553658426726783156020576256), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00010001010100011010101)))
;; y should be Float32(0x0008A8D5 [Rational(567509, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b0 #b00000000 #b00000000000000000000000))))
(check-sat)
(exit)
