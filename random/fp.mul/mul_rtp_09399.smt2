(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11011010 #b10110001101001010011110)))
;; x should be Float32(0x6D58D29E [Rational(4193961417339618773449048064), 4193961417339618773449048064.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b11001001101111111110111)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
