(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4825E2A0)))
;; x should be Float32(0x4825E2A0 [Rational(339733, 2), 169866.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01011011011000101001011)))
;; y should be Float32(0x002DB14B [Rational(2994507, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result ((_ to_fp 8 24) #x086CDDC3)))
(check-sat)
(exit)
