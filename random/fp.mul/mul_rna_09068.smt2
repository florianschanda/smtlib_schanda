(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x803424A0)))
;; x should be Float32(0x803424A0 [Rational(-106789, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x003B9607)))
;; y should be Float32(0x003B9607 [Rational(3905031, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x80000000))))
(check-sat)
(exit)
