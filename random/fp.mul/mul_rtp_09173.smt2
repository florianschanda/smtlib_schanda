(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01100100001110100000011)))
;; x should be Float32(0x4AB21D03 [Rational(11672835, 2), 5836417.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x003EF3CC)))
;; y should be Float32(0x003EF3CC [Rational(1031411, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x0B2F3299))))
(check-sat)
(exit)
