(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110010 #b11000101011101000111111)))
;; x should be Float32(0xF962BA3F [Rational(-73577288099336790837500922025738240), -73577288099336790837500922025738240.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010000 #b10011100010000110100000)))
;; y should be Float32(0x484E21A0 [Rational(422157, 2), 211078.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
