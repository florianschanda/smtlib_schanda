(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000111 #b11001011110100011010100)))
;; y should be Float32(0xE3E5E8D4 [Rational(-8482162854782698520576), -8482162854782698520576.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b1 #b01001001 #b11001011110100011010100)))
(check-sat)
(exit)
