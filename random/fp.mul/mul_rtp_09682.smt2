(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00111110 #b01011110011101001100001)))
;; x should be Float32(0x9F2F3A61 [Rational(-11483745, 309485009821345068724781056), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2A9CD9A5)))
;; y should be Float32(0x2A9CD9A5 [Rational(10279333, 36893488147419103232), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b1 #b00010100 #b10101101011100100011001))))
(check-sat)
(exit)
