(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111100 #b01001101100100000011110)))
(declare-const y Float32)
(assert (= y (fp #b1 #b01101000 #b11011110011111010110011)))
(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b1 #b00100110 #b00110111101110110101101)))
(check-sat)
(exit)
