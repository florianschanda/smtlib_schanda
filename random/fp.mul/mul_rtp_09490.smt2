(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b10100001010001011010100)))
;; x should be Float32(0xC9D0A2D4 [Rational(-3418293, 2), -1709146.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01011110010011001110101)))
;; y should be Float32(0xCAAF2675 [Rational(-11478645, 2), -5739322.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b0 #b10101010 #b00011101011111010100111)))
(check-sat)
(exit)
