(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A637A1A)))
;; x should be Float32(0x4A637A1A [Rational(7453965, 2), 3726982.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01010001101010101001001)))
;; y should be Float32(0x4AA8D549 [Rational(11064649, 2), 5532324.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b0 #b10101011 #b00101100000010110100110))))
(check-sat)
(exit)
