(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10111101001100110110011)))
;; y should be Float32(0x4ADE99B3 [Rational(14588339, 2), 7294169.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x0BDE99B3))))
(check-sat)
(exit)
