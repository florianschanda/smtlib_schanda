(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10011111010010110011000)))
;; x should be Float32(0x804FA598 [Rational(-652467, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEBB6521A)))
;; y should be Float32(0xEBB6521A [Rational(-440824425673607474304778240), -440824425673607474304778240.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b0 #b01011000 #b11000101110010100010100)))
(check-sat)
(exit)
