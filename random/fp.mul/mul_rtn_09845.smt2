(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01100101000011001110011)))
;; x should be Float32(0x4AB28673 [Rational(11699827, 2), 5849913.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10011000 #b11001011001001111111010)))
;; y should be Float32(0xCC6593FA [Rational(-60182504), -60182504.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b10101111 #b01000000001100101110111))))
(check-sat)
(exit)
