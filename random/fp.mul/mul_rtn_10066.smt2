(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0027612E)))
;; x should be Float32(0x0027612E [Rational(1290391, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA1F3E6E)))
;; y should be Float32(0xCA1F3E6E [Rational(-5218103, 2), -2609051.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (fp #b1 #b00010100 #b10000111111011110110001)))
(check-sat)
(exit)
