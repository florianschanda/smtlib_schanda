(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0BE573DA)))
;; x should be Float32(0x0BE573DA [Rational(7518701, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0B695DEA)))
;; y should be Float32(0x0B695DEA [Rational(7646965, 170141183460469231731687303715884105728), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result (_ +zero 8 24)))
(check-sat)
(exit)
