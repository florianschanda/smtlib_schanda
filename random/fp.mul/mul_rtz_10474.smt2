(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01100111101111101101101)))
;; x should be Float32(0x4AB3DF6D [Rational(11788141, 2), 5894070.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00010011100001001010001)))
;; y should be Float32(0x0009C251 [Rational(639569, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b0 #b00010011 #b10110110110110001100001)))
(check-sat)
(exit)
