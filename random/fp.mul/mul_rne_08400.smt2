(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC53D07A7)))
;; x should be Float32(0xC53D07A7 [Rational(-12388263, 4096), -3024.478271])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80436D56)))
;; y should be Float32(0x80436D56 [Rational(-2209451, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b0 #b00001011 #b10001110010011011110010)))
(check-sat)
(exit)
