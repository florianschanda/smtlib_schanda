(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD13D6896)))
;; x should be Float32(0xD13D6896 [Rational(-50843967488), -50843967488.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A8CCE73)))
;; y should be Float32(0x4A8CCE73 [Rational(9227891, 2), 4613945.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #xDC505BE3)))
(check-sat)
(exit)
