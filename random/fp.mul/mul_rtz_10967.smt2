(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000100 #b10011111000110001000111)))
;; x should be Float32(0x824F8C47 [Rational(-13601863, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10111000000010001011101)))
;; y should be Float32(0x4ADC045D [Rational(14419037, 2), 7209518.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x8DB26016))))
(check-sat)
(exit)
