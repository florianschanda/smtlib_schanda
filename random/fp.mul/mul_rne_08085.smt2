(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x219D0ABF)))
;; x should be Float32(0x219D0ABF [Rational(10291903, 9671406556917033397649408), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAC87653)))
;; y should be Float32(0xCAC87653 [Rational(-13137491, 2), -6568745.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b1 #b01011001 #b11101011111000111110110))))
(check-sat)
(exit)
