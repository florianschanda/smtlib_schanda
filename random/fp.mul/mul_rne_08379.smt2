(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB70805FF)))
;; x should be Float32(0xB70805FF [Rational(-8914431, 1099511627776), -0.000008])

(declare-const y Float32)
(assert (= y (fp #b0 #b10011011 #b01111110001011111101101)))
;; y should be Float32(0x4DBF17ED [Rational(400752032), 400752032.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #xC54B125F)))
(check-sat)
(exit)
