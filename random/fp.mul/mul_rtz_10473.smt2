(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A5F8E52)))
;; x should be Float32(0x4A5F8E52 [Rational(7325481, 2), 3662740.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x6024BCB0)))
;; y should be Float32(0x6024BCB0 [Rational(47482224927553093632), 47482224927553093632.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x6B0FDBF2)))
(check-sat)
(exit)
