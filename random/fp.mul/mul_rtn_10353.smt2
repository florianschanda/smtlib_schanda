(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00110001011000101000101)))
;; x should be Float32(0x8018B145 [Rational(-1618245, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x48F66930)))
;; y should be Float32(0x48F66930 [Rational(1009299, 2), 504649.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result ((_ to_fp 8 24) #x88BE23EE)))
(check-sat)
(exit)
