(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x324E0D88)))
;; x should be Float32(0x324E0D88 [Rational(1687985, 140737488355328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10000011001001010111101)))
;; y should be Float32(0xCAC192BD [Rational(-12686013, 2), -6343006.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result ((_ to_fp 8 24) #xBD9BCE50)))
(check-sat)
(exit)
