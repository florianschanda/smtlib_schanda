(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10100101100110101111011)))
;; x should be Float32(0x4AD2CD7B [Rational(13815163, 2), 6907581.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCADFA889)))
;; y should be Float32(0xCADFA889 [Rational(-14657673, 2), -7328836.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b1 #b10101100 #b01110000010101111000101))))
(check-sat)
(exit)
