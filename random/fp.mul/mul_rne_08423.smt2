(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8016BC09)))
;; x should be Float32(0x8016BC09 [Rational(-1489929, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01101010001010000000011)))
;; y should be Float32(0xCAB51403 [Rational(-11867139, 2), -5933569.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b0 #b00010101 #b00000001010010111001011))))
(check-sat)
(exit)
