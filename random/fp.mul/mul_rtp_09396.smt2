(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0608FBA1)))
;; x should be Float32(0x0608FBA1 [Rational(8977313, 348449143727040986586495598010130648530944), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80034636)))
;; y should be Float32(0x80034636 [Rational(-107291, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #x80000000)))
(check-sat)
(exit)
