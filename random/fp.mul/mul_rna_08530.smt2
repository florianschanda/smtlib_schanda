(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AC52A63)))
;; x should be Float32(0x4AC52A63 [Rational(12921443, 2), 6460721.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10111111110111000111111)))
;; y should be Float32(0xCADFEE3F [Rational(-14675519, 2), -7337759.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b1 #b10101100 #b01011000111011101101010))))
(check-sat)
(exit)
