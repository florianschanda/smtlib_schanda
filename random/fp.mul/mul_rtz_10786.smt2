(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11100110100011000000101)))
;; x should be Float32(0xCAF34605 [Rational(-15943173, 2), -7971586.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010001 #b10010010011100011110000)))
;; y should be Float32(0xC8C938F0 [Rational(-824207, 2), -412103.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b0 #b10101000 #b01111110011100000010101))))
(check-sat)
(exit)
