(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001101 #b10100110110101100100010)))
;; x should be Float32(0x86D36B22 [Rational(-6927761, 87112285931760246646623899502532662132736), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00100111011011111000000)))
;; y should be Float32(0x0013B7C0 [Rational(20191, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x80000000)))
(check-sat)
(exit)
