(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0042942E)))
;; x should be Float32(0x0042942E [Rational(2181655, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010000 #b00001110000011110110000)))
;; y should be Float32(0x480707B0 [Rational(553083, 4), 138270.750000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result (fp #b0 #b00010001 #b00011000111100010010001)))
(check-sat)
(exit)
