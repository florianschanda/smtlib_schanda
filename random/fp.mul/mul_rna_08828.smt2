(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB75C75)))
;; x should be Float32(0xCAB75C75 [Rational(-12016757, 2), -6008378.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x02760BCD)))
;; y should be Float32(0x02760BCD [Rational(16124877, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result (fp #b1 #b00011011 #b01100000011101101001100))))
(check-sat)
(exit)
