(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100111 #b10011101111011100011011)))
;; x should be Float32(0x33CEF71B [Rational(13563675, 140737488355328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10101001 #b01111001100010101000010)))
;; y should be Float32(0xD4BCC542 [Rational(-6486106308608), -6486106308608.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xC9189CF1)))
(check-sat)
(exit)
