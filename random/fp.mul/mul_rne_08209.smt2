(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01111101010111001001011)))
;; x should be Float32(0xCABEAE4B [Rational(-12496459, 2), -6248229.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80544D29)))
;; y should be Float32(0x80544D29 [Rational(-5524777, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #x0B7B2AA6)))
(check-sat)
(exit)
