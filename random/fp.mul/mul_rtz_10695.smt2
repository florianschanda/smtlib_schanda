(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001BE196)))
;; x should be Float32(0x001BE196 [Rational(913611, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00111001001110101101010)))
;; y should be Float32(0x4A1C9D6A [Rational(5131957, 2), 2565978.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b0 #b00010100 #b00010000111010011100010)))
(check-sat)
(exit)
