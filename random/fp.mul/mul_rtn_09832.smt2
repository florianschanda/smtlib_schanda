(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AD59345)))
;; x should be Float32(0x4AD59345 [Rational(13996869, 2), 6998434.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA51CB0E)))
;; y should be Float32(0xCA51CB0E [Rational(-6874503, 2), -3437251.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (= result ((_ to_fp 8 24) #xD5AF06A3)))
(check-sat)
(exit)
