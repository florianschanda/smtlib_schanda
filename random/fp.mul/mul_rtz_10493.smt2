(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10111011010000010110010)))
;; x should be Float32(0x4A5DA0B2 [Rational(7262297, 2), 3631148.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8078AC55)))
;; y should be Float32(0x8078AC55 [Rational(-7908437, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b1 #b00010110 #b10100001111000100001001))))
(check-sat)
(exit)
