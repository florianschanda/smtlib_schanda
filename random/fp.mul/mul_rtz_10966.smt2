(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00110111 #b01011110011110011011010)))
;; x should be Float32(0x9BAF3CDA [Rational(-5742189, 19807040628566084398385987584), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01101011100110101001100)))
;; y should be Float32(0x49B5CD4C [Rational(2978643, 2), 1489321.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xA5F8E51B)))
(check-sat)
(exit)
