(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11110101 #b00110111000101011000101)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10011011111100001110101)))
(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result (fp #b1 #b01110110 #b01111010111111011001011)))
(check-sat)
(exit)
