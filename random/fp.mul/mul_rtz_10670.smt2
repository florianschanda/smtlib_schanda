(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x6443D785)))
;; x should be Float32(0x6443D785 [Rational(14450579653053678354432), 14450579653053678354432.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x3233BC71)))
;; y should be Float32(0x3233BC71 [Rational(11779185, 1125899906842624), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x57097FDA)))
(check-sat)
(exit)
