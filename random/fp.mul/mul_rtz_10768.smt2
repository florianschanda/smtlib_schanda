(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b00011110101111011001000)))
;; x should be Float32(0xC90F5EC8 [Rational(-1174489, 2), -587244.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A88FD51)))
;; y should be Float32(0x4A88FD51 [Rational(8977745, 2), 4488872.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xD4197070)))
(check-sat)
(exit)
