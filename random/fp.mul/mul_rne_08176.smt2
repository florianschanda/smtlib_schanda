(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA8EA0A7)))
;; x should be Float32(0xCA8EA0A7 [Rational(-9347239, 2), -4673619.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A8D263D)))
;; y should be Float32(0x4A8D263D [Rational(9250365, 2), 4625182.500000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b1 #b10101011 #b00111010100011110010100))))
(check-sat)
(exit)
