(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111110 #b11000100010110011100001)))
;; x should be Float32(0xFF622CE1 [Rational(-300638551651327341317761125593382912000), -300638551651327341317761125593382912000.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x004D9A70)))
;; y should be Float32(0x004D9A70 [Rational(317863, 44601490397061246283071436545296723011960832), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b1 #b10000000 #b00010010001111111100100))))
(check-sat)
(exit)
