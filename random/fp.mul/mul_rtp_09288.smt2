(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11111111111111111111111)))
;; x should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00010101 #b11111101101111001001011)))
;; y should be Float32(0x8AFEDE4B [Rational(-16703051, 680564733841876926926749214863536422912), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #x80000000)))
(check-sat)
(exit)
