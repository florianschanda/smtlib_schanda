(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100110000110110111001)))
;; x should be Float32(0x80330DB9 [Rational(-3345849, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11101100 #b00100101100001001011101)))
;; y should be Float32(0x7612C25D [Rational(744157711941706137120209404690432), 744157711941706137120209404690432.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (not (= result (fp #b1 #b01101100 #b11010100010010010110011))))
(check-sat)
(exit)
