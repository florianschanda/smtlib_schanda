(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x803465F9)))
;; x should be Float32(0x803465F9 [Rational(-3433977, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE461F4D2)))
;; y should be Float32(0xE461F4D2 [Rational(-16672634317100051070976), -16672634317100051070976.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x24B8FEF0))))
(check-sat)
(exit)
