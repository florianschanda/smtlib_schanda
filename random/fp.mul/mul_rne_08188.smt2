(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11111000110110111001110)))
;; x should be Float32(0xCA7C6DCE [Rational(-8271591, 2), -4135795.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01000101 #b11110001011000101111101)))
;; y should be Float32(0x22F8B17D [Rational(16298365, 2417851639229258349412352), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (= result ((_ to_fp 8 24) #xADF53963)))
(check-sat)
(exit)
