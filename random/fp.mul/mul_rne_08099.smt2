(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001100 #b10011110010101011000000)))
;; x should be Float32(0x664F2AC0 [Rational(244579615065791910641664), 244579615065791910641664.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00101001111001101010111)))
;; y should be Float32(0x8014F357 [Rational(-1373015, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result (fp #b1 #b01001100 #b00001111010001000011000))))
(check-sat)
(exit)
