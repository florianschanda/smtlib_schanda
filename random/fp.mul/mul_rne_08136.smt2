(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000011100101001111010)))
;; x should be Float32(0x0001CA7A [Rational(58685, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xECF8B470)))
;; y should be Float32(0xECF8B470 [Rational(-2405325562133446615483023360), -2405325562133446615483023360.000000])

(declare-const result Float32)
(assert (= result (fp.mul RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xAADEB4AB))))
(check-sat)
(exit)
