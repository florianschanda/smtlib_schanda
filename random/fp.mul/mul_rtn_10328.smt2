(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB8FE8A0F)))
;; x should be Float32(0xB8FE8A0F [Rational(-16681487, 137438953472), -0.000121])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x264C3739)))
;; y should be Float32(0x264C3739 [Rational(13383481, 18889465931478580854784), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x9FCB0CED))))
(check-sat)
(exit)
