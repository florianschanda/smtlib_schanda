(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01011001010000011101101)))
;; x should be Float32(0xCAACA0ED [Rational(-11313389, 2), -5656694.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01001110100001011010111)))
;; y should be Float32(0x802742D7 [Rational(-2573015, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTP x y)))
(assert (= result ((_ to_fp 8 24) #x0AD3CCB5)))
(check-sat)
(exit)
