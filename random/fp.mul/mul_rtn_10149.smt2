(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC97BE2B8)))
;; x should be Float32(0xC97BE2B8 [Rational(-2063447, 2), -1031723.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCD2832A2)))
;; y should be Float32(0xCD2832A2 [Rational(-176368160), -176368160.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result (fp #b0 #b10101110 #b01001010111111010011010))))
(check-sat)
(exit)
