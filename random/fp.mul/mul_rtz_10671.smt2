(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x50E947D0)))
;; x should be Float32(0x50E947D0 [Rational(31310381056), 31310381056.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x001D15AC)))
;; y should be Float32(0x001D15AC [Rational(476523, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x10D40711))))
(check-sat)
(exit)
