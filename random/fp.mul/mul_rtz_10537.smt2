(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7F7FFFFF)))
;; x should be Float32(0x7F7FFFFF [Rational(340282346638528859811704183484516925440), 340282346638528859811704183484516925440.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00001010 #b10100111100010010100000)))
;; y should be Float32(0x0553C4A0 [Rational(433701, 43556142965880123323311949751266331066368), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x4553C49F))))
(check-sat)
(exit)
