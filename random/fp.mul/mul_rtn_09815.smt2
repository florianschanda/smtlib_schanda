(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00110011001011001101011)))
;; x should be Float32(0x4A99966B [Rational(10065515, 2), 5032757.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11110000010111000001110)))
;; y should be Float32(0x4A782E0E [Rational(8132359, 2), 4066179.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x5594E559))))
(check-sat)
(exit)
