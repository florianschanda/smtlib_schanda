(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0ACBF6AC)))
;; x should be Float32(0x0ACBF6AC [Rational(3341739, 170141183460469231731687303715884105728), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC99C85)))
;; y should be Float32(0x4AC99C85 [Rational(13212805, 2), 6606402.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (fp #b0 #b00101100 #b01000001010000101100001))))
(check-sat)
(exit)
