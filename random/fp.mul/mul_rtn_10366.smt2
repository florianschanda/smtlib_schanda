(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802D2500)))
;; x should be Float32(0x802D2500 [Rational(-11557, 2787593149816327892691964784081045188247552), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00100000 #b01110001111101010011010)))
;; y should be Float32(0x1038FA9A [Rational(6061389, 166153499473114484112975882535043072), 0.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x80000001))))
(check-sat)
(exit)
