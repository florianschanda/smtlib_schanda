(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA6BBD9C0)))
;; x should be Float32(0xA6BBD9C0 [Rational(-192359, 147573952589676412928), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10110001001011000010001)))
;; y should be Float32(0xCAD89611 [Rational(-14194193, 2), -7097096.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result (fp #b0 #b01100100 #b00111101110110111011000)))
(check-sat)
(exit)
