(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010001 #b00110010011010010000001)))
;; x should be Float32(0x08993481 [Rational(10040449, 10889035741470030830827987437816582766592), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000101 #b01010100000101001011000)))
;; y should be Float32(0xC2AA0A58 [Rational(-1392971, 16384), -85.020203])

(declare-const result Float32)
(assert (= result (fp.mul RNA x y)))
(assert (= result ((_ to_fp 8 24) #x8BCB861D)))
(check-sat)
(exit)
