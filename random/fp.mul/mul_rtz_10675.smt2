(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2BF150AC)))
;; x should be Float32(0x2BF150AC [Rational(3953707, 2305843009213693952), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA8BE773)))
;; y should be Float32(0xCA8BE773 [Rational(-9168755, 2), -4584377.500000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xB703E0F9)))
(check-sat)
(exit)
