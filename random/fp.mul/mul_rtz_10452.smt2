(set-info :status unsat)
(declare-const x Float32)
(assert (= x (_ +zero 8 24)))
;; x should be Float32(+zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b10111110 #b11101000100100100110010)))
;; y should be Float32(0xDF744932 [Rational(-17602655594135683072), -17602655594135683072.000000])

(declare-const result Float32)
(assert (= result (fp.mul RTZ x y)))
(assert (not (= result (_ -zero 8 24))))
(check-sat)
(exit)
