(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010000 #b01101110100110111001010)))
(declare-const result Float32)
(assert (= result (fp.sqrt RTP x)))
(assert (= result (fp #b0 #b01000111 #b10110001001111110100110)))
(check-sat)
(exit)
