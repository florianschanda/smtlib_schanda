(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01111101111010011100001)))
(declare-const result Float32)
(assert (= result (fp.sqrt RTP x)))
(assert (= result (fp #b0 #b00111111 #b01100111000100110100010)))
(check-sat)
(exit)
