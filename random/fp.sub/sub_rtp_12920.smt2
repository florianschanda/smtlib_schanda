(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10001100 #b10001000101000001110001)))
;; x should be Float32(0xC6445071 [Rational(-12865649, 1024), -12564.110352])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xAE521542)))
;; y should be Float32(0xAE521542 [Rational(-6884001, 144115188075855872), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #xC6445070)))
(check-sat)
(exit)
