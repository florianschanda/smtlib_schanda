(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA4225081)))
;; x should be Float32(0xA4225081 [Rational(-10637441, 302231454903657293676544), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10110000000000100100100)))
;; y should be Float32(0xC9D80124 [Rational(-3539017, 2), -1769508.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x49D80123)))
(check-sat)
(exit)
