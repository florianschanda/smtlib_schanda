(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111111 #b00100100010111010001000)))
;; x should be Float32(0x1F922E88 [Rational(1197521, 19342813113834066795298816), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01110001 #b00101110100110100011010)))
;; y should be Float32(0xB8974D1A [Rational(-4957837, 68719476736), -0.000072])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x38974D1A))))
(check-sat)
(exit)
