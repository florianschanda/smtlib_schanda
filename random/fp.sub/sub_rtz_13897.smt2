(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5B2D0277)))
;; x should be Float32(0x5B2D0277 [Rational(48697881095307264), 48697881095307264.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCADD2AC9)))
;; y should be Float32(0xCADD2AC9 [Rational(-14494409, 2), -7247204.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x5B2D0277)))
(check-sat)
(exit)
