(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9675735F)))
;; x should be Float32(0x9675735F [Rational(-16085855, 81129638414606681695789005144064), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00010101001010101100011)))
;; y should be Float32(0x4A8A9563 [Rational(9082211, 2), 4541105.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #xCA8A9563)))
(check-sat)
(exit)
