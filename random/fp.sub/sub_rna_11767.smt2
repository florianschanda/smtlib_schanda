(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49248348)))
;; x should be Float32(0x49248348 [Rational(1347689, 2), 673844.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9F8B564)))
;; y should be Float32(0xC9F8B564 [Rational(-4074841, 2), -2037420.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b0 #b10010100 #b01001010111101110000100))))
(check-sat)
(exit)
