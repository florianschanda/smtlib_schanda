(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011010 #b00110110001000100001101)))
;; x should be Float32(0x4D1B110D [Rational(162599120), 162599120.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49D5C8FC)))
;; y should be Float32(0x49D5C8FC [Rational(3502655, 2), 1751327.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result (fp #b0 #b10011010 #b00110010110010101111100))))
(check-sat)
(exit)
