(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x074D2DBE)))
;; x should be Float32(0x074D2DBE [Rational(6723295, 43556142965880123323311949751266331066368), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00109FF1)))
;; y should be Float32(0x00109FF1 [Rational(1089521, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b0 #b00001110 #b10011010010110100111001)))
(check-sat)
(exit)
