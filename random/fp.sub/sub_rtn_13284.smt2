(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11000011011110110011100)))
;; x should be Float32(0x0061BD9C [Rational(1601383, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00110101 #b00011000110101101000010)))
;; y should be Float32(0x1A8C6B42 [Rational(4601249, 79228162514264337593543950336), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b00110101 #b00011000110101101000010)))
(check-sat)
(exit)
