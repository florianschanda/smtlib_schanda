(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01001011 #b00101000011011111111110)))
;; x should be Float32(0x259437FE [Rational(4856831, 18889465931478580854784), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x7F8C88D7)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
