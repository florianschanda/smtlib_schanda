(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8B88DC59)))
;; x should be Float32(0x8B88DC59 [Rational(-8969305, 170141183460469231731687303715884105728), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000000)))
;; y should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result (fp #b1 #b00010111 #b00010001101110001011001))))
(check-sat)
(exit)
