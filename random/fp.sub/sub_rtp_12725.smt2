(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA6F9E16)))
;; x should be Float32(0xCA6F9E16 [Rational(-7851787, 2), -3925893.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010000111100011011111)))
;; y should be Float32(0x800878DF [Rational(-555231, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #xCA6F9E15)))
(check-sat)
(exit)
