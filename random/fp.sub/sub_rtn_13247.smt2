(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x651148D6)))
;; x should be Float32(0x651148D6 [Rational(42880420369658110017536), 42880420369658110017536.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01110100 #b10101101110101110111101)))
;; y should be Float32(0x3A56EBBD [Rational(14085053, 17179869184), 0.000820])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b11001010 #b00100010100100011010101))))
(check-sat)
(exit)
