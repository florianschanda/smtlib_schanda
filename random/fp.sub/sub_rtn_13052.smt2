(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00100110111100101001011)))
;; x should be Float32(0x4A93794B [Rational(9664843, 2), 4832421.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00736F07)))
;; y should be Float32(0x00736F07 [Rational(7565063, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result ((_ to_fp 8 24) #x4A93794A)))
(check-sat)
(exit)
