(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x489D9270)))
;; x should be Float32(0x489D9270 [Rational(645415, 2), 322707.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00111111111011110001011)))
;; y should be Float32(0x4A9FF78B [Rational(10483595, 2), 5241797.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xCA961E64)))
(check-sat)
(exit)
