(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b01110110000100101111000)))
;; x should be Float32(0xC93B0978 [Rational(-1532207, 2), -766103.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010110 #b10001110101101110111110)))
;; y should be Float32(0x6B475BBE [Rational(241009478334626348295782400), 241009478334626348295782400.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b11010110 #b10001110101101110111110)))
(check-sat)
(exit)
