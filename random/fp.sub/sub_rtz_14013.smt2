(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10101001100011000100010)))
;; x should be Float32(0xCA54C622 [Rational(-6972177, 2), -3486088.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xDC041FE3)))
;; y should be Float32(0xDC041FE3 [Rational(-148759026975375360), -148759026975375360.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x5C041FE2))))
(check-sat)
(exit)
