(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xAC0CA040)))
;; x should be Float32(0xAC0CA040 [Rational(-144001, 72057594037927936), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010010 #b10000111010000001100111)))
;; y should be Float32(0xE943A067 [Rational(-14781125842296964024107008), -14781125842296964024107008.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x6943A066))))
(check-sat)
(exit)
