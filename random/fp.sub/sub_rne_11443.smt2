(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA1B6C4A)))
;; x should be Float32(0xCA1B6C4A [Rational(-5092901, 2), -2546450.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80587669)))
;; y should be Float32(0x80587669 [Rational(-5797481, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA1B6C4A))))
(check-sat)
(exit)
