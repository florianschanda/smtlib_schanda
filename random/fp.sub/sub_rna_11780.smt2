(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11001001111111111000011)))
;; x should be Float32(0x4AE4FFC3 [Rational(15007683, 2), 7503841.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11101000011110110111110)))
;; y should be Float32(0x80743DBE [Rational(-3808991, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x4AE4FFC3))))
(check-sat)
(exit)
