(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x15EF7EB3)))
;; x should be Float32(0x15EF7EB3 [Rational(15695539, 162259276829213363391578010288128), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00010000 #b01111111101110001111101)))
;; y should be Float32(0x083FDC7D [Rational(12573821, 21778071482940061661655974875633165533184), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x15EF7EB2)))
(check-sat)
(exit)
