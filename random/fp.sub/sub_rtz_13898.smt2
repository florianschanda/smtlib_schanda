(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11011110 #b11001010111011001110101)))
;; x should be Float32(0x6F657675 [Rational(71015273012681042729080193024), 71015273012681042729080193024.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b01110011011010011011100)))
