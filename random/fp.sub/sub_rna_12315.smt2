(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10011001000000110110101)))
;; x should be Float32(0x804C81B5 [Rational(-5013941, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00010101 #b10011111111101010110110)))
;; y should be Float32(0x8ACFFAB6 [Rational(-6815067, 340282366920938463463374607431768211456), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b0 #b00010101 #b10011111111101010110001)))
(check-sat)
(exit)
