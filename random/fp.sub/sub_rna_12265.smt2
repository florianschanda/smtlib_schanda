(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010000 #b11101001111111111111001)))
;; x should be Float32(0xC874FFF9 [Rational(-16056313, 64), -250879.890625])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11110110110111011000100)))
;; y should be Float32(0xC9FB6EC4 [Rational(-4119473, 2), -2059736.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #x49DCCEC5)))
(check-sat)
(exit)
