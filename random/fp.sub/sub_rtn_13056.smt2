(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11001100000010111110001)))
;; x should be Float32(0x4AE605F1 [Rational(15074801, 2), 7537400.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA9E8DC7)))
;; y should be Float32(0xCA9E8DC7 [Rational(-10390983, 2), -5195491.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b10010110 #b10000100100100111011100))))
(check-sat)
(exit)
