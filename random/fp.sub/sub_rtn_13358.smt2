(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10111101111100000110001)))
;; x should be Float32(0xCADEF831 [Rational(-14612529, 2), -7306264.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00319E9F)))
;; y should be Float32(0x00319E9F [Rational(3251871, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result ((_ to_fp 8 24) #xCADEF832)))
(check-sat)
(exit)
