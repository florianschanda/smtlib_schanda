(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA158DF6)))
;; x should be Float32(0xCA158DF6 [Rational(-4900603, 2), -2450301.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x489C4810)))
;; y should be Float32(0x489C4810 [Rational(640129, 2), 320064.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b1 #b10010100 #b01010010001011011111000))))
(check-sat)
(exit)
