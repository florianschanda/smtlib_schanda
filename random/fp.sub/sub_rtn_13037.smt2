(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10101100100000110111010)))
;; x should be Float32(0x4A5641BA [Rational(7020765, 2), 3510382.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00110100010110100000001)))
;; y should be Float32(0x4A9A2D01 [Rational(10104065, 2), 5052032.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b1 #b10010011 #b01111000011000010010000))))
(check-sat)
(exit)
