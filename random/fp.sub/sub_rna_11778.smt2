(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00001011000011111110101)))
;; x should be Float32(0x4A8587F5 [Rational(8751093, 2), 4375546.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01101100 #b00100110001001100111100)))
;; y should be Float32(0xB613133C [Rational(-2409679, 1099511627776), -0.000002])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b0 #b10010101 #b00001011000011111110101))))
(check-sat)
(exit)
