(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010000 #b00010111000000011001010)))
;; x should be Float32(0xC80B80CA [Rational(-4571237, 32), -142851.156250])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A57465E)))
;; y should be Float32(0x4A57465E [Rational(7054127, 2), 3527063.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result (fp #b1 #b10010100 #b10111111111111001101010))))
(check-sat)
(exit)
