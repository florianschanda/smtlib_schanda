(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x800D0C0B)))
;; x should be Float32(0x800D0C0B [Rational(-855051, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10110011010000100001101)))
;; y should be Float32(0xCAD9A10D [Rational(-14262541, 2), -7131270.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b10010101 #b10110011010000100001100))))
(check-sat)
(exit)
