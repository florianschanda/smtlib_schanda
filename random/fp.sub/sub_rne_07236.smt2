(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11100010010110111110100)))
(declare-const y Float32)
(assert (= y (fp #b1 #b01000010 #b00100100111001101110101)))
(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b01000010 #b00100100111001101110101)))
(check-sat)
(exit)
