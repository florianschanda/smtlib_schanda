(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80797701)))
;; x should be Float32(0x80797701 [Rational(-7960321, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x39112584)))
;; y should be Float32(0x39112584 [Rational(2378081, 17179869184), 0.000138])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b01110010 #b00100010010010110000101)))
(check-sat)
(exit)
