(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A12E772)))
;; x should be Float32(0x4A12E772 [Rational(4813753, 2), 2406876.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8D921672)))
;; y should be Float32(0x8D921672 [Rational(-4787001, 5316911983139663491615228241121378304), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #x4A12E772)))
(check-sat)
(exit)
