(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x329743CA)))
;; x should be Float32(0x329743CA [Rational(4956645, 281474976710656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFDFF9C9F)))
;; y should be Float32(0xFDFF9C9F [Rational(-42470795267276495164514870676475084800), -42470795267276495164514870676475084800.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result ((_ to_fp 8 24) #x7DFF9C9F)))
(check-sat)
(exit)
