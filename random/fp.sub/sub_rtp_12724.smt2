(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10010010110110100011001)))
;; x should be Float32(0xCAC96D19 [Rational(-13200665, 2), -6600332.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xBE381CB1)))
;; y should be Float32(0xBE381CB1 [Rational(-12065969, 67108864), -0.179797])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b10010101 #b10010010110110100011000)))
(check-sat)
(exit)
