(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9514FC8)))
;; x should be Float32(0xC9514FC8 [Rational(-1714681, 2), -857340.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAB74821)))
;; y should be Float32(0xCAB74821 [Rational(-12011553, 2), -6005776.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b0 #b10010101 #b00111010001111000101000)))
(check-sat)
(exit)
