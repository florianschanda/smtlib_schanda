(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABB81F3)))
;; x should be Float32(0x4ABB81F3 [Rational(12288499, 2), 6144249.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01110111111011100110010)))
;; y should be Float32(0x803BF732 [Rational(-1964953, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b10010101 #b01110111000000111110011))))
(check-sat)
(exit)
