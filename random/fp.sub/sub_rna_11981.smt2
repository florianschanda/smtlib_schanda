(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11001010011111110101010)))
;; x should be Float32(0x00653FAA [Rational(3317717, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10110100100110101100111)))
;; y should be Float32(0x4ADA4D67 [Rational(14306663, 2), 7153331.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b1 #b10010101 #b10110100100110101100111)))
(check-sat)
(exit)
