(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11100010 #b10000011001000110001000)))
;; x should be Float32(0x71419188 [Rational(958504694099286236255427231744), 958504694099286236255427231744.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10011100 #b10101000100111010011101)))
;; y should be Float32(0x4E544E9D [Rational(890480448), 890480448.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x71419188))))
(check-sat)
(exit)
