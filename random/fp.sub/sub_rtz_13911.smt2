(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110101 #b11111110011110001000101)))
;; x should be Float32(0x1AFF3C45 [Rational(16727109, 158456325028528675187087900672), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00011000000011010010011)))
;; y should be Float32(0x800C0693 [Rational(-788115, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x1AFF3C45)))
(check-sat)
(exit)
