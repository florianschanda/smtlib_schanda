(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010011 #b11110010101110000111000)))
;; x should be Float32(0x89F95C38 [Rational(-2042759, 340282366920938463463374607431768211456), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01010011011110100101110)))
;; y should be Float32(0xCA29BD2E [Rational(-5562007, 2), -2781003.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A29BD2E))))
(check-sat)
(exit)
