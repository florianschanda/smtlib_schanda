(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x525E1FAA)))
;; x should be Float32(0x525E1FAA [Rational(238503493632), 238503493632.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11100000001011110110001)))
;; y should be Float32(0xCAF017B1 [Rational(-15734705, 2), -7867352.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b0 #b10100100 #b10111100010000110001010)))
(check-sat)
(exit)
