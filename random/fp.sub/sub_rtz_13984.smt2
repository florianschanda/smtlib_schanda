(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAF21CF1)))
;; x should be Float32(0xCAF21CF1 [Rational(-15867121, 2), -7933560.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC71087)))
;; y should be Float32(0x4AC71087 [Rational(13045895, 2), 6522947.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b10010110 #b10111001001011010111100)))
(check-sat)
(exit)
