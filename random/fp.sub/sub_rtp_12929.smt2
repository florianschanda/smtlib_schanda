(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802416AA)))
;; x should be Float32(0x802416AA [Rational(-1182549, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00111110111000010101100)))
;; y should be Float32(0x499F70AC [Rational(2612267, 2), 1306133.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #xC99F70AC)))
(check-sat)
(exit)
