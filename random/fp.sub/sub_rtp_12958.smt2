(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x805E99D2)))
;; x should be Float32(0x805E99D2 [Rational(-3099881, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010111 #b11100101111110101001010)))
;; y should be Float32(0xABF2FD4A [Rational(-7962277, 4611686018427387904), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #x2BF2FD4A)))
(check-sat)
(exit)
