(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001011 #b00110101011100101001011)))
;; x should be Float32(0x659AB94B [Rational(91332811291889309319168), 91332811291889309319168.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10011001110101010110100)))
;; y should be Float32(0xC9CCEAB4 [Rational(-3357357, 2), -1678678.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x659AB94B))))
(check-sat)
(exit)
