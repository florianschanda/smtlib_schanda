(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA1226DE)))
;; x should be Float32(0xCA1226DE [Rational(-4789103, 2), -2394551.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x3812BEBC)))
;; y should be Float32(0x3812BEBC [Rational(2404271, 68719476736), 0.000035])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b10010100 #b00100100010011011011110)))
(check-sat)
(exit)
