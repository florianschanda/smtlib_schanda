(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11110000101110111010010)))
;; x should be Float32(0xCA785DD2 [Rational(-8138473, 2), -4069236.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8031ECD0)))
;; y should be Float32(0x8031ECD0 [Rational(-204493, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result (fp #b1 #b10010100 #b11110000101110111010001))))
(check-sat)
(exit)
