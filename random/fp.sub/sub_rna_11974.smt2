(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00011100 #b11101001111010010011101)))
;; x should be Float32(0x0E74F49D [Rational(16053405, 5316911983139663491615228241121378304), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xD42B0C8A)))
;; y should be Float32(0xD42B0C8A [Rational(-2938599112704), -2938599112704.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b0 #b10101000 #b01010110000110010001010)))
(check-sat)
(exit)
