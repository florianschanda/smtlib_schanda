(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9E56F2E8)))
;; x should be Float32(0x9E56F2E8 [Rational(-1760861, 154742504910672534362390528), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01101111110101110011001)))
;; y should be Float32(0x0037EB99 [Rational(3664793, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b00111100 #b10101101111001011101000))))
(check-sat)
(exit)
