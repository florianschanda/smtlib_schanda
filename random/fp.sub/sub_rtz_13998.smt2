(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00010101001110000001011)))
;; x should be Float32(0xCA8A9C0B [Rational(-9083915, 2), -4541957.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x004CF4D2)))
;; y should be Float32(0x004CF4D2 [Rational(2521705, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xCA8A9C0B)))
(check-sat)
(exit)
