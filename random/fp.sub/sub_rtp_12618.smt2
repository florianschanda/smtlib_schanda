(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1FB77B5E)))
;; x should be Float32(0x1FB77B5E [Rational(6012335, 77371252455336267181195264), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800D685E)))
;; y should be Float32(0x800D685E [Rational(-439343, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #x1FB77B5F)))
(check-sat)
(exit)
