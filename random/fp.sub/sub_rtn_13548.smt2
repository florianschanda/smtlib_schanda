(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEAEF2FE4)))
;; x should be Float32(0xEAEF2FE4 [Rational(-144579713985120025928794112), -144579713985120025928794112.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00010101 #b00010010011110001110011)))
;; y should be Float32(0x0A893C73 [Rational(8993907, 680564733841876926926749214863536422912), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b11010101 #b11011110010111111100101)))
(check-sat)
(exit)
