(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110110 #b00100100001110010011100)))
;; x should be Float32(0xFB121C9C [Rational(-758655608408339305773987579958394880), -758655608408339305773987579958394880.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x006F7ABF)))
;; y should be Float32(0x006F7ABF [Rational(7305919, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xFB121C9C))))
(check-sat)
(exit)
