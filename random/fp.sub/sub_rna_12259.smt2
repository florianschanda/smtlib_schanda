(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD61990C0)))
;; x should be Float32(0xD61990C0 [Rational(-42211743891456), -42211743891456.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x68049AD4)))
;; y should be Float32(0x68049AD4 [Rational(2504833802144321919516672), 2504833802144321919516672.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b1 #b11010000 #b00001001001101011010100)))
(check-sat)
(exit)
