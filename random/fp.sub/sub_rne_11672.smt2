(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807EA799)))
;; x should be Float32(0x807EA799 [Rational(-8300441, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8BCCEF71)))
;; y should be Float32(0x8BCCEF71 [Rational(-13430641, 170141183460469231731687303715884105728), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b00010111 #b10011001110111101101111)))
(check-sat)
(exit)
