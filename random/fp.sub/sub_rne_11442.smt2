(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00000110001011110101011)))
;; x should be Float32(0xCA8317AB [Rational(-8591275, 2), -4295637.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00101100000010000011100)))
;; y should be Float32(0x8016041C [Rational(-360711, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b10010101 #b00000110001011110101011))))
(check-sat)
(exit)
