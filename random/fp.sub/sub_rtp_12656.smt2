(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00011011111011111110111)))
;; x should be Float32(0x000DF7F7 [Rational(915447, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80041B19)))
;; y should be Float32(0x80041B19 [Rational(-269081, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #x00121310)))
(check-sat)
(exit)
