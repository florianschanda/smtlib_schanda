(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10001101111101101100100)))
;; x should be Float32(0x0046FB64 [Rational(1162969, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00001101010100111110111)))
;; y should be Float32(0xCA86A9F7 [Rational(-8825335, 2), -4412667.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result ((_ to_fp 8 24) #x4A86A9F7)))
(check-sat)
(exit)
