(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110111 #b10011100011111010000001)))
;; x should be Float32(0xDBCE3E81 [Rational(-116105137948196864), -116105137948196864.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01101110010110010101100)))
;; y should be Float32(0x49B72CAC [Rational(3001131, 2), 1500565.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result ((_ to_fp 8 24) #xDBCE3E81)))
(check-sat)
(exit)
