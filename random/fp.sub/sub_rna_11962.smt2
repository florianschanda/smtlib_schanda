(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x38AF79CD)))
;; x should be Float32(0x38AF79CD [Rational(11499981, 137438953472), 0.000084])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA9F42EF)))
;; y should be Float32(0xCA9F42EF [Rational(-10437359, 2), -5218679.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b0 #b10010101 #b00111110100001011101111))))
(check-sat)
(exit)
