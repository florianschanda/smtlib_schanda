(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11101011011000011100101)))
;; x should be Float32(0xCAF5B0E5 [Rational(-16101605, 2), -8050802.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80066E4F)))
;; y should be Float32(0x80066E4F [Rational(-421455, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b10010101 #b11101011011000011100100)))
(check-sat)
(exit)
