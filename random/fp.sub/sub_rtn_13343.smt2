(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA005CBE)))
;; x should be Float32(0xCA005CBE [Rational(-4206175, 2), -2103087.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AC4E037)))
;; y should be Float32(0x4AC4E037 [Rational(12902455, 2), 6451227.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b10010110 #b00000101000011101001011)))
(check-sat)
(exit)
