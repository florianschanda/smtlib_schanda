(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00001101001111110000100)))
;; x should be Float32(0xC9869F84 [Rational(-2205665, 2), -1102832.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11110101110111011010101)))
;; y should be Float32(0xCAFAEED5 [Rational(-16445141, 2), -8222570.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b0 #b10010101 #b10110010100011011110100)))
(check-sat)
(exit)
