(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001110 #b11111001011011110101100)))
;; x should be Float32(0x477CB7AC [Rational(4140523, 64), 64695.671875])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001011 #b10111101110011100001111)))
;; y should be Float32(0x45DEE70F [Rational(14608143, 2048), 7132.882324])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #x4760DACA)))
(check-sat)
(exit)
