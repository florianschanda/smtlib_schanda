(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10000110100011100000011)))
;; x should be Float32(0x4AC34703 [Rational(12797699, 2), 6398849.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01111001 #b10100100001100111110001)))
;; y should be Float32(0x3CD219F1 [Rational(13769201, 536870912), 0.025647])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #x4AC34703)))
(check-sat)
(exit)
