(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDEA14930)))
;; x should be Float32(0xDEA14930 [Rational(-5810936544982204416), -5810936544982204416.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11101011100010100101111)))
;; y should be Float32(0x8075C52F [Rational(-7718191, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xDEA1492F)))
(check-sat)
(exit)
