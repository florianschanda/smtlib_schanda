(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01111001 #b00111101011101110100000)))
;; x should be Float32(0xBC9EBBA0 [Rational(-325085, 16777216), -0.019377])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x048C0A3D)))
;; y should be Float32(0x048C0A3D [Rational(9177661, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b1 #b01111001 #b00111101011101110100000))))
(check-sat)
(exit)
