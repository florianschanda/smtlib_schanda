(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111001 #b01001110011101100100101)))
;; x should be Float32(0x1CA73B25 [Rational(10959653, 9903520314283042199192993792), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b10010011001001000100011)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
