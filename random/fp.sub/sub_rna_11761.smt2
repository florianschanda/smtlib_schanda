(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b00011101100001001100100)))
;; x should be Float32(0x498EC264 [Rational(2338969, 2), 1169484.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011010 #b10011101000010100100010)))
;; y should be Float32(0x0D4E8522 [Rational(6767249, 10633823966279326983230456482242756608), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b0 #b10010011 #b00011101100001001100100)))
(check-sat)
(exit)
