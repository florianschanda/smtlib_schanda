(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11111001100001010001000)))
;; x should be Float32(0x007CC288 [Rational(1022033, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
