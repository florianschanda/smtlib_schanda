(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A14CAA2)))
;; x should be Float32(0x4A14CAA2 [Rational(4875601, 2), 2437800.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x000A4DAC)))
;; y should be Float32(0x000A4DAC [Rational(168811, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b10010100 #b00101001100101010100010))))
(check-sat)
(exit)
