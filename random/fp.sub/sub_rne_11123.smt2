(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11001101110100001000101)))
;; x should be Float32(0x4AE6E845 [Rational(15132741, 2), 7566370.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x6CD6EEBA)))
;; y should be Float32(0x6CD6EEBA [Rational(2078699837718810631396655104), 2078699837718810631396655104.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xECD6EEBA))))
(check-sat)
(exit)
