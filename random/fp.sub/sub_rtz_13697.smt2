(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010011 #b11011011000100010011100)))
;; x should be Float32(0x49ED889C [Rational(3891751, 2), 1945875.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111010111000000100000)))
;; y should be Float32(0x001D7020 [Rational(60289, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x49ED889B)))
(check-sat)
(exit)
