(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01101101110001000100101)))
;; x should be Float32(0xCAB6E225 [Rational(-11985445, 2), -5992722.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800443F2)))
;; y should be Float32(0x800443F2 [Rational(-139769, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAB6E225))))
(check-sat)
(exit)
