(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00011011 #b11000101110100111010001)))
;; x should be Float32(0x0DE2E9D1 [Rational(14870993, 10633823966279326983230456482242756608), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA9E4B37)))
;; y should be Float32(0xCA9E4B37 [Rational(-10373943, 2), -5186971.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A9E4B37))))
(check-sat)
(exit)
