(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111111 #b11011000101010111001110)))
;; x should be Float32(0x3FEC55CE [Rational(7744231, 4194304), 1.846369])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A1BCBF2)))
;; y should be Float32(0x4A1BCBF2 [Rational(5105145, 2), 2552572.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #xCA1BCBEA)))
(check-sat)
(exit)
