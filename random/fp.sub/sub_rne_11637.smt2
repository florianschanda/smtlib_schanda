(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE222DC2C)))
;; x should be Float32(0xE222DC2C [Rational(-751059398882066694144), -751059398882066694144.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00100010110011011111000)))
;; y should be Float32(0x801166F8 [Rational(-142559, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b11000100 #b01000101101110000101100)))
(check-sat)
(exit)
