(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00100100000001111001111)))
;; x should be Float32(0x4A9203CF [Rational(9569231, 2), 4784615.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01011110 #b10100010101000011101100)))
;; y should be Float32(0xAF5150EC [Rational(-3429435, 18014398509481984), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b10010101 #b00100100000001111001111)))
(check-sat)
(exit)
