(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x31C1BC9B)))
;; x should be Float32(0x31C1BC9B [Rational(12696731, 2251799813685248), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A2EA0EE)))
;; y should be Float32(0x4A2EA0EE [Rational(5722231, 2), 2861115.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b10010100 #b01011101010000011101110))))
(check-sat)
(exit)
