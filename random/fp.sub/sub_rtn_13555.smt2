(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x976B82A7)))
;; x should be Float32(0x976B82A7 [Rational(-15434407, 20282409603651670423947251286016), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11001101110010000000100)))
;; y should be Float32(0xC9E6E404 [Rational(-3782913, 2), -1891456.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b10010011 #b11001101110010000000011))))
(check-sat)
(exit)
