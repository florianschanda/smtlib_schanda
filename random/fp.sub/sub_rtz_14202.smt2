(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8EA2BAE9)))
;; x should be Float32(0x8EA2BAE9 [Rational(-10664681, 2658455991569831745807614120560689152), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10111101 #b11100101000001110111000)))
;; y should be Float32(0xDEF283B8 [Rational(-8737506644633583616), -8737506644633583616.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x5EF283B7))))
(check-sat)
(exit)
