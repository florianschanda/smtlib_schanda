(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110001 #b00101010011100010110000)))
;; x should be Float32(0xD89538B0 [Rational(-1312565627977728), -1312565627977728.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01110010110110010110101)))
;; y should be Float32(0x80396CB5 [Rational(-3763381, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b10110001 #b00101010011100010101111)))
(check-sat)
(exit)
