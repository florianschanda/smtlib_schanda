(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00010000 #b10110011100100000100010)))
;; x should be Float32(0x8859C822 [Rational(-7136273, 10889035741470030830827987437816582766592), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00110101010000011001111)))
;; y should be Float32(0x801AA0CF [Rational(-1745103, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b00010000 #b10110011100011111101101)))
(check-sat)
(exit)
