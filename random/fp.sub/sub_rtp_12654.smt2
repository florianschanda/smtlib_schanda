(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00000100001010100101010)))
;; x should be Float32(0x0002152A [Rational(68245, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEAA7A0BC)))
;; y should be Float32(0xEAA7A0BC [Rational(-101324829250394036402913280), -101324829250394036402913280.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #x6AA7A0BD)))
(check-sat)
(exit)
