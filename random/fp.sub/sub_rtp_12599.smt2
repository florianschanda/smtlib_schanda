(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01110101 #b01001111101000110101011)))
;; x should be Float32(0x3AA7D1AB [Rational(10998187, 8589934592), 0.001280])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0071B831)))
;; y should be Float32(0x0071B831 [Rational(7452721, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #x3AA7D1AB)))
(check-sat)
(exit)
