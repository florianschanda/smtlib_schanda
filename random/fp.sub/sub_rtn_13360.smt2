(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11010000001000000101101)))
;; x should be Float32(0xCAE8102D [Rational(-15208493, 2), -7604246.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000000)))
;; y should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b10010101 #b11010000001000000101101)))
(check-sat)
(exit)
