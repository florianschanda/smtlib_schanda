(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7DCC8CA9)))
;; x should be Float32(0x7DCC8CA9 [Rational(33986607553442591384084069608593555456), 33986607553442591384084069608593555456.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00101001001010110010100)))
;; y should be Float32(0x00149594 [Rational(337253, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b0 #b11111011 #b10011001000110010101001)))
(check-sat)
(exit)
