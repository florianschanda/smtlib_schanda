(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AEFF7B5)))
;; x should be Float32(0x4AEFF7B5 [Rational(15726517, 2), 7863258.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA9FE136)))
;; y should be Float32(0xCA9FE136 [Rational(-5238939), -5238939.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b0 #b10010110 #b10001111110110001110101)))
(check-sat)
(exit)
