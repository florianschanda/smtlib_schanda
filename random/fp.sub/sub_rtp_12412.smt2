(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01100001010011011000111)))
;; x should be Float32(0x4AB0A6C7 [Rational(11577031, 2), 5788515.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11000011100011101100011)))
;; y should be Float32(0xCAE1C763 [Rational(-14796643, 2), -7398321.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x4B493715))))
(check-sat)
(exit)
