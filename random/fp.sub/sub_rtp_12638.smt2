(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01111001000010100000011)))
;; x should be Float32(0x003C8503 [Rational(3966211, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00111000010001100011110)))
;; y should be Float32(0x001C231E [Rational(921999, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x002061E5))))
(check-sat)
(exit)
