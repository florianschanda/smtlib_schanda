(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC82A32E0)))
;; x should be Float32(0xC82A32E0 [Rational(-348567, 2), -174283.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00111011001100100111000)))
;; y should be Float32(0x801D9938 [Rational(-242471, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xC82A32E0))))
(check-sat)
(exit)
