(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3B051ED8)))
;; x should be Float32(0x3B051ED8 [Rational(1090523, 536870912), 0.002031])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11100001111111100100110)))
;; y should be Float32(0x0070FF26 [Rational(3702675, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result (fp #b0 #b01110110 #b00001010001111011010111))))
(check-sat)
(exit)
