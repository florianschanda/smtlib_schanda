(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9426C28)))
;; x should be Float32(0xC9426C28 [Rational(-1592709, 2), -796354.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01110101 #b00101011101101100001011)))
;; y should be Float32(0x3A95DB0B [Rational(9820939, 8589934592), 0.001143])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xC9426C28))))
(check-sat)
(exit)
