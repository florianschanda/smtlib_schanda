(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11000001100101010001100)))
;; x should be Float32(0x0060CA8C [Rational(1585827, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b00111010001011000110010)))
;; y should be Float32(0xCA1D1632 [Rational(-5147417, 2), -2573708.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b10010100 #b00111010001011000110010))))
(check-sat)
(exit)
