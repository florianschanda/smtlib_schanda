(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC8C527)))
;; x should be Float32(0xCAC8C527 [Rational(-13157671, 2), -6578835.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC986CE4C)))
;; y should be Float32(0xC986CE4C [Rational(-2208659, 2), -1104329.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b10010101 #b01001110001000110010100))))
(check-sat)
(exit)
