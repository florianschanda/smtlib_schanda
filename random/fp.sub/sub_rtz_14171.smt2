(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11010001 #b11100111001011110110010)))
;; x should be Float32(0xE8F397B2 [Rational(-9202666719553541443158016), -9202666719553541443158016.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A130D9E)))
;; y should be Float32(0x4A130D9E [Rational(4818639, 2), 2409319.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b11010001 #b11100111001011110110010)))
(check-sat)
(exit)
