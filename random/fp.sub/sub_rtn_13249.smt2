(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000010 #b10100101100101101000010)))
;; x should be Float32(0x4152CB42 [Rational(6907297, 524288), 13.174623])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00791D4C)))
;; y should be Float32(0x00791D4C [Rational(1984339, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b10000010 #b10100101100101101000001))))
(check-sat)
(exit)
