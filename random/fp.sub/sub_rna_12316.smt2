(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802086F2)))
;; x should be Float32(0x802086F2 [Rational(-1065849, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000101 #b00111001010100000010111)))
;; y should be Float32(0x829CA817 [Rational(-10266647, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b0 #b00000101 #b00110101001111110101000)))
(check-sat)
(exit)
