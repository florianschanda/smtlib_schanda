(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10001111010000110110101)))
;; x should be Float32(0x4AC7A1B5 [Rational(13083061, 2), 6541530.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01001011001001000011010)))
;; y should be Float32(0xCA25921A [Rational(-5425421, 2), -2712710.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b10010110 #b00011010011010101100001))))
(check-sat)
(exit)
