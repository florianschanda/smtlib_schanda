(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA53829E)))
;; x should be Float32(0xCA53829E [Rational(-6930767, 2), -3465383.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01101011 #b01101110010000010100110)))
;; y should be Float32(0xB5B720A6 [Rational(-6000723, 4398046511104), -0.000001])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA53829D))))
(check-sat)
(exit)
