(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b11001011101110101010000)))
;; x should be Float32(0xC8E5DD50 [Rational(-941525, 2), -470762.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x9D94909F)))
;; y should be Float32(0x9D94909F [Rational(-9736351, 2475880078570760549798248448), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b10010001 #b11001011101110101010000))))
(check-sat)
(exit)
