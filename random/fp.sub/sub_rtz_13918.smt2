(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01011111011100001000001)))
;; x should be Float32(0x002FB841 [Rational(3127361, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010001 #b11010000001111110110000)))
;; y should be Float32(0x48E81FB0 [Rational(950779, 2), 475389.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result (fp #b1 #b10010001 #b11010000001111110101111))))
(check-sat)
(exit)
