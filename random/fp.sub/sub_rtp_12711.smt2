(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11000010000110001100110)))
;; x should be Float32(0xCA610C66 [Rational(-7374387, 2), -3687193.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10000001010011010001100)))
;; y should be Float32(0xC9C0A68C [Rational(-3156387, 2), -1578193.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA00B920))))
(check-sat)
(exit)
