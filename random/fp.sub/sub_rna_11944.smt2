(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001110 #b11011110101010000111000)))
;; x should be Float32(0x676F5438 [Rational(1130199151133302932373504), 1130199151133302932373504.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A9AC669)))
;; y should be Float32(0x4A9AC669 [Rational(10143337, 2), 5071668.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x676F5438))))
(check-sat)
(exit)
