(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00011110 #b10001111011100100010010)))
;; x should be Float32(0x8F47B912 [Rational(-6544521, 664613997892457936451903530140172288), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10011001011000000000111)))
;; y should be Float32(0x4ACCB007 [Rational(13414407, 2), 6707203.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b1 #b10010101 #b10011001011000000000111)))
(check-sat)
(exit)
