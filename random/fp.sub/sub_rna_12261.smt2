(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01101111 #b11101001111111100101001)))
;; x should be Float32(0xB7F4FF29 [Rational(-16056105, 549755813888), -0.000029])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10111011011100011101000)))
;; y should be Float32(0x005DB8E8 [Rational(767773, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b1 #b01101111 #b11101001111111100101001))))
(check-sat)
(exit)
