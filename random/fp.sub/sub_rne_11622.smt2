(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9FC8CE36)))
;; x should be Float32(0x9FC8CE36 [Rational(-6579995, 77371252455336267181195264), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b10100001100000111101000)))
;; y should be Float32(0xC950C1E8 [Rational(-1710141, 2), -855070.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x4950C1E8))))
(check-sat)
(exit)
