(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11000111011110001111110)))
;; x should be Float32(0xCA63BC7E [Rational(-7462463, 2), -3731231.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10110110100010101110011)))
;; y should be Float32(0x005B4573 [Rational(5981555, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA63BC7E))))
(check-sat)
(exit)
