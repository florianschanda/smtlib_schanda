(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x58014AD3)))
;; x should be Float32(0x58014AD3 [Rational(568633470222336), 568633470222336.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00546C95)))
;; y should be Float32(0x00546C95 [Rational(5532821, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x58014AD3))))
(check-sat)
(exit)
