(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110100 #b01110010001011000110010)))
;; x should be Float32(0xFA391632 [Rational(-240256273312087290311087084407881728), -240256273312087290311087084407881728.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010101 #b11011010100101001011101)))
;; y should be Float32(0xAAED4A5D [Rational(-15551069, 36893488147419103232), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xFA391632))))
(check-sat)
(exit)
