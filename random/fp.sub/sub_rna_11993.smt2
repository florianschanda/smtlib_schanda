(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01000110011001101010011)))
;; x should be Float32(0x00233353 [Rational(2306899, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10110110 #b01100010000010010000011)))
;; y should be Float32(0x5B310483 [Rational(49826031565012992), 49826031565012992.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b1 #b10110110 #b01100010000010010000011)))
(check-sat)
(exit)
