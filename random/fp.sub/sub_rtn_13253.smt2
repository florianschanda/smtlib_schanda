(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2EFFB6A0)))
;; x should be Float32(0x2EFFB6A0 [Rational(523701, 4503599627370496), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b11111110101010101001100)))
;; y should be Float32(0xC9FF554C [Rational(-4183379, 2), -2091689.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result ((_ to_fp 8 24) #x49FF554C)))
(check-sat)
(exit)
