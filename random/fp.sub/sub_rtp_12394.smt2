(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00001001111101100110110)))
;; x should be Float32(0x4A04FB36 [Rational(4357531, 2), 2178765.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A359182)))
;; y should be Float32(0x4A359182 [Rational(5949633, 2), 2974816.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result (fp #b1 #b10010010 #b10000100101100100110000))))
(check-sat)
(exit)
