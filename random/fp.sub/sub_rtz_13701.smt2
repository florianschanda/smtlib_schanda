(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A11216E)))
;; x should be Float32(0x4A11216E [Rational(4755639, 2), 2377819.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9FCB8EC)))
;; y should be Float32(0xC9FCB8EC [Rational(-4140603, 2), -2070301.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b0 #b10010101 #b00001111011111011110010)))
(check-sat)
(exit)
