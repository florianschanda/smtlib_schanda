(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b01100011100110010010010)))
;; x should be Float32(0x4A31CC92 [Rational(5826121, 2), 2913060.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b11001000011000101011100)))
;; y should be Float32(0x49E4315C [Rational(3738711, 2), 1869355.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #x497ECF90)))
(check-sat)
(exit)
