(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x55FBDC28)))
;; x should be Float32(0x55FBDC28 [Rational(34615372808192), 34615372808192.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00000101110011100100110)))
;; y should be Float32(0x4A02E726 [Rational(4289427, 2), 2144713.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x55FBDC27))))
(check-sat)
(exit)
