(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110100 #b01101111011100000000101)))
;; x should be Float32(0x1A37B805 [Rational(12040197, 316912650057057350374175801344), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b00001111110011010100100)))
;; y should be Float32(0xC987E6A4 [Rational(-2226601, 2), -1113300.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x4987E6A5))))
(check-sat)
(exit)
