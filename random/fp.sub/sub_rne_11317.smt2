(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x464D43BE)))
;; x should be Float32(0x464D43BE [Rational(6726111, 512), 13136.935547])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0030ECBC)))
;; y should be Float32(0x0030ECBC [Rational(801583, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b10001100 #b10011010100001110111110)))
(check-sat)
(exit)
