(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4918D448)))
;; x should be Float32(0x4918D448 [Rational(1251977, 2), 625988.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4DA7D155)))
;; y should be Float32(0x4DA7D155 [Rational(351939232), 351939232.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b10011011 #b01001111000010011101010)))
(check-sat)
(exit)
