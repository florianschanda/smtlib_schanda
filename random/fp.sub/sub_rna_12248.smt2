(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11100111 #b00001000000110111001110)))
;; x should be Float32(0xF3840DCE [Rational(-20924779591458821323702426140672), -20924779591458821323702426140672.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b01010100001101000110100)))
;; y should be Float32(0x49AA1A34 [Rational(2786957, 2), 1393478.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xF3840DCE))))
(check-sat)
(exit)
