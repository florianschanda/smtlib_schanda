(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01011001110000000011100)))
;; x should be Float32(0xC9ACE01C [Rational(-2832391, 2), -1416195.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x801D5D7B)))
;; y should be Float32(0x801D5D7B [Rational(-1924475, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xC9ACE01C))))
(check-sat)
(exit)
