(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AFE376F)))
;; x should be Float32(0x4AFE376F [Rational(16660335, 2), 8330167.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x807C29DF)))
;; y should be Float32(0x807C29DF [Rational(-8137183, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b0 #b10010101 #b11111100011011101101111)))
(check-sat)
(exit)
