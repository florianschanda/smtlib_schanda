(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
;; x should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00101100 #b10101010000011110001010)))
;; y should be Float32(0x9655078A [Rational(-6980549, 40564819207303340847894502572032), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #x1655078A)))
(check-sat)
(exit)
