(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01011001 #b11001111011001011111000)))
;; x should be Float32(0x2CE7B2F8 [Rational(1898079, 288230376151711744), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4F241A23)))
;; y should be Float32(0x4F241A23 [Rational(2753176320), 2753176320.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result ((_ to_fp 8 24) #xCF241A23)))
(check-sat)
(exit)
