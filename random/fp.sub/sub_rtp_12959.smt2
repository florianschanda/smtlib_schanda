(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8001B97A)))
;; x should be Float32(0x8001B97A [Rational(-56509, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10110111 #b11011100100011101111000)))
;; y should be Float32(0xDBEE4778 [Rational(-134139250357567488), -134139250357567488.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x5BEE4778))))
(check-sat)
(exit)
