(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010000 #b11010101010101100100000)))
;; x should be Float32(0xC86AAB20 [Rational(-480601, 2), -240300.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00111010000011110100111)))
;; y should be Float32(0xCA9D07A7 [Rational(-10291111, 2), -5145555.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b10010101 #b00101011011001001001110))))
(check-sat)
(exit)
