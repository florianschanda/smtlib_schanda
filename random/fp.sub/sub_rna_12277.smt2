(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDA33965F)))
;; x should be Float32(0xDA33965F [Rational(-12637338899316736), -12637338899316736.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFA1D8D08)))
;; y should be Float32(0xFA1D8D08 [Rational(-204512765092345734330941308617621504), -204512765092345734330941308617621504.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b0 #b11110100 #b00111011000110100001000))))
(check-sat)
(exit)
