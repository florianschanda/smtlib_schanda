(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC983562C)))
;; x should be Float32(0xC983562C [Rational(-2151819, 2), -1075909.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB7BE4555)))
;; y should be Float32(0xB7BE4555 [Rational(-12469589, 549755813888), -0.000023])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b1 #b10010011 #b00000110101011000101100))))
(check-sat)
(exit)
