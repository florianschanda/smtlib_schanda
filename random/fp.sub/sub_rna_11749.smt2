(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01110110110011101111001)))
;; x should be Float32(0x4ABB6779 [Rational(12281721, 2), 6140860.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10101011111101100110010)))
;; y should be Float32(0x4A55FB32 [Rational(7011737, 2), 3505868.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b0 #b10010100 #b01000001101001111000000)))
(check-sat)
(exit)
