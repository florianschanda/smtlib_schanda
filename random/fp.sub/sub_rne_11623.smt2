(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xEC7418ED)))
;; x should be Float32(0xEC7418ED [Rational(-1180382434639615437108674560), -1180382434639615437108674560.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010000 #b00110011001111111100000)))
;; y should be Float32(0xC8199FE0 [Rational(-314623, 2), -157311.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b11011000 #b11101000001100011101101)))
(check-sat)
(exit)
