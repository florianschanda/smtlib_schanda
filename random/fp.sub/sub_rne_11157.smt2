(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7F800000)))
;; x should be Float32(+oo)

(declare-const y Float32)
(assert (= y (fp #b0 #b11011011 #b11100110100000010100111)))
;; y should be Float32(0x6DF340A7 [Rational(9410377159280603399756709888), 9410377159280603399756709888.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x7F800000))))
(check-sat)
(exit)
