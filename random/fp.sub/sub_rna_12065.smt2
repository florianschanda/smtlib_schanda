(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC99BE8F4)))
;; x should be Float32(0xC99BE8F4 [Rational(-2554429, 2), -1277214.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0071A763)))
;; y should be Float32(0x0071A763 [Rational(7448419, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b1 #b10010011 #b00110111110100011110100))))
(check-sat)
(exit)
