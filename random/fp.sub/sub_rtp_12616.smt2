(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10101001 #b11101000110011010101001)))
;; x should be Float32(0x54F466A9 [Rational(8397554974720), 8397554974720.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00100101 #b01001010000111010001011)))
;; y should be Float32(0x92A50E8B [Rational(-10817163, 10384593717069655257060992658440192), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #x54F466AA)))
(check-sat)
(exit)
