(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A9D16EF)))
;; x should be Float32(0x4A9D16EF [Rational(10295023, 2), 5147511.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x005CAEC0)))
;; y should be Float32(0x005CAEC0 [Rational(94907, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b10010101 #b00111010001011011101110))))
(check-sat)
(exit)
