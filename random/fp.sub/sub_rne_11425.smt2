(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10010110000000010010010)))
;; x should be Float32(0xCA4B0092 [Rational(-6651977, 2), -3325988.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11011000110110101011111)))
;; y should be Float32(0x006C6D5F [Rational(7105887, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA4B0092))))
(check-sat)
(exit)
