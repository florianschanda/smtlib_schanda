(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9B1B214)))
;; x should be Float32(0xC9B1B214 [Rational(-2911365, 2), -1455682.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11111000010111110001010)))
;; y should be Float32(0xCA7C2F8A [Rational(-8263621, 2), -4131810.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A235680))))
(check-sat)
(exit)
