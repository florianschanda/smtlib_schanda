(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001F1CCF)))
;; x should be Float32(0x001F1CCF [Rational(2038991, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A012626)))
;; y should be Float32(0x4A012626 [Rational(4231955, 2), 2115977.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA012626))))
(check-sat)
(exit)
