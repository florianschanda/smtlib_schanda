(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB28239)))
;; x should be Float32(0xCAB28239 [Rational(-11698745, 2), -5849372.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01101011101100000100110)))
;; y should be Float32(0x0035D826 [Rational(1764371, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result ((_ to_fp 8 24) #xCAB2823A)))
(check-sat)
(exit)
