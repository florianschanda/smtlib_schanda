(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA5ABCB6)))
;; x should be Float32(0xCA5ABCB6 [Rational(-7167579, 2), -3583789.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC2508724)))
;; y should be Float32(0xC2508724 [Rational(-3416521, 65536), -52.131973])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b10010100 #b10110101011101111100110)))
(check-sat)
(exit)
