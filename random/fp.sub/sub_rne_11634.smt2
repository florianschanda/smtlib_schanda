(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC48FEEAA)))
;; x should be Float32(0xC48FEEAA [Rational(-4716373, 4096), -1151.458252])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xAC5AF1D9)))
;; y should be Float32(0xAC5AF1D9 [Rational(-14348761, 4611686018427387904), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b10001001 #b00011111110111010101010)))
(check-sat)
(exit)
