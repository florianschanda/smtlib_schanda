(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AFE2F6B)))
;; x should be Float32(0x4AFE2F6B [Rational(16658283, 2), 8329141.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xECAD190D)))
;; y should be Float32(0xECAD190D [Rational(-1674099726104604372629454848), -1674099726104604372629454848.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result (fp #b0 #b11011001 #b01011010001100100001101))))
(check-sat)
(exit)
