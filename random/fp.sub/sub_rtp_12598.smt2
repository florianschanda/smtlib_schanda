(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110101 #b01111111000010001011011)))
;; x should be Float32(0x1ABF845B [Rational(12551259, 158456325028528675187087900672), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x54ED4296)))
;; y should be Float32(0x54ED4296 [Rational(8152195006464), 8152195006464.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result (fp #b1 #b10101001 #b11011010100001010010101))))
(check-sat)
(exit)
