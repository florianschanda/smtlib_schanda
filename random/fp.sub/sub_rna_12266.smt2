(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xBC1F3483)))
;; x should be Float32(0xBC1F3483 [Rational(-10433667, 1073741824), -0.009717])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00010101010110101111101)))
;; y should be Float32(0xCA8AAD7D [Rational(-9088381, 2), -4544190.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b0 #b10010101 #b00010101010110101111101)))
(check-sat)
(exit)
