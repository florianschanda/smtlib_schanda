(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01101110101011001110011)))
;; x should be Float32(0xCAB75673 [Rational(-12015219, 2), -6007609.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01101101101111100000110)))
;; y should be Float32(0x4A36DF06 [Rational(5992323, 2), 2996161.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xCB0962FB))))
(check-sat)
(exit)
