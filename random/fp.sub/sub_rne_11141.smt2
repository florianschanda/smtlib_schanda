(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01110000010101110101101)))
;; x should be Float32(0x4AB82BAD [Rational(12069805, 2), 6034902.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000010 #b00101101110011100011001)))
;; y should be Float32(0xE116E719 [Rational(-173978997009600741376), -173978997009600741376.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b11000010 #b00101101110011100011001)))
(check-sat)
(exit)
