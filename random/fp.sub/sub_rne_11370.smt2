(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01110101010000110100011)))
;; x should be Float32(0x003AA1A3 [Rational(3842467, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11101100 #b10100111111001110111100)))
;; y should be Float32(0xF653F3BC [Rational(-1074724763260828776590255365029888), -1074724763260828776590255365029888.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b11101100 #b10100111111001110111100)))
(check-sat)
(exit)
