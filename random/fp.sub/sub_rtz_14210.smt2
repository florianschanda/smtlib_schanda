(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807E0A22)))
;; x should be Float32(0x807E0A22 [Rational(-4130065, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49D6E99C)))
;; y should be Float32(0x49D6E99C [Rational(3521127, 2), 1760563.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b10010011 #b10101101110100110011100)))
(check-sat)
(exit)
