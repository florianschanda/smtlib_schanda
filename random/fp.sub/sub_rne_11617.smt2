(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11100010 #b10010011100101101111100)))
;; x should be Float32(0xF149CB7C [Rational(-999239751822656069954445508608), -999239751822656069954445508608.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00101001 #b00110101111001000100000)))
;; y should be Float32(0x149AF220 [Rational(317329, 20282409603651670423947251286016), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b11100010 #b10010011100101101111100))))
(check-sat)
(exit)
