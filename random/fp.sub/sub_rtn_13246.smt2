(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000010 #b00100100101111111100011)))
;; x should be Float32(0x21125FE3 [Rational(9592803, 19342813113834066795298816), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01010011 #b01001110101110101111100)))
;; y should be Float32(0x29A75D7C [Rational(2742111, 36893488147419103232), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xA9A75D33))))
(check-sat)
(exit)
