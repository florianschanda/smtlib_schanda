(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xBA6CC1CA)))
;; x should be Float32(0xBA6CC1CA [Rational(-7758053, 8589934592), -0.000903])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b10000100101000010111100)))
;; y should be Float32(0x49C250BC [Rational(3183663, 2), 1591831.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b10010011 #b10000100101000010111101)))
(check-sat)
(exit)
