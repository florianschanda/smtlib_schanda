(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCABE0989)))
;; x should be Float32(0xCABE0989 [Rational(-12454281, 2), -6227140.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11000111101100000111110)))
;; y should be Float32(0x4A63D83E [Rational(7466015, 2), 3733007.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b10010110 #b00101111111101011010100)))
(check-sat)
(exit)
