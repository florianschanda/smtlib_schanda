(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01110001 #b01000001011111000001111)))
;; x should be Float32(0x38A0BE0F [Rational(10534415, 137438953472), 0.000077])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA973863)))
;; y should be Float32(0xCA973863 [Rational(-9910371, 2), -4955185.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b10010101 #b00101110011100001100011))))
(check-sat)
(exit)
