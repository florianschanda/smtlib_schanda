(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A932851)))
;; x should be Float32(0x4A932851 [Rational(9644113, 2), 4822056.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01011100 #b11000111111001010001010)))
;; y should be Float32(0xAE63F28A [Rational(-7469381, 144115188075855872), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #x4A932852)))
(check-sat)
(exit)
