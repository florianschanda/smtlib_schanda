(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0050A649)))
;; x should be Float32(0x0050A649 [Rational(5285449, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10101000 #b10101011100101101110110)))
;; y should be Float32(0x5455CB76 [Rational(3672966168576), 3672966168576.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b1 #b10101000 #b10101011100101101110110))))
(check-sat)
(exit)
