(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA95274B)))
;; x should be Float32(0xCA95274B [Rational(-9774923, 2), -4887461.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAFCA6D7)))
;; y should be Float32(0xCAFCA6D7 [Rational(-16557783, 2), -8278891.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b0 #b10010100 #b10011101111111100011000))))
(check-sat)
(exit)
