(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01000011101110111101001)))
;; x should be Float32(0x4AA1DDE9 [Rational(10608105, 2), 5304052.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800132B4)))
;; y should be Float32(0x800132B4 [Rational(-19629, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b0 #b10010101 #b01000011101110111101010)))
(check-sat)
(exit)
