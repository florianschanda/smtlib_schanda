(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0DBF42BB)))
;; x should be Float32(0x0DBF42BB [Rational(12534459, 10633823966279326983230456482242756608), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80110704)))
;; y should be Float32(0x80110704 [Rational(-278977, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result (fp #b0 #b00011011 #b01111110100001010111011))))
(check-sat)
(exit)
