(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10110100 #b00000010010010010011110)))
;; x should be Float32(0xDA01249E [Rational(-9087633254776832), -9087633254776832.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001101 #b00100011000110101000000)))
;; y should be Float32(0x46918D40 [Rational(149045, 8), 18630.625000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xDA01249E))))
(check-sat)
(exit)
