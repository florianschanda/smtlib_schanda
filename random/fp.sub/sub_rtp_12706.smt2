(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11000010011011110000001)))
;; x should be Float32(0xCAE13781 [Rational(-14759809, 2), -7379904.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x76D1FB85)))
;; y should be Float32(0x76D1FB85 [Rational(2129475518730292853117547723096064), 2129475518730292853117547723096064.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result ((_ to_fp 8 24) #xF6D1FB85)))
(check-sat)
(exit)
