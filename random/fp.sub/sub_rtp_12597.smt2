(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x23283622)))
;; x should be Float32(0x23283622 [Rational(5511953, 604462909807314587353088), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x43E875DE)))
;; y should be Float32(0x43E875DE [Rational(7617263, 16384), 464.920837])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xC3E875DD))))
(check-sat)
(exit)
