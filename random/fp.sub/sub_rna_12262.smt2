(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xECD534FB)))
;; x should be Float32(0xECD534FB [Rational(-2062011142142301894887866368), -2062011142142301894887866368.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11100011111100001000111)))
;; y should be Float32(0x0071F847 [Rational(7469127, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b1 #b11011001 #b10101010011010011111011))))
(check-sat)
(exit)
