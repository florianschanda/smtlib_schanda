(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A52320A)))
;; x should be Float32(0x4A52320A [Rational(6887685, 2), 3443842.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11100110101101100000111)))
;; y should be Float32(0xCAF35B07 [Rational(-15948551, 2), -7974275.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x4B2E3A06))))
(check-sat)
(exit)
