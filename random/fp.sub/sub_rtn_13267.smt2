(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x68D5DA24)))
;; x should be Float32(0x68D5DA24 [Rational(8079104361061507825401856), 8079104361061507825401856.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x805CE47A)))
;; y should be Float32(0x805CE47A [Rational(-3043901, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b11010001 #b10101011101101000100100))))
(check-sat)
(exit)
