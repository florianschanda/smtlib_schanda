(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b11001001111110001101000)))
;; x should be Float32(0xC964FC68 [Rational(-1875853, 2), -937926.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00100000000101110001101)))
;; y should be Float32(0x4A900B8D [Rational(9440141, 2), 4720070.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b10010101 #b01011001010101100011010))))
(check-sat)
(exit)
