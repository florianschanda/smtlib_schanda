(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000101 #b11011001110111100100111)))
;; x should be Float32(0x22ECEF27 [Rational(15527719, 2417851639229258349412352), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AA6B915)))
;; y should be Float32(0x4AA6B915 [Rational(10926357, 2), 5463178.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAA6B915))))
(check-sat)
(exit)
