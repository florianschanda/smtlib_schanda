(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11111001111110011111100)))
;; x should be Float32(0x807CFCFC [Rational(-2047807, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11110000 #b00010001010100111110000)))
;; y should be Float32(0x7808A9F0 [Rational(11087486167855579894106516199833600), 11087486167855579894106516199833600.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b11110000 #b00010001010100111110000)))
(check-sat)
(exit)
