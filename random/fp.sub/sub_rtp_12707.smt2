(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAF42C99)))
;; x should be Float32(0xCAF42C99 [Rational(-16002201, 2), -8001100.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x000BAA8D)))
;; y should be Float32(0x000BAA8D [Rational(764557, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b10010101 #b11101000010110010011001)))
(check-sat)
(exit)
