(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE13631)))
;; x should be Float32(0xCAE13631 [Rational(-14759473, 2), -7379736.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00428BB3)))
;; y should be Float32(0x00428BB3 [Rational(4361139, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b10010101 #b11000010011011000110001)))
(check-sat)
(exit)
