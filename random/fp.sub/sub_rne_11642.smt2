(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100100001110010100100)))
;; x should be Float32(0x80321CA4 [Rational(-821033, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11100111001100101000011)))
;; y should be Float32(0x4AF39943 [Rational(15964483, 2), 7982241.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAF39943))))
(check-sat)
(exit)
