(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10111001011011000010110)))
;; x should be Float32(0x4A5CB616 [Rational(7232267, 2), 3616133.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00101011 #b00101111001001011001010)))
;; y should be Float32(0x959792CA [Rational(-4966757, 81129638414606681695789005144064), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b0 #b10010100 #b10111001011011000010110)))
(check-sat)
(exit)
