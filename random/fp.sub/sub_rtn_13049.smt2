(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00110110010000000100001)))
;; x should be Float32(0x4A9B2021 [Rational(10166305, 2), 5083152.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x5F3181A4)))
;; y should be Float32(0x5F3181A4 [Rational(12790684736615874560), 12790684736615874560.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b1 #b10111110 #b01100011000000110100100))))
(check-sat)
(exit)
