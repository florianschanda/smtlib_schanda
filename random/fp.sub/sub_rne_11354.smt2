(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10011101101100111100000)))
;; x should be Float32(0x004ED9E0 [Rational(161487, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00384FA7)))
;; y should be Float32(0x00384FA7 [Rational(3690407, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b00000000 #b00101101000101000111001))))
(check-sat)
(exit)
