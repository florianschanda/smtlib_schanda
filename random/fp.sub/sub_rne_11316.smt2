(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00100110 #b10111010111110011000001)))
;; x should be Float32(0x135D7CC1 [Rational(14515393, 5192296858534827628530496329220096), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0002C55C)))
;; y should be Float32(0x0002C55C [Rational(45399, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b00100110 #b10111010111110011000001))))
(check-sat)
(exit)
