(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x000E5443)))
;; x should be Float32(0x000E5443 [Rational(939075, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10001100 #b01000010000000000011100)))
;; y should be Float32(0xC621001C [Rational(-2637831, 256), -10304.027344])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b0 #b10001100 #b01000010000000000011100))))
(check-sat)
(exit)
