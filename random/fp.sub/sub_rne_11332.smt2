(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x2DC8B369)))
;; x should be Float32(0x2DC8B369 [Rational(13153129, 576460752303423488), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111110 #b00110111001010001100100)))
;; y should be Float32(0xFF1B9464 [Rational(-206800827522685479956155002525306585088), -206800827522685479956155002525306585088.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result ((_ to_fp 8 24) #x7F1B9464)))
(check-sat)
(exit)
