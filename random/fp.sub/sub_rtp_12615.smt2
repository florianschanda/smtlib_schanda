(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x111FCFE0)))
;; x should be Float32(0x111FCFE0 [Rational(327295, 2596148429267413814265248164610048), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB9CADFDA)))
;; y should be Float32(0xB9CADFDA [Rational(-6647789, 17179869184), -0.000387])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result (fp #b0 #b01110011 #b10010101101111111011011))))
(check-sat)
(exit)
