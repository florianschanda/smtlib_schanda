(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A4C4096)))
;; x should be Float32(0x4A4C4096 [Rational(6692939, 2), 3346469.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01010110 #b11101010000010000001000)))
;; y should be Float32(0xAB750408 [Rational(-2007169, 2305843009213693952), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b0 #b10010100 #b10011000100000010010110))))
(check-sat)
(exit)
