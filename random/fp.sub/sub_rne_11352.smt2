(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01000100011010101011100)))
;; x should be Float32(0x0022355C [Rational(560471, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01001111 #b10011010100010110000011)))
;; y should be Float32(0x27CD4583 [Rational(13452675, 2361183241434822606848), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b01001111 #b10011010100010110000011)))
(check-sat)
(exit)
