(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000111 #b01000100101000010110110)))
;; x should be Float32(0x43A250B6 [Rational(5318747, 16384), 324.630554])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00101011101110100010001)))
;; y should be Float32(0x4A95DD11 [Rational(9821457, 2), 4910728.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xCA95DA87)))
(check-sat)
(exit)
