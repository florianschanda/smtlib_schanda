(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01010101101001000010111)))
;; x should be Float32(0xCAAAD217 [Rational(-11194903, 2), -5597451.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A41DF5E)))
;; y should be Float32(0x4A41DF5E [Rational(6352815, 2), 3176407.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xCB05E0E3))))
(check-sat)
(exit)
