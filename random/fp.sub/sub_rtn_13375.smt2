(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA9E0865)))
;; x should be Float32(0xCA9E0865 [Rational(-10356837, 2), -5178418.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11010100101000001111111)))
;; y should be Float32(0x806A507F [Rational(-6967423, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b1 #b10010101 #b00111100000100001100101)))
(check-sat)
(exit)
