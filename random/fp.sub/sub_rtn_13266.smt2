(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7019226B)))
;; x should be Float32(0x7019226B [Rational(189571261094985439835477508096), 189571261094985439835477508096.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80671B6A)))
;; y should be Float32(0x80671B6A [Rational(-3378613, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b0 #b11100000 #b00110010010001001101011)))
(check-sat)
(exit)
