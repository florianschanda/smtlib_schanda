(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100011 #b01001111000110111010011)))
;; x should be Float32(0x31A78DD3 [Rational(10980819, 2251799813685248), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA2E785E)))
;; y should be Float32(0xCA2E785E [Rational(-5717039, 2), -2858519.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b0 #b10010100 #b01011100111100001011111)))
(check-sat)
(exit)
