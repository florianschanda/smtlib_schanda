(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11101101101011110001010)))
;; x should be Float32(0xCA76D78A [Rational(-8088517, 2), -4044258.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00110101 #b01011111011010100010101)))
;; y should be Float32(0x1AAFB515 [Rational(11515157, 158456325028528675187087900672), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xCA76D78A)))
(check-sat)
(exit)
