(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB64ABA89)))
;; x should be Float32(0xB64ABA89 [Rational(-13286025, 4398046511104), -0.000003])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001100 #b11010110010101111001001)))
;; y should be Float32(0x466B2BC9 [Rational(15412169, 1024), 15050.946289])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b10001100 #b11010110010101111001001)))
(check-sat)
(exit)
