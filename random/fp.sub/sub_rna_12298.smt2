(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8042B97D)))
;; x should be Float32(0x8042B97D [Rational(-4372861, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01001010 #b01111000011111110011111)))
;; y should be Float32(0x253C3F9F [Rational(12337055, 75557863725914323419136), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #xA53C3F9F)))
(check-sat)
(exit)
