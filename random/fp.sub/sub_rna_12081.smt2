(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9625178)))
;; x should be Float32(0xC9625178 [Rational(-1853999, 2), -926999.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xAB7DAFBB)))
;; y should be Float32(0xAB7DAFBB [Rational(-16625595, 18446744073709551616), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #xC9625178)))
(check-sat)
(exit)
