(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF772FB74)))
;; x should be Float32(0xF772FB74 [Rational(-4928265293135923867359186417352704), -4928265293135923867359186417352704.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11001011001111101011001)))
;; y should be Float32(0x4AE59F59 [Rational(15048537, 2), 7524268.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b11101110 #b11100101111101101110100)))
(check-sat)
(exit)
