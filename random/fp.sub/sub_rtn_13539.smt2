(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11000011 #b01111111011010000001100)))
;; x should be Float32(0xE1BFB40C [Rational(-442037732838133989376), -442037732838133989376.000000])

(declare-const y Float32)
(assert (= y (_ +oo 8 24)))
;; y should be Float32(+oo)

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
