(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x053F0ECB)))
;; x should be Float32(0x053F0ECB [Rational(12521163, 1393796574908163946345982392040522594123776), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x894292B5)))
;; y should be Float32(0x894292B5 [Rational(-12751541, 5444517870735015415413993718908291383296), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b0 #b00010010 #b10000110101000111000011)))
(check-sat)
(exit)
