(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xDA1388EB)))
;; x should be Float32(0xDA1388EB [Rational(-10381841118789632), -10381841118789632.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01001011000101111110001)))
;; y should be Float32(0x4AA58BF1 [Rational(10849265, 2), 5424632.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result (fp #b1 #b10110100 #b00100111000100011101011))))
(check-sat)
(exit)
