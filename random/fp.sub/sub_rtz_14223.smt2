(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x804B2545)))
;; x should be Float32(0x804B2545 [Rational(-4924741, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101000100010110001111)))
;; y should be Float32(0x0054458F [Rational(5522831, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b00000001 #b00111110110101011010100)))
(check-sat)
(exit)
