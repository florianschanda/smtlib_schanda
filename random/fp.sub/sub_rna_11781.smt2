(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10011010111011010101010)))
;; x should be Float32(0x4A4D76AA [Rational(6732629, 2), 3366314.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800C8BCC)))
;; y should be Float32(0x800C8BCC [Rational(-205555, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result (fp #b0 #b10010100 #b10011010111011010101010))))
(check-sat)
(exit)
