(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x98D1835A)))
;; x should be Float32(0x98D1835A [Rational(-6865325, 1267650600228229401496703205376), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x803C351F)))
;; y should be Float32(0x803C351F [Rational(-3945759, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b00110001 #b10100011000001101011010))))
(check-sat)
(exit)
