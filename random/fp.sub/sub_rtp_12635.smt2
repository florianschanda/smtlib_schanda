(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x004D3D46)))
;; x should be Float32(0x004D3D46 [Rational(2530979, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00101010 #b10011101001010111000101)))
;; y should be Float32(0x154E95C5 [Rational(13538757, 324518553658426726783156020576256), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b00101010 #b10011101001010111000100)))
(check-sat)
(exit)
