(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10011010010011010100110)))
;; x should be Float32(0x804D26A6 [Rational(-2528083, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11100101011101010001101)))
;; y should be Float32(0x4AF2BA8D [Rational(15907469, 2), 7953734.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result ((_ to_fp 8 24) #xCAF2BA8E)))
(check-sat)
(exit)
