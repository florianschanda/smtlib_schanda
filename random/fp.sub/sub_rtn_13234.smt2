(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00100000 #b10011000000011001111010)))
;; x should be Float32(0x104C067A [Rational(6685501, 166153499473114484112975882535043072), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00001101001010011010101)))
;; y should be Float32(0x4A8694D5 [Rational(8819925, 2), 4409962.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b1 #b10010101 #b00001101001010011010101))))
(check-sat)
(exit)
