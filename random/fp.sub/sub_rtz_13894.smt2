(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11110111 #b11100110100010000101000)))
;; x should be Float32(0x7BF34428 [Rational(2526221019207023995790485525691891712), 2526221019207023995790485525691891712.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10111000101010100111000)))
;; y should be Float32(0x005C5538 [Rational(756391, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b0 #b11110111 #b11100110100010000100111)))
(check-sat)
(exit)
