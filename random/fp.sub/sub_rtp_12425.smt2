(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01010000100001010110101)))
;; x should be Float32(0x4AA842B5 [Rational(11027125, 2), 5513562.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00110111 #b11000000100010101111111)))
;; y should be Float32(0x9BE0457F [Rational(-14697855, 39614081257132168796771975168), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b0 #b10010101 #b01010000100001010110110)))
(check-sat)
(exit)
