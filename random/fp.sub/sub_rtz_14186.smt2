(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00101111 #b00100001000100110111111)))
;; x should be Float32(0x979089BF [Rational(-9472447, 10141204801825835211973625643008), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101111000101000111100)))
;; y should be Float32(0x00578A3C [Rational(1434255, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x979089BF))))
(check-sat)
(exit)
