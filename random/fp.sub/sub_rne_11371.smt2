(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0046BC9A)))
;; x should be Float32(0x0046BC9A [Rational(2317901, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xDEF4DFF9)))
;; y should be Float32(0xDEF4DFF9 [Rational(-8822547821728104448), -8822547821728104448.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b10111101 #b11101001101111111111001)))
(check-sat)
(exit)
