(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0DEBF552)))
;; x should be Float32(0x0DEBF552 [Rational(7731881, 5316911983139663491615228241121378304), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00100101111010110011111)))
;; y should be Float32(0x4A92F59F [Rational(9631135, 2), 4815567.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xCA92F59F))))
(check-sat)
(exit)
