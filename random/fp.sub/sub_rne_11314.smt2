(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00010000 #b01001001110101111011100)))
;; x should be Float32(0x0824EBDC [Rational(2702071, 5444517870735015415413993718908291383296), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0F02FC2A)))
;; y should be Float32(0x0F02FC2A [Rational(4292117, 664613997892457936451903530140172288), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b00011110 #b00000101111100110010110))))
(check-sat)
(exit)
