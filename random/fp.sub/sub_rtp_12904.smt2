(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC7B08E11)))
;; x should be Float32(0xC7B08E11 [Rational(-11570705, 128), -90396.132812])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x001C9B43)))
;; y should be Float32(0x001C9B43 [Rational(1874755, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xC7B08E11))))
(check-sat)
(exit)
