(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABF2977)))
;; x should be Float32(0x4ABF2977 [Rational(12527991, 2), 6263995.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b00001011010101000111100)))
;; y should be Float32(0x4985AA3C [Rational(2189967, 2), 1094983.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result (fp #b0 #b10010101 #b00111011011111011101000))))
(check-sat)
(exit)
