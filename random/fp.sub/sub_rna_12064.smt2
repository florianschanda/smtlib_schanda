(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAEBFA67)))
;; x should be Float32(0xCAEBFA67 [Rational(-15465063, 2), -7732531.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x725FB8D3)))
;; y should be Float32(0x725FB8D3 [Rational(4431270141459003365690380910592), 4431270141459003365690380910592.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #xF25FB8D3)))
(check-sat)
(exit)
