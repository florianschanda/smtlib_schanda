(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00001010001110110101010)))
;; x should be Float32(0x80051DAA [Rational(-167637, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAA3C40D)))
;; y should be Float32(0xCAA3C40D [Rational(-10732557, 2), -5366278.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b10010101 #b01000111100010000001101)))
(check-sat)
(exit)
