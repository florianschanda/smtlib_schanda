(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10101010011010111100000)))
;; x should be Float32(0x005535E0 [Rational(174511, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y (_ -zero 8 24)))
;; y should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result (fp #b0 #b00000000 #b10101010011010111100000))))
(check-sat)
(exit)
