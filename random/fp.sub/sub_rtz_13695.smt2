(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A65C5C6)))
;; x should be Float32(0x4A65C5C6 [Rational(7529187, 2), 3764593.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001011 #b11111010111110010001110)))
;; y should be Float32(0x45FD7C8E [Rational(8306247, 1024), 8111.569336])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b0 #b10010100 #b11001010100011100000111)))
(check-sat)
(exit)
