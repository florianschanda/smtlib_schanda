(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11100101 #b00001111010110110000010)))
;; x should be Float32(0x7287AD82 [Rational(5374750003240664086905266110464), 5374750003240664086905266110464.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x3DE7A4A0)))
;; y should be Float32(0x3DE7A4A0 [Rational(474405, 4194304), 0.113107])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b0 #b11100101 #b00001111010110110000001)))
(check-sat)
(exit)
