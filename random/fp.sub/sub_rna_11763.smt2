(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ADB9D93)))
;; x should be Float32(0x4ADB9D93 [Rational(14392723, 2), 7196361.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00354D60)))
;; y should be Float32(0x00354D60 [Rational(109163, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result ((_ to_fp 8 24) #x4ADB9D93)))
(check-sat)
(exit)
