(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC28B93)))
;; x should be Float32(0xCAC28B93 [Rational(-12749715, 2), -6374857.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01100010 #b11110001011011101001100)))
;; y should be Float32(0xB178B74C [Rational(-4074963, 1125899906842624), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b10010101 #b10000101000101110010010)))
(check-sat)
(exit)
