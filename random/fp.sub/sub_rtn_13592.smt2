(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00101111010011011011111)))
;; x should be Float32(0x8017A6DF [Rational(-1550047, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11110001010110000010010)))
;; y should be Float32(0xCA78AC12 [Rational(-8148489, 2), -4074244.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (= result (fp #b0 #b10010100 #b11110001010110000010001)))
(check-sat)
(exit)
