(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01010110 #b11000010000010101000110)))
;; x should be Float32(0xAB610546 [Rational(-7373475, 9223372036854775808), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x93E1FBD4)))
;; y should be Float32(0x93E1FBD4 [Rational(-3702517, 649037107316853453566312041152512), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result (fp #b1 #b01010110 #b11000010000010101000101)))
(check-sat)
(exit)
