(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAFD7D15)))
;; x should be Float32(0xCAFD7D15 [Rational(-16612629, 2), -8306314.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10000000 #b01111101100100001001111)))
;; y should be Float32(0x403EC84F [Rational(12503119, 4194304), 2.980976])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAFD7D1B))))
(check-sat)
(exit)
