(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x48EA7210)))
;; x should be Float32(0x48EA7210 [Rational(960289, 2), 480144.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010101 #b10010001100000001010110)))
;; y should be Float32(0x6AC8C056 [Rational(121346722353813572921851904), 121346722353813572921851904.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xEAC8C056))))
(check-sat)
(exit)
