(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF696E9)))
;; x should be Float32(0x4AF696E9 [Rational(16160489, 2), 8080244.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x002BD182)))
;; y should be Float32(0x002BD182 [Rational(1435841, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b10010101 #b11101101001011011101001)))
(check-sat)
(exit)
