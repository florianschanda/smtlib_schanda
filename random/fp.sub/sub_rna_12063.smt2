(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00100000010101011011110)))
;; x should be Float32(0xCA102ADE [Rational(-4724079, 2), -2362039.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10101000 #b00001111010010011101110)))
;; y should be Float32(0x5407A4EE [Rational(2330350583808), 2330350583808.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xD407A4F7))))
(check-sat)
(exit)
