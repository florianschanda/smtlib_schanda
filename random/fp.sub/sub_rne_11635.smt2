(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE1BC2A36)))
;; x should be Float32(0xE1BC2A36 [Rational(-433878688056966381568), -433878688056966381568.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00011110 #b00010100000110111110001)))
;; y should be Float32(0x8F0A0DF1 [Rational(-9047537, 1329227995784915872903807060280344576), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b1 #b11000011 #b01111000010101000110110))))
(check-sat)
(exit)
