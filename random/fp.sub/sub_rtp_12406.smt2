(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4980C9E4)))
;; x should be Float32(0x4980C9E4 [Rational(2110073, 2), 1055036.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x156F98C5)))
;; y should be Float32(0x156F98C5 [Rational(15702213, 324518553658426726783156020576256), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b0 #b10010011 #b00000001100100111100100)))
(check-sat)
(exit)
