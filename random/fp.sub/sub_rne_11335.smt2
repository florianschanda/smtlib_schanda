(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01101101 #b11110101100101100011011)))
;; x should be Float32(0x36FACB1B [Rational(16435995, 2199023255552), 0.000007])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8027C4BB)))
;; y should be Float32(0x8027C4BB [Rational(-2606267, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b01101101 #b11110101100101100011011))))
(check-sat)
(exit)
