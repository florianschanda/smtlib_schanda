(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB6A4F5)))
;; x should be Float32(0xCAB6A4F5 [Rational(-11969781, 2), -5984890.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11001110001011010000011)))
;; y should be Float32(0x4AE71683 [Rational(15144579, 2), 7572289.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b1 #b10010110 #b10011101101110110111100)))
(check-sat)
(exit)
