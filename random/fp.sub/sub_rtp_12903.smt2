(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000001 #b01111000000100000010100)))
;; x should be Float32(0x80BC0814 [Rational(-3080709, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01011111 #b11101101101111101100001)))
;; y should be Float32(0x2FF6DF61 [Rational(16179041, 36028797018963968), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result (fp #b1 #b01011111 #b11101101101111101100001))))
(check-sat)
(exit)
