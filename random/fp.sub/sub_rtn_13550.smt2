(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF846EAB2)))
;; x should be Float32(0xF846EAB2 [Rational(-16138046319532467193442712200151040), -16138046319532467193442712200151040.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11100010110001011111011)))
;; y should be Float32(0x007162FB [Rational(7430907, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
