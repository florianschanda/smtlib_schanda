(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A7A00DE)))
;; x should be Float32(0x4A7A00DE [Rational(8192111, 2), 4096055.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01101010110010101100000)))
;; y should be Float32(0x80356560 [Rational(-109355, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (= result (fp #b0 #b10010100 #b11110100000000011011111)))
(check-sat)
(exit)
