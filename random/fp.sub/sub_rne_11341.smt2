(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0014EB40)))
;; x should be Float32(0x0014EB40 [Rational(21421, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10011001010111011001101)))
;; y should be Float32(0x4ACCAECD [Rational(13414093, 2), 6707046.500000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b10010101 #b10011001010111011001101)))
(check-sat)
(exit)
