(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01111101 #b00110011000010101100010)))
;; x should be Float32(0xBE998562 [Rational(-5030577, 16777216), -0.299846])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x14E17553)))
;; y should be Float32(0x14E17553 [Rational(14775635, 649037107316853453566312041152512), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xBE998563))))
(check-sat)
(exit)
