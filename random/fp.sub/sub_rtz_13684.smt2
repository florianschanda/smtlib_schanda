(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49405478)))
;; x should be Float32(0x49405478 [Rational(1575567, 2), 787783.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01111010001010110101001)))
;; y should be Float32(0x4ABD15A9 [Rational(12391849, 2), 6195924.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xCAA50B1A))))
(check-sat)
(exit)
