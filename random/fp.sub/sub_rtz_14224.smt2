(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x801CE140)))
;; x should be Float32(0x801CE140 [Rational(-29573, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01000010111011001110110)))
;; y should be Float32(0x00217676 [Rational(1096507, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x803E57B6)))
(check-sat)
(exit)
