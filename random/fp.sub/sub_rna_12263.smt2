(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101001 #b11100100110010101100000)))
;; x should be Float32(0xF4F26560 [Rational(-153636714970580868431836885286912), -153636714970580868431836885286912.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000000)))
;; y should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.sub RNA x y)))
(assert (= result (fp #b1 #b11101001 #b11100100110010101100000)))
(check-sat)
(exit)
