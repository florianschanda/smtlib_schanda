(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00010011111110100010111)))
;; x should be Float32(0xCA89FD17 [Rational(-9043223, 2), -4521611.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xDA8FCC21)))
;; y should be Float32(0xDA8FCC21 [Rational(-20237681887805440), -20237681887805440.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b0 #b10110101 #b00011111100110000100001)))
(check-sat)
(exit)
