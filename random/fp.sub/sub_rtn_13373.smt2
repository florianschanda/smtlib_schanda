(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11110100010001100001110)))
;; x should be Float32(0xCA7A230E [Rational(-8196487, 2), -4098243.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xEB959163)))
;; y should be Float32(0xEB959163 [Rational(-361633032980518285665632256), -361633032980518285665632256.000000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x6B959162))))
(check-sat)
(exit)
