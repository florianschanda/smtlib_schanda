(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8021A1C7)))
;; x should be Float32(0x8021A1C7 [Rational(-2204103, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b10011101000011000111110)))
;; y should be Float32(0xCA4E863E [Rational(-6767391, 2), -3383695.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x4A4E863E))))
(check-sat)
(exit)
