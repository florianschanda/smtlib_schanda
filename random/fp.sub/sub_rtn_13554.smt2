(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE9C9E30D)))
;; x should be Float32(0xE9C9E30D [Rational(-30508288342728103975452672), -30508288342728103975452672.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b10011011000001110011000)))
;; y should be Float32(0xC94D8398 [Rational(-1683571, 2), -841785.500000])

(declare-const result Float32)
(assert (= result (fp.sub RTN x y)))
(assert (not (= result (fp #b1 #b11010011 #b10010011110001100001101))))
(check-sat)
(exit)
