(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01010010010000101101101)))
;; x should be Float32(0x4AA9216D [Rational(11084141, 2), 5542070.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x802FB4E0)))
;; y should be Float32(0x802FB4E0 [Rational(-97703, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (not (= result (fp #b0 #b10010101 #b01010010010000101101101))))
(check-sat)
(exit)
