(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00001101110010000010100)))
;; x should be Float32(0xC986E414 [Rational(-2210053, 2), -1105026.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11101011 #b00111010110101110001110)))
;; y should be Float32(0x759D6B8E [Rational(399107458504216646460248321687552), 399107458504216646460248321687552.000000])

(declare-const result Float32)
(assert (= result (fp.sub RNE x y)))
(assert (= result (fp #b1 #b11101011 #b00111010110101110001110)))
(check-sat)
(exit)
