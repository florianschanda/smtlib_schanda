(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10101010010111111011101)))
;; x should be Float32(0xCAD52FDD [Rational(-13971421, 2), -6985710.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111111 #b11111111010001100110101)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result (fp #b1 #b11111111 #b11111111111111111111111)))
(check-sat)
(exit)
