(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A903C59)))
;; x should be Float32(0x4A903C59 [Rational(9452633, 2), 4726316.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11001011000110010000101)))
;; y should be Float32(0x00658C85 [Rational(6655109, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b0 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
