(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11110000 #b00111111001100101010011)))
;; x should be Float32(0x781F9953 [Rational(12948202892404452850800517342822400), 12948202892404452850800517342822400.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10001011101110000000101)))
;; y should be Float32(0xCAC5DC05 [Rational(-12966917, 2), -6483458.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result ((_ to_fp 8 24) #xECCE7F27)))
(check-sat)
(exit)
