(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ADD06ED)))
;; x should be Float32(0x4ADD06ED [Rational(14485229, 2), 7242614.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10001101 #b11101011011001100100011)))
;; y should be Float32(0xC6F5B323 [Rational(-16102179, 512), -31449.568359])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (fp #b1 #b10000110 #b11001100100101100000000))))
(check-sat)
(exit)
