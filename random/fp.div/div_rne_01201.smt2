(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8B575EFE)))
;; x should be Float32(0x8B575EFE [Rational(-7057279, 170141183460469231731687303715884105728), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001101 #b01000000111010010001101)))
;; y should be Float32(0x46A0748D [Rational(10515597, 512), 20538.275391])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result ((_ to_fp 8 24) #x842BCED7)))
(check-sat)
(exit)
