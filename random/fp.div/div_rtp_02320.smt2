(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b01011101000111001000110)))
;; x should be Float32(0xCA2E8E46 [Rational(-5719843, 2), -2859921.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x006AEC22)))
;; y should be Float32(0x006AEC22 [Rational(3503633, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b1 #b11111110 #b11111111111111111111111))))
(check-sat)
(exit)
