(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111111 #b10111001110010010101001)))
;; x should be Float32(0x1FDCE4A9 [Rational(14476457, 154742504910672534362390528), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b10111001111000000100111)))
;; y should be Float32(0x4ADCF027 [Rational(14479399, 2), 7239699.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b00101000 #b11111111111001010101111))))
(check-sat)
(exit)
