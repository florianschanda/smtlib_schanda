(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB1FA17A9)))
;; x should be Float32(0xB1FA17A9 [Rational(-16390057, 2251799813685248), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01101111 #b01001001001111111001010)))
;; y should be Float32(0x37A49FCA [Rational(5394405, 274877906944), 0.000020])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b1 #b01110011 #b10000100111010000111001)))
(check-sat)
(exit)
