(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80177061)))
;; x should be Float32(0x80177061 [Rational(-1536097, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x003E677A)))
;; y should be Float32(0x003E677A [Rational(2044861, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #xBEC04E8F)))
(check-sat)
(exit)
