(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00110110110101111100000)))
;; x should be Float32(0x801B6BE0 [Rational(-56159, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11000110011110100100010)))
;; y should be Float32(0xCA633D22 [Rational(-7446161, 2), -3723080.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x00000000))))
(check-sat)
(exit)
