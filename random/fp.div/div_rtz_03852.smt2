(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00001110011110101000100)))
;; x should be Float32(0x80073D44 [Rational(-118609, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00101001 #b00101111000100001001011)))
;; y should be Float32(0x1497884B [Rational(9930827, 649037107316853453566312041152512), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b1 #b01010010 #b10000111010111010111101)))
(check-sat)
(exit)
