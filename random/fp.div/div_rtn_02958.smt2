(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9C5927C)))
;; x should be Float32(0xC9C5927C [Rational(-3237023, 2), -1618511.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1F26EC13)))
;; y should be Float32(0x1F26EC13 [Rational(10939411, 309485009821345068724781056), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xEA1780D2))))
(check-sat)
(exit)
