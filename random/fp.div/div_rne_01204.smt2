(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000111 #b00110000001000100010110)))
;; x should be Float32(0x83981116 [Rational(-4982923, 5575186299632655785383929568162090376495104), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11010110001111111000001)))
;; y should be Float32(0x006B1FC1 [Rational(7020481, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #xC2B5B374))))
(check-sat)
(exit)
