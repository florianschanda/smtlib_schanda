(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001001 #b10000011111101110010111)))
;; x should be Float32(0x04C1FB97 [Rational(12712855, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00001000100111000011110)))
;; y should be Float32(0x80044E1E [Rational(-141071, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b1 #b10001100 #b01101000011101111000001)))
(check-sat)
(exit)
