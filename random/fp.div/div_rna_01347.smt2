(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A2C4E86)))
;; x should be Float32(0x4A2C4E86 [Rational(5646147, 2), 2823073.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0C8C87F0)))
;; y should be Float32(0x0C8C87F0 [Rational(575615, 2658455991569831745807614120560689152), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #x7D1CF13C)))
(check-sat)
(exit)
