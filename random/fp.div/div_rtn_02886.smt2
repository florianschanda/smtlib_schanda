(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00324BE0)))
;; x should be Float32(0x00324BE0 [Rational(103007, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00010110 #b00011100010011011101010)))
;; y should be Float32(0x0B0E26EA [Rational(4658037, 170141183460469231731687303715884105728), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b01101000 #b01101010010100000001011)))
(check-sat)
(exit)
