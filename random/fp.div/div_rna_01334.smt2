(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A83F963)))
;; x should be Float32(0x4A83F963 [Rational(8649059, 2), 4324529.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AEC37F7)))
;; y should be Float32(0x4AEC37F7 [Rational(15480823, 2), 7740411.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b0 #b01111110 #b00011110000011010100011)))
(check-sat)
(exit)
