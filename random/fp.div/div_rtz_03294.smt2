(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00010011000100100111010)))
;; x should be Float32(0x4A09893A [Rational(4506781, 2), 2253390.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB1B293)))
;; y should be Float32(0x4AB1B293 [Rational(11645587, 2), 5822793.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b01111101 #b10001100010010000101100))))
(check-sat)
(exit)
