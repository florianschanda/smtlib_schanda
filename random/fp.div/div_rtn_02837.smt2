(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100000 #b00000110010101100000101)))
;; x should be Float32(0x30032B05 [Rational(8596229, 18014398509481984), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AAB7DD3)))
;; y should be Float32(0x4AAB7DD3 [Rational(11238867, 2), 5619433.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x24C3CE44)))
(check-sat)
(exit)
