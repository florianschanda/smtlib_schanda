(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11100111010110011111100)))
;; x should be Float32(0xC9F3ACFC [Rational(-3992383, 2), -1996191.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x004B15B9)))
;; y should be Float32(0x004B15B9 [Rational(4920761, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
