(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x410C2512)))
;; x should be Float32(0x410C2512 [Rational(4592265, 524288), 8.759050])

(declare-const y Float32)
(assert (= y (fp #b0 #b00011100 #b10100101110010101101110)))
;; y should be Float32(0x0E52E56E [Rational(6910647, 2658455991569831745807614120560689152), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x722A1E00))))
(check-sat)
(exit)
