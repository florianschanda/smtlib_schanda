(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010000 #b11101010001011001100000)))
;; x should be Float32(0xC8751660 [Rational(-501939, 2), -250969.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1DBD986B)))
;; y should be Float32(0x1DBD986B [Rational(12425323, 2475880078570760549798248448), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xEA2576C0))))
(check-sat)
(exit)
