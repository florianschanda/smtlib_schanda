(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4F63CAC8)))
;; x should be Float32(0x4F63CAC8 [Rational(3821717504), 3821717504.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA395AD2)))
;; y should be Float32(0xCA395AD2 [Rational(-6073705, 2), -3036852.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b1 #b10001001 #b00111010100111001001101)))
(check-sat)
(exit)
