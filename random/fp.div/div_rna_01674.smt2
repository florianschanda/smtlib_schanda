(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00100110111110101100100)))
;; x should be Float32(0xC9937D64 [Rational(-2416473, 2), -1208236.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80683AF1)))
;; y should be Float32(0x80683AF1 [Rational(-6830833, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result (fp #b0 #b11111111 #b00000000000000000000000))))
(check-sat)
(exit)
