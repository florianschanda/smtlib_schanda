(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00101010111010011101011)))
;; x should be Float32(0x001574EB [Rational(1406187, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11000000 #b11011101011101110011100)))
;; y should be Float32(0xE06EBB9C [Rational(-68810058901942697984), -68810058901942697984.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (_ -zero 8 24)))
(check-sat)
(exit)
