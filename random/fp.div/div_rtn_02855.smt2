(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000011 #b01010110001001010010000)))
;; x should be Float32(0x01AB1290 [Rational(700713, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA81847F)))
;; y should be Float32(0xCA81847F [Rational(-8488063, 2), -4244031.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000001011)))
(check-sat)
(exit)
