(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10001101010001111110000)))
;; x should be Float32(0x8046A3F0 [Rational(-289343, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0056A5EC)))
;; y should be Float32(0x0056A5EC [Rational(1419643, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xBF50B499))))
(check-sat)
(exit)
