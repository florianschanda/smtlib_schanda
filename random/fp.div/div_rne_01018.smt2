(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00000000010101000010101)))
;; x should be Float32(0xCA802A15 [Rational(-8399381, 2), -4199690.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB99AF4E0)))
;; y should be Float32(0xB99AF4E0 [Rational(-317351, 1073741824), -0.000296])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (fp #b0 #b10100000 #b10100111011110011000001))))
(check-sat)
(exit)
