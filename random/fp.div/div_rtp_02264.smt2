(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00587B0E)))
;; x should be Float32(0x00587B0E [Rational(2899335, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800F43C2)))
;; y should be Float32(0x800F43C2 [Rational(-500193, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b1 #b10000001 #b01110010111110001100000)))
(check-sat)
(exit)
