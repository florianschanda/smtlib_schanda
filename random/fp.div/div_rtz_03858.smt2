(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11110011100100010101101)))
;; x should be Float32(0x8079C8AD [Rational(-7981229, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA2BCD42)))
;; y should be Float32(0xCA2BCD42 [Rational(-5629601, 2), -2814800.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b0 #b00000000 #b00000000000000000000010)))
(check-sat)
(exit)
