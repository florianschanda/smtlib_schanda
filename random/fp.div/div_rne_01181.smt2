(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000001)))
;; x should be Float32(0x80000001 [Rational(-1, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111110 #b11011010000110000000101)))
;; y should be Float32(0xFF6D0C05 [Rational(-315089443975375498068096758978648735744), -315089443975375498068096758978648735744.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (_ +zero 8 24))))
(check-sat)
(exit)
