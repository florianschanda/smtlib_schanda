(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111001 #b11111111000101011000011)))
;; x should be Float32(0x7CFF8AC3 [Rational(10614800967547002058716896205591281664), 10614800967547002058716896205591281664.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b10011010001100111010101)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
