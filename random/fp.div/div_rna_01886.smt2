(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80607D20)))
;; x should be Float32(0x80607D20 [Rational(-197609, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00000000101100001110001)))
;; y should be Float32(0x4A805871 [Rational(8411249, 2), 4205624.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000000010)))
(check-sat)
(exit)
