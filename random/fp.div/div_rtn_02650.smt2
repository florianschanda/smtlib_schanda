(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b10110110011110110101000)))
;; x should be Float32(0x495B3DA8 [Rational(1796021, 2), 898010.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00110010 #b11100110100011001010011)))
;; y should be Float32(0x19734653 [Rational(15943251, 1267650600228229401496703205376), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x6F66B57F))))
(check-sat)
(exit)
