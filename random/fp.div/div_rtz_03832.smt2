(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xF4932799)))
;; x should be Float32(0xF4932799 [Rational(-93270358165042328977559657644032), -93270358165042328977559657644032.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010000 #b11111011010101011100110)))
;; y should be Float32(0xE87DAAE6 [Rational(-4791644959620718345060352), -4791644959620718345060352.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x4B9481FB)))
(check-sat)
(exit)
