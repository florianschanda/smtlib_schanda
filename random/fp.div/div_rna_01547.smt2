(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10111001 #b11010110110101111010101)))
;; x should be Float32(0x5CEB6BD5 [Rational(530121456793681920), 530121456793681920.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x002E4437)))
;; y should be Float32(0x002E4437 [Rational(3032119, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
