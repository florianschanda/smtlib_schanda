(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b11011101000111011011000)))
;; x should be Float32(0x496E8ED8 [Rational(1954267, 2), 977133.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCADEC0D3)))
;; y should be Float32(0xCADEC0D3 [Rational(-14598355, 2), -7299177.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xBE0914F3))))
(check-sat)
(exit)
