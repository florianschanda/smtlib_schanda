(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAF31A41)))
;; x should be Float32(0xCAF31A41 [Rational(-15931969, 2), -7965984.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A91F56D)))
;; y should be Float32(0x4A91F56D [Rational(9565549, 2), 4782774.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #xBFD530FA)))
(check-sat)
(exit)
