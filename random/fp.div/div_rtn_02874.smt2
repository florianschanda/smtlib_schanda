(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00110111101111010011001)))
;; x should be Float32(0x001BDE99 [Rational(1826457, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00001110011101000100110)))
;; y should be Float32(0x4A073A26 [Rational(4431123, 2), 2215561.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result (fp #b0 #b00000000 #b00000000000000000000000))))
(check-sat)
(exit)
