(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b1 #b11000111 #b01111011110000011111011)))
;; y should be Float32(0xE3BDE0FB [Rational(-7005292362429510975488), -7005292362429510975488.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (_ +zero 8 24)))
(check-sat)
(exit)
