(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00111100 #b10011001011101001011001)))
;; x should be Float32(0x9E4CBA59 [Rational(-13417049, 1237940039285380274899124224), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFD1AFBC1)))
;; y should be Float32(0xFD1AFBC1 [Rational(-12875518272963924433396203980081594368), -12875518272963924433396203980081594368.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (_ +zero 8 24)))
(check-sat)
(exit)
