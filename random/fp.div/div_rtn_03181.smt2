(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01100100 #b10000111100110100101101)))
;; x should be Float32(0xB243CD2D [Rational(-12832045, 1125899906842624), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00110011101001100111100)))
;; y should be Float32(0x8019D33C [Rational(-423119, 178405961588244985132285746181186892047843328), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x72729E40)))
(check-sat)
(exit)
