(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A95756F)))
;; x should be Float32(0x4A95756F [Rational(9794927, 2), 4897463.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10000000101000100000011)))
;; y should be Float32(0xCAC05103 [Rational(-12603651, 2), -6301825.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b1 #b01111110 #b10001101111001101001100)))
(check-sat)
(exit)
