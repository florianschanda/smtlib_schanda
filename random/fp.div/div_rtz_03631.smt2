(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01110110101100000000101)))
;; x should be Float32(0xCABB5805 [Rational(-12277765, 2), -6138882.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCEF5AEBC)))
;; y should be Float32(0xCEF5AEBC [Rational(-2060934656), -2060934656.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b0 #b01110110 #b10000110011011000011001)))
(check-sat)
(exit)
