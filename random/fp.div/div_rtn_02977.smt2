(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b01011101111110000110100)))
;; x should be Float32(0xC9AEFC34 [Rational(-2866957, 2), -1433478.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x802E3643)))
;; y should be Float32(0x802E3643 [Rational(-3028547, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
