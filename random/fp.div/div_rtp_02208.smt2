(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10100000 #b00111011011011001011100)))
;; x should be Float32(0x501DB65C [Rational(10583896064), 10583896064.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01101011010001011100110)))
;; y should be Float32(0x0035A2E6 [Rational(1757555, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (_ +oo 8 24))))
(check-sat)
(exit)
