(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b01011010110111010000110)))
;; x should be Float32(0xCA2D6E86 [Rational(-5683011, 2), -2841505.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x14973D51)))
;; y should be Float32(0x14973D51 [Rational(9911633, 649037107316853453566312041152512), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b1 #b11101010 #b00100101100100000111011)))
(check-sat)
(exit)
