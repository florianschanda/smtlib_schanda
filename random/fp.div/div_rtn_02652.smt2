(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00010001011110010011001)))
;; x should be Float32(0x4A88BC99 [Rational(8961177, 2), 4480588.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11010111001001100010110)))
;; y should be Float32(0x006B9316 [Rational(3525003, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x7F7FFFFF))))
(check-sat)
(exit)
