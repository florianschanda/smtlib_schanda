(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101011 #b10001110010000110100111)))
;; x should be Float32(0xF5C721A7 [Rational(-504858215560786662766570874339328), -504858215560786662766570874339328.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11100111011101100010110)))
;; y should be Float32(0x8073BB16 [Rational(-3792267, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
