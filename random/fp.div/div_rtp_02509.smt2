(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE1DD26DF)))
;; x should be Float32(0xE1DD26DF [Rational(-509941424722882330624), -509941424722882330624.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01000011101101001111000)))
;; y should be Float32(0x0021DA78 [Rational(277327, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result ((_ to_fp 8 24) #xFF7FFFFF)))
(check-sat)
(exit)
