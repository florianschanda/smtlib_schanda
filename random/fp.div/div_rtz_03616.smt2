(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00101010011011111100010)))
;; x should be Float32(0xCA1537E2 [Rational(-4889585, 2), -2444792.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10100100111100101000100)))
;; y should be Float32(0x00527944 [Rational(1351249, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF7FFFFF))))
(check-sat)
(exit)
