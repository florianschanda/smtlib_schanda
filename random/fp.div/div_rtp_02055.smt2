(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (_ +oo 8 24)))
;; x should be Float32(+oo)

(declare-const y Float32)
(assert (= y (fp #b1 #b11111001 #b00011111101000001000010)))
;; y should be Float32(0xFC8FD042 [Rational(-5973779368214126718194585417973497856), -5973779368214126718194585417973497856.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (_ -oo 8 24)))
(check-sat)
(exit)
