(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b00111010101000001101000)))
;; x should be Float32(0xC89D5068 [Rational(-1288717, 4), -322179.250000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAAC8B8F)))
;; y should be Float32(0xCAAC8B8F [Rational(-11307919, 2), -5653959.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b0 #b01111010 #b11010010110011011110010)))
(check-sat)
(exit)
