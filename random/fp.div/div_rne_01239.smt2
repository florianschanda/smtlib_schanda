(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11000101111000100000011)))
;; x should be Float32(0x8062F103 [Rational(-6484227, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00111001 #b01100111000001110111101)))
;; y should be Float32(0x1CB383BD [Rational(11764669, 9903520314283042199192993792), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (fp #b1 #b01000110 #b00011010001100011100100))))
(check-sat)
(exit)
