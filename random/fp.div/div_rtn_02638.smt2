(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A73D136)))
;; x should be Float32(0x4A73D136 [Rational(7989403, 2), 3994701.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A746D32)))
;; y should be Float32(0x4A746D32 [Rational(8009369, 2), 4004684.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b01111110 #b11111110101110010100001)))
(check-sat)
(exit)
