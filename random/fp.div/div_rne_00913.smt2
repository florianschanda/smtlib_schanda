(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10000110 #b11001100111001110101110)))
;; x should be Float32(0x436673AE [Rational(7551447, 32768), 230.451874])

(declare-const y Float32)
(assert (= y (fp #b1 #b11101011 #b00110100010101011110100)))
;; y should be Float32(0xF59A2AF4 [Rational(-390861772016294094457946798817280), -390861772016294094457946798817280.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result (fp #b1 #b00011010 #b01111110101011000000001)))
(check-sat)
(exit)
