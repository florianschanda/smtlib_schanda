(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b01011111000001101011001)))
;; x should be Float32(0xC8AF8359 [Rational(-11502425, 32), -359450.781250])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10000010110010101111010)))
;; y should be Float32(0x0041657A [Rational(2142909, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF800000))))
(check-sat)
(exit)
