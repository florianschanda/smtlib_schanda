(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10000101 #b00100100011101000100011)))
;; x should be Float32(0xC2923A23 [Rational(-9583139, 131072), -73.113548])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000111 #b01100101110011011100000)))
;; y should be Float32(0x83B2E6E0 [Rational(-366391, 348449143727040986586495598010130648530944), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result ((_ to_fp 8 24) #x7E513E76)))
(check-sat)
(exit)
