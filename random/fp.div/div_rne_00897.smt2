(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111000 #b11010011111001101011111)))
;; x should be Float32(0x1C69F35F [Rational(15332191, 19807040628566084398385987584), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00001011011000111100011)))
;; y should be Float32(0x0005B1E3 [Rational(373219, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result ((_ to_fp 8 24) #x5DA452E5)))
(check-sat)
(exit)
