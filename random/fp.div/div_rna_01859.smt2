(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC6155CA7)))
;; x should be Float32(0xC6155CA7 [Rational(-9788583, 1024), -9559.163086])

(declare-const y Float32)
(assert (= y (fp #b0 #b10011011 #b01011111011000000101010)))
;; y should be Float32(0x4DAFB02A [Rational(368444736), 368444736.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b1 #b01101111 #b10110011010001110101100)))
(check-sat)
(exit)
