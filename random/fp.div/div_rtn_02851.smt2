(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001111 #b11000010000101110011011)))
;; x should be Float32(0x67E10B9B [Rational(2125493083515113713958912), 2125493083515113713958912.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00443EBE)))
;; y should be Float32(0x00443EBE [Rational(2236255, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x7F7FFFFF)))
(check-sat)
(exit)
