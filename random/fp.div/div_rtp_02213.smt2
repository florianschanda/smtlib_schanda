(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11011110 #b10001001100000011000101)))
;; x should be Float32(0x6F44C0C5 [Rational(60892105988546767591707770880), 60892105988546767591707770880.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA91598D)))
;; y should be Float32(0xCA91598D [Rational(-9525645, 2), -4762822.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xE42D4474))))
(check-sat)
(exit)
