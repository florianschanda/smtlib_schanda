(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11000111 #b11011101110101001101001)))
;; x should be Float32(0x63EEEA69 [Rational(8814432242840606081024), 8814432242840606081024.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00010000100101110110101)))
;; y should be Float32(0x80084BB5 [Rational(-543669, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b1 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
