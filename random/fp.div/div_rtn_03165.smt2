(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFE23AB1E)))
;; x should be Float32(0xFE23AB1E [Rational(-54388163637009713089477996028882845696), -54388163637009713089477996028882845696.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00000000000000000000000)))
;; y should be Float32(-zero)

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
