(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10000110 #b11001010101110111100010)))
;; x should be Float32(0xC3655DE2 [Rational(-7515889, 32768), -229.366730])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10110001011010110101110)))
;; y should be Float32(0x4A58B5AE [Rational(7101143, 2), 3550571.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b1 #b01110001 #b00001110111100111010101)))
(check-sat)
(exit)
