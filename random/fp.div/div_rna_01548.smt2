(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5064AC49)))
;; x should be Float32(0x5064AC49 [Rational(15345984512), 15345984512.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00363448)))
;; y should be Float32(0x00363448 [Rational(444041, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b0 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
