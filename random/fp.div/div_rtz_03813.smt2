(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11111010 #b00101101001101110001010)))
;; x should be Float32(0xFD169B8A [Rational(-12511987772083473946781986918359891968), -12511987772083473946781986918359891968.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10111101 #b01101000011011100101011)))
;; y should be Float32(0x5EB4372B [Rational(6492947664773054464), 6492947664773054464.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xDDD5F0F9)))
(check-sat)
(exit)
