(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10001000100110011000111)))
;; x should be Float32(0xCAC44CC7 [Rational(-12864711, 2), -6432355.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01011101000101110011001)))
;; y should be Float32(0x802E8B99 [Rational(-3050393, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
