(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A5AD046)))
;; x should be Float32(0x4A5AD046 [Rational(7170083, 2), 3585041.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10100000101001000001101)))
;; y should be Float32(0x0050520D [Rational(5263885, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x7F7FFFFF)))
(check-sat)
(exit)
