(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802518E8)))
;; x should be Float32(0x802518E8 [Rational(-303901, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A3B8656)))
;; y should be Float32(0x4A3B8656 [Rational(6144811, 2), 3072405.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x80000001)))
(check-sat)
(exit)
