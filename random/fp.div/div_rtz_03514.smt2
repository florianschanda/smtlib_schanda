(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x7DBDA674)))
;; x should be Float32(0x7DBDA674 [Rational(31511045655172488119865596833098104832), 31511045655172488119865596833098104832.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCABE0D67)))
;; y should be Float32(0xCABE0D67 [Rational(-12455271, 2), -6227635.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xF27F7553)))
(check-sat)
(exit)
