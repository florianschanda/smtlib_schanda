(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00000100110011011110011)))
;; x should be Float32(0xCA8266F3 [Rational(-8546035, 2), -4273017.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xBC370958)))
;; y should be Float32(0xBC370958 [Rational(-1499435, 134217728), -0.011172])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b10011011 #b01101100110001001010101)))
(check-sat)
(exit)
