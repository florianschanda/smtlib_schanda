(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11111111110011001110011)))
;; x should be Float32(0xCAFFE673 [Rational(-16770675, 2), -8385337.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AD4DC8F)))
;; y should be Float32(0x4AD4DC8F [Rational(13950095, 2), 6975047.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xBF99E162)))
(check-sat)
(exit)
