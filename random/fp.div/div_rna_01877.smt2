(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10011100 #b01001000011000010011000)))
;; x should be Float32(0xCE243098 [Rational(-688662016), -688662016.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11100011 #b11001101110010001110010)))
;; y should be Float32(0xF1E6E472 [Rational(-2286647222257937548738784395264), -2286647222257937548738784395264.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #x1BB60B4A)))
(check-sat)
(exit)
