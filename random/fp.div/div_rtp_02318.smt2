(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9A9EFF4)))
;; x should be Float32(0xC9A9EFF4 [Rational(-2784253, 2), -1392126.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00101001 #b00001011011100101010111)))
;; y should be Float32(0x1485B957 [Rational(8763735, 649037107316853453566312041152512), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b1 #b11101001 #b01000101010100111001010))))
(check-sat)
(exit)
