(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11000010011011010011111)))
;; x should be Float32(0x8061369F [Rational(-6370975, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x30DC73AB)))
;; y should be Float32(0x30DC73AB [Rational(14447531, 9007199254740992), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result (fp #b1 #b00011101 #b11000011100011101000000))))
(check-sat)
(exit)
