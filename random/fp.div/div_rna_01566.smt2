(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3B914051)))
;; x should be Float32(0x3B914051 [Rational(9519185, 2147483648), 0.004433])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01001011110000110011110)))
;; y should be Float32(0x8025E19E [Rational(-1241295, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #xFB75666D)))
(check-sat)
(exit)
