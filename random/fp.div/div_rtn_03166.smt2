(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8CC4C625)))
;; x should be Float32(0x8CC4C625 [Rational(-12895781, 42535295865117307932921825928971026432), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b00000011101001101011011)))
;; y should be Float32(0xCA81D35B [Rational(-8508251, 2), -4254125.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x01C201C8))))
(check-sat)
(exit)
