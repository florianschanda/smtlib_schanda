(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00000000000000000000000)))
;; x should be Float32(-zero)

(declare-const y Float32)
(assert (= y (fp #b0 #b11000011 #b01010100100010111001110)))
;; y should be Float32(0x61AA45CE [Rational(392622056295555399680), 392622056295555399680.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (_ -zero 8 24)))
(check-sat)
(exit)
