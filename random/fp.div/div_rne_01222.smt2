(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01110111 #b00100010001111110110101)))
;; x should be Float32(0xBB911FB5 [Rational(-9510837, 2147483648), -0.004429])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x802065B8)))
;; y should be Float32(0x802065B8 [Rational(-265399, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result ((_ to_fp 8 24) #x7B8F580F)))
(check-sat)
(exit)
