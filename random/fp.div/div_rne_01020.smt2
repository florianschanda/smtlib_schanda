(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC968D748)))
;; x should be Float32(0xC968D748 [Rational(-1907433, 2), -953716.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8055E072)))
;; y should be Float32(0x8055E072 [Rational(-2814009, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result ((_ to_fp 8 24) #x7F800000)))
(check-sat)
(exit)
