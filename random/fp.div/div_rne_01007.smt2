(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10101101001100011100010)))
;; x should be Float32(0xCA5698E2 [Rational(-7031921, 2), -3515960.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11001111010110010100001)))
;; y should be Float32(0xCAE7ACA1 [Rational(-15183009, 2), -7591504.500000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x3EED2139))))
(check-sat)
(exit)
