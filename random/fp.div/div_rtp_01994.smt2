(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010001 #b00100100100010011110000)))
;; x should be Float32(0x489244F0 [Rational(599119, 2), 299559.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b10000111000011001110110)))
;; y should be Float32(0x4A438676 [Rational(6406971, 2), 3203485.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x3DBF826E))))
(check-sat)
(exit)
