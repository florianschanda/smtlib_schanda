(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA688027)))
;; x should be Float32(0xCA688027 [Rational(-15237159, 4), -3809289.750000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b10111000001111010110010)))
;; y should be Float32(0xCA5C1EB2 [Rational(-7212889, 2), -3606444.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b01111111 #b00001110011001100001011))))
(check-sat)
(exit)
