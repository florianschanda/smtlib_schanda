(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01100000110000011100011)))
;; x should be Float32(0x003060E3 [Rational(3170531, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00110101 #b01010010001100010111101)))
;; y should be Float32(0x9AA918BD [Rational(-11081917, 158456325028528675187087900672), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b1 #b01001001 #b00100100111101110100010)))
(check-sat)
(exit)
