(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0D8118CF)))
;; x should be Float32(0x0D8118CF [Rational(8460495, 10633823966279326983230456482242756608), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE72365B4)))
;; y should be Float32(0xE72365B4 [Rational(-771621828226123661574144), -771621828226123661574144.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b1 #b00000000 #b00000000000000000000000)))
(check-sat)
(exit)
