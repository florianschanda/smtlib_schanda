(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b11010011010011101110110)))
;; x should be Float32(0x4A69A776 [Rational(7656379, 2), 3828189.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA78846A)))
;; y should be Float32(0xCA78846A [Rational(-8143413, 2), -4071706.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #xBF70B07B)))
(check-sat)
(exit)
