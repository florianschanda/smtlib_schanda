(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00526DA1)))
;; x should be Float32(0x00526DA1 [Rational(5402017, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00100110 #b00010100001101101111001)))
;; y should be Float32(0x130A1B79 [Rational(9051001, 5192296858534827628530496329220096), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b01011001 #b00110001100101010100010))))
(check-sat)
(exit)
