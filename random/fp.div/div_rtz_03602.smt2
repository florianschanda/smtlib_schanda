(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA669162)))
;; x should be Float32(0xCA669162 [Rational(-7555249, 2), -3777624.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00101010111111101001001)))
;; y should be Float32(0x4A957F49 [Rational(9797449, 2), 4898724.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b1 #b01111110 #b10001010110100110111001)))
(check-sat)
(exit)
