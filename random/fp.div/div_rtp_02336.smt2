(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9213A48)))
;; x should be Float32(0xC9213A48 [Rational(-1320777, 2), -660388.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xF070D88E)))
;; y should be Float32(0xF070D88E [Rational(-298152803640800575541217329152), -298152803640800575541217329152.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x182B5F45))))
(check-sat)
(exit)
