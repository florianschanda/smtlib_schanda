(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00101010110110110010111)))
;; x should be Float32(0x4A956D97 [Rational(9792919, 2), 4896459.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8079091F)))
;; y should be Float32(0x8079091F [Rational(-7932191, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
