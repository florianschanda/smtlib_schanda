(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01000001 #b11010101100111101101101)))
;; x should be Float32(0xA0EACF6D [Rational(-15388525, 38685626227668133590597632), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC31622C3)))
;; y should be Float32(0xC31622C3 [Rational(-9839299, 65536), -150.135788])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x1D4830B0))))
(check-sat)
(exit)
