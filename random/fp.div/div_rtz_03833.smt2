(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01111010 #b11100011100110000100000)))
;; x should be Float32(0xBD71CC20 [Rational(-495201, 8388608), -0.059033])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11011000011011010101001)))
;; y should be Float32(0x806C36A9 [Rational(-7091881, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result ((_ to_fp 8 24) #x7C8F012D)))
(check-sat)
(exit)
