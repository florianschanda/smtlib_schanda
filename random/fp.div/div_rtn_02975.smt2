(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b00000110010101010110010)))
;; x should be Float32(0xCA032AB2 [Rational(-4298073, 2), -2149036.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00011001 #b11111001001011110101000)))
;; y should be Float32(0x8CFC97A8 [Rational(-2069237, 5316911983139663491615228241121378304), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b11111001 #b00001001110111110110000)))
(check-sat)
(exit)
