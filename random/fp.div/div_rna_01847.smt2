(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFA7D6173)))
;; x should be Float32(0xFA7D6173 [Rational(-328906902544888685612040428055429120), -328906902544888685612040428055429120.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49B535BC)))
;; y should be Float32(0x49B535BC [Rational(2968943, 2), 1484471.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b1 #b11100000 #b01100101111101010010001)))
(check-sat)
(exit)
