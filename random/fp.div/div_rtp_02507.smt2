(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11100001 #b01110010010011100000100)))
;; x should be Float32(0xF0B92704 [Rational(-458415150507037917843831128064), -458415150507037917843831128064.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x12AFAF6A)))
;; y should be Float32(0x12AFAF6A [Rational(5756853, 5192296858534827628530496329220096), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b1 #b11111110 #b11111111111111111111111))))
(check-sat)
(exit)
