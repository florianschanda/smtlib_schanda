(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x75C5AC43)))
;; x should be Float32(0x75C5AC43 [Rational(501160333920936321212912827891712), 501160333920936321212912827891712.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC49F1000)))
;; y should be Float32(0xC49F1000 [Rational(-2545, 2), -1272.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xF09F1203)))
(check-sat)
(exit)
