(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49F51F24)))
;; x should be Float32(0x49F51F24 [Rational(4016073, 2), 2008036.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x1AECC3E5)))
;; y should be Float32(0x1AECC3E5 [Rational(15516645, 158456325028528675187087900672), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x6E848484))))
(check-sat)
(exit)
