(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00101100110101100101101)))
;; x should be Float32(0xCA966B2D [Rational(-9857837, 2), -4928918.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC8A9AC50)))
;; y should be Float32(0xC8A9AC50 [Rational(-694981, 2), -347490.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result ((_ to_fp 8 24) #x4162F300)))
(check-sat)
(exit)
