(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000101 #b00101101011100011000010)))
;; x should be Float32(0x2296B8C2 [Rational(4938849, 1208925819614629174706176), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB2272B)))
;; y should be Float32(0x4AB2272B [Rational(11675435, 2), 5837717.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x17589507))))
(check-sat)
(exit)
