(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11011010 #b01001000101000001010000)))
;; x should be Float32(0x6D245050 [Rational(3178289591599274448528605184), 3178289591599274448528605184.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01010100 #b11101010001001010111110)))
;; y should be Float32(0x2A7512BE [Rational(8030559, 36893488147419103232), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b0 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
