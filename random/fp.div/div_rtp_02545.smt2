(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802C4ABA)))
;; x should be Float32(0x802C4ABA [Rational(-1451357, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01010011 #b00001010011111011110000)))
;; y should be Float32(0x29853EF0 [Rational(545775, 9223372036854775808), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b1 #b00101011 #b01010100011000101001100))))
(check-sat)
(exit)
