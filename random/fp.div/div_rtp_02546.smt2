(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807B0221)))
;; x should be Float32(0x807B0221 [Rational(-8061473, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01111110 #b00111001001010100011101)))
;; y should be Float32(0x3F1C951D [Rational(10261789, 16777216), 0.611650])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x80C91BE0))))
(check-sat)
(exit)
