(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00111001011001110010010)))
;; x should be Float32(0x801CB392 [Rational(-940489, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111111 #b01000101100000001000101)))
;; y should be Float32(NaN)

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result (_ NaN 8 24))))
(check-sat)
(exit)
