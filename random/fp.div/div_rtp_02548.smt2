(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807D3779)))
;; x should be Float32(0x807D3779 [Rational(-8206201, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10111000101001001101001)))
;; y should be Float32(0x005C5269 [Rational(6050409, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b1 #b01111111 #b01011011001101101101000))))
(check-sat)
(exit)
