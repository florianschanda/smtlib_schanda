(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A95C545)))
;; x should be Float32(0x4A95C545 [Rational(9815365, 2), 4907682.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFBE8641B)))
;; y should be Float32(0xFBE8641B [Rational(-2413286502601666123997169798388645888), -2413286502601666123997169798388645888.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x8E24FC6B))))
(check-sat)
(exit)
