(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x803C08A8)))
;; x should be Float32(0x803C08A8 [Rational(-491797, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA41AFE2)))
;; y should be Float32(0xCA41AFE2 [Rational(-6346737, 2), -3173368.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b00000000 #b00000000000000000000001))))
(check-sat)
(exit)
