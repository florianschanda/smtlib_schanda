(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x001D3D45)))
;; x should be Float32(0x001D3D45 [Rational(1916229, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11101010100111001001000)))
;; y should be Float32(0x80754E48 [Rational(-960969, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b1 #b01111100 #b11111110011110101010100))))
(check-sat)
(exit)
