(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11101011111000011100011)))
;; x should be Float32(0xCAF5F0E3 [Rational(-16117987, 2), -8058993.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b01010111110110101001010)))
;; y should be Float32(0xCA2BED4A [Rational(-5633701, 2), -2816850.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x40371A88))))
(check-sat)
(exit)
