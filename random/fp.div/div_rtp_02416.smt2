(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x807FFFFF)))
;; x should be Float32(0x807FFFFF [Rational(-8388607, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b01101101000000100001100)))
;; y should be Float32(0xC9B6810C [Rational(-2990147, 2), -1495073.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b0 #b00000000 #b00000000000000000000110)))
(check-sat)
(exit)
