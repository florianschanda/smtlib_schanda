(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01011111 #b01001000100011111001101)))
;; x should be Float32(0xAFA447CD [Rational(-10766285, 36028797018963968), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x003F9E40)))
;; y should be Float32(0x003F9E40 [Rational(65145, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b1 #b11011110 #b01001010100010000111000)))
(check-sat)
(exit)
