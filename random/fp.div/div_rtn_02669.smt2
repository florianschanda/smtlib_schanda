(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10010010111111100010111)))
;; x should be Float32(0x4AC97F17 [Rational(13205271, 2), 6602635.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10000011 #b10101101100001100000111)))
;; y should be Float32(0xC1D6C307 [Rational(-14074631, 524288), -26.845228])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result (fp #b1 #b10010000 #b11100000010111111111100))))
(check-sat)
(exit)
