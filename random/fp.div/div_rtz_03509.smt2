(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x38589103)))
;; x should be Float32(0x38589103 [Rational(14192899, 274877906944), 0.000052])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x007934FE)))
;; y should be Float32(0x007934FE [Rational(3971711, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b11101110 #b11001001011010000101110))))
(check-sat)
(exit)
