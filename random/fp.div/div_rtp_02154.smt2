(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11010001 #b11110010101101011010011)))
;; y should be Float32(0xE8F95AD3 [Rational(-9420357322828140165201920), -9420357322828140165201920.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (_ -zero 8 24)))
(check-sat)
(exit)
