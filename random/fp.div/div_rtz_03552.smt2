(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00011010101100101000011)))
;; x should be Float32(0x000D5943 [Rational(874819, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b10011001010011100010111)))
;; y should be Float32(0xCACCA717 [Rational(-13412119, 2), -6706059.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (_ -zero 8 24)))
(check-sat)
(exit)
