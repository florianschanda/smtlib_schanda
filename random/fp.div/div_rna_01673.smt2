(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10110000111110001001010)))
;; x should be Float32(0xCA587C4A [Rational(-7093797, 2), -3546898.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10001011 #b11101000111010011111000)))
;; y should be Float32(0xC5F474F8 [Rational(-2002591, 256), -7822.621094])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result (fp #b0 #b10000111 #b11000101011010100110011))))
(check-sat)
(exit)
