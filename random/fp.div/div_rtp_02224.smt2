(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00001001 #b00001011101011000010001)))
;; x should be Float32(0x0485D611 [Rational(8771089, 2787593149816327892691964784081045188247552), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11101111 #b10010100001110111111100)))
;; y should be Float32(0xF7CA1DFC [Rational(-8198844693746052140769752358322176), -8198844693746052140769752358322176.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (_ -zero 8 24))))
(check-sat)
(exit)
