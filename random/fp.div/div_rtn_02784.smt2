(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01111110 #b00111010100001100010001)))
;; y should be Float32(0x3F1D4311 [Rational(10306321, 16777216), 0.614305])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x00D05D9D))))
(check-sat)
(exit)
