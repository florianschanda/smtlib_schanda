(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0BC00A38)))
;; x should be Float32(0x0BC00A38 [Rational(1573191, 21267647932558653966460912964485513216), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11101011001111000011011)))
;; y should be Float32(0x80759E1B [Rational(-7708187, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xCAD0FDE4)))
(check-sat)
(exit)
