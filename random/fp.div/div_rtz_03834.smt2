(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xFF6BF2B2)))
;; x should be Float32(0xFF6BF2B2 [Rational(-313628725118130108415834501888281149440), -313628725118130108415834501888281149440.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01101101100010001010011)))
;; y should be Float32(0x8036C453 [Rational(-3589203, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b0 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
