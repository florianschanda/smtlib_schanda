(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x26633224)))
;; x should be Float32(0x26633224 [Rational(3722377, 4722366482869645213696), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80182475)))
;; y should be Float32(0x80182475 [Rational(-1582197, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b1 #b11001101 #b00101101001001000001010)))
(check-sat)
(exit)
