(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x72A6E6CB)))
;; x should be Float32(0x72A6E6CB [Rational(6611650970784085588028730376192), 6611650970784085588028730376192.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10100001 #b11101110001001000011111)))
;; y should be Float32(0x50F7121F [Rational(33161279488), 33161279488.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #x612CEEF4)))
(check-sat)
(exit)
