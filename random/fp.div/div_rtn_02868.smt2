(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00111110111001111111010)))
;; x should be Float32(0x4A9F73FA [Rational(5224957), 5224957.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01010000011110011011101)))
;; y should be Float32(0x80283CDD [Rational(-2637021, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF800000))))
(check-sat)
(exit)
