(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00101000 #b01011100111001001000110)))
;; x should be Float32(0x142E7246 [Rational(5716259, 649037107316853453566312041152512), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b01100000101100011001100)))
;; y should be Float32(0xC9B058CC [Rational(-2889267, 2), -1444633.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x89FD3DB9)))
(check-sat)
(exit)
