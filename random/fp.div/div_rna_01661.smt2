(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11111110010110100111001)))
;; x should be Float32(0xCAFF2D39 [Rational(-16723257, 2), -8361628.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11110001011111011101010)))
;; y should be Float32(0xCA78BEEA [Rational(-8150901, 2), -4075450.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x40034F29))))
(check-sat)
(exit)
