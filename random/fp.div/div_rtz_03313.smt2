(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ADA0EC1)))
;; x should be Float32(0x4ADA0EC1 [Rational(14290625, 2), 7145312.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b01111100000100100001000)))
;; y should be Float32(0xC93E0908 [Rational(-1556769, 2), -778384.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xC112DFED))))
(check-sat)
(exit)
