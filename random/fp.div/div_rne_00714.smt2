(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11011011111011110011111)))
;; x should be Float32(0x4AEDF79F [Rational(15595423, 2), 7797711.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA97630B0)))
;; y should be Float32(0xA97630B0 [Rational(-1008395, 18446744073709551616), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (fp #b1 #b11000001 #b11101110111001100001110))))
(check-sat)
(exit)
