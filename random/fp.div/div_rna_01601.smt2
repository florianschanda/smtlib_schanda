(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x003A39C0)))
;; x should be Float32(0x003A39C0 [Rational(59623, 11150372599265311570767859136324180752990208), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x83CA0FEA)))
;; y should be Float32(0x83CA0FEA [Rational(-6621173, 5575186299632655785383929568162090376495104), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result (fp #b1 #b01110111 #b00100111000100101001010))))
(check-sat)
(exit)
