(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xBC892A87)))
;; x should be Float32(0xBC892A87 [Rational(-8989319, 536870912), -0.016744])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCEDA3595)))
;; y should be Float32(0xCEDA3595 [Rational(-1830472320), -1830472320.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b01011010 #b01000001110101111011011)))
(check-sat)
(exit)
