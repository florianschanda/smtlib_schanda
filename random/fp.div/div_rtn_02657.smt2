(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AAF7393)))
;; x should be Float32(0x4AAF7393 [Rational(11498387, 2), 5749193.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA9A0CA5)))
;; y should be Float32(0xCA9A0CA5 [Rational(-10095781, 2), -5047890.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xBF91C875))))
(check-sat)
(exit)
