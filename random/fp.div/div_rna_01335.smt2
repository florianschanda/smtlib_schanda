(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11100000111111011000111)))
;; x should be Float32(0x4AF07EC7 [Rational(15761095, 2), 7880547.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11000111011000001001010)))
;; y should be Float32(0x4A63B04A [Rational(7460901, 2), 3730450.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #x40073312)))
(check-sat)
(exit)
