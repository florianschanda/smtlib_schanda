(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA68D976)))
;; x should be Float32(0xCA68D976 [Rational(-7630011, 2), -3815005.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01011100000001111111010)))
;; y should be Float32(0x802E03FA [Rational(-1507837, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x7F7FFFFF))))
(check-sat)
(exit)
