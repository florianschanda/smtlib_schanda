(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11011101001011100010001)))
;; x should be Float32(0xCAEE9711 [Rational(-15636241, 2), -7818120.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00100110110000101011100)))
;; y should be Float32(0x0013615C [Rational(317527, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF7FFFFF))))
(check-sat)
(exit)
