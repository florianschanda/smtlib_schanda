(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49EA8734)))
;; x should be Float32(0x49EA8734 [Rational(3842509, 2), 1921254.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x2EE9BBF6)))
;; y should be Float32(0x2EE9BBF6 [Rational(7659003, 72057594037927936), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x5A806F4D)))
(check-sat)
(exit)
