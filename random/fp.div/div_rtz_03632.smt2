(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC8270C20)))
;; x should be Float32(0xC8270C20 [Rational(-342113, 2), -171056.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00001110 #b01010011101000101101011)))
;; y should be Float32(0x8729D16B [Rational(-11129195, 87112285931760246646623899502532662132736), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b0 #b11111110 #b11111111111111111111111)))
(check-sat)
(exit)
