(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01001001 #b11000110111011111110100)))
;; x should be Float32(0x24E377F4 [Rational(3726845, 37778931862957161709568), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00110101 #b01001100101110111101110)))
;; y should be Float32(0x9AA65DEE [Rational(-5451511, 79228162514264337593543950336), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xC9AF02B7))))
(check-sat)
(exit)
