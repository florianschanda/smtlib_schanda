(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11001100 #b10100111101110010101101)))
;; x should be Float32(0x6653DCAD [Rational(250122519386369950744576), 250122519386369950744576.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xE7E8267B)))
;; y should be Float32(0xE7E8267B [Rational(-2192597726769250635350016), -2192597726769250635350016.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xBDE9A09E))))
(check-sat)
(exit)
