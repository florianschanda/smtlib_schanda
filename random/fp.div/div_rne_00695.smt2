(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49D5AC04)))
;; x should be Float32(0x49D5AC04 [Rational(3500801, 2), 1750400.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x646C09FF)))
;; y should be Float32(0x646C09FF [Rational(17416607583443427000320), 17416607583443427000320.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (fp #b0 #b01001001 #b11001111011110111001011))))
(check-sat)
(exit)
