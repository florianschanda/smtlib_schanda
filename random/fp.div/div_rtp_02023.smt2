(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4919AA08)))
;; x should be Float32(0x4919AA08 [Rational(1258817, 2), 629408.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000101 #b10111000000010011011000)))
;; y should be Float32(0x82DC04D8 [Rational(-1802395, 5575186299632655785383929568162090376495104), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b1 #b11111110 #b11111111111111111111111))))
(check-sat)
(exit)
