(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10000000 #b00111111010110101110000)))
;; x should be Float32(0xC01FAD70 [Rational(-654039, 262144), -2.494961])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9C8F2B4)))
;; y should be Float32(0xC9C8F2B4 [Rational(-3292333, 2), -1646166.500000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (fp #b0 #b01101011 #b10010110110110001000011))))
(check-sat)
(exit)
