(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB2112AA3)))
;; x should be Float32(0xB2112AA3 [Rational(-9513635, 1125899906842624), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A8C1AD9)))
;; y should be Float32(0x4A8C1AD9 [Rational(9181913, 2), 4590956.500000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result ((_ to_fp 8 24) #xA7049FD6)))
(check-sat)
(exit)
