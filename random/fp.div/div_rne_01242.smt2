(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01011100000111110111010)))
;; x should be Float32(0x802E0FBA [Rational(-1509341, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00407ECA)))
;; y should be Float32(0x00407ECA [Rational(2113381, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (fp #b1 #b01111110 #b01101101101010010110100))))
(check-sat)
(exit)
