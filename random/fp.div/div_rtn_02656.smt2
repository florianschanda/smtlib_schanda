(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010001 #b01001011110111110010000)))
;; x should be Float32(0x48A5EF90 [Rational(679673, 2), 339836.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCAF3E9EB)))
;; y should be Float32(0xCAF3E9EB [Rational(-15985131, 2), -7992565.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b1 #b01111010 #b01011100010100001111100)))
(check-sat)
(exit)
