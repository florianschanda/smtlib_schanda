(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11011101 #b00001101001110011000100)))
;; x should be Float32(0xEE869CC4 [Rational(-20830254663875381905418354688), -20830254663875381905418354688.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b00111011011110001100111)))
;; y should be Float32(0x4A9DBC67 [Rational(10337383, 2), 5168691.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #xE35A78A3)))
(check-sat)
(exit)
