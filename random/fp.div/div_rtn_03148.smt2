(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE0816E5F)))
;; x should be Float32(0xE0816E5F [Rational(-74611970655484116992), -74611970655484116992.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49DB9964)))
;; y should be Float32(0x49DB9964 [Rational(3597913, 2), 1798956.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xD616E2BC))))
(check-sat)
(exit)
