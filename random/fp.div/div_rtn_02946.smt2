(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA482FD6)))
;; x should be Float32(0xCA482FD6 [Rational(-6559723, 2), -3279861.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x49BDC3A4)))
;; y should be Float32(0x49BDC3A4 [Rational(3109097, 2), 1554548.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xC00707C2))))
(check-sat)
(exit)
