(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11001100 #b11000100001010000111110)))
;; x should be Float32(0xE662143E [Rational(-266907056895211090214912), -266907056895211090214912.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0075D06B)))
;; y should be Float32(0x0075D06B [Rational(7721067, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF7FFFFF))))
(check-sat)
(exit)
