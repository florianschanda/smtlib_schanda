(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49492338)))
;; x should be Float32(0x49492338 [Rational(1647719, 2), 823859.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b11001001001110010011100)))
;; y should be Float32(0x49E49C9C [Rational(3745575, 2), 1872787.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x3EE13BFD))))
(check-sat)
(exit)
