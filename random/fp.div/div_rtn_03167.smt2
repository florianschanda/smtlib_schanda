(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD9FAA701)))
;; x should be Float32(0xD9FAA701 [Rational(-8819045864308736), -8819045864308736.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010011 #b10101101001001110000100)))
;; y should be Float32(0xC9D69384 [Rational(-3515617, 2), -1757808.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b10011111 #b00101011000010100110010)))
(check-sat)
(exit)
