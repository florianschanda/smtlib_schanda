(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA5080B2)))
;; x should be Float32(0xCA5080B2 [Rational(-6832217, 2), -3416108.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0041C955)))
;; y should be Float32(0x0041C955 [Rational(4311381, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF7FFFFF))))
(check-sat)
(exit)
