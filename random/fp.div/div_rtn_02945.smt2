(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10011011011101011111010)))
;; x should be Float32(0xCA4DBAFA [Rational(-6741373, 2), -3370686.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x499E185C)))
;; y should be Float32(0x499E185C [Rational(2590231, 2), 1295115.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #xC026913D)))
(check-sat)
(exit)
