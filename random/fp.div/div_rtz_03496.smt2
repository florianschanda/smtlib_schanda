(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111010 #b00010101010101100110000)))
;; x should be Float32(0x1D0AAB30 [Rational(567987, 309485009821345068724781056), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b00110011000101000100110)))
;; y should be Float32(0x4A198A26 [Rational(5031187, 2), 2515593.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b00100100 #b11001110011010010010010))))
(check-sat)
(exit)
