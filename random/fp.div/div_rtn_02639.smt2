(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AC45765)))
;; x should be Float32(0x4AC45765 [Rational(12867429, 2), 6433714.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01100001100000100101001)))
;; y should be Float32(0x4AB0C129 [Rational(11583785, 2), 5791892.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b01111111 #b00011100010111100100110)))
(check-sat)
(exit)
