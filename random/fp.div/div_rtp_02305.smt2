(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAB7D4E1)))
;; x should be Float32(0xCAB7D4E1 [Rational(-12047585, 2), -6023792.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A589BFA)))
;; y should be Float32(0x4A589BFA [Rational(7097853, 2), 3548926.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result ((_ to_fp 8 24) #xBFD942F8)))
(check-sat)
(exit)
