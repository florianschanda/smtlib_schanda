(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11000110 #b11011010011110010010001)))
;; x should be Float32(0xE36D3C91 [Rational(-4376242614983062454272), -4376242614983062454272.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111111111111111111111)))
;; y should be Float32(0x007FFFFF [Rational(8388607, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result (_ -oo 8 24))))
(check-sat)
(exit)
