(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11111110111100101010101)))
;; x should be Float32(0x007F7955 [Rational(8354133, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x63E74341)))
;; y should be Float32(0x63E74341 [Rational(8532088071401867575296), 8532088071401867575296.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result (_ +zero 8 24))))
(check-sat)
(exit)
