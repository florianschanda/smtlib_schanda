(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAE61C7D)))
;; x should be Float32(0xCAE61C7D [Rational(-15080573, 2), -7540286.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x5C72415A)))
;; y should be Float32(0x5C72415A [Rational(272755196667363328), 272755196667363328.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b1 #b01011011 #b11100110010101011000000)))
(check-sat)
(exit)
