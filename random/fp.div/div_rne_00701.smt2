(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AF72229)))
;; x should be Float32(0x4AF72229 [Rational(16196137, 2), 8098068.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC95C7658)))
;; y should be Float32(0xC95C7658 [Rational(-1806027, 2), -903013.500000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result ((_ to_fp 8 24) #xC10F7C38)))
(check-sat)
(exit)
