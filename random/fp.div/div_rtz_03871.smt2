(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x800AB000)))
;; x should be Float32(0x800AB000 [Rational(-171, 174224571863520493293247799005065324265472), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b01111101101101010000011)))
;; y should be Float32(0x803EDA83 [Rational(-4119171, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b01111100 #b01011100001111001110111))))
(check-sat)
(exit)
