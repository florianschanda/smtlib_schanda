(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A9A0FBF)))
;; x should be Float32(0x4A9A0FBF [Rational(10096575, 2), 5048287.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xFE70FCA1)))
;; y should be Float32(0xFE70FCA1 [Rational(-80081610816169193494560408762425802752), -80081610816169193494560408762425802752.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x8BA3A8CA))))
(check-sat)
(exit)
