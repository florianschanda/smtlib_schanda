(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001010 #b00100110010001111100100)))
;; x should be Float32(0x851323E4 [Rational(-2410745, 348449143727040986586495598010130648530944), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x000F45C3)))
;; y should be Float32(0x000F45C3 [Rational(1000899, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b1 #b10001011 #b00110100010011000101011)))
(check-sat)
(exit)
