(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAC55481)))
;; x should be Float32(0xCAC55481 [Rational(-12932225, 2), -6466112.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80643F32)))
;; y should be Float32(0x80643F32 [Rational(-3284889, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result (fp #b0 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
