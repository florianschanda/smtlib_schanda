(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b10001100010101101010101)))
;; x should be Float32(0x80462B55 [Rational(-4598613, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00101110 #b00000101110001001000110)))
;; y should be Float32(0x9702E246 [Rational(-4288803, 10141204801825835211973625643008), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x28893F0F))))
(check-sat)
(exit)
