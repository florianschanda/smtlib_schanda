(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A491A66)))
;; x should be Float32(0x4A491A66 [Rational(6589747, 2), 3294873.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x20A49A47)))
;; y should be Float32(0x20A49A47 [Rational(10787399, 38685626227668133590597632), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b11010010 #b00111000110001001000101))))
(check-sat)
(exit)
