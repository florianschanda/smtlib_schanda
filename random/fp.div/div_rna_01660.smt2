(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10101010010010101100111)))
;; x should be Float32(0xCAD52567 [Rational(-13968743, 2), -6984371.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010010 #b11001101101110011011000)))
;; y should be Float32(0xC966DCD8 [Rational(-1891227, 2), -945613.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b0 #b10000001 #b11011000101101010111000)))
(check-sat)
(exit)
