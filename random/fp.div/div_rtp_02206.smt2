(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x308B79A9)))
;; x should be Float32(0x308B79A9 [Rational(9140649, 9007199254740992), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00100010 #b10011100011010100011001)))
;; y should be Float32(0x114E3519 [Rational(13514009, 83076749736557242056487941267521536), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b0 #b10111101 #b01011010010011101110100))))
(check-sat)
(exit)
