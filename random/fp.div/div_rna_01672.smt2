(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA0BA492)))
;; x should be Float32(0xCA0BA492 [Rational(-4575817, 2), -2287908.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB30701AF)))
;; y should be Float32(0xB30701AF [Rational(-8847791, 281474976710656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b0 #b10101101 #b00001000110010101001010)))
(check-sat)
(exit)
