(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00110100 #b10010010011001000010111)))
;; x should be Float32(0x1A493217 [Rational(13185559, 316912650057057350374175801344), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01101011 #b11011100101010111100100)))
;; y should be Float32(0xB5EE55E4 [Rational(-3904889, 2199023255552), -0.000002])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b1 #b01000111 #b10110000001101110000101)))
(check-sat)
(exit)
