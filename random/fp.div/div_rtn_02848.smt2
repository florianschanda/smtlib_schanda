(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10100001 #b00000110011111101000101)))
;; x should be Float32(0x50833F45 [Rational(17615693824), 17615693824.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x71AEAB25)))
;; y should be Float32(0x71AEAB25 [Rational(1729833368052096311163506130944), 1729833368052096311163506130944.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #x1E405C34))))
(check-sat)
(exit)
