(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9D05834)))
;; x should be Float32(0xC9D05834 [Rational(-3413517, 2), -1706758.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A6AD432)))
;; y should be Float32(0x4A6AD432 [Rational(7694873, 2), 3847436.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b1 #b01111101 #b11000110010000011000001))))
(check-sat)
(exit)
