(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00011110 #b01001101101101101100001)))
;; x should be Float32(0x8F26DB61 [Rational(-10935137, 1329227995784915872903807060280344576), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10000010001011100101110)))
;; y should be Float32(0x8041172E [Rational(-2132887, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result ((_ to_fp 8 24) #x4EA40FB7))))
(check-sat)
(exit)
