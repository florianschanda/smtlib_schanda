(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x491D0488)))
;; x should be Float32(0x491D0488 [Rational(1286289, 2), 643144.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010010 #b10111100010001001111001)))
;; y should be Float32(0x495E2279 [Rational(14557817, 16), 909863.562500])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result ((_ to_fp 8 24) #x3F34F4A9)))
(check-sat)
(exit)
