(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00101011 #b01100000100100110001011)))
;; x should be Float32(0x15B0498B [Rational(11553163, 162259276829213363391578010288128), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00110010011101000001100)))
;; y should be Float32(0x00193A0C [Rational(413315, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b10101011 #b10111111001111010011001))))
(check-sat)
(exit)
