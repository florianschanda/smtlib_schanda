(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCACCDF09)))
;; x should be Float32(0xCACCDF09 [Rational(-13426441, 2), -6713220.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11001111001100111110001)))
;; y should be Float32(0xCAE799F1 [Rational(-15178225, 2), -7589112.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b0 #b01111110 #b11000100111010000110101))))
(check-sat)
(exit)
