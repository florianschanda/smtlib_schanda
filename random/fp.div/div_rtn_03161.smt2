(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10100101 #b10010000010100101111000)))
;; x should be Float32(0xD2C82978 [Rational(-429844594688), -429844594688.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x77937642)))
;; y should be Float32(0x77937642 [Rational(5981767121848253905861639921467392), 5981767121848253905861639921467392.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x9AADBEA7)))
(check-sat)
(exit)
