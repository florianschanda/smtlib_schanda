(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8018E460)))
;; x should be Float32(0x8018E460 [Rational(-50979, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80065F65)))
;; y should be Float32(0x80065F65 [Rational(-417637, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (not (= result (fp #b0 #b10000000 #b11110011111110101100100))))
(check-sat)
(exit)
