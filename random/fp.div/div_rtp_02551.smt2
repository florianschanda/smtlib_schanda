(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01100011101010011101011)))
;; x should be Float32(0x8031D4EB [Rational(-3265771, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01100100101001000000111)))
;; y should be Float32(0xCAB25207 [Rational(-11686407, 2), -5843203.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result (fp #b0 #b00000000 #b00000000000000000000001))))
(check-sat)
(exit)
