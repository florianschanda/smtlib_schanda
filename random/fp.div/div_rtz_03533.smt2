(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00011010011010001100101)))
;; x should be Float32(0x000D3465 [Rational(865381, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A09C69A)))
;; y should be Float32(0x4A09C69A [Rational(4514637, 2), 2257318.500000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (_ +zero 8 24)))
(check-sat)
(exit)
