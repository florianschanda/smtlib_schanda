(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101111 #b10111110011100010101100)))
;; x should be Float32(0xF7DF38AC [Rational(-9054934700273621157594592320684032), -9054934700273621157594592320684032.000000])

(declare-const y Float32)
(assert (= y (_ -oo 8 24)))
;; y should be Float32(-oo)

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (_ +zero 8 24)))
(check-sat)
(exit)
