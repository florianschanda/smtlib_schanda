(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00100000 #b11001110000110000001101)))
(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111110000010010001111)))
(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result (fp #b1 #b10011110 #b11010001101010101101101)))
(check-sat)
(exit)
