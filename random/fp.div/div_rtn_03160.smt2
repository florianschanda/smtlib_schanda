(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD39DA809)))
;; x should be Float32(0xD39DA809 [Rational(-1354258055168), -1354258055168.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01010011 #b10111111001100001101000)))
;; y should be Float32(0x29DF9868 [Rational(1831693, 18446744073709551616), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #xE934813C)))
(check-sat)
(exit)
