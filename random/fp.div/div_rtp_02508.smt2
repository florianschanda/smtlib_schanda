(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC875C2C0)))
;; x should be Float32(0xC875C2C0 [Rational(-251659), -251659.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01010010 #b00011001111100111110111)))
;; y should be Float32(0x290CF9F7 [Rational(9239031, 295147905179352825856), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xDEDF23A6))))
(check-sat)
(exit)
