(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0CC36FA9)))
;; x should be Float32(0x0CC36FA9 [Rational(12808105, 42535295865117307932921825928971026432), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b01001100011010001010011)))
;; y should be Float32(0x4AA63453 [Rational(10892371, 2), 5446185.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x01968330))))
(check-sat)
(exit)
