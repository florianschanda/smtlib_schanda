(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11101101 #b10111100111000011001101)))
;; x should be Float32(0xF6DE70CD [Rational(-2255815965319640907832927644024832), -2255815965319640907832927644024832.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x00240078)))
;; y should be Float32(0x00240078 [Rational(294927, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF800000))))
(check-sat)
(exit)
