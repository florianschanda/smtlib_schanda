(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010001 #b10101100110100011010000)))
;; x should be Float32(0xC8D668D0 [Rational(-878221, 2), -439110.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00111001 #b01001001000001110001011)))
;; y should be Float32(0x1CA4838B [Rational(10781579, 9903520314283042199192993792), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #xEBA6D23F)))
(check-sat)
(exit)
