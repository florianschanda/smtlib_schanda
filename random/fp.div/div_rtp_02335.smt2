(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11110001011100011001100)))
;; x should be Float32(0xC9F8B8CC [Rational(-4075059, 2), -2037529.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x82702026)))
;; y should be Float32(0x82702026 [Rational(-7868435, 44601490397061246283071436545296723011960832), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (_ +oo 8 24)))
(check-sat)
(exit)
