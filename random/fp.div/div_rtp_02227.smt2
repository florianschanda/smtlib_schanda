(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10100010 #b10110101110011011010001)))
;; x should be Float32(0x515AE6D1 [Rational(58760957952), 58760957952.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8033F30E)))
;; y should be Float32(0x8033F30E [Rational(-1702279, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF7FFFFF))))
(check-sat)
(exit)
