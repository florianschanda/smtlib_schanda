(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49910E14)))
;; x should be Float32(0x49910E14 [Rational(2376581, 2), 1188290.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111110 #b11011110000010101001011)))
;; y should be Float32(0x7F6F054B [Rational(317712973657607841637434335932494905344), 317712973657607841637434335932494905344.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result ((_ to_fp 8 24) #x099B5BF9)))
(check-sat)
(exit)
