(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x49D93564)))
;; x should be Float32(0x49D93564 [Rational(3558745, 2), 1779372.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xAD56768B)))
;; y should be Float32(0xAD56768B [Rational(-14055051, 1152921504606846976), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result (fp #b1 #b10111000 #b00000011010001101111100))))
(check-sat)
(exit)
