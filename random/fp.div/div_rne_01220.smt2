(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xAA8381E7)))
;; x should be Float32(0xAA8381E7 [Rational(-8618471, 36893488147419103232), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC03C8C4B)))
;; y should be Float32(0xC03C8C4B [Rational(-12356683, 4194304), -2.946063])

(declare-const result Float32)
(assert (= result (fp.div RNE x y)))
(assert (= result ((_ to_fp 8 24) #x29B28DB0)))
(check-sat)
(exit)
