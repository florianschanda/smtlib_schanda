(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110100 #b10101011110000101101101)))
;; x should be Float32(0xFA55E16D [Rational(-277632852224613491383795386488455168), -277632852224613491383795386488455168.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AB0D28F)))
;; y should be Float32(0x4AB0D28F [Rational(11588239, 2), 5794119.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b1 #b11011110 #b00110101101001101110010)))
(check-sat)
(exit)
