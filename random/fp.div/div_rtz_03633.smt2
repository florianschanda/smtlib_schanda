(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b00011100000100011011100)))
;; x should be Float32(0xC98E08DC [Rational(-2327095, 2), -1163547.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b10110110001100011011101)))
;; y should be Float32(0x805B18DD [Rational(-5970141, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x7F7FFFFF))))
(check-sat)
(exit)
