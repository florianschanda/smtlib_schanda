(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b11010000111000101101000)))
;; x should be Float32(0xC9687168 [Rational(-1904173, 2), -952086.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b11111100010010110010011)))
;; y should be Float32(0x007E2593 [Rational(8267155, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result (fp #b1 #b11111111 #b00000000000000000000000))))
(check-sat)
(exit)
