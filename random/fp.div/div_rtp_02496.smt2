(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01100100 #b10001111101110111001011)))
;; x should be Float32(0xB247DDCB [Rational(-13098443, 1125899906842624), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AFD4A89)))
;; y should be Float32(0x4AFD4A89 [Rational(16599689, 2), 8299844.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result ((_ to_fp 8 24) #xA6CA00FD)))
(check-sat)
(exit)
