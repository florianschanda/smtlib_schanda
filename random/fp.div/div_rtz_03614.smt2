(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA8D1703)))
;; x should be Float32(0xCA8D1703 [Rational(-9246467, 2), -4623233.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10110100 #b01010110110000000100010)))
;; y should be Float32(0x5A2B6022 [Rational(12059480040669184), 12059480040669184.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (= result ((_ to_fp 8 24) #xAFD2C26B)))
(check-sat)
(exit)
