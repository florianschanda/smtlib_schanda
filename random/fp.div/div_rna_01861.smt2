(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11100011 #b00001100111010100011101)))
;; x should be Float32(0xF186751D [Rational(-1331602322738660929352719400960), -1331602322738660929352719400960.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01011101011110000010111)))
;; y should be Float32(0x002EBC17 [Rational(3062807, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #xFF800000))))
(check-sat)
(exit)
