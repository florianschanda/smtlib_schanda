(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000001 #b00000000000000000000000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b11001011001001011101110)))
;; y should be Float32(0x4A6592EE [Rational(7522679, 2), 3761339.500000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (not (= result ((_ to_fp 8 24) #x00000002))))
(check-sat)
(exit)
