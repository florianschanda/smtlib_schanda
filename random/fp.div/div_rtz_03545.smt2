(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b10101101111010010110100)))
;; x should be Float32(0x0056F4B4 [Rational(1424685, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01000100 #b01111100100100101101000)))
;; y should be Float32(0x223E4968 [Rational(1558829, 604462909807314587353088), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTZ x y)))
(assert (not (= result ((_ to_fp 8 24) #x1D69F857))))
(check-sat)
(exit)
