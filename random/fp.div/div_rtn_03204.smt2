(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b01111100111101001101000)))
;; x should be Float32(0x803E7A68 [Rational(-511821, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010100 #b11001000010000001101010)))
;; y should be Float32(0xCA64206A [Rational(-7475253, 2), -3737626.500000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x00000001)))
(check-sat)
(exit)
