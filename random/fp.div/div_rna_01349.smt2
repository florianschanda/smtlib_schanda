(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A1E15AE)))
;; x should be Float32(0x4A1E15AE [Rational(5180119, 2), 2590059.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0002AC54)))
;; y should be Float32(0x0002AC54 [Rational(43797, 178405961588244985132285746181186892047843328), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b0 #b11111111 #b00000000000000000000000)))
(check-sat)
(exit)
