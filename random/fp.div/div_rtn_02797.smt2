(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00800000)))
;; x should be Float32(0x00800000 [Rational(1, 85070591730234615865843651857942052864), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB05AD708)))
;; y should be Float32(0xB05AD708 [Rational(-1792737, 2251799813685248), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result ((_ to_fp 8 24) #x8F95BC29)))
(check-sat)
(exit)
