(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00100001 #b01111001101000000101011)))
;; x should be Float32(0x90BCD02B [Rational(-12374059, 166153499473114484112975882535043072), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA875A09)))
;; y should be Float32(0xCA875A09 [Rational(-8870409, 2), -4435204.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b0 #b00001011 #b01100101000111011000100)))
(check-sat)
(exit)
