(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x006D9585)))
;; x should be Float32(0x006D9585 [Rational(7181701, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0647D5A4)))
;; y should be Float32(0x0647D5A4 [Rational(3274089, 87112285931760246646623899502532662132736), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b0 #b01110011 #b00011000110001000111101)))
(check-sat)
(exit)
