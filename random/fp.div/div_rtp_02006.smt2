(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11010100000100101000111)))
;; x should be Float32(0x4AEA0947 [Rational(15337799, 2), 7668899.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b01001111 #b01000001010010101101000)))
;; y should be Float32(0x27A0A568 [Rational(1316013, 295147905179352825856), 0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result (fp #b0 #b11000101 #b01110100111100111011001)))
(check-sat)
(exit)
