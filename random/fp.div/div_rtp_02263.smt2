(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x00141BFD)))
;; x should be Float32(0x00141BFD [Rational(1317885, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11110101 #b00100110010100110000100)))
;; y should be Float32(0xFA932984 [Rational(-382054837557910631386963572749762560), -382054837557910631386963572749762560.000000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (= result ((_ to_fp 8 24) #x80000000)))
(check-sat)
(exit)
