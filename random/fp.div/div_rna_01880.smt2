(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x9D729741)))
;; x should be Float32(0x9D729741 [Rational(-15898433, 4951760157141521099596496896), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80669DFB)))
;; y should be Float32(0x80669DFB [Rational(-6725115, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RNA x y)))
(assert (= result (fp #b0 #b10111001 #b00101110100110001101010)))
(check-sat)
(exit)
