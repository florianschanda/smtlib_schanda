(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10111001111100101011011)))
;; x should be Float32(0xCADCF95B [Rational(-14481755, 2), -7240877.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11101100001011101001001)))
;; y should be Float32(0xCAF61749 [Rational(-16127817, 2), -8063908.500000])

(declare-const result Float32)
(assert (= result (fp.div RTP x y)))
(assert (not (= result ((_ to_fp 8 24) #x3F65DF2B))))
(check-sat)
(exit)
