(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11000100111101011100011)))
;; x should be Float32(0x4AE27AE3 [Rational(14842595, 2), 7421297.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xDA898000)))
;; y should be Float32(0xDA898000 [Rational(-19351404648857600), -19351404648857600.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result ((_ to_fp 8 24) #xAFD2D513))))
(check-sat)
(exit)
