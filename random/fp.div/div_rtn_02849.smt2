(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01000111 #b00111101110110010011001)))
;; x should be Float32(0x239EEC99 [Rational(10415257, 604462909807314587353088), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10001010 #b10100100011110100100101)))
;; y should be Float32(0x45523D25 [Rational(13778213, 4096), 3363.821533])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (= result (fp #b0 #b00111011 #b10000011000010000011101)))
(check-sat)
(exit)
