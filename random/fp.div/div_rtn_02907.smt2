(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11001100100100001100110)))
;; x should be Float32(0x00664866 [Rational(3351603, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x801E040D)))
;; y should be Float32(0x801E040D [Rational(-1967117, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.div RTN x y)))
(assert (not (= result (fp #b1 #b10000000 #b10110100001011010011011))))
(check-sat)
(exit)
