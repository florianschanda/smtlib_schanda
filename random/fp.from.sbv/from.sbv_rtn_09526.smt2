(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11100101010100111100011001000010100001001110110010000011001101001011010010010001100100010100010001100000111000110100010110010010))
;; x should be -35454174059636141510946646867947862638
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r (fp #b1 #b11111011 #b10101010110000111001110)))
(check-sat)
(exit)
