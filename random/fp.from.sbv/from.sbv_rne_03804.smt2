(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10100100010110001010011111010001000110011000101101110011010010100101011001111010010001110010011110000110011110000111100010001100))
;; x should be -121828649759665709787736186720554026868
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.leq r (fp #b1 #b11111101 #b01101110100111010110000)))
(check-sat)
(exit)
