(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1001001001111101011010101000000101110010001010011100001000110100))
;; x should be -7891033867884576204
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.leq r (fp #b1 #b10000111101 #b1011011000001010010101011111101000110111010110001111)))
(check-sat)
(exit)
