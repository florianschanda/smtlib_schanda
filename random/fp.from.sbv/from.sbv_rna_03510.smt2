(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1111011100011010101000101010100011100001010101111010100000011000))
;; x should be -641021150728050664
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r ((_ to_fp 8 24) #xDD0E55D5)))
(check-sat)
(exit)
