(declare-const x (_ BitVec 64))
(assert (= x #b1010110111101110100101000101001100110010010100110100000100001110))
;; x should be -5913626175643369202
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
