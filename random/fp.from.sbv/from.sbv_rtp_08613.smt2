(declare-const x (_ BitVec 64))
(assert (= x #b1110110101111110110100111001010100110011100001101110000010010111))
;; x should be -1333395801887022953
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
