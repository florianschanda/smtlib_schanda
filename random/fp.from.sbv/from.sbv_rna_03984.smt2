(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11011101011000110010111001011011101100100100100001011101101010001100101100110000001001010110000110101111010011011110000100011111))
;; x should be -46008002207696734140525675103296560865
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFE0A7347)))
(check-sat)
(exit)
