(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10111010110110111101010100100111100010100010000001011010101011001011000111111100100011000010101010001110010100000111010010011111))
;; x should be -91904523407033022810286842093815958369
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFE8A4856)))
(check-sat)
(exit)
