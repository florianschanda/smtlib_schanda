(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1011100000011000001011001101000111111100101100110110001100000000))
;; x should be -5181342090890353920
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3D1F9F4CB80D327)))
(check-sat)
(exit)
