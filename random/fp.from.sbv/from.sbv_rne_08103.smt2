(declare-const x (_ BitVec 64))
(assert (= x #b0100000110000101111110101011101111001011111001100100010101011111))
;; x should be 4721455468854527327
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
