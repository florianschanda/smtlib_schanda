(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b00100100011010110100111111111001))
;; x should be 611012601
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r (fp #b0 #b10000011100 #b0010001101011010011111111100100000000000000000000000)))
(check-sat)
(exit)
