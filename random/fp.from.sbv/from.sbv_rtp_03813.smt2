(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10011010001110010101110111101010010001110001000101000111101001010011111010001011100001110100001110101100111101000001011111011000))
;; x should be -135283389823647440145668808815011424296
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r (fp #b1 #b11111101 #b10010111000110101000100)))
(check-sat)
(exit)
