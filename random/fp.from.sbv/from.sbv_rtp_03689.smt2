(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b00110111101011111110011110000110100010101101101101011001111000001101010101101110010010011000111101000100001011101011101111001111))
;; x should be 74020887614580298839899576362802789327
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7E5EBF9F)))
(check-sat)
(exit)
