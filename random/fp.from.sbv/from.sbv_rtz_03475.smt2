(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1001001110101001010010001010101011000111111111111111010010010110))
;; x should be -7806628580732963690
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r (fp #b1 #b10111101 #b10110001010110101101110)))
(check-sat)
(exit)
