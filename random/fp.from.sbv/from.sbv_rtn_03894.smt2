(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10000110000110001011111101011110101110101011011111101111100111001011011100001000010100110001111110001001100000010101011110111011))
;; x should be -162037318915686750033050997554826618949
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
