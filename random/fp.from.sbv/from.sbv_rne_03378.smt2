(declare-const x (_ BitVec 64))
(assert (= x #b0101000001111011001010010111100000000001010101010001101011000011))
;; x should be 5799274540564814531
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
