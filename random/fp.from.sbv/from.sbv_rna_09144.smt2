(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10100001101101001110010010101100000110100111001111110000110010111111110110010101010101111100111000101110010001010101110010011100))
;; x should be -125337408140210380062875257898517832548
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFEBC9637)))
(check-sat)
(exit)
