(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11111000010110000101011000001001110110111101100000001111111111111011011010000010111100001000101111010010011111011010101111001111))
;; x should be -10175156774410464799404624861489943601
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTN x)))
(assert (fp.geq r ((_ to_fp 5 11) #xFC00)))
(check-sat)
(exit)
