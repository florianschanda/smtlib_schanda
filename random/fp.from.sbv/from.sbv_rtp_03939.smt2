(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b11011001100100100100101100011110101010000000001011001011000010111000001001010110001100101011100110111000100100101111010010001101))
;; x should be -51080292884703626551943941037631540083
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.eq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
