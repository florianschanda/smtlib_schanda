(declare-const x (_ BitVec 64))
(assert (= x #b1011110000100100000111000000000001110001100100111011110000101010))
;; x should be -4889752507186430934
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
