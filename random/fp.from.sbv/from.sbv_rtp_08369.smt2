(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1000010110011010100100011000000101111110000011100111011101001011))
;; x should be -8819576934900009141
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.leq r ((_ to_fp 8 24) #xDEF4CADC)))
(check-sat)
(exit)
