(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10101111101010001010101010101110001000000111010011111101000011111000000100111001010110100011110111100010111011000111101011111001))
;; x should be -106791699980966485643557196954279838983
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFEA0AEAA)))
(check-sat)
(exit)
