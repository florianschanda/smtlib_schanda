(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0011010010000100101010010110010010110100010011110100001101101100))
;; x should be 3784335836884976492
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.leq r ((_ to_fp 11 53) #x43CA4254B25A27A2)))
(check-sat)
(exit)
