(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1000110011000100111010010001101100100100101100011000001011001111))
;; x should be -8303255510175350065
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.geq r ((_ to_fp 11 53) #xC3DCCEC5B936D39F)))
(check-sat)
(exit)
