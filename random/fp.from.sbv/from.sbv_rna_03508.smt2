(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1110010000010011100101000010110101111100001011101000011001000100))
;; x should be -2012101685426616764
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNA x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3BBEC6BD283D17A)))
(check-sat)
(exit)
