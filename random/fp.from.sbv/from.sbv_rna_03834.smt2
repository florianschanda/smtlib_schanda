(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10111001100101000000011100000110110110101011110011101010010010101010001111000100001100111010101110110001001110011001001001101001))
;; x should be -93606585245733555234111352676700024215
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFE8CD7F2)))
(check-sat)
(exit)
