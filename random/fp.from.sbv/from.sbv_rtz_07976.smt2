(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0100111100010010111000001000010010111100101010000100110111010100))
;; x should be 5697863339282550228
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r ((_ to_fp 8 24) #x5E9E25C1)))
(check-sat)
(exit)
