(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1110100010010110000010101010100111011011000101100110101100110010))
;; x should be -1687149285762241742
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3B769F55624E995)))
(check-sat)
(exit)
