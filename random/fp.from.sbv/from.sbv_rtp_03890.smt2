(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10110111010111110101111000100000001011001011100100110100110001100110100100001011101110101000111101111100000101011000110010100111))
;; x should be -96538466395092865451939190252757480281
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.leq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
