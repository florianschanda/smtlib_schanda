(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b01000001001101001011010010011101))
;; x should be 1093973149
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r (fp #b0 #b10000011101 #b0000010011010010110100100111010000000000000000000000)))
(check-sat)
(exit)
