(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b0011111101010100111111010101110100011101010000111111100100111001))
;; x should be 4563550898797934905
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.eq r (fp #b0 #b10000111100 #b1111101010100111111010101110100011101010000111111101)))
(check-sat)
(exit)
