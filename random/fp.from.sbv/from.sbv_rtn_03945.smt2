(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11100100000101100111100100000111011100111000011001111111100110100000011100110000001011001100010100000101101110001010010111010101))
;; x should be -37101698589177324010364347805556234795
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTN x)))
(assert (fp.leq r ((_ to_fp 5 11) #xFC00)))
(check-sat)
(exit)
