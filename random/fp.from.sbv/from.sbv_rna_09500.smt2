(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11001110011101110110100101110101110110111010111010101001101100100011100111111000001101111010000111111111110110010001101100110010))
;; x should be -65841377472388379951952495604037772494
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFE46225A)))
(check-sat)
(exit)
