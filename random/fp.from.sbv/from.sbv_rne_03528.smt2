(declare-const x (_ BitVec 64))
(assert (bvsle x #b1111111010100101100111010011001101011000000010101100101101110001))
;; x should be -97498973072602255
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
