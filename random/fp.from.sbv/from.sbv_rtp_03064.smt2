(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b10011000111010001011000111010010))
;; x should be -1729580590
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.geq r (fp #b1 #b10000011101 #b1001110001011101001110001011100000000000000000000000)))
(check-sat)
(exit)
