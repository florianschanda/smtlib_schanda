(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b01100001001011111111101111110101))
;; x should be 1630534645
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.geq r (fp #b0 #b10000011101 #b1000010010111111111011111101010000000000000000000000)))
(check-sat)
(exit)
