(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1110101001111010010101001110010000000010011100100011011010000100))
;; x should be -1550833783405398396
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r ((_ to_fp 11 53) #xC3B585AB1BFD8DC9)))
(check-sat)
(exit)
