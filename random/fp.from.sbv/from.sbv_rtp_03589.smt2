(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1110101011101001111100001001011010110110010101100001100001010111))
;; x should be -1519418869165975465
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.eq r (fp #b1 #b10111011 #b01010001011000001111011)))
(check-sat)
(exit)
