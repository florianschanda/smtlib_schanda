(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b0101010011100000100101011111100101010101101110010100110100011110))
;; x should be 6116053192086736158
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.eq r (fp #b0 #b10000111101 #b0101001110000010010101111110010101010110111001010011)))
(check-sat)
(exit)
