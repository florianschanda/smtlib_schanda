(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0111110110100001101100100000101001011110101010010000001011111101))
;; x should be 9052712483598959357
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.geq r (fp #b0 #b10000111101 #b1111011010000110110010000010100101111010101001000001)))
(check-sat)
(exit)
