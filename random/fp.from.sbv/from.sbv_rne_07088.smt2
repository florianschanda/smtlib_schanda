(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b01100011110000001110010011111010))
;; x should be 1673585914
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.eq r (fp #b0 #b10000011101 #b1000111100000011100100111110100000000000000000000000)))
(check-sat)
(exit)
