(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b01111001011100000101110110100100))
;; x should be 2037407140
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.eq r (fp #b0 #b10000011101 #b1110010111000001011101101001000000000000000000000000)))
(check-sat)
(exit)
