(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b00000011001100000001001000101100))
;; x should be 53482028
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.leq r (fp #b0 #b10000011000 #b1001100000001001000101100000000000000000000000000000)))
(check-sat)
(exit)
