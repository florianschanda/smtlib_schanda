(declare-const x (_ BitVec 16))
(assert (= x #b1010000000011111))
;; x should be -24545
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
(assert (not (fp.eq r ((_ to_fp 5 11) #xF5FE))))
(check-sat)
(exit)
