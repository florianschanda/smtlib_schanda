(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b00101111001100101010001110000001))
;; x should be 791847809
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.geq r (fp #b0 #b10000011100 #b0111100110010101000111000000100000000000000000000000)))
(check-sat)
(exit)
