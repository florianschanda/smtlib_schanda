(declare-const x (_ BitVec 64))
(assert (= x #b0111000000000110100111100010000000000101011101011011101011010101))
;; x should be 8072313242475936469
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
