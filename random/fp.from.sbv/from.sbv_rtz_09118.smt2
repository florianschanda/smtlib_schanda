(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01110000110100011011001010101110110110001010000011001101010000000011000110010000111111110100110111000000101101101000101100000011))
;; x should be 149962349692997243740760588092683225859
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.eq r ((_ to_fp 8 24) #x7EE1A365)))
(check-sat)
(exit)
