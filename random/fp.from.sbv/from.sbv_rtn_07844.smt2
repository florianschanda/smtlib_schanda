(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0011001001000010001100110101110101001111000011001001111000100010))
;; x should be 3621513526210502178
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.geq r (fp #b0 #b10000111100 #b1001001000010001100110101110101001111000011001001111)))
(check-sat)
(exit)
