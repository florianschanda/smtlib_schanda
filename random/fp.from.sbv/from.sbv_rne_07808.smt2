(declare-const x (_ BitVec 64))
(assert (bvsge x #b0001111110010110010001101111001011110101010110000010110101000010))
;; x should be 2276084670994591042
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
