(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b0010110011011000100101001001100100001000001001111011101101100101))
;; x should be 3231496117626059621
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.eq r (fp #b0 #b10111100 #b01100110110001001010010)))
(check-sat)
(exit)
