(declare-const x (_ BitVec 32))
(assert (= x #b11000000000000000000000000000001))
;; x should be -1073741823
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNA x)))
(assert (fp.eq r (_ -oo 5 11)))
(check-sat)
(exit)
