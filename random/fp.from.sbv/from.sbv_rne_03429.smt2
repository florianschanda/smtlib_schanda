(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1001101011010011000000100010101100001010001000110111100011001101))
;; x should be -7290480987905754931
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.eq r ((_ to_fp 11 53) #xC3D94B3F753D7722)))
(check-sat)
(exit)
