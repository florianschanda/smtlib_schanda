(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b00110010111011110101100011000010100011110000010011111101010110001001010110011100100001101010100001110111100100001000111110101010))
;; x should be 67704159005006655063233029659499204522
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7E4BBD63)))
(check-sat)
(exit)
