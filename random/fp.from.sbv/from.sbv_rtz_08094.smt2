(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0111101000011100010010011010000111010001011110001111111000100110))
;; x should be 8798988731328036390
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r (fp #b0 #b10111101 #b11101000011100010010011)))
(check-sat)
(exit)
