(declare-const x (_ BitVec 64))
(assert (= x #b1010001100101101001101010001010001101000110101010011111011010101))
;; x should be -6688631509800894763
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
