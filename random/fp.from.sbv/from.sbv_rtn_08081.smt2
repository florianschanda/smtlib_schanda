(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0101001001100011001010001000111111110000011001001101010001001010))
;; x should be 5936633332483019850
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.leq r ((_ to_fp 8 24) #x5EA4C651)))
(check-sat)
(exit)
