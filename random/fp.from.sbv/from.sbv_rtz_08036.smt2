(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0101100101111101100001100001010000011000001111011101001010001001))
;; x should be 6448457662328590985
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.leq r ((_ to_fp 11 53) #x43D65F6185060F74)))
(check-sat)
(exit)
