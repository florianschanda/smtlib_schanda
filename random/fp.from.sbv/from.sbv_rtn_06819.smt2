(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b00110101001011110010101101000101))
;; x should be 892283717
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.eq r (fp #b0 #b10000011100 #b1010100101111001010110100010100000000000000000000000)))
(check-sat)
(exit)
