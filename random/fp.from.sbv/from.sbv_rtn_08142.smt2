(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0101111010010100100101100001111101110110100101011000001100011111))
;; x should be 6815237197996065567
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.geq r ((_ to_fp 11 53) #x43D7A52587DDA560)))
(check-sat)
(exit)
