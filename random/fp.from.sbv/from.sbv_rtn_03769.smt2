(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01110101100011000010110000011000100101011000001101000011111101101001010001100110010101000101100010101110100111011101101001111010))
;; x should be 156247491440800450706479104628761680506
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.eq r (fp #b0 #b11111101 #b11010110001100001011000)))
(check-sat)
(exit)
