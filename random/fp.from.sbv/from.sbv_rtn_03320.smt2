(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0110110010000011001011101000111110000110100110000000000010001100))
;; x should be 7819144572018622604
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.leq r ((_ to_fp 8 24) #x5ED9065D)))
(check-sat)
(exit)
