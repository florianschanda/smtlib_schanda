(assert (= x #b0011100110110111100011111010110100110100110110011001011000010011))
;; x should be 4158950754978731539
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.eq r (fp #b0 #b10000111100 #b1100110110111100011111010110100110100110110011001011)))
(check-sat)
(exit)
