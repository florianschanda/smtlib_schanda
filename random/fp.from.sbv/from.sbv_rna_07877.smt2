(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0001111001100100111000010101100111010011100101101001000010011011))
;; x should be 2190123094727102619
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r ((_ to_fp 8 24) #x5DF3270B)))
(check-sat)
(exit)
