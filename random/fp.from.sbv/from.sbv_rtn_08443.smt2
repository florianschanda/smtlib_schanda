(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1110110101001111000010100001010110011110100100100110101101101100))
;; x should be -1346846675589502100
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.eq r (fp #b1 #b10111011 #b00101011000011110110000)))
(check-sat)
(exit)
