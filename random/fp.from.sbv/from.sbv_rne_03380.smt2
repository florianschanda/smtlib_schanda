(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0100000110101011101011100010100101011100111101000011001110111111))
;; x should be 4732067326159238079
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r (fp #b0 #b10111101 #b00000110101011101011100)))
(check-sat)
(exit)
