(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1111011111111001011010111100001011001010011100100100100001100111))
;; x should be -578312592776083353
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r (fp #b1 #b10000111010 #b0000000011010010100001111010011010110001101101101111)))
(check-sat)
(exit)
