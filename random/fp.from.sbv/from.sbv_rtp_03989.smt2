(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b11101010011101000011010000011000011111000101010011110111100001100111100101110100110011101011000000000111010111010110010101001011))
;; x should be -28639652846423959077936830966539655861
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.eq r ((_ to_fp 8 24) #xFDAC5E5F)))
(check-sat)
(exit)
