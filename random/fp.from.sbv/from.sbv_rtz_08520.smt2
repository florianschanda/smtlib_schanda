(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1100110111101011100001010011101110111010101111100000010111111001))
;; x should be -3608644184824740359
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.geq r ((_ to_fp 8 24) #xDE4851EB)))
(check-sat)
(exit)
