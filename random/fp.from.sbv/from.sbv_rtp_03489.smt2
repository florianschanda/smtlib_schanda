(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1000101011010010100111011011000110111110101100100000001100110111))
;; x should be -8443512964632149193
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.leq r (fp #b1 #b10111101 #b11010100101101011000100)))
(check-sat)
(exit)
