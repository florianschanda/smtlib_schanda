(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1111010111010001110010000001000001010101010100001011110010110010))
;; x should be -733585291808293710
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.eq r ((_ to_fp 11 53) #xC3A45C6FDF555E87)))
(check-sat)
(exit)
