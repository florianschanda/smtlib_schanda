(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10100110010110101000100110001100000011011111010101000110000011011110011011111000101011010001111110111100001001011001101100101110))
;; x should be -119160423116996019023331760236647834834
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
(assert (fp.geq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
