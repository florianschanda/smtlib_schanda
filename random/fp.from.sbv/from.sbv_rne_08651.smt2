(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00111101101000111111000001100001111011011010101100000110111111000111001001010101011111000111001010011100011110100100110010110000))
;; x should be 81934127667812392068955110087199444144
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.eq r (fp #b0 #b11111100 #b11101101000111111000010)))
(check-sat)
(exit)
