(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11011010110101000000010111000001001111010100100001000111010010000001111101100111101000001011010010110101100111000111010001011011))
;; x should be -49409780183768071325163814198152760229
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.geq r (fp #b1 #b10001111100 #b0010100101011111110100011111011000010101101111011101)))
(check-sat)
(exit)
