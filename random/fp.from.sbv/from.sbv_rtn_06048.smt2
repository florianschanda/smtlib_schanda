(declare-const x (_ BitVec 16))
(assert (bvsge x #b0111011101011000))
;; x should be 30552
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (not (fp.geq r (fp #b0 #b10000001101 #b1101110101100000000000000000000000000000000000000000))))
(check-sat)
(exit)
