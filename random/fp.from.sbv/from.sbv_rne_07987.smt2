(declare-const x (_ BitVec 64))
(assert (= x #b0111100001110011011001001000111011101010111010001010000011110101))
;; x should be 8679391471862325493
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNE x)))
