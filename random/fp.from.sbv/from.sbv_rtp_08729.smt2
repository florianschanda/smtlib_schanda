(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b00011111010001100100111011101001111001111011011110010101100111010110010001010100101010110100001110000010111010111111010101000001))
;; x should be 41571129209253757877593427565293073729
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.geq r ((_ to_fp 5 11) #x7C00)))
(check-sat)
(exit)
