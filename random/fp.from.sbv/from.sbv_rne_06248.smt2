(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 16)
(declare-const x (_ BitVec 16))
(assert (= x #b1010100001011100))
;; x should be -22436

(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.eq r ((_ to_fp 11 53) #xC0D5E90000000000)))
(check-sat)
(exit)
