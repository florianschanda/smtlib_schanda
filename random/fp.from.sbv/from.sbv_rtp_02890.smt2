(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b00000000010110100101101000000011))
;; x should be 5921283
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.leq r (fp #b0 #b10000010101 #b0110100101101000000011000000000000000000000000000000)))
(check-sat)
(exit)
