(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b00001010001011110100110110100110))
;; x should be 170872230
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.geq r (fp #b0 #b10000011010 #b0100010111101001101101001100000000000000000000000000)))
(check-sat)
(exit)
