(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01010011010011100110100101010001110100000001011111100110010111010101101011110011010001110010111000000011011101111100011110111010))
;; x should be 110733058940005056289930054040259381178
(declare-const r Float64)
