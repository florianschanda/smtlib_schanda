(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b00110101101100011100100000100000101011001111101011100010101111110001011111101000110111110101001011110110100101100100010100010000))
;; x should be 71372179391317860468746392626361943312
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.leq r (fp #b0 #b11111100 #b10101101100011100100001)))
(check-sat)
(exit)
