(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1000101000101110101010011010000111100111010100000101100111011011))
;; x should be -8489661734711174693
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r (fp #b1 #b10111101 #b11010111010001010101101)))
(check-sat)
(exit)
