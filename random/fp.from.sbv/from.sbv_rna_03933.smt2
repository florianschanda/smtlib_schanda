(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11111000011010001011011100110010011000011101010011000110110100111100111000001100010001101110100011101001100001001011000111000001))
;; x should be -10090109420348833778856122008314007103
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFCF2E91A)))
(check-sat)
(exit)
