(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1001110110000001010000010110000001101000101001110000110010101101))
;; x should be -7097319655430746963
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.eq r (fp #b1 #b10111101 #b10001001111110101111101)))
(check-sat)
(exit)
