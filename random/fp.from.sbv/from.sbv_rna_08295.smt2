(declare-const x (_ BitVec 64))
(assert (bvsle x #b1011011011110010111100010110110001111001010101110010000110010110))
;; x should be -5263879566248173162
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
