(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0010100011110011100010100101000110100110000101010011010011001101))
;; x should be 2950854264141198541
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.leq r (fp #b0 #b10000111100 #b0100011110011100010100101000110100110000101010011010)))
(check-sat)
(exit)
