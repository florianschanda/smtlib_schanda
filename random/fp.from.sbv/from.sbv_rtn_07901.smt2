(declare-const x (_ BitVec 64))
(assert (= x #b0011101001011011100101110111101000101000100101010000000010101011))
;; x should be 4205121228003147947
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
