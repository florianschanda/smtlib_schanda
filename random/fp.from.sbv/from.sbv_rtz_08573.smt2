(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1110110000000011110101011000100110011001100011101110010011011010))
;; x should be -1440072668864912166
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r (fp #b1 #b10000111011 #b0011111111000010101001110110011001100111000100011011)))
(check-sat)
(exit)
