(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10111110110000111100010111001000101101010110000100110111111101100110011100011001111100011110001111010010100010000110101001011111))
;; x should be -86712538297931417134495871423726720417
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.eq r ((_ to_fp 8 24) #xFE827874)))
(check-sat)
(exit)
