(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b11011011111111101111011100011001))
;; x should be -604047591
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.eq r (fp #b1 #b10000011100 #b0010000000001000010001110011100000000000000000000000)))
(check-sat)
(exit)
