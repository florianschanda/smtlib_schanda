(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0010111011100110000010011110001111011100110101010010111111111100))
;; x should be 3379399444655321084
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r (fp #b0 #b10111100 #b01110111001100000100111)))
(check-sat)
(exit)
