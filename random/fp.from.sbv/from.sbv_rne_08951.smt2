(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01111111111111111000000100101011000010011110100011101011111001111101110011111101011100110000100000100001100011100100100111011101))
;; x should be 170138611004327505994698044254158997981
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EFFFF02)))
(check-sat)
(exit)
