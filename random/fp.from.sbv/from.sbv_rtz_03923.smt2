(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11001000000001000101011101101010000110111111110010101001101011101111011010001000101001111010011000111001110101010111101010101100))
;; x should be -74414225600038859485599494798224033108
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFE5FEEA2)))
(check-sat)
(exit)
