(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11100000110110010100001001111101111111100010000110010101011001101111011100111010010110000110100001101111111000000101111010111000))
;; x should be -41407218825611302465583106717986562376
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFDF935EC)))
(check-sat)
(exit)
