(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1011111110011001110000000101110001000011100100001100111011010000))
;; x should be -4640466438525497648
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r ((_ to_fp 11 53) #xC3D0198FE8EF1BCC)))
(check-sat)
(exit)
