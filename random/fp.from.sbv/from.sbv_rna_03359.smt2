(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0110100100011001001111110010001001000110100001001100110101010110))
;; x should be 7573153664844746070
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r (fp #b0 #b10111101 #b10100100011001001111110)))
(check-sat)
(exit)
