(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01000100111001101101000111011010001011101000101000101010110110000000010111100010110010110110100110110100011001111000000010110011))
;; x should be 91585988300587225559966912026480443571
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
