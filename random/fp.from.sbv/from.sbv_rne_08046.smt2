(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0101111111100111010110101100001011011101111101010010111100111110))
;; x should be 6910591946217303870
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.leq r ((_ to_fp 8 24) #x5EBFCEB6)))
(check-sat)
(exit)
