(declare-const x (_ BitVec 64))
(assert (bvsge x #b1111011101000010000101111111011010001111110110100011001010100010))
;; x should be -629914650135612766
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
