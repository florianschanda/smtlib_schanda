(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1000101011110100110001101101101010010100010010001100101001100111))
;; x should be -8433897560065127833
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.leq r (fp #b1 #b10000111101 #b1101010000101100111001001001010110101110110111001110)))
(check-sat)
(exit)
