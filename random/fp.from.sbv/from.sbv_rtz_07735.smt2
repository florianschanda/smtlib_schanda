(declare-const x (_ BitVec 64))
(assert (bvsge x #b0011010011000000100001111000011111111100001001010100001001001100))
;; x should be 3801187103621333580
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
