(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b00000110110010101010000001110011))
;; x should be 113942643
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.leq r (fp #b0 #b10000011001 #b1011001010101000000111001100000000000000000000000000)))
(check-sat)
(exit)
