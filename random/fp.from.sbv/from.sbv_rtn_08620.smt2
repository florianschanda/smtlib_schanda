(declare-const x (_ BitVec 64))
(assert (bvsle x #b1110110101101110001110011011011100101000000110001011100100010011))
;; x should be -1338068580467951341
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
