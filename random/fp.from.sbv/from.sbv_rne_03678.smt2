(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b00111001010011011011000011111011101101000100000100010100001100110001010011000111001011101111101100010011110100101010101000010111))
;; x should be 76169392263992397592985086825023580695
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNE x)))
(assert (fp.leq r (fp #b0 #b11111 #b0000000000)))
(check-sat)
(exit)
