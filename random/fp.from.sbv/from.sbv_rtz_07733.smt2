(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0000010101101101100000100111011001100011111100111101010001001100))
;; x should be 391112187645776972
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.geq r (fp #b0 #b10111001 #b01011011011000001001110)))
(check-sat)
(exit)
