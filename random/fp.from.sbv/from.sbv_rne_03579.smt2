(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1101111100000111011010000001110101011111010000000101001010110011))
;; x should be -2375815803053256013
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.eq r (fp #b1 #b10000111100 #b0000011111000100101111110001010100000101111111010111)))
(check-sat)
(exit)
