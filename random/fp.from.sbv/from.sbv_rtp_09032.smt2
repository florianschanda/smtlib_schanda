(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01110001011001111000001000000010100001111001100010100111100111111000000010101010110010100010100011110001100000001111001101100010))
;; x should be 150740207013794405257522027186408715106
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EE2CF05)))
(check-sat)
(exit)
