(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0010101001111000011000000101100001000011011010100101011111100010))
;; x should be 3060301879002683362
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r (fp #b0 #b10111100 #b01010011110000110000001)))
(check-sat)
(exit)
