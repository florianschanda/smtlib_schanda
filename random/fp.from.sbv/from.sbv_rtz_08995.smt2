(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01010010001010111101010000001111110011001000000011010011101110000110110001011110010110101101100011101010111101001110001010101101))
;; x should be 109224265541829195364036571309100294829
(declare-const r Float16)
