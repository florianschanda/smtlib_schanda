(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10110111101111010110000000010010010101101010001100110100011111000110100001000011101001110111010001111000001111101000110010100110))
;; x should be -96050351021793886871867608424924672858
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFE908540)))
(check-sat)
(exit)
