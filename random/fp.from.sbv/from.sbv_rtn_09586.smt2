(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11000011001000110110000100101011100111100000000001101000111010110011010011001110010101100011111100011110100010111011011110011111))
;; x should be -80899206503389479938059353712181397601
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.geq r (fp #b1 #b10001111100 #b1110011011100100111101101010001100001111111111001100)))
(check-sat)
(exit)
