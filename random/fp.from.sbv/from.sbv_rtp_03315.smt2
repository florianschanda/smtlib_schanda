(declare-const x (_ BitVec 64))
(assert (= x #b0110010001101010100000110100110100010011101101011000110100111000))
;; x should be 7235740118390508856
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
