(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00000101010000100100001011100011001011000011110101111101001000001011100001110000100010010001101111111001010100111011010000000101))
;; x should be 6990188209106285542786780033707127813
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNE x)))
(assert (fp.eq r (fp #b0 #b11111 #b0000000000)))
(check-sat)
(exit)
