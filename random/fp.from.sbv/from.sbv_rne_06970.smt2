(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b01101111111011110010100000000100))
;; x should be 1877944324
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.geq r (fp #b0 #b10000011101 #b1011111110111100101000000001000000000000000000000000)))
(check-sat)
(exit)
