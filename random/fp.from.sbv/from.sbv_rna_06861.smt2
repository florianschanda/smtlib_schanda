(declare-const x (_ BitVec 32))
(assert (bvsle x #b00000100010111110000100011011110))
;; x should be 73337054
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNA x)))
(assert (fp.leq r (fp #b0 #b10000011001 #b0001011111000010001101111000000000000000000000000000)))
(check-sat)
(exit)
