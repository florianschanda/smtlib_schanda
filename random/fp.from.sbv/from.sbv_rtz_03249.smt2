(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0010100101010110111110101010010110110110000001010001000001101101))
;; x should be 2978843793182494829
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.geq r (fp #b0 #b10111100 #b01001010101101111101010)))
(check-sat)
(exit)
