(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1100001111010111100001110000011101110011000110101110111011001101))
;; x should be -4334847650255147315
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.eq r (fp #b1 #b10111100 #b11100001010000111100100)))
(check-sat)
(exit)
