(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01000110001000110010100001100010000000110100110111000000110110110000000110110001100101011101001111110000101111000001100001111100))
;; x should be 93228509156759355586885171817830160508
(declare-const r Float16)
