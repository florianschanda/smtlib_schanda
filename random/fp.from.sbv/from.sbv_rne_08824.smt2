(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00100111101001101100100101000110110001011011000001001101011100001111010000100111111001001110101011111000000011100101010001110011))
;; x should be 52705895485611893998748335346031678579
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.eq r ((_ to_fp 8 24) #x7E1E9B25)))
(check-sat)
(exit)
