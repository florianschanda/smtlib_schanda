(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b00010111010010001101010001010000110010010110001111101110110110101101000111010001100011010101001011101011000110010101010001110111))
;; x should be 30950395548283846551283001380491056247
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7DBA46A3)))
(check-sat)
(exit)
