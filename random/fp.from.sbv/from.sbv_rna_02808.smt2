(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b00100100110110100100001000000000))
;; x should be 618283520
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNA x)))
(assert (fp.leq r (fp #b0 #b10000011100 #b0010011011010010000100000000000000000000000000000000)))
(check-sat)
(exit)
