(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0100000000001110010000111111100100010110000001001001111100111010))
;; x should be 4615701405196656442
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r ((_ to_fp 8 24) #x5E801C87)))
(check-sat)
(exit)
