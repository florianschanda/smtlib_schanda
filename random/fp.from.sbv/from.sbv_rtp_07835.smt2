(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0011100010110000101011011111100111100000111101011001010010100110))
;; x should be 4084956150757692582
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.leq r (fp #b0 #b10000111100 #b1100010110000101011011111100111100000111101011001011)))
(check-sat)
(exit)
