(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0000000111011110110111001110110001000110001001100110101100100011))
;; x should be 134787946215009059
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.leq r (fp #b0 #b10000110111 #b1101111011011100111011000100011000100110011010110010)))
(check-sat)
(exit)
