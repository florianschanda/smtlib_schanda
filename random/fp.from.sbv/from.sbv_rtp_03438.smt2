(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1010010000001010101101111001010111101101010001010000111101000101))
;; x should be -6626282047163527355
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3D6FD521A84AEBC)))
(check-sat)
(exit)
