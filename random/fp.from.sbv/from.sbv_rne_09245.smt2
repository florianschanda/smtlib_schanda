(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10111110100000111011100111100011101011011010101010100000000110010000010111000001110000001010000111001001110101110001110100010110))
;; x should be -87045086549019638766632837706546733802
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNE x)))
(assert (fp.leq r (_ -oo 5 11)))
(check-sat)
(exit)
