(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0011011010100010100001010000101101101101001001110101010111001101))
;; x should be 3936855308397663693
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r ((_ to_fp 8 24) #x5E5A8A15)))
(check-sat)
(exit)
