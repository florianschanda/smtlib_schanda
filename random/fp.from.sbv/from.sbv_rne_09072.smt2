(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01100111001001010011011111100001011011010001101111011111010100010000101000011110011001011110000101010011001100110010001110110011))
;; x should be 137103731942244451732994320807459234739
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.geq r ((_ to_fp 11 53) #x47D9C94DF85B46F8)))
(check-sat)
(exit)
