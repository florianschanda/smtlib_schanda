(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b0010101110111110100111011011101000001000000010001111111100010111))
;; x should be 3152130211530211095
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.eq r (fp #b0 #b10111100 #b01011101111101001110110)))
(check-sat)
(exit)
