(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0001011111111111101101110101011111000001100010011011100110111000))
;; x should be 1729302369470626232
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r ((_ to_fp 8 24) #x5DBFFDBA)))
(check-sat)
(exit)
