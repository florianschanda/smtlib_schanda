(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1101111000100110001011001001111111000101010011001100000001001110))
;; x should be -2439213083452981170
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNA x)))
(assert (fp.geq r (fp #b1 #b10000111100 #b0000111011001110100110110000000111010101100110100000)))
(check-sat)
(exit)
