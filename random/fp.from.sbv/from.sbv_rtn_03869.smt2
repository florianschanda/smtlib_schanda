(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10111011101110110100011100111111100010001000000001011001100001101001001100111010101110001010001000010100110101111000110011111010))
;; x should be -90744327111911958182870837662734381830
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r (fp #b1 #b11111101 #b00010001000100101110010)))
(check-sat)
(exit)
