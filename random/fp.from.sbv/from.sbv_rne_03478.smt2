(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1011001010101001011001000001011000001000011010001111100000111011))
;; x should be -5572813018101123013
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.geq r (fp #b1 #b10000111101 #b0011010101011010011011111010011111011110010111000010)))
(check-sat)
(exit)
