(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1101011111011011100100101010100001011011010011101101101000010111))
;; x should be -2892557083871356393
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.eq r (fp #b1 #b10111100 #b01000001001000110110110)))
(check-sat)
(exit)
