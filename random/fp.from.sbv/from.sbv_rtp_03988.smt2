(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11010001011110111110101100111100010000000010111101111001010110101111011000111011101100001100010101010010000011010000011110111111))
;; x should be -61830292148480220003377462218977376321
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.geq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
