(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1100110100100101100001110001100100000110111011110101010100001010))
;; x should be -3664374180235750134
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3C96D3C737C8855)))
(check-sat)
(exit)
