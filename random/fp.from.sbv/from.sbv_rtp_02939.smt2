(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b01001001011011101100110011000001))
;; x should be 1231998145
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.leq r (fp #b0 #b10000011101 #b0010010110111011001100110000010000000000000000000000)))
(check-sat)
(exit)
