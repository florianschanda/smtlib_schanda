(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1000001000111110001011011101000101001001101111101110010111100010))
;; x should be -9061755023314197022
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.eq r (fp #b1 #b10111101 #b11110111000001110100101)))
(check-sat)
(exit)
