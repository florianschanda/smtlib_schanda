(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1110001100010100110001011000000010110101101110010001011100001011))
;; x should be -2083823570970405109
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3BCEB3A7F4A46E9)))
(check-sat)
(exit)
