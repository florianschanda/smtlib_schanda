(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b10011110111101100011110101000110))
;; x should be -1628029626
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.eq r (fp #b1 #b10000011101 #b1000010000100111000010101110100000000000000000000000)))
(check-sat)
(exit)
