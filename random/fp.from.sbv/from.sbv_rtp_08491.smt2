(declare-const x (_ BitVec 64))
(assert (bvsge x #b1110100010111011110111011011101011100010000001111111001000010001))
;; x should be -1676502641539550703
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
