(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01001111001110001011000111100111011010010011001011011110010011010111110110001111000000010110110001100101111111110010101100101100))
;; x should be 105303388611849113564353394481913867052
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r ((_ to_fp 8 24) #x7E9E7163)))
(check-sat)
(exit)
