(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10100001101101010001010010010001100011100010010000100011010001010111110011101100101101100101000001110011000101111101000001111111))
;; x should be -125336436687906007234569082737544146817
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNE x)))
(assert (fp.leq r (_ -oo 5 11)))
(check-sat)
(exit)
