(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10000111110001011010000101100000010100010101010101010111011110011011010011110011111010011001000011101011000110001011100110010111))
;; x should be -159810431909822212709050223250045879913
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.geq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
