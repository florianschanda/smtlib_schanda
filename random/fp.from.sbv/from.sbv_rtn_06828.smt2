;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b00000111011100010001000010101001))
;; x should be 124850345

(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTN x)))
