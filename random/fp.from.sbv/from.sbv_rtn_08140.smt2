(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0100110100010011111011001101000001101010010000101011000000111011))
;; x should be 5554043145358061627
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r ((_ to_fp 8 24) #x5E9A27D9)))
(check-sat)
(exit)
