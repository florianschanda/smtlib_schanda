(declare-const x (_ BitVec 64))
(assert (bvsle x #b1111010001001000011100110000011010011101101010010001111100001001))
;; x should be -844298457879863543
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
