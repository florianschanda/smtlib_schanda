(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1001101000010010010011101101111101101000011010100111110101111011))
;; x should be -7344721320851374725
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.leq r ((_ to_fp 8 24) #xDECBDB62)))
(check-sat)
(exit)
