(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01011011011100101100110110110111100111000000000100110011100111111100100110011001000110110011110110001101001111100001111101000010))
;; x should be 121555841899303917732908659318301007682
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EB6E59B)))
(check-sat)
(exit)
