(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b11011100110001111001001001101010))
;; x should be -590900630

(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.eq r (fp #b1 #b10000011100 #b0001100111000011011011001011000000000000000000000000)))
(check-sat)
(exit)
