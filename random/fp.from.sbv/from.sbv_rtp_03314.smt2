(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0100111110011001011100100010110010010100010011010001001001010100))
;; x should be 5735741136225243732
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.leq r (fp #b0 #b10000111101 #b0011111001100101110010001011001001010001001101000101)))
(check-sat)
(exit)
