(declare-const x (_ BitVec 64))
(assert (= x #b0110001010110101000001100010011001110111010000100101001010010010))
;; x should be 7112597948780925586
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
