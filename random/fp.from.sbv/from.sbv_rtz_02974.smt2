(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b01000010000110011101101101010010))
;; x should be 1108990802
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.eq r (fp #b0 #b10000011101 #b0000100001100111011011010100100000000000000000000000)))
(check-sat)
(exit)
