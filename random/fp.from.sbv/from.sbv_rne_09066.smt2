(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01101110101000100010100010011000011001111010110000100010100110001101111100111110111000000100010111101001100101000001110000110011))
;; x should be 147057054998573290963261305723284626483

(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EDD4451)))
(check-sat)
(exit)
