(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11000110110001001110010000011010011000100110100100101011101000011011010101101001011101011001010101000001011011000100100001111010))
;; x should be -76072907091473762243191374072614467462
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r (fp #b1 #b11111100 #b11001001110110001101111)))
(check-sat)
(exit)
