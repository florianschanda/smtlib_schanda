(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10101011110100000110100000010101110110010001011111000000100000011101001011100001101000101110000110010011110010011100010011011110))
;; x should be -111902270793565450962540758336630307618
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
(assert (fp.geq r (fp #b1 #b11110 #b1111111111)))
(check-sat)
(exit)
