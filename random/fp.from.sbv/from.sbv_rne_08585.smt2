(declare-const x (_ BitVec 64))
(assert (bvsle x #b1111010001001001000110000011010100111001011001111011110000110111))
;; x should be -844116838279824329
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
