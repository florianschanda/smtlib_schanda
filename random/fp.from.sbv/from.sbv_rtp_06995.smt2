(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b01000100110000101010010111110100))
;; x should be 1153607156
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.eq r (fp #b0 #b10000011101 #b0001001100001010100101111101000000000000000000000000)))
(check-sat)
(exit)
