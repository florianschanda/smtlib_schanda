(set-logic QF_FPBV)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b11000100011101010001000011011110))
;; x should be -998960930
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (not (fp.geq r (fp #b1 #b10000011100 #b1101110001010111011110010001000000000000000000000000))))
(check-sat)
(exit)
