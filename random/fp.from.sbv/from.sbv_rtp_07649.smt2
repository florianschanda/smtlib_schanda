(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b11011010111010100100111000011111))
;; x should be -622178785

(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.geq r (fp #b1 #b10000011100 #b0010100010101101100011110000100000000000000000000000)))
(check-sat)
(exit)
