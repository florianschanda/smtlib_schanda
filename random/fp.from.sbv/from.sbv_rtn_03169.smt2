(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b11010100100010010000001111000110))
;; x should be -729218106
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.eq r (fp #b1 #b10000011100 #b0101101110110111111000011101000000000000000000000000)))
(check-sat)
(exit)
