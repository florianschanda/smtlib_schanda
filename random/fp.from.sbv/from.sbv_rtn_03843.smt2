(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10111010011101101001000001001101011011111100000111100111111111001111111111010010000011010100000001011000000100001011100011110001))
;; x should be -92430341873498307697179066228899006223
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r (fp #b1 #b11111101 #b00010110001001011100000)))
(check-sat)
(exit)
