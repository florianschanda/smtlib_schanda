(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01001110111110011011100001101010001110110111100000100000100111100100001010100101011001101001011001011010001100010011111000011000))
;; x should be 104976405968955749298307322901825666584
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNA x)))
(assert (fp.leq r ((_ to_fp 5 11) #x7C00)))
(check-sat)
(exit)
