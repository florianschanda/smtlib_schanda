(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b10100011110111001001000000001111))
;; x should be -1545826289
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.leq r (fp #b1 #b10000011101 #b0111000010001101101111111100010000000000000000000000)))
(check-sat)
(exit)
