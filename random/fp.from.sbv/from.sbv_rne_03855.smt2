(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10001101101110010101100001000010000101001111101001111001000010101011000110000101101011110001011010110001010011011001111011110110))
;; x should be -151898854508840031729281832118366920970
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.leq r (fp #b1 #b11111101 #b11001001000110101001111)))
(check-sat)
(exit)
