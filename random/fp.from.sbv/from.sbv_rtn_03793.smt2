(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01100100011011101001000110011101010001011101010010011111111100011101010100111011100110001111000000101001111110100100011000001111))
;; x should be 133496905642755975829963787037692282383
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTN x)))
(assert (fp.leq r ((_ to_fp 5 11) #x7BFF)))
(check-sat)
(exit)
