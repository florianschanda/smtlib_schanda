(declare-const x (_ BitVec 64))
(assert (= x #b1110011011100000111100010110100000001100011101000011101111111001))
;; x should be -1810181621015102471
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
