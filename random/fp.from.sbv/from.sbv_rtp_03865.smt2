(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10101001001111011100101111001100011000100000001101100010111111111111110010110100000101101110001011010101101111010101011001000110))
;; x should be -115321972002888737004274998592741222842
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.leq r (fp #b1 #b11110 #b1111111111)))
(check-sat)
(exit)
