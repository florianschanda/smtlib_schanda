(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b10001011000111110000000001101111))
;; x should be -1960902545
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r (fp #b1 #b10000011101 #b1101001110000011111111100100010000000000000000000000)))
(check-sat)
(exit)
