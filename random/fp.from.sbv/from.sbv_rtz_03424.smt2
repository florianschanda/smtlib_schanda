(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1000001011100100111111001010000000010001101001101011011111101100))
;; x should be -9014802789667784724
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r ((_ to_fp 11 53) #xC3DF46C0D7FB9652)))
(check-sat)
(exit)
