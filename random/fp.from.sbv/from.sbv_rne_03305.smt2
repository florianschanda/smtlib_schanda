(declare-const x (_ BitVec 64))
(assert (bvsle x #b0111001100000110101101010101000001101100101111100110111000001100))
;; x should be 8288511521248407052
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
