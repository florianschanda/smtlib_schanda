(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11100000100010111010001110010001100011011011110110011011101000111111011011001010110001011001110001010100110011000010101001100001))
;; x should be -41810249037065398745823840705644320159
(declare-const r Float64)
