(set-logic QF_FPBV)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b11111010110111100110100110000100))
;; x should be -86087292
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (not (fp.leq r (fp #b1 #b10000011001 #b0100100001100101100111110000000000000000000000000000))))
(check-sat)
(exit)
