(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1001001000100110100111011101000110001110110000011000011000000000))
;; x should be -7915465771688294912
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r (fp #b1 #b10111101 #b10110111011001011000100)))
(check-sat)
(exit)
