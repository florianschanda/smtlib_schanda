(declare-const x (_ BitVec 64))
(assert (= x #b1101011011101000111101110010101101010001101111100101100110010101))
;; x should be -2960844989569017451
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
