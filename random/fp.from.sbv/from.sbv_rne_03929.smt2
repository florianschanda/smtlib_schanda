(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11000000001001010000110100001101011011011101010000101001100000100000000101111011001011110001100010111001101100010110011010001101))
;; x should be -84878212011187512726941951550274050419
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFE7F6BCC)))
(check-sat)
(exit)
