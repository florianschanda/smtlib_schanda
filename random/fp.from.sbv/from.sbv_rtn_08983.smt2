(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01101110101010110001000100101010100000000000010100110111100011101111000111000111010010111111000000101101111110000001000000001001))
;; x should be 147103310467316677474643312953582424073
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.eq r ((_ to_fp 8 24) #x7EDD5622)))
(check-sat)
(exit)
