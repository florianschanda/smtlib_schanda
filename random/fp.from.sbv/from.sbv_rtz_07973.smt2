(declare-const x (_ BitVec 64))
(assert (= x #b0101100100011010001111111011000111010011111001001010010001100000))
;; x should be 6420514251766801504
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
