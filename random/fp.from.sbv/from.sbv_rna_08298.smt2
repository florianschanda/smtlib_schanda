(declare-const x (_ BitVec 64))
(assert (= x #b1000101100000101001000011001100000101011001110000101100001100010))
;; x should be -8429294190110156702
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
