(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b00100100001101010000101010111100))
;; x should be 607455932
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.leq r (fp #b0 #b10000011100 #b0010000110101000010101011110000000000000000000000000)))
(check-sat)
(exit)
