(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11001011100001011110110001101100010110010111101111101000100001011001100101011011001110010001000000101001101010110011111111011100))
;; x should be -69753713061413434051134919610702544932
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
