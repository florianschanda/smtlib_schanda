(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10100001101111000011100010011010000101011000011010101001110111110101100000011111000111010000110111011110101100110110011111110011))
;; x should be -125299359767425645175460240859973064717
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNE x)))
(assert (fp.geq r (fp #b1 #b11111 #b0000000000)))
(check-sat)
(exit)
