(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0111100101001100100100011101111001010010101111010101010110010010))
;; x should be 8740521360876197266
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.leq r ((_ to_fp 11 53) #x43DE53247794AF56)))
(check-sat)
(exit)
