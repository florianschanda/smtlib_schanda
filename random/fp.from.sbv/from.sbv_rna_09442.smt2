(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11100111010000011111010101010100111100100001001010001111101100000011011111001101101111000001010001101010111100001000100010010001))
;; x should be -32888224678381775578708815184472799087
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r (fp #b1 #b11111011 #b10001011111000001010101)))
(check-sat)
(exit)
