(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsle x #b00101010011011000001101110101001))
;; x should be 711728041

(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
