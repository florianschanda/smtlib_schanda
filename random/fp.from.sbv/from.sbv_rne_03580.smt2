(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1100001111010110110011100001001000101111111010111001001010111011))
;; x should be -4335051013788822853
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3CE1498F6E80A37)))
(check-sat)
(exit)
