(declare-const x (_ BitVec 64))
(assert (bvsle x #b1011101001100011111101011111110000101010101111000010110000111001))
;; x should be -5015895096563061703
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
