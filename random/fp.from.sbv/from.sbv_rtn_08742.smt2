(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b00000101011000010000010011010000100101010011110010110101100011101011000110011000001000000000001110000001011000010100111111001101))
;; x should be 7149890429485334611275773095098535885
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.leq r ((_ to_fp 8 24) #x7CAC209A)))
(check-sat)
(exit)
