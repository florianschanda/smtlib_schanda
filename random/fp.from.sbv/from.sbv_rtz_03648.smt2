(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00010111010010100011101110111001101010000101111000101010110110000011011011010101101010000001011111000000110001111110100011000101))
;; x should be 30957685242068645913626283010874140869
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.eq r (fp #b0 #b11111011 #b01110100101000111011101)))
(check-sat)
(exit)
