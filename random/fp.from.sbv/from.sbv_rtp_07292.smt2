(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b10101110101010010101010001100101))
;; x should be -1364634523
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.geq r (fp #b1 #b10000011101 #b0100010101011010101011100110110000000000000000000000)))
(check-sat)
(exit)
