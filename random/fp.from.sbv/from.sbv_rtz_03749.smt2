(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01100001101000100100011000001110100101011001011010111100000100101111001110111111100000001000010101001111100011000101111100110001))
;; x should be 129777688606381506054948308687824641841
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r (fp #b0 #b11111101 #b10000110100010010001100)))
(check-sat)
(exit)
