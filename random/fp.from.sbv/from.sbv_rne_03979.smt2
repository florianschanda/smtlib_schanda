(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11101010011111101010011000110100001011100111100001001000010011111010100101100011010001111111110101010110111010100100110010001011))
;; x should be -28585415488852380973632127012357649269
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNE x)))
(assert (fp.geq r (_ -oo 5 11)))
(check-sat)
(exit)
