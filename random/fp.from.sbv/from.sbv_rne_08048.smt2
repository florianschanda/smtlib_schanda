(declare-const x (_ BitVec 64))
(assert (= x #b0101010101101011111110010010110101111001000110100100110110001010))
;; x should be 6155287289432526218
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
