(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1010100111111101100011011011000110101101100000000011010110101100))
;; x should be -6197641717932345940
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r (fp #b1 #b10111101 #b01011000000010011100101)))
(check-sat)
(exit)
