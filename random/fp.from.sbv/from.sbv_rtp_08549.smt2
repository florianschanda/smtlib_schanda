(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1100110110100000100010111011010101001011010010111000001111010010))
;; x should be -3629747688892038190
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3C92FBA255A5A3E)))
(check-sat)
(exit)
