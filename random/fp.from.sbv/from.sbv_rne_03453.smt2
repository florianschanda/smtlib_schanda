(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1000100010001100101101000001001100000110110111010011100111010110))
;; x should be -8607306793999320618
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.leq r (fp #b1 #b10111101 #b11011101110011010011000)))
(check-sat)
(exit)
