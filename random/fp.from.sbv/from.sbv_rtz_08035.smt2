(declare-const x (_ BitVec 64))
(assert (bvsle x #b0101110111100010110100101100010010111000110001110001000110011101))
;; x should be 6765201332619383197
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
