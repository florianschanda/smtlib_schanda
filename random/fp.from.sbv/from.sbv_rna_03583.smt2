(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1101000010110000111011000111110001111100011001010101110010000011))
;; x should be -3408964898512348029
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r (fp #b1 #b10111100 #b01111010011110001001110)))
(check-sat)
(exit)
