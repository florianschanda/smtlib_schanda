(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b01100010101011000011011110000101))
;; x should be 1655453573
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.eq r (fp #b0 #b10000011101 #b1000101010110000110111100001010000000000000000000000)))
(check-sat)
(exit)
