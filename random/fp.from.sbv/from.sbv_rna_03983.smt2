(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11001100000101101010000000101111111011010001011000100011001100110000010100101111000100100000001100000100001100100100011111111111))
;; x should be -69002376267292926825803909333917743105
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFE4FA57F)))
(check-sat)
(exit)
