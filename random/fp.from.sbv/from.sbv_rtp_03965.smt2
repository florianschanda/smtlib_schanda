(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11110101000010001001101011111100100010101000111001001110100110001111100010000101010101011110000111000100111111011000000111000010))
;; x should be -14576826079308910040925798458200325694
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.leq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
