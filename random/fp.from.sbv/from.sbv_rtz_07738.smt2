(declare-const x (_ BitVec 64))
(assert (= x #b0011010010110010111100111001010010010100000111100011010111101100))
;; x should be 3797365255292466668
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.eq r (fp #b0 #b10000111100 #b1010010110010111100111001010010010100000111100011010)))
(check-sat)
(exit)
