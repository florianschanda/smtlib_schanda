(declare-const x (_ BitVec 64))
(assert (bvsle x #b1010100100001100100011010111011100100001100010100111010111110011))
;; x should be -6265477438775855629
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
