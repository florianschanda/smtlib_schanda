(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10111000000000100111000110010010000010100100001000100111100011101101101011000110000000010010000101100111101010101100010000111001))
;; x should be -95691727620025107462663057266374425543
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFE8FFB1C)))
(check-sat)
(exit)
