(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.sbv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1000101111110000110101100110011101110111010010111001001011111100))
;; x should be -8362948768155593988
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.eq r (fp #b1 #b10000111101 #b1101000000111100101001100110001000100010110100011011)))
(check-sat)
(exit)
