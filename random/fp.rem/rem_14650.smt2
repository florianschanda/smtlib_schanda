(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01010011110011010001001)))
;; x should be Float32(0xCAA9E689 [Rational(-11134601, 2), -5567300.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01010111011010101110001)))
;; y should be Float32(0xCAABB571 [Rational(-11253105, 2), -5626552.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b0 #b10001110 #b11001110111010000000000)))
(check-sat)
(exit)
