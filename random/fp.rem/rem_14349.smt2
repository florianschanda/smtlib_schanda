(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10110111111111101001010)))
;; x should be Float32(0x4A5BFF4A [Rational(7208869, 2), 3604434.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01110011 #b11110101111010110101101)))
;; y should be Float32(0xB9FAF5AD [Rational(-16446893, 34359738368), -0.000479])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result ((_ to_fp 8 24) #x395901F2))))
(check-sat)
(exit)
