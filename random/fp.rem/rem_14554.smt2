(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4C6521D4)))
;; x should be Float32(0x4C6521D4 [Rational(60065616), 60065616.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x8B0F1A6C)))
;; y should be Float32(0x8B0F1A6C [Rational(-2344603, 85070591730234615865843651857942052864), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b1 #b00010100 #b00110111011000101000000)))
(check-sat)
(exit)
