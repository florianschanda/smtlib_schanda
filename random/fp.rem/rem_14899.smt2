(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x801BB67D)))
;; x should be Float32(0x801BB67D [Rational(-1816189, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01001101 #b11110001001100011000011)))
;; y should be Float32(0xA6F898C3 [Rational(-16292035, 9444732965739290427392), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b00000000 #b00110111011011001111101))))
(check-sat)
(exit)
