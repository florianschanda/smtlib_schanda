(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11000011 #b11001100001010001100000)))
(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11111101000111100100100)))
(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b0 #b00000000 #b00001100111010111100100)))
(check-sat)
(exit)
