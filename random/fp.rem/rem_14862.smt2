(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11001000 #b10111010001101000100011)))
;; x should be Float32(0xE45D1A23 [Rational(-16314455157435927625728), -16314455157435927625728.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00111001 #b00110101111000011100101)))
;; y should be Float32(0x9C9AF0E5 [Rational(-10154213, 9903520314283042199192993792), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x1B8B99E8)))
(check-sat)
(exit)
