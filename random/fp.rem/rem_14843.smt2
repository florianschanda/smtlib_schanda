(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x93C02110)))
;; x should be Float32(0x93C02110 [Rational(-786961, 162259276829213363391578010288128), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010000 #b11100011110010000101101)))
;; y should be Float32(0x6871E42D [Rational(4569197690448487924105216), 4569197690448487924105216.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b00100111 #b10000000010000100010000))))
(check-sat)
(exit)
