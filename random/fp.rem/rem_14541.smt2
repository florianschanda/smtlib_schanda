(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01110111 #b01011101111111011101111)))
;; x should be Float32(0x3BAEFEEF [Rational(11468527, 2147483648), 0.005340])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9F684A4)))
;; y should be Float32(0xC9F684A4 [Rational(-4038953, 2), -2019476.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b0 #b01110111 #b01011101111111011101111))))
(check-sat)
(exit)
