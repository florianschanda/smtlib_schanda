(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01000100 #b00101100010010111000110)))
;; x should be Float32(0xA21625C6 [Rational(-4920035, 2417851639229258349412352), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b11010010100110000101001)))
;; y should be Float32(0x80694C29 [Rational(-6900777, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b0 #b00000000 #b01000001010110010101111))))
(check-sat)
(exit)
