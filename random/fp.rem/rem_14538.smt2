(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b01111000 #b11100100000111100110010)))
;; x should be Float32(0x3C720F32 [Rational(7931801, 536870912), 0.014774])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0077B39A)))
;; y should be Float32(0x0077B39A [Rational(3922381, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result ((_ to_fp 8 24) #x001DD4C8))))
(check-sat)
(exit)
