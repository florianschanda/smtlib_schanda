(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80421D45)))
;; x should be Float32(0x80421D45 [Rational(-4332869, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b01000011111010111010011)))
;; y should be Float32(0xCAA1F5D3 [Rational(-10614227, 2), -5307113.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b00000000 #b10000100001110101000101))))
(check-sat)
(exit)
