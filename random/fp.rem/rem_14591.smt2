(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11110100001100010111011)))
;; x should be Float32(0x007A18BB [Rational(8001723, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11111101 #b11110001111010100110011)))
;; y should be Float32(0xFEF8F533 [Rational(-165460845043944977742162871929999982592), -165460845043944977742162871929999982592.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b0 #b00000000 #b11110100001100010111011)))
(check-sat)
(exit)
