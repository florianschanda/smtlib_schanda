(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x80722239)))
;; x should be Float32(0x80722239 [Rational(-7479865, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00101111 #b00000010111000010001111)))
;; y should be Float32(0x1781708F [Rational(8482959, 10141204801825835211973625643008), 0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b1 #b00000000 #b11100100010001000111001)))
(check-sat)
(exit)
