(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x61E06445)))
;; x should be Float32(0x61E06445 [Rational(517411981711015673856), 517411981711015673856.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AED4D0D)))
;; y should be Float32(0x4AED4D0D [Rational(15551757, 2), 7775878.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x4A69537A)))
(check-sat)
(exit)
