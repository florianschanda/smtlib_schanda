(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b11110100100101000010100)))
;; x should be Float32(0xC9FA4A14 [Rational(-4100741, 2), -2050370.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA81A3F1)))
;; y should be Float32(0xCA81A3F1 [Rational(-8496113, 2), -4248056.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #xC9FA4A14)))
(check-sat)
(exit)
