(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAF2BB13)))
;; x should be Float32(0xCAF2BB13 [Rational(-15907603, 2), -7953801.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x800979A0)))
;; y should be Float32(0x800979A0 [Rational(-19405, 22300745198530623141535718272648361505980416), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b00000000 #b00000100000011111100000))))
(check-sat)
(exit)
