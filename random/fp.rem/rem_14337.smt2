(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b01101000011000001111111)))
;; x should be Float32(0x4AB4307F [Rational(11808895, 2), 5904447.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10010101 #b11110100010111111010001)))
;; y should be Float32(0xCAFA2FD1 [Rational(-16396241, 2), -8198120.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b1 #b10010100 #b00010111111111010100100)))
(check-sat)
(exit)
