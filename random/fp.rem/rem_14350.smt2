(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A648B76)))
;; x should be Float32(0x4A648B76 [Rational(7488955, 2), 3744477.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00110010 #b11111100001101100000011)))
;; y should be Float32(0x997E1B03 [Rational(-16653059, 1267650600228229401496703205376), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b00110001 #b10000011100110000001110))))
(check-sat)
(exit)
