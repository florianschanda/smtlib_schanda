(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b11110100 #b10000101011101100100000)))
;; x should be Float32(0xFA42BB20 [Rational(-252775234113209969690749354316398592), -252775234113209969690749354316398592.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xB250A273)))
;; y should be Float32(0xB250A273 [Rational(-13673075, 1125899906842624), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result ((_ to_fp 8 24) #xB0A5CA70))))
(check-sat)
(exit)
