(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A0F14FE)))
;; x should be Float32(0x4A0F14FE [Rational(4688511, 2), 2344255.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x006A29E6)))
;; y should be Float32(0x006A29E6 [Rational(3478771, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b0 #b00000000 #b00010110101010110000100))))
(check-sat)
(exit)
