(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x8002EC68)))
;; x should be Float32(0x8002EC68 [Rational(-23949, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b1 #b11110101 #b00111000111011000011000)))
;; y should be Float32(0xFA9C7618 [Rational(-406196767870282174752442724032446464), -406196767870282174752442724032446464.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b1 #b00000000 #b00000101110110001101000)))
(check-sat)
(exit)
