(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x56F7B3C7)))
;; x should be Float32(0x56F7B3C7 [Rational(136175754936320), 136175754936320.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x807874B9)))
;; y should be Float32(0x807874B9 [Rational(-7894201, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x0005079B)))
(check-sat)
(exit)
