(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x394D494F)))
;; x should be Float32(0x394D494F [Rational(13453647, 68719476736), 0.000196])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x310C6C51)))
;; y should be Float32(0x310C6C51 [Rational(9202769, 4503599627370496), 0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #xAF26A400)))
(check-sat)
(exit)
