(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11010111101111100011000)))
;; x should be Float32(0x806BDF18 [Rational(-883683, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b00011011011011000110000)))
