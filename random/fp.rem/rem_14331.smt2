(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10001110 #b00010100011101010000000)))
;; x should be Float32(0x470A3A80 [Rational(70773, 2), 35386.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11111010 #b01001000100000010111110)))
;; y should be Float32(0x7D2440BE [Rational(13645596997843570371364428726799433728), 13645596997843570371364428726799433728.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result ((_ to_fp 8 24) #x470A3A80))))
(check-sat)
(exit)
