(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x5650E5A8)))
;; x should be Float32(0x5650E5A8 [Rational(57421196165120), 57421196165120.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xCA1B097A)))
;; y should be Float32(0xCA1B097A [Rational(-5080253, 2), -2540126.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x49412720)))
(check-sat)
(exit)
