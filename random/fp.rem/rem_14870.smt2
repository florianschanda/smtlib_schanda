(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x802BA5FA)))
;; x should be Float32(0x802BA5FA [Rational(-1430269, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010100 #b01010011000111110101010)))
;; y should be Float32(0x4A298FAA [Rational(5556181, 2), 2778090.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b1 #b00000000 #b01010111010010111111010)))
(check-sat)
(exit)
