(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AE749F1)))
;; x should be Float32(0x4AE749F1 [Rational(15157745, 2), 7578872.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x80337F5B)))
;; y should be Float32(0x80337F5B [Rational(-3374939, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b0 #b00000000 #b00101010011100111011011)))
(check-sat)
(exit)
