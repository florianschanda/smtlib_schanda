(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABC9F17)))
;; x should be Float32(0x4ABC9F17 [Rational(12361495, 2), 6180747.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b00000000 #b00101011000111010000001)))
;; y should be Float32(0x80158E81 [Rational(-1412737, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x8001612E)))
(check-sat)
(exit)
