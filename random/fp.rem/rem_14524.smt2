(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x07C24C8A)))
;; x should be Float32(0x07C24C8A [Rational(6366789, 21778071482940061661655974875633165533184), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A06FE26)))
;; y should be Float32(0x4A06FE26 [Rational(4423443, 2), 2211721.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b0 #b00001111 #b10000100100110010001010))))
(check-sat)
(exit)
