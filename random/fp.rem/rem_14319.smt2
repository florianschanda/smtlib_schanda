(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b00101010110011101011010)))
;; x should be Float32(0x4A15675A [Rational(4895661, 2), 2447830.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A2A2C32)))
;; y should be Float32(0x4A2A2C32 [Rational(5576217, 2), 2788108.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b10010001 #b01001100010011011000000))))
(check-sat)
(exit)
