(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x3F22C506)))
;; x should be Float32(0x3F22C506 [Rational(5333635, 8388608), 0.635819])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x0050BF5B)))
;; y should be Float32(0x0050BF5B [Rational(5291867, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x00258063)))
(check-sat)
(exit)
