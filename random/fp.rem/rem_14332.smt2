(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11010111001111111000111)))
;; x should be Float32(0x4AEB9FC7 [Rational(15441863, 2), 7720931.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x662F88E5)))
;; y should be Float32(0x662F88E5 [Rational(207234848221311774228480), 207234848221311774228480.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x4AEB9FC7)))
(check-sat)
(exit)
