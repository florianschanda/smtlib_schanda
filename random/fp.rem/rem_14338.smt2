(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A8CF673)))
;; x should be Float32(0x4A8CF673 [Rational(9238131, 2), 4619065.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xC9EE4B34)))
;; y should be Float32(0xC9EE4B34 [Rational(-3904205, 2), -1952102.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b0 #b10010010 #b01011101000011011001000))))
(check-sat)
(exit)
