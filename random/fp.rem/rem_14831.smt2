(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b00111010 #b00001100010101011000110)))
;; x should be Float32(0x9D062AC6 [Rational(-4396387, 2475880078570760549798248448), -0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4ADF7933)))
;; y should be Float32(0x4ADF7933 [Rational(14645555, 2), 7322777.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result ((_ to_fp 8 24) #x9D062AC6))))
(check-sat)
(exit)
