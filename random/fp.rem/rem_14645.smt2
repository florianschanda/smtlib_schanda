(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCAACEB73)))
;; x should be Float32(0xCAACEB73 [Rational(-11332467, 2), -5666233.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b11010001 #b10111001011010011000111)))
;; y should be Float32(0x68DCB4C7 [Rational(8338043037006425711706112), 8338043037006425711706112.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b10010101 #b01011001110101101110011))))
(check-sat)
(exit)
