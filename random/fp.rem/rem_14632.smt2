(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010011 #b10100110110001001011100)))
;; x should be Float32(0xC9D3625C [Rational(-3463319, 2), -1731659.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010101 #b11011000110100100010011)))
;; y should be Float32(0x4AEC6913 [Rational(15493395, 2), 7746697.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b1 #b10010011 #b10100110110001001011100)))
(check-sat)
(exit)
