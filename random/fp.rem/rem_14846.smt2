(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB1314A1E)))
;; x should be Float32(0xB1314A1E [Rational(-5809423, 2251799813685248), -0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b10101010011111011011111)))
;; y should be Float32(0x00553EDF [Rational(5586655, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x002A55C9)))
(check-sat)
(exit)
