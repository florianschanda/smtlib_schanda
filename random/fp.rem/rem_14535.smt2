(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x33E34631)))
;; x should be Float32(0x33E34631 [Rational(14894641, 140737488355328), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x011CAAC0)))
;; y should be Float32(0x011CAAC0 [Rational(160427, 5575186299632655785383929568162090376495104), 0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x80621500)))
(check-sat)
(exit)
