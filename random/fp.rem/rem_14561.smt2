(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x003A6DE8)))
;; x should be Float32(0x003A6DE8 [Rational(478653, 89202980794122492566142873090593446023921664), 0.000000])

(declare-const y Float32)
(assert (= y (fp #b0 #b10010011 #b11110001011001010101100)))
;; y should be Float32(0x49F8B2AC [Rational(4074667, 2), 2037333.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b0 #b00000000 #b01110100110110111101000))))
(check-sat)
(exit)
