(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00111110 #b00010001011011001111110)))
;; x should be Float32(0x1F08B67E [Rational(4479807, 154742504910672534362390528), 0.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #xA6D99325)))
;; y should be Float32(0xA6D99325 [Rational(-14258981, 9444732965739290427392), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result ((_ to_fp 8 24) #x1F08B67E))))
(check-sat)
(exit)
