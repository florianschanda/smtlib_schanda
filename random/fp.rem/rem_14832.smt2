(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xE984C421)))
;; x should be Float32(0xE984C421 [Rational(-20063050095290991742287872), -20063050095290991742287872.000000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4AF6F4AB)))
;; y should be Float32(0x4AF6F4AB [Rational(16184491, 2), 8092245.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b10010010 #b00110001110011110010000))))
(check-sat)
(exit)
