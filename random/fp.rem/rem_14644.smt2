(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA827C65)))
;; x should be Float32(0xCA827C65 [Rational(-8551525, 2), -4275762.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x55CCE662)))
;; y should be Float32(0x55CCE662 [Rational(28161232338944), 28161232338944.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result (fp #b1 #b10010101 #b00000100111110001100101))))
(check-sat)
(exit)
