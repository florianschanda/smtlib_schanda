(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA39C8AE)))
;; x should be Float32(0xCA39C8AE [Rational(-6087767, 2), -3043883.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b10011110 #b10101110110101111001001)))
;; y should be Float32(0xCF576BC9 [Rational(-3614165248), -3614165248.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #xCA39C8AE)))
(check-sat)
(exit)
