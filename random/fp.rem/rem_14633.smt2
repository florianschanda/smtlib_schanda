(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10110000111111100011111)))
;; x should be Float32(0xCAD87F1F [Rational(-14188319, 2), -7094159.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A9F194D)))
;; y should be Float32(0x4A9F194D [Rational(10426701, 2), 5213350.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result ((_ to_fp 8 24) #xC9E59748))))
(check-sat)
(exit)
