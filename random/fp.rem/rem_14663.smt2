(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b11011100010000000101101)))
;; x should be Float32(0xCAEE202D [Rational(-15605805, 2), -7802902.500000])

(declare-const y Float32)
(assert (= y (fp #b1 #b01000000 #b11010110101100110111100)))
;; y should be Float32(0xA06B59BC [Rational(-3855983, 19342813113834066795298816), -0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result ((_ to_fp 8 24) #x1E716700)))
(check-sat)
(exit)
