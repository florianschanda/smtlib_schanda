(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010010 #b00010011010001001001000)))
;; x should be Float32(0x4909A248 [Rational(1127497, 2), 563748.500000])

(declare-const y Float32)
(assert (= y ((_ to_fp 8 24) #x4A8E1361)))
;; y should be Float32(0x4A8E1361 [Rational(9311073, 2), 4655536.500000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (not (= result ((_ to_fp 8 24) #x4909A248))))
(check-sat)
(exit)
