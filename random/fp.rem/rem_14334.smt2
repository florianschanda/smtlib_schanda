(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4984AC2C)))
;; x should be Float32(0x4984AC2C [Rational(2173707, 2), 1086853.500000])

(declare-const y Float32)
(assert (= y (fp #b0 #b00000000 #b01110011010001000111010)))
;; y should be Float32(0x0039A23A [Rational(1888541, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const result Float32)
(assert (= result (fp.rem x y)))
(assert (= result (fp #b0 #b00000000 #b00101010111000000001010)))
(check-sat)
(exit)
