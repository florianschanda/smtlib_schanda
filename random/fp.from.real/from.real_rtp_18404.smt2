(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x115AF0BB)))
;; x should be Float32(0x115AF0BB [Rational(14348475, 83076749736557242056487941267521536), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (/ 7174237.0 41538374868278621028243970633760768.0))))
;; w should be Float32(0x115AF0BA [Rational(7174237, 41538374868278621028243970633760768), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
