(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b00011101 #b11011010010011110110111)))
;; x should be Float32(0x0EED27B7 [Rational(15542199, 2658455991569831745807614120560689152), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE (/ 31084397.0 5316911983139663491615228241121378304.0))))
;; w should be Float32(0x0EED27B6 [Rational(7771099, 1329227995784915872903807060280344576), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
