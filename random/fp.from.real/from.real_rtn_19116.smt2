(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b1 #b01010001 #b10101010000000111110111)))
;; x should be Float32(0xA8D501F7 [Rational(-13959671, 590295810358705651712), -0.000000])

(declare-const r Real)
(assert (and (>= r (- (/ 13959671.0 590295810358705651712.0))) (< r (- (/ 6979835.0 295147905179352825856.0)))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN r)))
(assert (= x w))
(check-sat)
(exit)
