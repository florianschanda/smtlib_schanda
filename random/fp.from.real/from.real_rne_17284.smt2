(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (inclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A60D262)))
;; x should be Float32(0x4A60D262 [Rational(7366961, 2), 3683480.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE (/ 29467843.0 8.0))))
;; w should be Float32(0x4A60D262 [Rational(7366961, 2), 3683480.500000])

(assert (= x w))
(check-sat)
(exit)
