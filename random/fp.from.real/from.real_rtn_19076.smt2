(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b01110001 #b11000011111000000100010)))
;; x should be Float32(0xB8E1F022 [Rational(-7403537, 68719476736), -0.000108])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 7403537.0 68719476736.0)))))
;; w should be Float32(0xB8E1F022 [Rational(-7403537, 68719476736), -0.000108])

(assert (= x w))
(check-sat)
(exit)
