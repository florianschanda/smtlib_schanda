(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x426C2A14)))
;; x should be Float32(0x426C2A14 [Rational(3869317, 65536), 59.041092])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ (/ 15477269.0 262144.0))))
;; w should be Float32(0x426C2A15 [Rational(15477269, 262144), 59.041096])

(assert (distinct x w))
(check-sat)
(exit)
