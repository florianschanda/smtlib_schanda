(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b0 #b01011101 #b00101111011111111011100)))
;; x should be Float32(0x2E97BFDC [Rational(2486263, 36028797018963968), 0.000000])

(declare-const r Real)
(assert (and (>= r (/ 2486263.0 36028797018963968.0)) (< r (/ 9945053.0 144115188075855872.0))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ r)))
(assert (= x w))
(check-sat)
(exit)
