(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xBADA8697)))
;; x should be Float32(0xBADA8697 [Rational(-14321303, 8589934592), -0.001667])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE (- (/ 28642607.0 17179869184.0)))))
;; w should be Float32(0xBADA8698 [Rational(-1790163, 1073741824), -0.001667])

(assert (distinct x w))
(check-sat)
(exit)
