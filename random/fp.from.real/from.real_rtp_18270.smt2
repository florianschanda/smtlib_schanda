(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ADA6629)))
;; x should be Float32(0x4ADA6629 [Rational(14313001, 2), 7156500.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP 7156500.0)))
;; w should be Float32(0x4ADA6628 [Rational(7156500), 7156500.000000])

(assert (not (distinct x w)))
(check-sat)
(exit)
