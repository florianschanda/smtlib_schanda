(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b01010101100000000011011)))
;; x should be Float32(0xCAAAC01B [Rational(-11190299, 2), -5595149.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE (- (/ 22380597.0 4.0)))))
;; w should be Float32(0xCAAAC01A [Rational(-5595149), -5595149.000000])

(assert (distinct x w))
(check-sat)
(exit)
