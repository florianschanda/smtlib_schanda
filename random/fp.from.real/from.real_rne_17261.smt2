(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AA7823F)))
;; x should be Float32(0x4AA7823F [Rational(10977855, 2), 5488927.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE (/ 21955711.0 4.0))))
;; w should be Float32(0x4AA78240 [Rational(5488928), 5488928.000000])

(assert (distinct x w))
(check-sat)
(exit)
