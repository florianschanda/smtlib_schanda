(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (inclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9D39E0C)))
;; x should be Float32(0xC9D39E0C [Rational(-3467139, 2), -1733569.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE (- (/ 27737111.0 16.0)))))
;; w should be Float32(0xC9D39E0C [Rational(-3467139, 2), -1733569.500000])

(assert (= x w))
(check-sat)
(exit)
