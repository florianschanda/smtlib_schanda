(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xD6D7A031)))
;; x should be Float32(0xD6D7A031 [Rational(-118541508411392), -118541508411392.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (- 118541516800000.0))))
;; w should be Float32(0xD6D7A032 [Rational(-118541516800000), -118541516800000.000000])

(assert (distinct x w))
(check-sat)
(exit)
