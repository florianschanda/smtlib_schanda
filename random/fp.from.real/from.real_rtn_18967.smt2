(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010010 #b10100001100110001001000)))
;; x should be Float32(0xC950CC48 [Rational(-1710473, 2), -855236.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 13683783.0 16.0)))))
;; w should be Float32(0xC950CC47 [Rational(-13683783, 16), -855236.437500])

(assert (not (distinct x w)))
(check-sat)
(exit)
