(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ (- (/ 170815.0 44601490397061246283071436545296723011960832.0)))))
;; w should be Float32(0x8029B3F0 [Rational(-170815, 44601490397061246283071436545296723011960832), -0.000000])

(assert (distinct x w))
(check-sat)
(exit)
