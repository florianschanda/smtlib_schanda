(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (inclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCADB9F2F)))
;; x should be Float32(0xCADB9F2F [Rational(-14393135, 2), -7196567.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (- (/ 14393135.0 2.0)))))
;; w should be Float32(0xCADB9F2F [Rational(-14393135, 2), -7196567.500000])

(assert (= x w))
(check-sat)
(exit)
