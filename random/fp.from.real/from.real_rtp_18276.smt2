(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b10011001001011100101110)))
;; x should be Float32(0x4A4C972E [Rational(6704023, 2), 3352011.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (/ 13408045.0 4.0))))
;; w should be Float32(0x4A4C972D [Rational(13408045, 4), 3352011.250000])

(assert (distinct x w))
(check-sat)
(exit)
