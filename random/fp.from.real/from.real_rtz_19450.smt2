(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA97914D)))
;; x should be Float32(0xCA97914D [Rational(-9933133, 2), -4966566.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ (- 4966567.0))))
;; w should be Float32(0xCA97914E [Rational(-4966567), -4966567.000000])

(assert (not (distinct x w)))
(check-sat)
(exit)
