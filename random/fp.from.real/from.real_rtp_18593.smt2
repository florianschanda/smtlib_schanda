(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10100110 #b01111100110010111001001)))
;; x should be Float32(0xD33E65C9 [Rational(-817751457792), -817751457792.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (- 817751457792.0))))
;; w should be Float32(0xD33E65C9 [Rational(-817751457792), -817751457792.000000])

(assert (= x w))
(check-sat)
(exit)
