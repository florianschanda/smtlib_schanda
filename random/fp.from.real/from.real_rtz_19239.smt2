(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AFA3B0D)))
;; x should be Float32(0x4AFA3B0D [Rational(16399117, 2), 8199558.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ 8199559.0)))
;; w should be Float32(0x4AFA3B0E [Rational(8199559), 8199559.000000])

(assert (not (distinct x w)))
(check-sat)
(exit)
