(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA0D3C92)))
;; x should be Float32(0xCA0D3C92 [Rational(-4628041, 2), -2314020.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 9256081.0 4.0)))))
;; w should be Float32(0xCA0D3C91 [Rational(-9256081, 4), -2314020.250000])

(assert (distinct x w))
(check-sat)
(exit)
