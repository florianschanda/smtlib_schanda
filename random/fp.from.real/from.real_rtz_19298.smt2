;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b0 #b00010101 #b10101111001001000011100)))
;; x should be Float32(0x0AD7921C [Rational(3531911, 170141183460469231731687303715884105728), 0.000000])

(declare-const r Real)
(assert (and (>= r (/ 3531911.0 170141183460469231731687303715884105728.0)) (< r (/ 14127645.0 680564733841876926926749214863536422912.0))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ r)))
(assert (= x w))
(check-sat)
(exit)
