(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b00011001001110010000101)))
;; x should be Float32(0xCA8C9C85 [Rational(-9215109, 2), -4607554.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- 4607554.0))))
;; w should be Float32(0xCA8C9C84 [Rational(-4607554), -4607554.000000])

(assert (distinct x w))
(check-sat)
(exit)
