(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b00101111001011100101111)))
;; x should be Float32(0x0017972F [Rational(1546031, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (/ 96627.0 44601490397061246283071436545296723011960832.0))))
;; w should be Float32(0x00179730 [Rational(96627, 44601490397061246283071436545296723011960832), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
