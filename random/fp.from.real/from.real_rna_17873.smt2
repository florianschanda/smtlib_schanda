(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x59C7511D)))
;; x should be Float32(0x59C7511D [Rational(7012838170165248), 7012838170165248.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNA 7012838438600704.0)))
;; w should be Float32(0x59C7511E [Rational(7012838707036160), 7012838707036160.000000])

(assert (distinct x w))
(check-sat)
(exit)
