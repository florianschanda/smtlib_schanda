(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A98C5A7)))
;; x should be Float32(0x4A98C5A7 [Rational(10012071, 2), 5006035.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN 5006036.0)))
;; w should be Float32(0x4A98C5A8 [Rational(5006036), 5006036.000000])

(assert (not (distinct x w)))
(check-sat)
(exit)
