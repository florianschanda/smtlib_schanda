;; x should be Float32(0x004B7CA0 [Rational(154597, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (/ 4947103.0 713623846352979940529142984724747568191373312.0))))
;; w should be Float32(0x004B7C9F [Rational(4947103, 713623846352979940529142984724747568191373312), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
