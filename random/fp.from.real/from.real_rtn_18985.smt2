(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11101000000000001011110)))
;; x should be Float32(0xCA74005E [Rational(-7995439, 2), -3997719.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 15990877.0 4.0)))))
;; w should be Float32(0xCA74005D [Rational(-15990877, 4), -3997719.250000])

(assert (not (distinct x w)))
(check-sat)
(exit)
