(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xB3B63C55)))
;; x should be Float32(0xB3B63C55 [Rational(-11942997, 140737488355328), -0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNA (- (/ 23885995.0 281474976710656.0)))))
;; w should be Float32(0xB3B63C56 [Rational(-5971499, 70368744177664), -0.000000])

(assert (distinct x w))
(check-sat)
(exit)
