(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4ABDF1F5)))
;; x should be Float32(0x4ABDF1F5 [Rational(12448245, 2), 6224122.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN 6224123.0)))
;; w should be Float32(0x4ABDF1F6 [Rational(6224123), 6224123.000000])

(assert (distinct x w))
(check-sat)
(exit)
