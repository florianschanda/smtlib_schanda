(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b01010101 #b00111010110111000100000)))
;; x should be Float32(0x2A9D6E20 [Rational(322417, 1152921504606846976), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (/ 322417.0 1152921504606846976.0))))
;; w should be Float32(0x2A9D6E20 [Rational(322417, 1152921504606846976), 0.000000])

(assert (= x w))
(check-sat)
(exit)
