(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10111011 #b10111110010010001000000)))
;; x should be Float32(0xDDDF2440 [Rational(-2009880867295461376), -2009880867295461376.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- 2009880729856507904.0))))
;; w should be Float32(0xDDDF243F [Rational(-2009880729856507904), -2009880729856507904.000000])

(assert (distinct x w))
(check-sat)
(exit)
