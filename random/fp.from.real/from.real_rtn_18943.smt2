(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10001000 #b01001001110000000000000)))
;; x should be Float32(0xC424E000 [Rational(-1319, 2), -659.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 10805247.0 16384.0)))))
;; w should be Float32(0xC424DFFF [Rational(-10805247, 16384), -659.499939])

(assert (distinct x w))
(check-sat)
(exit)
