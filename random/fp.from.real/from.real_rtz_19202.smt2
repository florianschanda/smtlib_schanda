(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on low bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10000001111011000000101)))
;; x should be Float32(0x4AC0F605 [Rational(12645893, 2), 6322946.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ (/ 12645893.0 2.0))))
;; w should be Float32(0x4AC0F605 [Rational(12645893, 2), 6322946.500000])

(assert (not (= x w)))
(check-sat)
(exit)
