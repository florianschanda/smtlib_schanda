(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b01100011 #b10001101111110110010011)))
;; x should be Float32(0xB1C6FD93 [Rational(-13041043, 2251799813685248), -0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ (- (/ 3260261.0 562949953421312.0)))))
;; w should be Float32(0xB1C6FD94 [Rational(-3260261, 562949953421312), -0.000000])

(assert (distinct x w))
(check-sat)
(exit)
