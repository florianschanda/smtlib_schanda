(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xBE574D7F)))
;; x should be Float32(0xBE574D7F [Rational(-14110079, 67108864), -0.210257])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 7055039.0 33554432.0)))))
;; w should be Float32(0xBE574D7E [Rational(-7055039, 33554432), -0.210257])

(assert (distinct x w))
(check-sat)
(exit)
