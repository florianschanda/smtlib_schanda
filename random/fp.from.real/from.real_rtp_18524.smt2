(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b11010110111111100100010)))
;; x should be Float32(0xCA6B7F22 [Rational(-7716753, 2), -3858376.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (- (/ 15433507.0 4.0)))))
;; w should be Float32(0xCA6B7F23 [Rational(-15433507, 4), -3858376.750000])

(assert (distinct x w))
(check-sat)
(exit)
