(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x189A6759)))
;; x should be Float32(0x189A6759 [Rational(10119001, 2535301200456458802993406410752), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (/ 5059501.0 1267650600228229401496703205376.0))))
;; w should be Float32(0x189A675A [Rational(5059501, 1267650600228229401496703205376), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
