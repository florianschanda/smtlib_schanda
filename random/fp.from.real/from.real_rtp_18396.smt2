(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x72BE4FD2)))
;; x should be Float32(0x72BE4FD2 [Rational(7539027033954114737664648282112), 7539027033954114737664648282112.000000])

(declare-const r Real)
(assert (and (> r 7539026429491204930350060929024.0) (<= r 7539027033954114737664648282112.0)))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP r)))
(assert (= x w))
(check-sat)
(exit)
