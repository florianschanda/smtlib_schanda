(set-info :status unsat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b11100001000110001110101)))
;; x should be Float32(0x4AF08C75 [Rational(15764597, 2), 7882298.500000])

(declare-const r Real)
(assert (and (>= r (/ 15764597.0 2.0)) (< r 7882299.0)))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN r)))
(assert (not (= x w)))
(check-sat)
(exit)
