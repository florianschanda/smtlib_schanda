(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011011 #b11001010000100010001110)))
;; x should be Float32(0x4DE5088E [Rational(480317888), 480317888.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ 480317920.0)))
;; w should be Float32(0x4DE5088F [Rational(480317920), 480317920.000000])

(assert (distinct x w))
(check-sat)
(exit)
