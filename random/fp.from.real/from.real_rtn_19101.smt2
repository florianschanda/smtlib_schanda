(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b00001000 #b01001000111111000101101)))
;; x should be Float32(0x84247E2D [Rational(-10780205, 5575186299632655785383929568162090376495104), -0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 2695051.0 1393796574908163946345982392040522594123776.0)))))
;; w should be Float32(0x84247E2C [Rational(-2695051, 1393796574908163946345982392040522594123776), -0.000000])

(assert (distinct x w))
(check-sat)
(exit)
