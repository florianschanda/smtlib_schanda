(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b0 #b10010100 #b11011100100101000100110)))
;; x should be Float32(0x4A6E4A26 [Rational(7808275, 2), 3904137.500000])

(declare-const r Real)
(assert (and (>= r (/ 31233099.0 8.0)) (<= r (/ 31233101.0 8.0))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE r)))
(assert (not (= x w)))
(check-sat)
(exit)
