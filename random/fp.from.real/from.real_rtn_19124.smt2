(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00100011101100010001000)))
;; x should be Float32(0x8011D888 [Rational(-146193, 89202980794122492566142873090593446023921664), -0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 146193.0 89202980794122492566142873090593446023921664.0)))))
;; w should be Float32(0x8011D888 [Rational(-146193, 89202980794122492566142873090593446023921664), -0.000000])

(assert (= x w))
(check-sat)
(exit)
