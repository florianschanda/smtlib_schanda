(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x1CD80924)))
;; x should be Float32(0x1CD80924 [Rational(3539529, 2475880078570760549798248448), 0.000000])

(declare-const r Real)
(assert (and (> r (/ 14158115.0 9903520314283042199192993792.0)) (<= r (/ 3539529.0 2475880078570760549798248448.0))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP r)))
(assert (= x w))
(check-sat)
(exit)
