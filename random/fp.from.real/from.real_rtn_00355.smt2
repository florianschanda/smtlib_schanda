(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA7ACCAE)))
;; x should be Float32(0xCA7ACCAE [Rational(-8218199, 2), -4109099.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 16436397.0 4.0)))))
;; w should be Float32(0xCA7ACCAD [Rational(-16436397, 4), -4109099.250000])

(assert (distinct x w))
(check-sat)
(exit)
