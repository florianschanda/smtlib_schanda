(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010100 #b10101110110011101110110)))
;; x should be Float32(0xCA576776 [Rational(-7058363, 2), -3529181.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNA (- (/ 28233453.0 8.0)))))
;; w should be Float32(0xCA576777 [Rational(-14116727, 4), -3529181.750000])

(assert (not (distinct x w)))
(check-sat)
(exit)
