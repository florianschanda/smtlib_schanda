(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC782D640)))
;; x should be Float32(0xC782D640 [Rational(-133977, 2), -66988.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 8574527.0 128.0)))))
;; w should be Float32(0xC782D63F [Rational(-8574527, 128), -66988.492188])

(assert (distinct x w))
(check-sat)
(exit)
