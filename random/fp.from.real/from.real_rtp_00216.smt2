(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10110110111100111110111)))
;; x should be Float32(0x4ADB79F7 [Rational(14383607, 2), 7191803.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP 7191803.0)))
;; w should be Float32(0x4ADB79F6 [Rational(7191803), 7191803.000000])

(assert (not (distinct x w)))
(check-sat)
(exit)
