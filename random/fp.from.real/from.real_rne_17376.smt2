(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b10011100 #b00000000100000010110001)))
;; x should be Float32(0x4E0040B1 [Rational(537930816), 537930816.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE 537930784.0)))
;; w should be Float32(0x4E0040B0 [Rational(537930752), 537930752.000000])

(assert (not (distinct x w)))
(check-sat)
(exit)
