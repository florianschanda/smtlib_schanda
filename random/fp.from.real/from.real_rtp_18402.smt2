(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b0 #b00001101 #b00001011111000001001011)))
;; x should be Float32(0x0685F04B [Rational(8777803, 174224571863520493293247799005065324265472), 0.000000])

(declare-const r Real)
(assert (and (> r (/ 4388901.0 87112285931760246646623899502532662132736.0)) (<= r (/ 8777803.0 174224571863520493293247799005065324265472.0))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP r)))
(assert (= x w))
(check-sat)
(exit)
