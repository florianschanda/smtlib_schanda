(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A3099AA)))
;; x should be Float32(0x4A3099AA [Rational(5786837, 2), 2893418.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNA (/ 23147349.0 8.0))))
;; w should be Float32(0x4A3099AB [Rational(11573675, 4), 2893418.750000])

(assert (distinct x w))
(check-sat)
(exit)
