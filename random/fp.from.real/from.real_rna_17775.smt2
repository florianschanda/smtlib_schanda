(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10110111100011001010111)))
;; x should be Float32(0x4ADBC657 [Rational(14403159, 2), 7201579.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNA (/ 28806319.0 4.0))))
;; w should be Float32(0x4ADBC658 [Rational(7201580), 7201580.000000])

(assert (distinct x w))
(check-sat)
(exit)
