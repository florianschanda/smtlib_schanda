(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNA (- (/ 20894023.0 1048576.0)))))
;; w should be Float32(0xC19F68A4 [Rational(-2611753, 131072), -19.926094])

(assert (not (distinct x w)))
(check-sat)
(exit)
