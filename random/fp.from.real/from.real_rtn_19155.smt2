(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 95495.0 44601490397061246283071436545296723011960832.0)))))
;; w should be Float32(0x80175070 [Rational(-95495, 44601490397061246283071436545296723011960832), -0.000000])

(assert (distinct x w))
(check-sat)
(exit)
