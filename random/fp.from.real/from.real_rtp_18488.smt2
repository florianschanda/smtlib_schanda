(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10010101 #b10101101100110001010001)))
;; x should be Float32(0xCAD6CC51 [Rational(-14077009, 2), -7038504.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (- 7038505.0))))
;; w should be Float32(0xCAD6CC52 [Rational(-7038505), -7038505.000000])

(assert (not (distinct x w)))
(check-sat)
(exit)
