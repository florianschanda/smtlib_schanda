(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AED58EF)))
;; x should be Float32(0x4AED58EF [Rational(15554799, 2), 7777399.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ 7777400.0)))
;; w should be Float32(0x4AED58F0 [Rational(7777400), 7777400.000000])

(assert (distinct x w))
(check-sat)
(exit)
