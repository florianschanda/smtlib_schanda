(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x26ED90BA)))
;; x should be Float32(0x26ED90BA [Rational(7784541, 4722366482869645213696), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (/ 15569081.0 9444732965739290427392.0))))
;; w should be Float32(0x26ED90B9 [Rational(15569081, 9444732965739290427392), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
