(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00111100000111010100011)))
;; x should be Float32(0x4A9E0EA3 [Rational(10358435, 2), 5179217.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP 5179217.0)))
;; w should be Float32(0x4A9E0EA2 [Rational(5179217), 5179217.000000])

(assert (distinct x w))
(check-sat)
(exit)
