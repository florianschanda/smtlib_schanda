(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00100111011101101101110)))
;; x should be Float32(0x8013BB6E [Rational(-646583, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const r Real)
(assert (and (>= r (- (/ 646583.0 356811923176489970264571492362373784095686656.0))) (< r (- (/ 1293165.0 713623846352979940529142984724747568191373312.0)))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN r)))
(assert (= x w))
(check-sat)
(exit)
