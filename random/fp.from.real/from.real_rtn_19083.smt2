(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b01010101 #b01100110111001001101100)))
;; x should be Float32(0xAAB3726C [Rational(-2940059, 9223372036854775808), -0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- (/ 11760235.0 36893488147419103232.0)))))
;; w should be Float32(0xAAB3726B [Rational(-11760235, 36893488147419103232), -0.000000])

(assert (not (distinct x w)))
(check-sat)
(exit)
