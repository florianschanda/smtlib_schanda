(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b01010101 #b01010001000111001111001)))
;; x should be Float32(0xAAA88E79 [Rational(-11046521, 36893488147419103232), -0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNA (- (/ 22093043.0 73786976294838206464.0)))))
;; w should be Float32(0xAAA88E7A [Rational(-5523261, 18446744073709551616), -0.000000])

(assert (distinct x w))
(check-sat)
(exit)
