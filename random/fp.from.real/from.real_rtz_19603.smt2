(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b11100100000110001100001)))
;; x should be Float32(0x80720C61 [Rational(-7474273, 713623846352979940529142984724747568191373312), -0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ (- (/ 7474273.0 713623846352979940529142984724747568191373312.0)))))
;; w should be Float32(0x80720C61 [Rational(-7474273, 713623846352979940529142984724747568191373312), -0.000000])

(assert (= x w))
(check-sat)
(exit)
