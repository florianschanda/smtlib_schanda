(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x11B3FDA7)))
;; x should be Float32(0x11B3FDA7 [Rational(11795879, 41538374868278621028243970633760768), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (/ 5897939.0 20769187434139310514121985316880384.0))))
;; w should be Float32(0x11B3FDA6 [Rational(5897939, 20769187434139310514121985316880384), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
