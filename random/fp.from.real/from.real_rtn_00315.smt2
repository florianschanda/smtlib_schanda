(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4AD43BEB)))
;; x should be Float32(0x4AD43BEB [Rational(13908971, 2), 6954485.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN 6954486.0)))
;; w should be Float32(0x4AD43BEC [Rational(6954486), 6954486.000000])

(assert (distinct x w))
(check-sat)
(exit)
