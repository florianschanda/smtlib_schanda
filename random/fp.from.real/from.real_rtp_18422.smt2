;; x should be Float32(0x001584A0 [Rational(44069, 22300745198530623141535718272648361505980416), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (/ 1410207.0 713623846352979940529142984724747568191373312.0))))
;; w should be Float32(0x0015849F [Rational(1410207, 713623846352979940529142984724747568191373312), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
