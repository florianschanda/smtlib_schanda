(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xA3E16288)))
;; x should be Float32(0xA3E16288 [Rational(-1846353, 75557863725914323419136), -0.000000])

(declare-const r Real)
(assert (and (> r (- (/ 14770825.0 604462909807314587353088.0))) (<= r (- (/ 1846353.0 75557863725914323419136.0)))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP r)))
(assert (= x w))
(check-sat)
(exit)
