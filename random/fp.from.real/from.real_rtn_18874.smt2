(declare-const r Real)
(assert (and (>= r 65391208587713915081105988273536237568.0) (< r 65391213658316315994023594260349059072.0)))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN r)))
(assert (= x w))
(check-sat)
(exit)
