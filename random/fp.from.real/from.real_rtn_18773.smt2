(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b10011110110100001001011)))
;; x should be Float32(0x4ACF684B [Rational(13592651, 2), 6796325.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN 6796326.0)))
;; w should be Float32(0x4ACF684C [Rational(6796326), 6796326.000000])

(assert (distinct x w))
(check-sat)
(exit)
