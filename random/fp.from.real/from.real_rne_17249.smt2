(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (inclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x4A72272E)))
;; x should be Float32(0x4A72272E [Rational(7934871, 2), 3967435.500000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE (/ 31739485.0 8.0))))
;; w should be Float32(0x4A72272E [Rational(7934871, 2), 3967435.500000])

(assert (= x w))
(check-sat)
(exit)
