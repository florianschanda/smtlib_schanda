(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b0 #b10001110 #b01000111110110010000000)))
;; x should be Float32(0x4723EC80 [Rational(83929, 2), 41964.500000])

(declare-const r Real)
(assert (and (>= r (/ 21485823.0 512.0)) (< r (/ 21485825.0 512.0))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNA r)))
(assert (not (= x w)))
(check-sat)
(exit)
