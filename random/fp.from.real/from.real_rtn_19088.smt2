(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
;; on low bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b10100110 #b01001100110010010010010)))
;; x should be Float32(0xD3266492 [Rational(-714651860992), -714651860992.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (- 714651860992.0))))
;; w should be Float32(0xD3266492 [Rational(-714651860992), -714651860992.000000])

(assert (not (= x w)))
(check-sat)
(exit)
