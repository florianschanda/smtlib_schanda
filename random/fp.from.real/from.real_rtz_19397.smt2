(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x0052F43F)))
;; x should be Float32(0x0052F43F [Rational(5436479, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ (/ 84945.0 11150372599265311570767859136324180752990208.0))))
;; w should be Float32(0x0052F440 [Rational(84945, 11150372599265311570767859136324180752990208), 0.000000])

(assert (distinct x w))
(check-sat)
(exit)
