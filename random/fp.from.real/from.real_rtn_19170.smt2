(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00101100110101011010110)))
;; x should be Float32(0x80166AD6 [Rational(-734571, 356811923176489970264571492362373784095686656), -0.000000])

(declare-const r Real)
(assert (and (>= r (- (/ 734571.0 356811923176489970264571492362373784095686656.0))) (< r (- (/ 1469141.0 713623846352979940529142984724747568191373312.0)))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN r)))
(assert (= x w))
(check-sat)
(exit)
