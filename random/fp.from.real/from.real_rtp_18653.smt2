(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b00000000 #b00111000001101011000000)))
;; x should be Float32(0x801C1AC0 [Rational(-28779, 11150372599265311570767859136324180752990208), -0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTP (- (/ 28779.0 11150372599265311570767859136324180752990208.0)))))
;; w should be Float32(0x801C1AC0 [Rational(-28779, 11150372599265311570767859136324180752990208), -0.000000])

(assert (= x w))
(check-sat)
(exit)
