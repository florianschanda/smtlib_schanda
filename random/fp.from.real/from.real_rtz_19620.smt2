(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ (- (/ 3753979.0 356811923176489970264571492362373784095686656.0)))))
;; w should be Float32(0x80728FF6 [Rational(-3753979, 356811923176489970264571492362373784095686656), -0.000000])

(assert (distinct x w))
(check-sat)
(exit)
