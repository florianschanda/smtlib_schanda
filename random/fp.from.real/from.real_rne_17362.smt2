(declare-const r Real)
(assert (and (>= r (/ 20906259.0 1180591620717411303424.0)) (<= r (/ 20906261.0 1180591620717411303424.0))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE r)))
(assert (= x w))
(check-sat)
(exit)
