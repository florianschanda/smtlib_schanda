(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; hard: non-literal interval check
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b01100001000101100111110)))
;; x should be Float32(0x00308B3E [Rational(1590687, 356811923176489970264571492362373784095686656), 0.000000])

(declare-const r Real)
(assert (and (>= r (/ 1590687.0 356811923176489970264571492362373784095686656.0)) (< r (/ 3181375.0 713623846352979940529142984724747568191373312.0))))
(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTZ r)))
(assert (= x w))
(check-sat)
(exit)
