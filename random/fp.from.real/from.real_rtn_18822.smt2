(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on low bound (inclusive)
(declare-const x Float32)
(assert (= x (fp #b0 #b01100100 #b01000011001010000000111)))
;; x should be Float32(0x32219407 [Rational(10589191, 1125899906842624), 0.000000])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RTN (/ 10589191.0 1125899906842624.0))))
;; w should be Float32(0x32219407 [Rational(10589191, 1125899906842624), 0.000000])

(assert (= x w))
(check-sat)
(exit)
