(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
;; on high bound (exclusive)
(declare-const x Float32)
(assert (= x (fp #b1 #b01110101 #b10110101000011010010111)))
;; x should be Float32(0xBADA8697 [Rational(-14321303, 8589934592), -0.001667])

(declare-const w Float32)
(assert (= w ((_ to_fp 8 24) RNE (- (/ 28642605.0 17179869184.0)))))
;; w should be Float32(0xBADA8696 [Rational(-7160651, 4294967296), -0.001667])

(assert (distinct x w))
(check-sat)
(exit)
