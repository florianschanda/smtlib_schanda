(set-info :smt-lib-version 2.6)
(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b00000000 #b11011010001010011001101)))
;; x should be Float32(0x006D14CD [Rational(7148749, 713623846352979940529142984724747568191373312), 0.000000])

(declare-const result Float32)
(assert (= result (fp.nextUp x)))
(assert (= result (fp #b0 #b00000000 #b11011010001010011001110)))
(check-sat)
(exit)
