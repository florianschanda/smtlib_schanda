(set-logic QF_FP)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xCA8CF401)))
(declare-const result Float32)
(assert (= result (fp.roundToIntegral RTZ x)))
(assert (not (= result (fp #b1 #b10010101 #b00011001111010000000000))))
(check-sat)
(exit)
