(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #x53783E83)))
(declare-const result Float32)
(assert (= result (fp.roundToIntegral RTZ x)))
(assert (= result (fp #b0 #b10100110 #b11110000011111010000011)))
(check-sat)
(exit)
