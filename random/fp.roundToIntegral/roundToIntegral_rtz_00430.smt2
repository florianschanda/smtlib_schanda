(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x ((_ to_fp 8 24) #xC9D9AADC)))
(declare-const result Float32)
(assert (= result (fp.roundToIntegral RTZ x)))
(assert (= result (fp #b1 #b10010011 #b10110011010101011011000)))
(check-sat)
(exit)
