(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b10010101 #b00010010000110100101111)))
(declare-const result Float32)
(assert (= result (fp.roundToIntegral RTZ x)))
(assert (= result ((_ to_fp 8 24) #x4A890D2E)))
(check-sat)
(exit)
