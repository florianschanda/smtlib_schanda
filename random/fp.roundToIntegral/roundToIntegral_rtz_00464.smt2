(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b0 #b11111111 #b11111000101111110001011)))
(declare-const result Float32)
(assert (= result (fp.roundToIntegral RTZ x)))
(assert (= result ((_ to_fp 8 24) #x7FFC5F8B)))
(check-sat)
(exit)
