(set-info :smt-lib-version 2.6)
(set-logic QF_FPLRA)
(set-info :source |Random FP created by PyMPF|)
(set-info :license |https://www.gnu.org/licenses/gpl-3.0.html|)
(set-info :category random)
(set-info :status unsat)
(declare-const x Float32)
(assert (or (fp.isZero x) (fp.isSubnormal x) (fp.isNormal x)))
(assert (distinct x (fp #b1 #b00000000 #b10000111010010111100100)))
(declare-const y Real)
(assert (= y (fp.to_real x)))
(declare-const z Real)
(assert (= z (- (/ 1108345.0 178405961588244985132285746181186892047843328.0))))
;; z should be a representable real

(assert (= y z))
(check-sat)
(exit)
