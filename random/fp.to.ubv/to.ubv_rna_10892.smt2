(declare-const x Float32)
(assert (= x (fp #b1 #b10000111 #b01110110100100101100100)))
;; x should be Float32(0xC3BB4964 [Rational(-3068505, 8192), -374.573364])

(declare-const y (_ BitVec 32))
(assert (= y ((_ fp.to_ubv 32) RNA x)))
;; y should be unspecified

(declare-const z (_ BitVec 32))
(assert (not (= y z)))
(check-sat)
(exit)
