(assert (= x (fp #b1 #b10010100 #b10110111110111110110010)))
;; x should be Float32(0xCA5BEFB2 [Rational(-7206873, 2), -3603436.500000])

(declare-const y (_ BitVec 8))
(assert (= y ((_ fp.to_ubv 8) RNA x)))
;; y should be unspecified

(declare-const z (_ BitVec 8))
(assert (not (= y z)))
(check-sat)
(exit)
