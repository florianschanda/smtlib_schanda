(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1100110011110101101011111010010010010110100010110011010010100001))
;; x should be -3677840399242939231
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3C985282DB4BA65)))
(check-sat)
(exit)
