(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1111111111111111111111111111111111111111111111111111111111111111))
;; x should be -1
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r (fp #b1 #b01111111111 #b0000000000000000000000000000000000000000000000000000)))
(check-sat)
(exit)
