(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01101110010100000101011100010000111010010100010111111011010000110110101001011010111101110100111000101110101010110001001110111101))
;; x should be 146632229194504259880767074521244701629
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.leq r ((_ to_fp 8 24) #x7EDCA0AE)))
(check-sat)
(exit)
