(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1011110110110110110100101000010001010011100101100100000000010011))
;; x should be -4776398888999960557
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r (fp #b1 #b10111101 #b00001001001001001011010)))
(check-sat)
(exit)
