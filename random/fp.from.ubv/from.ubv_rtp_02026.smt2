(declare-const x (_ BitVec 128))
(assert (bvule x #b00111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111))
;; x should be 85070591730234615865843651857942052863
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTP x)))
(assert (not (fp.leq r (fp #b0 #b10001111101 #b0000000000000000000000000000000000000000000000000000))))
(check-sat)
(exit)
