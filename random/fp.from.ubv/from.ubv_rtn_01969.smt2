(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11001110011000001100000011100111001001011111100101100111011110111111111001110011100110000000011110010000111101100101010101011110))
;; x should be -65959026754724551693561810872242383522
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r (fp #b1 #b11111100 #b10001100111110011111101)))
(check-sat)
(exit)
