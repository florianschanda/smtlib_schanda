(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01110010101101000001001101101001000010101011100111000101111111000101100101111100111101001010100111110011101101100110000010001100))
;; x should be 152466998642075648199730083287853850764
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.eq r ((_ to_fp 8 24) #x7EE56827)))
(check-sat)
(exit)
