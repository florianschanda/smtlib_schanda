(declare-const x (_ BitVec 64))
(assert (bvule x #b0101011001000100110000110101001010111111011111000100110111001110))
;; x should be 6216308145845456334
(declare-const r Float16)
(assert (= r ((_ to_fp_unsigned 5 11) RNA x)))
