(declare-const x (_ BitVec 64))
(assert (bvuge x #b0011111111101011010010011100101111111011000100011101010010000101))
;; x should be 4605856184355902597
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNA x)))
