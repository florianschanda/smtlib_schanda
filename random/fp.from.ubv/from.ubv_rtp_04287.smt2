(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b01110111000001101010100001000100110001010001010101011101111110101110101110010100011010000001110010000000001111101101101001100111))
;; x should be 158212698172879040477691521073470691943
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTP x)))
(assert (fp.leq r (fp #b0 #b11111101 #b11011100000110101010001)))
(check-sat)
(exit)
