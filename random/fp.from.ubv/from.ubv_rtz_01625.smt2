(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b00111101011110100010100111101101101100100001011101111111100101000100110010100001001100010110110111100011000110101001111010000000))
;; x should be 81717218370606122335182097760634183296
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
