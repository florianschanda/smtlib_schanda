(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b00110000101110100110001111100001001100001001011000110001110111101010000100111101011010100011110000110001011101110111100000010100))
;; x should be 64770736813287621910257502607535208468
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r (fp #b0 #b11111100 #b10000101110100110010000)))
(check-sat)
(exit)
