(declare-const x (_ BitVec 16))
(assert (bvuge x #b0110011001111111))
;; x should be 26239
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTP x)))
(assert (not (fp.geq r (fp #b0 #b10000001101 #b1001100111111100000000000000000000000000000000000000))))
(check-sat)
(exit)
