(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0011100010011000011010001010111110011001000100110110111000101110))
;; x should be 4078124565980737070
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.geq r ((_ to_fp 11 53) #x43CC4C3457CC89B7)))
(check-sat)
(exit)
