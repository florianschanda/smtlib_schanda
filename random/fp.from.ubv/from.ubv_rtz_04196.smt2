(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b01000010001010100111011100000111111010011100101100110111001011010001010110110000011000000100000100100001011110011111010010000001))
;; x should be 87949538423558562312258426488344933505
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTZ x)))
(assert (fp.geq r ((_ to_fp 11 53) #x47D08A9DC1FA72CD)))
(check-sat)
(exit)
