(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b11111101010111000000111001000001110110011000100101000011110010000000001110001100001000111111100100000111010100011011100111000011))
;; x should be 336772663415458124593733740704771979715
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTN x)))
(assert (fp.geq r (fp #b0 #b11111110 #b11111010101110000001110)))
(check-sat)
(exit)
