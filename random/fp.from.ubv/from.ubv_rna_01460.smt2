(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1001111101010001001010000010000110111000010111101111110010011010))
;; x should be -6966743023273182054
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.eq r (fp #b1 #b10111101 #b10000010101110110110000)))
(check-sat)
(exit)
