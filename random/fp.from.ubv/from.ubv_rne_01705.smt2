(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01010111011111110111000110100010010100011110111011111101001001011010010110111110010100100010100111111110100011101100101110100000))
;; x should be 116304562106926349594825186131647056800
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.eq r (fp #b0 #b11111101 #b01011101111111011100011)))
(check-sat)
(exit)
