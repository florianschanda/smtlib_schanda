(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b11110110100101100111101011111000001110010110001111110101101001111110100100100101110111101111111001010101011100010001110010001010))
;; x should be 327771425612186967213116715788997303434

(declare-const r Float16)
