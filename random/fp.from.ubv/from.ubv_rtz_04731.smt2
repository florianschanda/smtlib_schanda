(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b11101010100100001000010111101111100110101000000001010111011000111011110011100011111010100011000110000111000101110110110101100010))
;; x should be 311789758305123302722462779490075897186
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTZ x)))
(assert (fp.geq r (fp #b0 #b11111110 #b11010101001000010000101)))
(check-sat)
(exit)
