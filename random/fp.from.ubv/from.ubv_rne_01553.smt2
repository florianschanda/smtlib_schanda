(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1111110010010011110001100000101001101110111101100011010101010110))
;; x should be -246635806461643434
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r (fp #b1 #b10111000 #b10110110000111001111101)))
(check-sat)
(exit)
