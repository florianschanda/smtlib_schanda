(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01011111001100111011111011011101101011001010100001000000001001101010010000100110111001001101110000110000101101101100111011101110))
;; x should be 126545337960035717775942065276346355438
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r (fp #b0 #b11111101 #b01111100110011101111110)))
(check-sat)
(exit)
