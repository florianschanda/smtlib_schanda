(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11000001010000000010010011001000110000010011110010100110010000101000111101010110111100110111101101011000101001000110111111000010))
;; x should be -83408310663321309126364660475939754046
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
(assert (fp.leq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
