(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01010010010011111110100010000011001100011011011010011010100011010010100111100100011101010100010001000111100111011011101010001101))
;; x should be 109411603019490209345796382451637795469
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EA49FD1)))
(check-sat)
(exit)
