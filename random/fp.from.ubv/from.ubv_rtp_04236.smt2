(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b01010010001011100111111001000000000000110111101011010001011100101010011100111011100001101111001001101001110001001011100011010010))
;; x should be 109238101965145097741446742842508884178
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTP x)))
(assert (fp.leq r (fp #b0 #b11111101 #b01001000101110011111101)))
(check-sat)
(exit)
