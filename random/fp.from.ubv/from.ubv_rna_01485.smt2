(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1000010000111000001110101101000001010000011100110110010101010011))
;; x should be -8919314395629918893
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r (fp #b1 #b10111101 #b11101111000111110001010)))
(check-sat)
(exit)
