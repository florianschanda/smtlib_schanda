(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1110000100100000010001011110000010001111100101110010010011011100))
;; x should be -2224701385136986916
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.geq r (fp #b1 #b10111011 #b11101101111110111010000)))
(check-sat)
(exit)
