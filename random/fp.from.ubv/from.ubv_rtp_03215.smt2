(declare-const x (_ BitVec 64))
(assert (bvule x #b0101011100000001010110000111011011100100001110000010101000110101))
;; x should be 6269389423934712373
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTP x)))
