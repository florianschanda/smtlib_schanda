(declare-const x (_ BitVec 64))
(assert (= x #b0101101110111100011001001010000010010100000100110110010011000101))
;; x should be 6610268993914889413
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
