(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b00111101010110011001001001000101))
;; x should be 1029280325
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNA x)))
(assert (fp.geq r (fp #b0 #b10000011100 #b1110101011001100100100100010100000000000000000000000)))
(check-sat)
(exit)
