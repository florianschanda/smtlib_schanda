(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10110111011010100010010101011000011010000110110111100101001110001111011001000010111101110110010101110001001110111011011000100111))
;; x should be 243799864149197229245533463423131301415
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTZ x)))
(assert (fp.eq r (fp #b0 #b11111110 #b01101110110101000100101)))
(check-sat)
(exit)
