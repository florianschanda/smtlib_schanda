(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b00010111010010111110100001110101010111100000111110100000111100001110011001111111000010011010011011011010101111111101101101100101))
;; x should be 30966380985276723338358701112361474917
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTN x)))
(assert (fp.leq r (fp #b0 #b11111011 #b01110100101111101000011)))
(check-sat)
(exit)
