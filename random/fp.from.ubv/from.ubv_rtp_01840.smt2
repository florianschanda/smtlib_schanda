(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10000001010110011010000011101110100110011011000100111000000101110100001011110111011000010010011111000010111100010011101011101001))
;; x should be -168346576954870002283302932035218097431
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.leq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
