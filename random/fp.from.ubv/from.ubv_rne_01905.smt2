(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11000110010000010110100110111010010000100101011011010011100100010110010011100100101110000110110100111110010010000001001111111100))
;; x should be -76755580049742768383933473068596849668
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.geq r ((_ to_fp 11 53) #xC7CCDF4B22DED496)))
(check-sat)
(exit)
