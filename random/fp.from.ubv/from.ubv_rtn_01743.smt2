(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01011101000000101001110100100000011100000011010010000010101111111001101101010010011100011100001011100000001110010001011011010001))
;; x should be 123631775110049022288800112963019675345
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTN x)))
(assert (fp.leq r ((_ to_fp 5 11) #x7BFF)))
(check-sat)
(exit)
