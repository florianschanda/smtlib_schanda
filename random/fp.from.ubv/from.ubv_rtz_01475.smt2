(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1001010011000010110010101111101010010101100110100110011000000110))
;; x should be -7727390833013791226
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC3DACF4D415A9966)))
(check-sat)
(exit)
