(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10111101110100101001101111000110010111101111111111101011100110101010010000000001100101101000011000100110111000110010010010101000))
;; x should be 252317633033707137230013614985467602088
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNA x)))
(assert (fp.eq r ((_ to_fp 11 53) #x47E7BA5378CBDFFD)))
(check-sat)
(exit)
