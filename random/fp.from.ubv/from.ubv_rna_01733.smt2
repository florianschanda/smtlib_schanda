(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01111110101101000101001101000101101000010010100100001111110100101101011100000010101111001100011010010100100111011001111111001001))
;; x should be 168419029860052712796547068124699926473
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNA x)))
(assert (fp.eq r ((_ to_fp 5 11) #x7C00)))
(check-sat)
(exit)
