(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11100100011000100010000100110110111111100010101101000011001111001100011101001001000011100111100101110110110010010110101110111100))
;; x should be -36708865113342041233728290735816873028
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r (fp #b1 #b11111011 #b10111001110111011110110)))
(check-sat)
(exit)
