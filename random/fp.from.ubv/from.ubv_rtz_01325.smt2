(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b0100100110101000001001100000010111010100111100100000000010001000))
;; x should be 5307533967345451144
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.eq r (fp #b0 #b10111101 #b00100110101000001001100)))
(check-sat)
(exit)
