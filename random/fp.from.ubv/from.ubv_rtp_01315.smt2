(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0101011110010101101111101000001110110100111011111101111011100000))
;; x should be 6311159925715230432
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r (fp #b0 #b10111101 #b01011110010101101111110)))
(check-sat)
(exit)
