(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10111010101010101010011111001000000111100011010001010001111111011111100110110101011001111101110110101110100110101100110001001011))
;; x should be -92159866221609076090622913987251352501
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFE8AAAB0)))
(check-sat)
(exit)
