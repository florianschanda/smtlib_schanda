(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01001110010010110101000101000101110011011101001101011001100101001100000001011011000100010010010000100001001000110111010011101011))
;; x should be 104070854341234592824090586885361202411
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
(assert (fp.geq r (fp #b0 #b11110 #b1111111111)))
(check-sat)
(exit)
