(declare-const x (_ BitVec 64))
(assert (= x #b1101100101001000000101110101101101010010011110101011111000010000))
;; x should be 15656789785546767888
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTP x)))
