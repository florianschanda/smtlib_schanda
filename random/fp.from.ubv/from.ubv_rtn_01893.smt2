(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10101010110001111110011111111100111001100001010010101000000101000011100100100000011110001111000110001001110001000011101011110011))
;; x should be -113275635289332412735246748023481156877
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.leq r ((_ to_fp 11 53) #xC7D54E0600C67AD6)))
(check-sat)
(exit)
