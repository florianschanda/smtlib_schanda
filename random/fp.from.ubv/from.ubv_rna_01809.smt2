(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10001011111111000010010111110011111011110001100010111010010100010001000100101000001101001101010010111111101011110110011011100100))
;; x should be -154210446922888743066755417262393432348
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r (fp #b1 #b11111101 #b11010000000011110110100)))
(check-sat)
(exit)
