(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNA x)))
(assert (fp.eq r (fp #b0 #b10000111110 #b1001001101100011100111000100010101100111010001010110)))
(check-sat)
(exit)
