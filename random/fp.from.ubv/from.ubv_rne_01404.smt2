(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1001101001111011110110001001101010011110110010110110111001101100))
;; x should be -7315015011132543380
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.leq r (fp #b1 #b10111101 #b10010110000100001001111)))
(check-sat)
(exit)
