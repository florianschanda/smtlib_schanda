(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b01111100000111100010001100110000001100001110001110001010011010101001000001111000101000011001111100110100100110000101100000111100))
;; x should be 164980754085503901846897873198064883772
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTP x)))
(assert (fp.geq r ((_ to_fp 11 53) #x47DF0788CC0C38E3)))
(check-sat)
(exit)
