(declare-const x (_ BitVec 64))
(assert (bvule x #b0110110101100101100111111000100111100001111101011000011100010011))
;; x should be 7882882137332221715
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTN x)))
(assert (fp.leq r (fp #b0 #b10000111101 #b1011010110010110011111100010011110000111110101100001)))
(check-sat)
(exit)
