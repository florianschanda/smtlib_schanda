(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11001100110111111111001111111100101011100110100001101111010001110101011111110110000110010000011110011111100111011101101000001000))
;; x should be -67957024936355072386418901495641679352
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFE4C8030)))
(check-sat)
(exit)
