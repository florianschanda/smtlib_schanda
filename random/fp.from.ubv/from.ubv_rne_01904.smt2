(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11000011101110101011011010011000001101000001111110001011110101011111110100100001010011100010101011000010011010000101011011010000))
;; x should be -80113437069832465968573536171143440688
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r ((_ to_fp 8 24) #xFE711526)))
(check-sat)
(exit)
