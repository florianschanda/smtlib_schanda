(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10110101111111110011101011011100001100101111101110111011101100111101010110000100100011110110110011010010101001111110100111101000))
;; x should be 241915496761727496247578805096250075624
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNA x)))
(assert (fp.eq r ((_ to_fp 11 53) #x47E6BFE75B865F77)))
(check-sat)
(exit)
