(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00010111110011001000010101101011111010101001001111000111101011000011010001000010010110011111111001011101110110001000110110100111))
;; x should be 31634178572682991932672130626419723687
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.eq r (fp #b0 #b11111011 #b01111100110010000101011)))
(check-sat)
(exit)
