(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10010100001100010101011000110111010011011110001000010100010101110110010010111110111000010100010000011110011111011111111010011001))
;; x should be -143300452329824196340685785458637144423
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTN x)))
(assert (fp.geq r ((_ to_fp 5 11) #xFC00)))
(check-sat)
(exit)
