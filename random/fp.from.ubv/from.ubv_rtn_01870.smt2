(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10100111011110100100011100111100000110110010011110010100001111101010111111010010001001110010001010011001111011010010111001111110))
;; x should be -117666386594940710154549635543129575810
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTN x)))
(assert (fp.geq r ((_ to_fp 5 11) #xFC00)))
(check-sat)
(exit)
