(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0001101000101010100110101010101111010011011110100101110111111111))
;; x should be 1885489456786071039
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r (fp #b0 #b10111011 #b10100010101010011010101)))
(check-sat)
(exit)
