(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b01001101011100010111001010001011100111110010111000110011001110000110110100010011010111100111100011000011001001111100011000110101))
;; x should be 102939608477126332539652508212416464437
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7E9AE2E5)))
(check-sat)
(exit)
