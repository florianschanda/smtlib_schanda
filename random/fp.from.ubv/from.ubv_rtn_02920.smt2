(declare-const x (_ BitVec 64))
(assert (bvule x #b0000000101111001111001111101111001001001111010000101111011111000))
;; x should be 106371008128638712
(declare-const r Float16)
(assert (= r ((_ to_fp_unsigned 5 11) RTN x)))
