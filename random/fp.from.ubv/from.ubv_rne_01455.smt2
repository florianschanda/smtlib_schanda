(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1000101100010111100000111100010000000001010001010000110100011110))
;; x should be -8424119700115092194
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r ((_ to_fp 8 24) #xDEE9D0F8)))
(check-sat)
(exit)
