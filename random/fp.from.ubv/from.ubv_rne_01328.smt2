(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0101000100001101001100010011010011110001001100100010110000010001))
;; x should be 5840378395224058897
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.geq r ((_ to_fp 11 53) #x43D4434C4D3C4C8B)))
(check-sat)
(exit)
