(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b11010011000001001010010011100101000000010100110000000101101111101100001110111101000100111101100110100111110011100110111100000100))
;; x should be 280491220756876993636630544647826861828
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7F5304A5)))
(check-sat)
(exit)
