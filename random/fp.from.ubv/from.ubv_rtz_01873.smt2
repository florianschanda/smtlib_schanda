(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10101001000101111110110110100101000100111111101100000000010010010100011111101011010100001111000010011110000110010101010110010110))
;; x should be -115518592795634842688545363995497245290
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.eq r ((_ to_fp 8 24) #xFEADD024)))
(check-sat)
(exit)
