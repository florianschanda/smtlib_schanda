(declare-const x (_ BitVec 64))
(assert (bvule x #b1111010100011010101111100100101110011000101100111101011101111010))
;; x should be 17661638120580568954
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNE x)))
