(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0101110111100000101111010100010110110110100011010101000010101100))
;; x should be 6764614747423592620
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r (fp #b0 #b10000111101 #b0111011110000010111101010001011011011010001101010100)))
(check-sat)
(exit)
