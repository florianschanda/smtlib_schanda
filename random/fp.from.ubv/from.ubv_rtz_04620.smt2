(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b11110100101000010011111110000110000010100000111011000101101111111000010111110100001111010101011111110011111111000011001000001100))
;; x should be 325168879177235096102245985072780685836
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTZ x)))
(assert (fp.leq r ((_ to_fp 11 53) #x47EE9427F0C141D8)))
(check-sat)
(exit)
