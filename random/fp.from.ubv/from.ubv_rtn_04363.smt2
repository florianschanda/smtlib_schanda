(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10111000000110111101110101011110110110100111010000110100101100111100101100101010100100011100111101010010010000111010011001010000))
;; x should be 244722633167182860736074742868312565328
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTN x)))
