(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b01011100011001110111111111011111011000010000001101000110000110011001100011000000011001111011100010110111100000010010001110110111))
;; x should be 122826375752565255763938645132991079351
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTN x)))
(assert (fp.geq r (fp #b0 #b11111101 #b01110001100111011111111)))
(check-sat)
(exit)
