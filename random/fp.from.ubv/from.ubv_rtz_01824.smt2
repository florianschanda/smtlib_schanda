(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10001111010111000001110000010110010011011000001101110000010100100000011000110010001100001000100100010110000100011011111000100100))
;; x should be -149724502538232566478979439731843940828
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.eq r (fp #b1 #b11111101 #b11000010100011111000111)))
(check-sat)
(exit)
