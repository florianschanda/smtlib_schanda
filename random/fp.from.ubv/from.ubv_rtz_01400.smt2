(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b0111100001010100011011100010000110010011010111000111011010100011))
;; x should be 8670676273080333987
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.leq r (fp #b0 #b10000111101 #b1110000101010001101110001000011001001101011100011101)))
(check-sat)
(exit)
