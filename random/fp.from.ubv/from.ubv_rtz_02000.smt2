(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b11001010110000011111010101010010010110101111100101100001101010111101001110011010010010111011111001101010100111111100111011101111))
;; x should be -70771222763470879923374889137157976337
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.eq r ((_ to_fp 8 24) #xFE54F82A)))
(check-sat)
(exit)
