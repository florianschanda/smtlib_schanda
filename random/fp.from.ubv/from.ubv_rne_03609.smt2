(declare-const x (_ BitVec 64))
(assert (bvule x #b1100111010010100001000000001000011110010100100100010000111100000))
;; x should be 14885557925527560672
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNE x)))
