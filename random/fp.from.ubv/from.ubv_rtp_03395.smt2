(declare-const x (_ BitVec 64))
(assert (= x #b1001101001001010001010111010011010001010100001110111111011110100))
;; x should be 11117746624406191860
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTP x)))
