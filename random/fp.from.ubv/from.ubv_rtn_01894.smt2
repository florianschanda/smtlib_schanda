(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10101001101101111000111010100100100101110110101111000001111110001010000010110001101000000011010010110101100001010110111100110011))
;; x should be -114689752165730929061233760479431266509
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTN x)))
(assert (fp.leq r (fp #b1 #b11111 #b0000000000)))
(check-sat)
(exit)
