(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11100101010010111101100000011101000110001000010111100100010011010001101100101000010000100100111110110001110011000000010100010011))
;; x should be -35495350316122009330150338351305325293
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFDD5A13F)))
(check-sat)
(exit)
