(assert (= x #b0111010000100111))
;; x should be 29735
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTP x)))
(assert (fp.eq r (fp #b0 #b10000001101 #b1101000010011100000000000000000000000000000000000000)))
(check-sat)
(exit)
