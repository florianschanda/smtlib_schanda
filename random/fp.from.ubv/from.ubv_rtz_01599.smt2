(declare-const x (_ BitVec 64))
(assert (bvsge x #b1110011111110111110101001100100111010101111001001010010010100011))
;; x should be -1731681568358619997
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
