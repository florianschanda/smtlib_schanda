(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0100101011010101010001010111100000011001111111101101110101001011))
;; x should be 5392292510980562251
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r (fp #b0 #b10000111101 #b0010101101010101000101011110000001100111111110110111)))
(check-sat)
(exit)
