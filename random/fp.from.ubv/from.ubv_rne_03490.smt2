(declare-const x (_ BitVec 64))
(assert (= x #b1001110100000001100110101011111011111010100010001000100100100010))
;; x should be 11313493883969112354
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNE x)))
