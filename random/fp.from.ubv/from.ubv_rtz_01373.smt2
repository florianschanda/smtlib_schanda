(declare-const x (_ BitVec 64))
(assert (bvsge x #b0101010111101111010001000001111001110110010001111110001010101110))
;; x should be 6192242910281851566
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
