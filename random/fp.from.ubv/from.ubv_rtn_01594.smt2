(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1111001001010001111100110101010000011101101101100010101011010011))
;; x should be -985739300816147757
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.eq r ((_ to_fp 11 53) #xC3AB5C1957C493AB)))
(check-sat)
(exit)
