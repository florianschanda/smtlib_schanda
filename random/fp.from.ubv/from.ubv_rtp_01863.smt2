(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10111110101110000100000000100000000110000111111110110010101000011110101011101110101110000101110011011100101110101101000010101010))
;; x should be -86772364482736187781743962059457245014
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFE828F7F)))
(check-sat)
(exit)
