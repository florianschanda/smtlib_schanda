(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0000001001111101110010001111010011000001000100011010111101001101))
;; x should be 179520513701424973
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.geq r ((_ to_fp 8 24) #x5C1F723D)))
(check-sat)
(exit)
