(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1011010100000000100011010100101010100111100111100011010001100000))
;; x should be -5404164201065335712
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.eq r (fp #b1 #b10111101 #b00101011111111011100101)))
(check-sat)
(exit)
