(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01010100011101111010010011000001111111111100000110000101010101100110000011111110011111100010101111100110110000101101111000011100))
;; x should be 112276376657461571794461595386037591580
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.eq r ((_ to_fp 8 24) #x7EA8EF49)))
(check-sat)
(exit)
