(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01100111110101111101000011101110111101001111001010101010010011111001110011001100101011100001100011001010011101110101001011000100))
;; x should be 138031065063739267507235115889916859076
(declare-const r Float16)
