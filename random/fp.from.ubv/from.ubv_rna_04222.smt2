(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01100101001111101111100000000101011100100100101011101111011010111011000000100001101110011110110011010010001000001100110000100010))
;; x should be 134578980448600062924207702524142079010
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNA x)))
(assert (fp.eq r ((_ to_fp 11 53) #x47D94FBE015C92BC)))
(check-sat)
(exit)
