(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b10001000100000000001100001000110011011001111111101010011101101011110111010101101001111111001111000110001110011001001001101111001))
;; x should be -158842253138762530716226911536915115143
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNA x)))
