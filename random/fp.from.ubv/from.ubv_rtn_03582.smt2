(declare-const x (_ BitVec 64))
(assert (bvule x #b1001010001011011110110000110000100111010100110000001010111110011))
;; x should be 10690376052600477171
(declare-const r Float16)
(assert (= r ((_ to_fp_unsigned 5 11) RTN x)))
