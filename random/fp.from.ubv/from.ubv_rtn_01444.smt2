(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b1010100111010011000001001010100111000100110011001110100100100001))
;; x should be -6209614334016034527
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.eq r (fp #b1 #b10111101 #b01011000101100111110111)))
(check-sat)
(exit)
