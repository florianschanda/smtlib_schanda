(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b11010100010101000000000111010101100010010100101110100000100100001110100001101011000110111110010000110101110111101000000000010110))
;; x should be -58049841677920279663285329921482784746
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.eq r (fp #b1 #b11111100 #b01011101010111111111000)))
(check-sat)
(exit)
