(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11100000001010010011111101110111111101100111001111111001110011100100110001111000001101101011101001011011000101110110100111101000))
;; x should be -42321124397687492187358895965590820376
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
