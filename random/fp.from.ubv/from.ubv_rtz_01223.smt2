(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b0011100001111000011011011000001110001010100100010100001111011000))
;; x should be 4069122675062162392
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.geq r ((_ to_fp 11 53) #x43CC3C36C1C548A1)))
(check-sat)
(exit)
