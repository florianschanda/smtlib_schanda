(declare-const x (_ BitVec 64))
(assert (bvsge x #b0000110000100011101111000010100011111000110011110011001000101110))
;; x should be 874749636799050286
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
