(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00110011001111101011110001110001111100001110001110110110100010010001110010110111101100011111000100110100010011001011000100000111))
;; x should be 68116372310599409975121376893983305991
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTZ x)))
(assert (fp.eq r ((_ to_fp 8 24) #x7E4CFAF1)))
(check-sat)
(exit)
