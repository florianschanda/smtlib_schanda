(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00110010100101010001100100010011111011111001001001110000010101110110011001001110111010111010101001101111000100000110001111111001))
;; x should be 67235560660886613058874521780705321977
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.eq r (fp #b0 #b11111100 #b10010100101010001100100)))
(check-sat)
(exit)
