(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11101111110000100010100001100110011010101000000010111000000111011100011011110010000011001000100111101111111000001001000010000101))
;; x should be -21588750927170067746953200983429050235
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.leq r (fp #b1 #b11111011 #b00000011110111010111101)))
(check-sat)
(exit)
