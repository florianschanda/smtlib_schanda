(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01000001000101011011101110010111001100111101000111101110111011001011010011101000001010101011001011010111100001110100111101110100))
;; x should be 86512662750134714922109150544787885940
(declare-const r Float16)
