(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10001100110101100011000000011111100111100010110000000101111110110110101101100010000110000011111111110010110010000100111101001101))
;; x should be -153078319922637922384422762745905524915
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNE x)))
(assert (fp.eq r ((_ to_fp 5 11) #xFC00)))
(check-sat)
(exit)
