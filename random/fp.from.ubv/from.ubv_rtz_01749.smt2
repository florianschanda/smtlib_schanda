(assert (= x #b01000010010001101001010101001010101001000110001101001000110000010011001111011001100110011000100011010001100010000000101000110001))
;; x should be 88095536494692424541289549362591435313
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTZ x)))
(assert (fp.eq r (fp #b0 #b10001111101 #b0000100100011010010101010010101010010001100011010010)))
(check-sat)
(exit)
