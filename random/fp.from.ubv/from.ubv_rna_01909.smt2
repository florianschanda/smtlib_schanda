(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11100001001001100111000101010011010100101101001100101111001010010010000000001001010111000001001000100100001000100001110000001000))
;; x should be -41006462074852290424297271900935939064
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RNA x)))
(assert (fp.leq r ((_ to_fp 5 11) #xFC00)))
(check-sat)
(exit)
