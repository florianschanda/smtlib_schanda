(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvuge x #b11111101000010001110100010100111))
;; x should be 4245219495
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNA x)))
(assert (fp.geq r (fp #b0 #b10000011110 #b1111101000010001110100010100111000000000000000000000)))
(check-sat)
(exit)
