(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b10001010101101010111111101001111))
;; x should be -1967816881
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNA x)))
(assert (fp.geq r (fp #b1 #b10000011101 #b1101010100101010000000101100010000000000000000000000)))
(check-sat)
(exit)
