(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b10000110100011010001111101100000000011111001001010110101100110010010001110110100100011000100111111010000001010100111010101010101))
;; x should be 178849301657653088126134487540277343573
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTZ x)))
(assert (fp.leq r (fp #b0 #b11111110 #b00001101000110100011111)))
(check-sat)
(exit)
