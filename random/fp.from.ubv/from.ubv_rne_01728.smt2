(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01011010111100111010111000111101101001001100010011011010101011000001111101100010010001000010000000100100010000001110001110000101))
;; x should be 120895781780448864158193655778673484677
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EB5E75C)))
(check-sat)
(exit)
