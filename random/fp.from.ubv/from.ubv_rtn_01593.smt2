(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1100000111101111110110110111100010110111101100110011001001011011))
;; x should be -4472114593431080357
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.leq r ((_ to_fp 8 24) #xDE784093)))
(check-sat)
(exit)
