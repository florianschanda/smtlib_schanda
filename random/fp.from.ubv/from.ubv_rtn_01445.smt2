(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1010001101011110111111000011100011010000000000011100000111110101))
;; x should be -6674620276778352139
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r ((_ to_fp 8 24) #xDEB94208)))
(check-sat)
(exit)
