(declare-const x (_ BitVec 64))
(assert (bvuge x #b1100011011100000111010110001111101001011011100110101100100101000))
;; x should be 14330712533935282472
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTN x)))
