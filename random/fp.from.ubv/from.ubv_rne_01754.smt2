(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b01110001011000010101010011000111010011001101101001001011101001111010001010011110011101010101001000100001101111111000111101101110))
;; x should be 150708135831569182841925070832101461870
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.leq r ((_ to_fp 8 24) #x7EE2C2AA)))
(check-sat)
(exit)
