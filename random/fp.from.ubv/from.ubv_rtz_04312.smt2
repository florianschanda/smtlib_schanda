(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01110010010000000111001110101010001011010000111101011000100011100110001010111011101111001010110110110111000011110000110011110111))
;; x should be 151866644478264063370353487489818103031
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTZ x)))
(assert (fp.eq r (fp #b0 #b10001111101 #b1100100100000001110011101010100010110100001111010110)))
(check-sat)
(exit)
