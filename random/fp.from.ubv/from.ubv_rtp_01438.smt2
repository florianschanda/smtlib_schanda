(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1011000011110000101011011000111000010100100000011011100110000101))
;; x should be -5696862702882670203
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.geq r (fp #b1 #b10111101 #b00111100001111010100100)))
(check-sat)
(exit)
