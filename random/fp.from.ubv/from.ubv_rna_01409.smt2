(declare-const x (_ BitVec 64))
(assert (= x #b1001101111001010100010110101101001010100001001101101111001101000))
;; x should be -7220805832460018072
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
