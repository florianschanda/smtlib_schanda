(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11001110000100110010000110000010110100001110010101101010001000101100100001110010000011110001011110001011001010100011100011011100))
;; x should be -66362066465105357292748765550752417572
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTP x)))
(assert (fp.geq r ((_ to_fp 5 11) #xFBFF)))
(check-sat)
(exit)
