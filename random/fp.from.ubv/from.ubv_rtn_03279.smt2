(declare-const x (_ BitVec 64))
(assert (bvuge x #b0111011001101100011101101010101001110111100111011001100011110110))
;; x should be 8533325868483582198
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTN x)))
