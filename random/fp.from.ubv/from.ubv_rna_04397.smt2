(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10011011111111000000000100111001100011001111100101101001111000100100100000001100100011011110101101101100000101101011001011000001))
;; x should be 207338822997057027753926340967472214721
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNA x)))
(assert (fp.eq r (fp #b0 #b11111110 #b00110111111110000000001)))
(check-sat)
(exit)
