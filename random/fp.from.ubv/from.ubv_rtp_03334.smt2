(declare-const x (_ BitVec 64))
(assert (bvule x #b0111011101111001000110111110001001011000100001010100000111000011))
;; x should be 8608942821657100739
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTP x)))
