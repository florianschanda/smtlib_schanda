(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10011111001010010011000001000101000101100101011100100011110011101000110101010110010010001001101001010010111000111110001111001011))
;; x should be -128721252390618707119063239768208579637
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
