(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00110010011001111000000010011000001100110100110011110110000100000010100000011011100101100111000111001101110100101000110001110110))
;; x should be 66998814572661626336294644038505761910
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTN x)))
(assert (fp.eq r ((_ to_fp 8 24) #x7E499E02)))
(check-sat)
(exit)
