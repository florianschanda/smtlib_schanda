(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10000011110110100001110101110101111110100100111100000011111000100110001010000110011001001101100100101100100101111101001000011001))
;; x should be -165020981220913595684337536520537255399
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFEF84BC5)))
(check-sat)
(exit)
