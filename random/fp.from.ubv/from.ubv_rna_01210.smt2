(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b0001111100000101111001000011111011001011111110000101100101111100))
;; x should be 2235443748420475260
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.eq r (fp #b0 #b10111011 #b11110000010111100100010)))
(check-sat)
(exit)
