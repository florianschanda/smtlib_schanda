(declare-const x (_ BitVec 64))
(assert (bvule x #b0110001111010101110010100111000101010011010001010110111000010111))
;; x should be 7193878567871409687
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTN x)))
