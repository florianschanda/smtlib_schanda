(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01011110101100010010100000000100110101110101100001010001110001010111100101100101000011101000110001001111110101111111000101111100))
;; x should be 125867279827685601405238455733132456316
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EBD6250)))
(check-sat)
(exit)
