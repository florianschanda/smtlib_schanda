(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b11011100111010111101101101111000101001010100001111011110100100101001100110111010100101111110010001100110000111010000000100001001))
;; x should be -46627566680271509784191750301601169143
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
(assert (fp.leq r ((_ to_fp 8 24) #xFE0C5092)))
(check-sat)
(exit)
