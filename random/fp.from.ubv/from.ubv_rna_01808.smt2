(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10010101111000010111010101011100100111101100000011010000000001001011110110000111001111001001101110010110011100100101101101000100))
;; x should be -141056748375769355977718439466410812604
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNA x)))
