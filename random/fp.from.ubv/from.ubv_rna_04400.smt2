(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b10110111010010111111010011010100000110000001000000100100011000101100111100010000000001000100111100010110110110101000001010001001))
;; x should be 243643111204790615721880340766790419081
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNA x)))
(assert (fp.geq r (fp #b0 #b11111110 #b01101110100101111110101)))
(check-sat)
(exit)
