(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvuge x #b1000011011110010110100010100110011001011110111011000000000011101))
;; x should be 9724064673214332957
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNE x)))
(assert (fp.geq r ((_ to_fp 11 53) #x43E0DE5A29997BB0)))
(check-sat)
(exit)
