(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b10101001110011001010111100100010011000110000000000001101101100000111111010001000110110111101011001000011010101000100100010011111))
;; x should be 225702311992869132503097458628941334687

(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTP x)))
