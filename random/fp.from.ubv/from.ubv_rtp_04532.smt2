(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10010001110100001011101001000100101101001110001110110101100111000010110010100111011000100001100110111000100100100110100011100111))
;; x should be 193821835107071961456597933945838004455
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTP x)))
(assert (fp.eq r ((_ to_fp 8 24) #x7F11D0BB)))
(check-sat)
(exit)
