(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1011101101000000110110010001000010000101010101111111110100000111))
;; x should be -4953720925127705337
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.leq r ((_ to_fp 8 24) #xDE897E4D)))
(check-sat)
(exit)
