(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b0100001100000011001100111001110110101001111100110010101101010000))
;; x should be 4828759977725471568
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.eq r ((_ to_fp 11 53) #x43D0C0CCE76A7CCA)))
(check-sat)
(exit)
