(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01110110111000010100010101010010001001101010001101011100000100001010000010001101011110001111100001101111100110101011010001100001))
;; x should be 158018576290720307400245192198033421409
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.eq r (fp #b0 #b11111101 #b11011011100001010001011)))
(check-sat)
(exit)
