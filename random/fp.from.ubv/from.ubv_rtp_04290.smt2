(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01110011110101011010011010111000000000011010111100000100001110010011100111100110100111011100111110010010010010100011001000111111))
;; x should be 153970560204630419444099744580130976319
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTP x)))
(assert (fp.eq r ((_ to_fp 11 53) #x47DCF569AE006BC2)))
(check-sat)
(exit)
