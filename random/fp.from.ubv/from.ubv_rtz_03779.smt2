(declare-const x (_ BitVec 64))
(assert (bvule x #b1110011011011100110111111001100101110110110011101011011010000110))
;; x should be 16635416973816018566
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTZ x)))
