(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b11001000100101110010101011101101000110100001110111000011000100010000000011101101001101101110100100000111100100011110110010011001))
;; x should be 266630506628982393082724707267003935897
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTZ x)))
(assert (fp.geq r (fp #b0 #b10001111110 #b1001000100101110010101011101101000110100001110111000)))
(check-sat)
(exit)
