(declare-const x (_ BitVec 64))
(assert (= x #b0000101000011110101011000100001011110110100000100100011001011110))
;; x should be 729209593284150878
(declare-const r Float16)
(assert (= r ((_ to_fp_unsigned 5 11) RTZ x)))
