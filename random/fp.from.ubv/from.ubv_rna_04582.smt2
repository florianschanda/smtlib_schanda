(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b11001110001100011100000100110100101011101100011000000010010001000111110100011001011110011110000001111101110111011000101010001000))
;; x should be 274079308356768601615443296842160114312
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNA x)))
