(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b01001101110100100100110001010111111001110010010010010001110100011111010111000101001000111010000111001000010000000011111011110010))
;; x should be 103442486443246099497990103817170992882
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTN x)))
(assert (fp.geq r (fp #b0 #b11111101 #b00110111010010010011000)))
(check-sat)
(exit)
