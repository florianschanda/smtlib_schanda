;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b01000001001101001010110111100000100011000100100010010000010000101101110101100010110000110111100000011111010010110011110010001100))
;; x should be 86673345810048802997837741322236804236

(declare-const r Float16)
(assert (= r ((_ to_fp_unsigned 5 11) RNE x)))
