(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b01010001100100011010110100111001011111100101111111100011001010001100111101010111100100111111000110010001111000011111010001111000))
;; x should be 108423864115043462683750695300256363640
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNE x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EA3235A)))
(check-sat)
(exit)
