(declare-const x (_ BitVec 64))
(assert (bvuge x #b0101000001011101000000011110000111001001111110000111000101111101))
;; x should be 5790786765136097661
(declare-const r Float16)
(assert (= r ((_ to_fp_unsigned 5 11) RTP x)))
