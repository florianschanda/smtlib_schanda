(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b11101011001000100110110011110111))
;; x should be -350065417
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTN x)))
(assert (fp.geq r (fp #b1 #b10000011011 #b0100110111011001001100001001000000000000000000000000)))
(check-sat)
(exit)
