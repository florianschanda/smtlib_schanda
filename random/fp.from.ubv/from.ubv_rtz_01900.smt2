(declare-const x (_ BitVec 128))
(assert (bvsle x #b10111101111011001000001111110110001100100110000011000000000111001011111011011100110001010100101111110010111011100000001010000011))
;; x should be -87830217157597872691992403638526016893
(declare-const r Float32)
