(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsge x #b1000011000110001100100110101110111010101011010001111100001111110))
;; x should be -8777072167546718082
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (fp.geq r (fp #b1 #b10111101 #b11100111001110011011001)))
(check-sat)
(exit)
