(declare-const x (_ BitVec 64))
(assert (= x #b0110000110001111001111000000101101111001111110100000101010001001))
;; x should be 7029903563337370249
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTP x)))
