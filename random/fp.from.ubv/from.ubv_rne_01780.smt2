(assert (= x #b01101001000111100110001000000111010110011010000101111011100011010010111000111011000111000111111111010000100001000000100111011011))
;; x should be 139726696721649893333764827662443088347
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
