(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b10000010000010010110001000010000))
;; x should be -2113314288
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.geq r (fp #b1 #b10000011101 #b1111011111011010011101111100000000000000000000000000)))
(check-sat)
(exit)
