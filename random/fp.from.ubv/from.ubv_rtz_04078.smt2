(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00000011111011111101011100111100110001111100010010001110000000001011010101001101011111011000011100100000010100010011011010000011))
;; x should be 5233008470124244391128553672670262915
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTZ x)))
(assert (fp.eq r (fp #b0 #b11111000 #b11110111111010111001111)))
(check-sat)
(exit)
