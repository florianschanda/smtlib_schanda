(declare-const x (_ BitVec 64))
(assert (bvsge x #b1010011001011111101000000101110101110110001100010100100101010001))
;; x should be -6458267017350657711
(declare-const r Float16)
(assert (= r ((_ to_fp 5 11) RTZ x)))
