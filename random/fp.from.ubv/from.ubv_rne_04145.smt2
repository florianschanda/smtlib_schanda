(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b01111001011011101001101111010111000110000010010111000001011101100010010001111100110010100001010100010000011011111100001111100101))
;; x should be 161410900959430442323361519268443177957
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNE x)))
(assert (fp.geq r ((_ to_fp 8 24) #x7EF2DD38)))
(check-sat)
(exit)
