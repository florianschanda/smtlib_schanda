(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b10101101010111010100100100101001010011000110010101101000110111001000101000010101111000100000101101100001011010010111000100000110))
;; x should be 230440810766533372087142071990946984198
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RTP x)))
(assert (fp.leq r (fp #b0 #b10001111110 #b0101101010111010100100100101001010011000110010101110)))
(check-sat)
(exit)
