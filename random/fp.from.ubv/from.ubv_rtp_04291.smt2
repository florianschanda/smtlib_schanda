(set-logic QF_FPBV)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvule x #b01001101100101100010101011100010010101101100001001000110010101110110010011100000011100010000101100111100000001001001001111110011))
;; x should be 103130269997837402583201203366217946099
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RTP x)))
(assert (not (fp.leq r ((_ to_fp 8 24) #x7E9B2C56))))
(check-sat)
(exit)
