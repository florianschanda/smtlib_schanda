(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b01111010101011100100110110011001010010111011010111111010100001000111110011100011010100001111000010100111101001100010100001001111))
;; x should be 163070849030024516708236252116955834447
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
