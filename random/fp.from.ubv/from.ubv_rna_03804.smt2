(declare-const x (_ BitVec 64))
(assert (bvule x #b1100001100101110101010101011101000110100110010101000010110111111))
;; x should be 14064366403050964415
(declare-const r Float16)
(assert (= r ((_ to_fp_unsigned 5 11) RNA x)))
