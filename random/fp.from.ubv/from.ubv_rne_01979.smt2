(set-logic QF_FPBV)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11011100000001010000000101000111011101101100011010010110111000110001101111010110010010111100110010010011110111100001001011001111))
;; x should be -47826220419595844106364384708541934897
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RNE x)))
(assert (not (fp.geq r ((_ to_fp 8 24) #xFE0FEBFB))))
(check-sat)
(exit)
