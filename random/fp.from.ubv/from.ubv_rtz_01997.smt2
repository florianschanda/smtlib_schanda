(set-logic QF_FPBV)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status unsat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsge x #b11111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111))
;; x should be -1
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (not (fp.geq r ((_ to_fp 8 24) #xBF800000))))
(check-sat)
(exit)
