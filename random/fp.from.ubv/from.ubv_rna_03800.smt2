(declare-const x (_ BitVec 64))
(assert (bvule x #b1111110000011100101001100001100011011110111010001100111110101111))
;; x should be 18166577622654963631
(declare-const r Float16)
(assert (= r ((_ to_fp_unsigned 5 11) RNA x)))
