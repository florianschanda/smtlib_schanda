(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (= x #b0101000110110001001101100110111100000100100000001011000011110101))
;; x should be 5886546038394761461
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.eq r ((_ to_fp 11 53) #x43D46C4D9BC1202D)))
(check-sat)
(exit)
