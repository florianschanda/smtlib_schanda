(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00110101001111011010110000001010101111011101010010100011000100011010111011100010011001100100110000111101100000101101111110110001))
;; x should be 70769303310454348181124052897826529201
(declare-const r Float32)
(assert (= r ((_ to_fp_unsigned 8 24) RNA x)))
(assert (fp.eq r (fp #b0 #b11111100 #b10101001111011010110000)))
(check-sat)
(exit)
