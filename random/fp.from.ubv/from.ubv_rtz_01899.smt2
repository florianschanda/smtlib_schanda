(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b10110010010010001110010010101100100001111111101010000011010100001010111100011010000111000111100110011010001011101101111100000111))
;; x should be -103301300238692017563768576896866001145
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.eq r ((_ to_fp 8 24) #xFE9B6E36)))
(check-sat)
(exit)
