(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (= x #b11110100001111010000101100010101))
;; x should be -197326059
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RTP x)))
(assert (fp.eq r (fp #b1 #b10000011010 #b0111100001011110100111010110000000000000000000000000)))
(check-sat)
(exit)
