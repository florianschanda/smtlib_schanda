(declare-const x (_ BitVec 64))
(assert (bvuge x #b1101001001011110001100100010000110111101001111111111110110001100))
;; x should be 15158608516266065292
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNA x)))
