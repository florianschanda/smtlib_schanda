(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvuge x #b00100111101010010101110100000010101010010000010011001001111111100000011011010101000101010010000000101101000010011101110110101001))
;; x should be 52719276479562325791062381126335716777
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNA x)))
(assert (fp.geq r ((_ to_fp 11 53) #x47C3D4AE81548265)))
(check-sat)
(exit)
