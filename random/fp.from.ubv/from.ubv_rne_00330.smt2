(declare-const x (_ BitVec 8))
(assert (bvsge x #b11010001))
;; x should be -47
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (not (fp.geq r (fp #b1 #b10000000100 #b0111100000000000000000000000000000000000000000000000))))
(check-sat)
(exit)
