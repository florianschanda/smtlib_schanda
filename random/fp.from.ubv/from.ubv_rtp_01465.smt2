(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 64)
(declare-const x (_ BitVec 64))
(assert (bvsle x #b1011110011001101100010010000011001101111111100011110100001010110))
;; x should be -4842063363612481450
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.leq r ((_ to_fp 8 24) #xDE8664ED)))
(check-sat)
(exit)
