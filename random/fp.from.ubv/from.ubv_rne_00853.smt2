(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 32)
(declare-const x (_ BitVec 32))
(assert (bvsge x #b00000010011001000010110010010110))
;; x should be 40119446
(declare-const r Float64)
(assert (= r ((_ to_fp 11 53) RNE x)))
(assert (fp.geq r (fp #b0 #b10000011000 #b0011001000010110010010110000000000000000000000000000)))
(check-sat)
(exit)
