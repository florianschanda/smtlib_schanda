(declare-const x (_ BitVec 64))
(assert (bvsle x #b1111000010111010011001111001110001010011000101100110000110010000))
;; x should be -1100453237832130160
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
