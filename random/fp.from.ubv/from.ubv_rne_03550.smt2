(declare-const x (_ BitVec 64))
(assert (= x #b1010010011101100000010011011001001100000111101100000000100001011))
;; x should be 11883884178459459851
(declare-const r Float64)
(assert (= r ((_ to_fp_unsigned 11 53) RNE x)))
