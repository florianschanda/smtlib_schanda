(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (bvsle x #b10010111001111010111111010111101011011111101100000000101001000110111010000100011111001000110010101011101110111000111011111100110))
;; x should be -139249638856698778530082855405035423770
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTZ x)))
(assert (fp.leq r (fp #b1 #b11111101 #b10100011000010100000010)))
(check-sat)
(exit)
