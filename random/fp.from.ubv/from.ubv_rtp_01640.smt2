(set-logic QF_FPBV)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
;; fp.from.ubv(BitVec 128)
(declare-const x (_ BitVec 128))
(assert (= x #b00001101000111011001110100101001001001111111010011110101111110111110000111001011001101010000111111100110110100110101011010000001))
;; x should be 17433728153129907655280926415234750081
(declare-const r Float32)
(assert (= r ((_ to_fp 8 24) RTP x)))
(assert (fp.eq r (fp #b0 #b11111010 #b10100011101100111010011)))
(check-sat)
(exit)
