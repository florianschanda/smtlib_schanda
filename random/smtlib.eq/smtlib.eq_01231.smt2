(set-logic QF_FP)
(set-option :produce-models true)
(set-info :source |Random FP created by PyMPF|)
(set-info :category random)
(set-info :status sat)
(declare-const x Float32)
(assert (= x (fp #b1 #b01010001 #b01000011000001111000011)))
(declare-const y Float32)
(assert (= y (fp #b0 #b10111011 #b10001101111110001110111)))
(declare-const result Bool)
(assert (= result (= x y)))
(assert (not result))
(check-sat)
(exit)
